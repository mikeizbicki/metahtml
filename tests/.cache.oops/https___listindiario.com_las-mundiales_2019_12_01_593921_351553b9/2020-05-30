        function setNavi(w) {

            document.getElementById('artab_zone1').style.display = 'none';
            document.getElementById('artab_zone2').style.display = 'none';
            document.getElementById('artab_zone3').style.display = 'none';

            document.getElementById('artab_zone' + w).style.display = 'block';

            if (w == 1) {
                if (document.getElementById('artab_1')) {
                    document.getElementById('artab_1').style.backgroundImage = 'url(/dr2/img/article/tab_barticulo.jpg)';
                    document.getElementById('artab_1').style.width = '72px';
                }
            } else {
                if (document.getElementById('artab_1')) {
                    document.getElementById('artab_1').style.backgroundImage = 'url(/dr2/img/article/tab_barticulo_on.jpg)';
                    document.getElementById('artab_1').style.width = '72px';
                }
            }

            if (w == 2) {
                if (document.getElementById('artab_2')) {
                    document.getElementById('artab_2').style.backgroundImage = 'url(/dr2/img/article/tab_bgaleria_on.jpg)';
                    document.getElementById('artab_2').style.width = '78px';
                    setGallery();
                }
            } else {
                if (document.getElementById('artab_2')) {
                    document.getElementById('artab_2').style.backgroundImage = 'url(/dr2/img/article/tab_bgaleria.jpg)';
                    document.getElementById('artab_2').style.width = '78px';
                }
            }

            if (w == 3) {
                if (document.getElementById('artab_3')) {
                    document.getElementById('artab_3').style.backgroundImage = 'url(/dr2/img/article/tab_binfografia_on.jpg)';
                    document.getElementById('artab_3').style.width = '78px';
                    setInfografia();
                }
            } else {
                if (document.getElementById('artab_3')) {
                    document.getElementById('artab_3').style.backgroundImage = 'url(/dr2/img/article/tab_binfografia.jpg)';
                    document.getElementById('artab_3').style.width = '78px';
                }
            }

        }

        function setGallery() {

            var galeria = '';
            var w = 657;
            var h = 693;
            var f = '/dr2/swf/galeriaArticulos_final.swf?x=' + escape('/dr2/dataGalArticulo.xml.aspx?g=' + galeria);

            MD_WFlash(f, 'GaleriaDiv', w, h, false);

        }
        function setInfografia() {

            var infografia = '';
            var w = infografia.split('|')[1];
            var h = infografia.split('|')[2];
            var e = infografia.split('|')[3];
            var f = '/stor/NPArticles/swf/' + infografia.split('|')[0] + '.' + e + '';

            if (e == 'swf') {
                MD_WFlash(f, 'InfoGrafiaDiv', w, h, false);
            } else {
                document.getElementById('InfoGrafiaDiv').innerHTML = '<img src="' + f + '" width="' + w + '" height="' + h + '" alt= "' + f + '" />';
            }

        }

        function setVideo() {
            var video = '';
            var vs = video.split('|')[2];
            var vw = video.split('|')[3];
            var vh = video.split('|')[4];
            var vt = video.split('|')[5];
            var ft = video.split('|')[6];
            var ti = video.split('|')[7];
            var li = '';
            var f = '/dr2/swf/videoArticulo6.swf?vd=' + escape(vs) + '&vw=' + escape(vw) + '&vh=' + escape(vh) + '&vt=' + escape(vt) + '&ft=' + escape(ft) + '&ti=' + (ti) + '&li=' + escape(li);
            MD_WFlash(f, 'article-video-box', 300, 263, true);
        }

    </script>
