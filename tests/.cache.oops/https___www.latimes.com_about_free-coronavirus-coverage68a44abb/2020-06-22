                .some(function (level) {
                    return subLevels.includes(level)
                })) {
                    googletag.pubads().setTargeting('sub_status', '1')
                } else {
                    googletag.pubads().setTargeting('sub_status', '0')
                }
            }
        })

        // add screen size targeting
        let screenSize
