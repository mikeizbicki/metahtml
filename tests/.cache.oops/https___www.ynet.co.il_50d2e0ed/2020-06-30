                jQuery('img.floatingPlayer_right_button').click(function(){
                    var selectedItem = jQuery('.floatingPlayerAll_items_big li.first_to_show'),
                    nextItem = selectedItem.prev();
                    if (!selectedItem.hasClass('first_in_data')){
                        var lastItem = jQuery('.floatingPlayerAll_items_big li.last_to_show'),
                        lastItem_prev = lastItem.prev();
                        lastItem.removeClass('last_to_show');
                        lastItem.css('display','none');
                        nextItem.addClass('first_to_show');
                        selectedItem.removeClass('first_to_show');
                        lastItem_prev.addClass('last_to_show');
                        nextItem.css('display','block');
                       }
                });

                jQuery('img.floatingPlayer_left_button').click(function(){
                    var selectedItem = jQuery('.floatingPlayerAll_items_big li.last_to_show'),
                    nextItem = selectedItem.next();
                        var numOfElementsInLine= jQuery('.floatingPlayerAll_items_big li').length
                        if (!selectedItem.hasClass('last_in_data') && numOfElementsInLine > 6 ){
                        var firstItem = jQuery('.floatingPlayerAll_items_big li.first_to_show'),
                        firstItem_next = firstItem.next();
                        firstItem.removeClass('first_to_show');
                        firstItem_next.addClass('first_to_show');
                        firstItem.css('display','none');
                        nextItem.css('display','block');
                        nextItem.addClass('last_to_show');
                        selectedItem.removeClass('last_to_show');
                    }
                });
               
