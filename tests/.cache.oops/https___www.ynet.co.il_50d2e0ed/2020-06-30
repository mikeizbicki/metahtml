              function floatingPlayer_pageRefreshEnable(){
                    
                  //  jQuery('#floatingPlayerPlayer_box').css({'pointer-events': 'none' , 'cursor':'not-allowed'});
              
        if (typeof(window.pageRefreshEnable)=='function'){
                window.pageRefreshEnable('ynettv');
            };
              
               
                };
                function floatingPlayer_pageRefreshDisable(){
                    
                   // jQuery('#floatingPlayerPlayer_box').css({'pointer-events': 'visible' , 'cursor':'pointer'});
                
            if (typeof(window.pageRefreshDisable)=='function'){
                window.pageRefreshDisable('ynettv');
            };
            

                
               }

               


                function main_movie_size(this_class,this_width,this_height){
                    jQuery('.'+ this_class).css('width',this_width);
                    jQuery('.'+ this_class).css('height',this_height);
                } 

                function replaceClassToBig(listToReplace) {
                   
                   listToReplace.forEach(function(entry) {
                        jQuery('.' + entry).removeClass(entry).addClass(entry +'_big');
                    });

                //var ifr = jQuery('#floatingPlayer > iframe')[0]
                //try { 
                //ifr.contentWindow.contentWindow.flpVolumeVal(1);
                //ifr.contentWindow.fpControlstgllCss('none')
                //}
                //catch(err) { console.log(err.message)
                //}
                
                
                
                };
                
                function find_movie_id() {
                    var mainPlayer = jQuery('.floatingPlayer');
                    if(mainPlayer.length ==0){
                        var mainPlayer = jQuery('.floatingPlayer_big');
                    }
                    var this_src = jQuery('iframe',mainPlayer).attr('src');
                    this_src_array = this_src.split("/");
                    path_array =  this_src_array[2].split(",");
                    path_array =  (path_array == 'live' || path_array == 'vast' ) ? this_src_array[3].split(",")  : path_array ;
                    movie_id =path_array[2].replace('--floating','-live'); 
                    return movie_id

                };
                function replaceClassFromBig(listToReplace) {
                    
                        
                try {   
                    if (0) {
                           var p =  videojs.players[window.YitVideo.crrntPlr]
                            p.pause() ;
                           if ( typeof p.ads == "object" && p.ads.isAdPlaying() ) {
                               p.ima.pauseAd();
                           }
                          }  
                jQuery(".yntTvHvrAbvInSmallFaze").css('display','');
                jQuery(".floatingPlayerMain_movie_big").attr('src', '')
                
                }
                catch(err) { console.log(err.message)
                }
                    
                    listToReplace.forEach(function(entry) {
                        var this_class = entry.replace('_big','');
                        jQuery('.' + entry).removeClass(entry).addClass(this_class);
                        
                   });
                
                floatingPlayer_pageRefreshEnable()
            
            };
                
                function playNextMovie_floatingPlayer() {
                    var selectedItem = jQuery('.floatingPlayerItem.selected'),
                    nextItem = selectedItem.next();
                    firstItem = jQuery('.floatingPlayerItem.first_in_data');
                    if(nextItem.length == 0) {
                        nextItem = firstItem;
                    };
                    //Analitics 
                    movie_id = find_movie_id(); 
                    nextItem.addClass('selected');
                    bunner_to_show(nextItem);
                    nextItem.click();
                    };

                function bunner_to_show(nextItem){
                    var start=0
                    var end=4
                    if(nextItem[0].id >= 5){
                        start=nextItem[0].id-4;
                        end = nextItem[0].id;
                    }
                    var ul = document.getElementById("floatingPlayerPlayer_box");
                    var items = ul.getElementsByTagName("li");
                    for (var i = 0; i < items.length; ++i) {
                        if(items[i].className.includes("first_to_show")){
                        items[i].classList.remove('first_to_show');
                        }
                        if(items[i].className.includes("last_to_show")){
                            items[i].classList.remove('last_to_show');
                        }  
                        if(i>=start && i<=end){
                            if(i==start){items[i].classList.add("first_to_show");}
                            if(i==end){items[i].classList.add("last_to_show");}
                            items[i].style.display = "block";
                        }else{items[i].style.display = "none";}
                    }
                }
                
                
