					<script>
						function open_paywall_login(){
                            YitPaywall.openLoginPopUp('https://premium.ynet.co.il/web/login');
                        }
						function open_paywall_menu() {
							var paywall_menu = document.getElementById("paywall_logout");
							if (paywall_menu.style.display === "none") {
								paywall_menu.style.display = "block";
							} else {
								paywall_menu.style.display = "none";
							}
						}					
						var userProsPayWall = '';
						if(YitPaywall.user){
							userProsPayWall = YitPaywall.user.props.firstName;
							document.getElementById('ynet_login').style.display = 'none';
							document.getElementById('paywall_login').style.display = 'block';
							document.getElementById('paywall_logout_container').style.display = 'block';
							document.getElementById('paywall_login_name').innerHTML = '&nbsp שלום '+userProsPayWall;
						}

						yq('.logout-btn-sargel').click(function(event) {
                            event.preventDefault();
                            YitPaywall.httpGet(YitPaywall.ApiRroutesConfig.Origin + YitPaywall.ApiRroutesConfig.logout+'?_=' + new Date().getTime(), 'DELETE', function(response) {
                              setTimeout(function(){ window.location.href = document.querySelector("link[rel='canonical']").href; }, 500);
                            });
                        });
						
