			 setTimeout(function () {
			       	(function worker_breakingnews() {
			  yq.ajax({
			    url: '/ticker/flash/breakingnews.html?content_id=160917', 
			    success: function(data) {
			      var news = JSON.parse(data);
					if(news[0].is_active_on == 0 && news[0].title && news[0].title.length > 0) {
						ShowMivzakon(news[0].title);
