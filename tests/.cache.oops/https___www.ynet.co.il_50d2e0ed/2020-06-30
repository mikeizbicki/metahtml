                   function yitvidCnstrct() {
                    window.YitVideo.Constructor({
                    containerAttrPrefix : 'yitVideo',
                    dynamicPlayerType : false,
                    flaotOnScroll: false,
                    nextVideo: false,
                    popularity: true,
                    siteName : 'ynet',
                    dcPath: dcPath,
                    poster:false,
                    autoplay:true,
                    adTagUrl:ad_tag,
                            eventsCallback:function(event, data){
                        switch(event) {
                         case 'firstplay':
                    dataLayer.push(data);
                    break;
					case 'contentplay':
                    break;
                        
                        }
                    }
                 });
                yitvidCnstrctHasCnstrcd = true ; 
            }

                function shorten(text, maxLength) {
if (text.length > 26 ) {                
var res = text.split(" ");
fLen = res.length;
text = "";
for (i = 0; i < fLen; i++) {
    text +=  res[i] + " ";
if (text.length > maxLength) {
var secondRowarray= res.slice(i) ;
var firstRowarray=res.slice(0,i);
var firstRow = firstRowarray.join(' ')
var secondRow = secondRowarray.join(' ')
break;
     }
   }        
                    if (secondRow.length > maxLength ) {
                        secondRow = secondRow.substr(0,maxLength-4) + "...";
                    }
                
                    return firstRow+' '+ secondRow;
                
                } else {return text }
}                       
        


                            function isScrolledIntoView(el) {
                    var elemTop = ( el.getBoundingClientRect().top + (el.clientHeight / 2 ) );
                    var elemBottom = ( el.getBoundingClientRect().bottom - (el.clientHeight / 2 ) ) ;
                    var isVisible = (elemTop >= 0) && (elemBottom <= window.innerHeight);
                    return isVisible;
                    }

