           //                   yitvidCnstrct() ;
            // }

                function insertAfter(newNode, referenceNode) {
                                    referenceNode.parentNode.insertBefore(newNode, referenceNode.nextSibling);
                    }
                
                function dispose(el){
                         el.parentNode.removeChild(el) ;
                }


                function CnstrctVdoOnThmbnlGstrRg(plr_id,yitVideoDomElmCrrnt) {
                if (yitVideoDomElmCrrnt.getAttribute("yitvideo") != plr_id) {
                                yitVideoDomElmCrrnt.style.display="none";
                                var el = document.createElement("div")
                                el.style.height='360px'
                                el.setAttribute("yitvideo",plr_id)
                                insertAfter(el,yitVideoDomElmCrrnt)
                                dispose(yitVideoDomElmCrrnt)
                                window.YitVideo.crrntPlr= `player_${plr_id}`
                         }
                }

                function CnstrctVdoOnThmbnlGstrLive(plr_id,yitVideoDomElmCrrnt) {
                if (yitVideoDomElmCrrnt.getAttribute("yitvideo") != "live") {
                                yitVideoDomElmCrrnt.style.display="none";
                                var el = document.createElement("div")
                                el.style.height='360px'
                                el.setAttribute("yitvideo","live" )
                                el.setAttribute("stream",YnetLiveObjArray[(plr_id - 1)])
                                el.setAttribute("title","")
                                el.setAttribute("ads","1")
                                el.setAttribute("poster","")
                                insertAfter(el,yitVideoDomElmCrrnt)
                                dispose(yitVideoDomElmCrrnt)
                                window.YitVideo.crrntPlr= "player_live" ;
                         }
                }


                function CnstrctVdoOnThmbnlGstr(plr_id,yitVideoDomElmCrrnt,fltngPlyr_liveORnot) {                                                                                                      

                    if (fltngPlyr_liveORnot=="1") {
                                CnstrctVdoOnThmbnlGstrLive(plr_id,yitVideoDomElmCrrnt) 
                         
                         } else {
                                CnstrctVdoOnThmbnlGstrRg(plr_id,yitVideoDomElmCrrnt)   
                         }
                    
                    if (!yitvidCnstrctHasCnstrcd) {
                                yitvidCnstrct() ;
                    }  else {

                      new Promise(function(resolve, reject) {
                                      setTimeout(() => resolve(1), 500);
                                           }).then(function() {
                                             return window.YitVideo.InfrastructureInitialize();
                                          }).then(function() {
                                            setTimeout(() => window.YitVideo.InitPlayers(), 1000);   
                                          })
                    }                     
                                               
        }
                 
                function show_page(floatingPlayerId_movie,headline,floatingPlayerThis_id,secondery_title,campaing_title,campaign_url,splashYntTvUrl,fltngPlyr_liveORnot,PlayDummyPicCls,vidLiveImgSrcUrl, base) {
                    while(headline.indexOf("slashEscape") !== -1){
                        headline = headline.replace("slashEscape", '"');
                    }
                    while(headline.indexOf("oneSlash") !== -1){
                        headline = headline.replace("oneSlash", "'");
                    }
                    
                    
                     while(secondery_title.indexOf("slashEscape") !== -1){
                        secondery_title = secondery_title.replace("slashEscape", '"');
                    }
                    while(secondery_title.indexOf("oneSlash") !== -1){
                        secondery_title = secondery_title.replace("oneSlash", "'");
                    }
                    
                    
                     while(campaing_title.indexOf("slashEscape") !== -1){
                        campaing_title = campaing_title.replace("slashEscape", '"');
                    }
                    while(campaing_title.indexOf("oneSlash") !== -1){
                        campaing_title = campaing_title.replace("oneSlash", "'");
                    }
                    
                        jQuery('.PlayDummyPic , .floatingPlayer_click_bigger ').removeClass('PlayDummyPicCls')
                        jQuery('.PlayDummyPic , .floatingPlayer_click_bigger ').addClass(PlayDummyPicCls)

                        jQuery('#vidLiveImgMp4').attr('src','')
                        jQuery('#vidLiveImgMp4').attr('style','display:none;')
                        if (vidLiveImgSrcUrl) {
                            jQuery('#vidLiveImgMp4').attr('src',vidLiveImgSrcUrl)
                            jQuery('#vidLiveImgMp4').attr('style','display:visble;width:100%')
                        }
                    
                    jQuery(".floatingPlayer_hold_player.yntTvHvrAbvInSmallFaze").attr("indxAttrb",base.id)
                    jQuery('.floatingPlayerItem').removeClass('selected');
                    jQuery(base).addClass('selected');
                    var isFloatingPlayerBigOrNot = false;
                    var amIinFlpFrm = false;
                     if (0 ) { 
                               
                               //yitVideoDomElmCrrnt=document.getElementById("floatingPlayerOuter_flex_container").querySelectorAll('[yitvideo="'+ floatingPlayerThis_id +'"]')
                               yitVideoDomElmCrrnt=document.getElementById("floatingPlayerOuter_flex_container").querySelectorAll('[yitvideo]')[0] ;
                               CnstrctVdoOnThmbnlGstr(floatingPlayerThis_id,yitVideoDomElmCrrnt,fltngPlyr_liveORnot);
                     } else {
                     var ifrm=document.getElementById("floatingPlayerOuter_flex_container").getElementsByTagName('iframe')[0];
                     ifrm.src=floatingPlayerId_movie + "?obj=" + JSON.stringify({
                        poster:false,
                        splash:false,
                        YntTvHmPagFlg:true,
                        //volumeLevel:( isFloatingPlayerBigOrNot ? false : true ),
                        autoplay:true ,
                        callbacks: {
                            onFinish: 'playNextMovie_floatingPlayer',
                            //onFltPlayerHomePageProgress: 'floatingPlayer_disableClick',
                           onContentStart: ( isFloatingPlayerBigOrNot ?  'floatingPlayer_pageRefreshDisable' : '')
                        },
                        //fullscreen:false
                    
                    });
            }  
                    campaign_url = campaign_url ? campaign_url : '#'  ;
                    
                    document.getElementById("first_title").innerHTML =  headline ;
                    document.getElementById("first_titleD").innerHTML =  headline ;
                    document.getElementById("secondery_title").innerHTML =  secondery_title ;
                        if (campaign_url == '#' || campaing_title == '' ) { 
                            document.getElementById("campaing_title").innerHTML = '' ;
                            document.getElementById("campaing_titleD").innerHTML = '' ;
                            } else { 
                    document.getElementById("campaing_title").innerHTML = "<a href="+ campaign_url + " target='_blank'><p style='font-size:12px;font-weight:bold;'  >"+campaing_title+ "  <img  class=' campaing_title' src='/images/ynet_tv/arrow.png'/> </p></a>"
                    document.getElementById("campaing_titleD").innerHTML = "<a href="+ campaign_url + " target='_blank'><p style='font-size:12px;font-weight:bold;'  >"+campaing_title+ "  <img  class=' campaing_title' src='/images/ynet_tv/arrow.png'/> </p></a>"
                    }
                   if (campaign_url != '#' && campaing_title ) { document.getElementById("floatingPlayerBottm_line").innerHTML = "<p style='font-size:15px;font-weight:bold;'>" + headline+ "</p><p>" + secondery_title + "</p> <a href="+ campaign_url + " target='_blank'><p><span style='font-size:12px;background-color:#de1111;text-decoration: none'>"+campaing_title + "  </span><img  class=' campaing_title' src='/images/ynet_tv/arrow.png'/> </p></a>"
