                _scroll : function(direction, multiplier, speed) {                    this._itemsContainer.stop(true, true);                    var speedAnimation = (speed && speed > 0) ? speed : 400 ;                    multiplier = multiplier || 1;                    var currentOffset = this._getCurrentOffset();                    var rightOffset = currentOffset;                    var leftOffset = this._totalItemsWidth - currentOffset - this._viewportWidth;                    
                                        if ((direction == 'right' && rightOffset > 0) || (direction == 'left' && leftOffset > 0)) {                        this._currentPageNum += (direction == 'left' ? multiplier : -multiplier);                                                var offset = Math.min(this._visibleItemsWidth * multiplier, direction == 'left' ? leftOffset : rightOffset);                                                this._itemsContainer.animate({                             'margin-right': -currentOffset + (direction == 'left' ? -offset : offset)                        }, speedAnimation);                    };                                        var totalPages = Math.min(Math.ceil(this._totalItemsWidth / this._visibleItemsWidth), 10);                    var currStep = (this._currentPageNum);                    if (direction == 'left') {                        if (this._buttonsContainer.find('div#mils_right_butt_ident').css('display') == 'none') {                             this._buttonsContainer.find('div#mils_right_butt_ident').css('display', 'block');                        };                        if (currStep >= totalPages) {                            this._buttonsContainer.find('div#mils_left_butt_ident').css('display', 'none');                        };                    };                                        if (direction == 'right') {                        if (this._buttonsContainer.find('div#mils_left_butt_ident').css('display') == 'none') {                             this._buttonsContainer.find('div#mils_left_butt_ident').css('display', 'block');                        };                        if (currStep <=1) {                            this._buttonsContainer.find('div#mils_right_butt_ident').css('display', 'none');                        };                    };                    this._selectCurrentPage();                },                                _getCurrentOffset : function() {                    return Math.abs(parse