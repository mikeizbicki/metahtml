<script>
    $('#site-search-1724472-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-1724472').submit();
        return false;
    });

    $('#site-search-1724472').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-1724472 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-1724472 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-1724472-term').val() ){
            return true;
        }else{
            return false;
        }
    });
    
    $('.dropdown-toggle-1724472').click(function(){
        setTimeout(function(){
            $('#site-search-1724472-term').focus();
        }, 50);
    });</script></div></div>
                                
