                jQuery(document).ready(function($){
                    function throttleEvvnt(func, wait){
                        var timeout;
                        return function(){
                            var context = this, args = arguments;
                            var later = function(){
                                timeout = null;
                                func.apply(context, args);
                            };
                            clearTimeout(timeout);
                            timeout = setTimeout(later, wait);
                        }
                    }
                    var evvntLoaded = false;
                    var throttledEvvnt = throttleEvvnt(function(){
                        if(!evvntLoaded){
                            evvntLoaded = __tnt.elementOnScreen('#evvnt-widget-1818689');
                            if(evvntLoaded){
                                var s = document.createElement('script');
                                    s.setAttribute('src', '//production-evvnt-plugin-herokuapp-com.global.ssl.fastly.net/evvnt_discovery_plugin_s.js');
                                    s.onload = evvntDiscoveryInit;
                                document.body.appendChild(s);
                                window.removeEventListener('scroll', throttledEvvnt);
                            }
                        }   
                    }, 100);
                    window.addEventListener('scroll', throttledEvvnt);
                });
            
        </script>
