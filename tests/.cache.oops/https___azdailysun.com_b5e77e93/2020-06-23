            function evvntDiscoveryInit() {
                evvnt_require("evvnt/discovery_plugin").init({
                    api_key: "AZDAILYSUN",
                    publisher_id: 5291,
                    discovery: {
                        element: "#evvnt-submission-button-1818689",
