  <script>
      $(document).ready(function() {
          $(window).on("nimble.resize", function () {
              var cWidth = $('#offers-1772979 .item').width();
              var cClass = cWidth>=400? 'offer-lg' : (cWidth>=300? 'offer-md' : 'offer-sm');
              $('#offers-1772979 .item').each(function( index ) {
                  if( !$(this).hasClass(cClass) ){
                      $(this).removeClass('offer-sm').removeClass('offer-md').removeClass('offer-lg');
                      $(this).addClass(cClass);
                  }
              });
          }).resize();
          
          
      });
