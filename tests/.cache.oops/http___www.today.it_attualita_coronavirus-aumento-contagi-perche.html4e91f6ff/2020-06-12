<script type="text/javascript">/*<![CDATA[*/
  
  (function()
  {
    if ( !document.querySelectorAll || !window.al )
      return;
    
    if ( (function(_){ return (_ = location.search.match(/[&?]__vda=([^&]*)/i)) && parseInt(_[1]) ? true : false; })() )
      return;
    

    if ( window.performance )
      window.ga && ga( 'send', 'timing', 'AD', 'Init', Math.round(performance.now()) );
      

    {
      var bReferrer = (function( sReferrer )
      {
        if ( (function(_){ return (_ = location.search.match(/[&?]__referrer=([^&]*)/i)) && parseInt(_[1]) ? true : false; })() )
          return true;
        
        if ( !location.pathname.indexOf('/test/') )
          return true;
        
        
        var aDomains = ["agrifoodtoday.it","agrigentonotizie.it","anconatoday.it","arezzonotizie.it","avellinotoday.it","baritoday.it","bolognatoday.it","bresciatoday.it","brindisireport.it","casertanews.it","cataniatoday.it","catanzarotoday.it","cesenatoday.it","chietitoday.it","citynews.it","firenzetoday.it","foggiatoday.it","forlitoday.it","frosinonetoday.it","genovatoday.it","ilpescara.it","ilpiacenza.it","latinatoday.it","lecceprima.it","leccotoday.it","livornotoday.it","messinatoday.it","milanotoday.it","modenatoday.it","monzatoday.it","napolitoday.it","novaratoday.it","padovaoggi.it","palermotoday.it","parmatoday.it","perugiatoday.it","pisatoday.it","quicomo.it","ravennatoday.it","reggiotoday.it","riminitoday.it","romagnaoggi.it","romatoday.it","salernotoday.it","sondriotoday.it","sportpiacenza.it","ternitoday.it","today.it","torinotoday.it","trentotoday.it","trevisotoday.it","triesteprima.it","udinetoday.it","veneziatoday.it","veronasera.it","vicenzatoday.it","facebook.com","facebook.it","fb.com","twitter.com","t.co"];
        
        if ( !aDomains.length )
          return true;
          
        if ( !sReferrer || !~('' + sReferrer).indexOf('://') )
          return null;
        
        
        var oReferrerAnchor = document.createElement( 'a' );
          oReferrerAnchor.href = '' + sReferrer;
        
        var sReferrerHostName = ('' + oReferrerAnchor.hostname).toLowerCase().split('.').slice(-2).join('.');
        
        for ( var i = 0; i < aDomains.length; ++i )
        {
         var sDomain = aDomains[i].toLowerCase().split('.').slice(-2).join('.');
          
          if ( sDomain == sReferrerHostName )
            return true;
        }
        
        return false;
      })( document.referrer || '' );
      
      
      var fStripHTML = function( sHTML )
      {
        sHTML = sHTML ? ('' + sHTML) : '';
        sHTML = sHTML.replace( /\s*<div\b\s+id\s*=\s*["']?beacon_\w*["']?[^>]*>.*?<\/+div\b[^>]*>\s*/gi, '' );

        return sHTML;
      };
      

      
      var aExcludedFormats = [ ];
      var aReferrerFormats = [ 'interstitial', 'zzz-ref' ];
      var aInViewFormats   = [ 'adv-page-2', 'adv-page-3', 'adv-page-4', 'adv-page-5' ];
      
      
            aExcludedFormats = Math.random() <= 0.2 ? ['box-1a', 'box-1b'] : ['half-page'];
          }



    var fAd = function( )
    {
      {
        var aZones = [ ];
      
        var aNodes = document.querySelectorAll( 'script[type="text/revive-ad"]' );
        for ( var i = 0; i < aNodes.length; ++i )
        {
          var oNode = aNodes[i];
  
          var iZone = oNode.getAttribute( 'data-revive-ad' ) || 0;
            iZone   = ('' + iZone).replace( /^\s+|\s+$/gm, '' ) || 0;
            iZone   = parseInt(iZone, 10) || 0;
            
          if ( !iZone )
            continue;
            
            
          var sFormat = oNode.getAttribute( 'data-revive-format' ) || '';
            sFormat  = ('' + sFormat).replace( /^\s+|\s+$/gm, '' );
            sFormat  = sFormat.toLowerCase( );
            
   
          
          if ( ~aExcludedFormats.indexOf(sFormat) )
          {
            oNode.setAttribute( 'data-revive-internal-skip', '' );
            oNode.setAttribute( 'data-revive-internal-excluded', '' );
            
            continue;
          }
            
          if ( !bReferrer && ~aReferrerFormats.indexOf(sFormat) )
          {
            oNode.setAttribute( 'data-revive-internal-skip', '' );
            oNode.setAttribute( 'data-revive-internal-no-referrer', '' );
            
            continue;
          }
          
          
          if ( ~aInViewFormats.indexOf(sFormat) )
            oNode.setAttribute( 'data-revive-internal-in-view', '' );
          
          aZones.push( iZone );
          oNode.setAttribute( 'data-revive-internal-queued', '' );
        }
        
        if ( !aZones.length )
          return;
      }   
        
      {
        var oAC              = window.AdvContext;
        var aParameters      =
        [
          'zones=' + encodeURIComponent( aZones.join('|') ),
  
          'charset=' + encodeURIComponent( document.characterSet || document.charset || '' ),
          'loc=' + encodeURIComponent( location.href || '' ),

                  
          'bs_adult=' + encodeURIComponent( oAC && oAC.adult ? oAC.adult : ''),
          
          'bs_brand=' + encodeURIComponent( oAC && oAC.brand ? oAC.brand : ''  ),
          'bs_raw_brand=' + encodeURIComponent( oAC && oAC.raw_brand ? oAC.raw_brand : ''  ),
          
          'bs_city=' + encodeURIComponent( oAC && oAC.city ? oAC.city : ''  ),
          'bs_raw_city=' + encodeURIComponent( oAC && oAC.raw_city ? oAC.raw_city : ''  ),
          
          'bs_theme_color=' + encodeURIComponent( oAC && oAC.theme_color ? oAC.theme_color : ''),
          
          'bs_type=' + encodeURIComponent( oAC && oAC.type ? oAC.type : ''),
          'bs_channel=' + encodeURIComponent( oAC && oAC.channel ? oAC.channel : '' ),
          'bs_model=' + encodeURIComponent( oAC && oAC.model ? oAC.model : ''),
          
          'bs_municipality=' + encodeURIComponent( oAC && oAC.municipality ? oAC.municipality : ''),
          'bs_raw_municipality=' + encodeURIComponent( oAC && oAC.raw_municipality ? oAC.raw_municipality : ''),
          
          'bs_zone=' + encodeURIComponent( oAC && oAC.zone ? oAC.zone : ''),
          'bs_raw_zone=' + encodeURIComponent( oAC && oAC.raw_zone ? oAC.raw_zone : ''),

          
          'bs_place=' + encodeURIComponent( oAC && oAC.zone ? oAC.zone : (oAC && oAC.municipality ? oAC.municipality : (oAC && oAC.city ? oAC.city : '')) ),
          'bs_raw_place=' + encodeURIComponent( oAC && oAC.raw_zone ? oAC.raw_zone : (oAC && oAC.raw_municipality ? oAC.raw_municipality : (oAC && oAC.raw_city ? oAC.raw_city : '')) ),
          
          
          'bs_u_g=' + encodeURIComponent( window.BLACKSTONE && BLACKSTONE.USER && BLACKSTONE.USER.sex || '' ), 
          
          
          'dmp_channels=' + encodeURIComponent( window.DMP && DMP._ && DMP._.channels ? DMP._.channels : '' ),
          
          'dmp_city=' + encodeURIComponent( window.DMP && DMP._ && DMP._.city ? DMP._.city : '' ),
          'dmp_zone=' + encodeURIComponent( window.DMP && DMP._ && DMP._.zone ? DMP._.zone : '' ),
          'dmp_u_g=' + encodeURIComponent( window.DMP && DMP._ && DMP._[''] && DMP._[''].user && DMP._[''].user.g  ? DMP._[''].user.g : '' )
        ];
          
  
        if ( document.referrer )
          aParameters.push( 'referer=' + encodeURIComponent(document.referrer) );
  
        //var sUrl = '//adv.strategy.it/www/delivery/spc.php/?_=_&' + aParameters.join( '\x26' );
        //var sUrl = '//' + location.hostname.split('.').slice(-2).join('.') + '/~vda/www/delivery/spc.php?' + aParameters.join( '\x26' );
        var sUrl = '//' + location.hostname.split('.').slice(-2).join('.') + '/~vda/___/u.php/?_=_&' + aParameters.join( '\x26' );
      }
      
  
      if ( window.performance )
        window.ga && ga( 'send', 'timing', 'AD', 'Request', Math.round(performance.now()) );
      
      
      window.al && al( sUrl, function()
      {
        var oZones = window.OA_output || null;
        
        if ( !oZones )
        {
          if ( window.performance )
            window.ga && ga( 'send', 'timing', 'AD', 'Error', Math.round(performance.now()) );
          
          return;
        }
        
        
        
        if ( window.performance )
          window.ga && ga( 'send', 'timing', 'AD', 'Response', Math.round(performance.now()) );

        var aNodes = document.querySelectorAll( 'script[type="text/revive-ad"]' );
        for ( var i = 0; i < aNodes.length; ++i )
        {
          var oNode = aNodes[i];

          var iZone = oNode.getAttribute( 'data-revive-ad' ) || 0;
            iZone   = ('' + iZone).replace( /^\s+|\s+$/gm, '' ) || 0;
            iZone   = parseInt(iZone, 10) || 0;
          
          if ( !iZone )
            continue;
            
          if ( oNode.hasAttribute('data-revive-internal-skip') )
            continue;

          
          if ( !(iZone in oZones) )
          {
            oNode.setAttribute( 'data-revive-internal-missing', '' );
            continue;
          }
          
          
          var bInView    = oNode.hasAttribute( 'data-revive-internal-in-view' );
          
          var sCallStart = oNode.getAttribute( 'data-revive-callstart' ) || '';
          var sCallBack  = oNode.getAttribute( 'data-revive-callback' ) || '';

          var sHTML      = '' + oZones[iZone];
          var bHTML      = !!fStripHTML( sHTML );

          var sTarget    = oNode.getAttribute('data-revive-target') || '';
          var bTarget    = false;
          
          
          if ( bHTML )
          	oNode.setAttribute( 'data-revive-internal-filled', '' );
          else
            oNode.setAttribute( 'data-revive-internal-empty', '' );
          
          
          if ( bInView )
            oNode.setAttribute( 'data-revive-internal-on-viewport', '' );  
          
          
          if ( sTarget )
          {
            try
            {
              var aTargetNodes = document.querySelectorAll( sTarget );
              

              for ( var j = 0; j < aTargetNodes.length; ++j )
              {
                var oTargetNode = aTargetNodes[j];
              
                if ( bHTML )
                {
                  oTargetNode.setAttribute( 'data-revive-internal-filled', '' );
                  
                  oTargetNode.setAttribute( 'type', 'text/async-html' );
                  oTargetNode.setAttribute( 'data-async-html', sHTML );
                  
                  
                  if ( bInView )
                    oTargetNode.setAttribute( 'data-async-html-on-viewport', '' );
                    
                  if ( sCallStart )
                  	oTargetNode.setAttribute( 'data-async-html-callstart', sCallStart );
                  if ( sCallBack )
                  	oTargetNode.setAttribute( 'data-async-html-callback',  sCallBack );
                }
                  
                else
                  oTargetNode.setAttribute( 'data-revive-internal-empty', '' );  
                  
                  
                if ( bInView )
                  oTargetNode.setAttribute( 'data-revive-internal-on-viewport', '' );  

                oTargetNode.setAttribute( 'data-revive-internal-processed', '' );
              }

              
              oNode.setAttribute( 'data-revive-internal-targets', aTargetNodes.length || 0 );

              if ( bHTML && aTargetNodes.length )
                bTarget = true;
                
            } catch( _ ){ }
          }
          
          
          if ( !bTarget )
          {
            while ( oNode.firstChild )
              oNode.removeChild( oNode.firstChild );
              
            try
            {
              oNode.appendChild( document.createTextNode(sHTML) );
            }
            
            catch ( _ )
            {
              oNode.innerText = oNode.textContent = oNode.text = sHTML;
            }
            
            oNode.setAttribute( 'type', 'text/async-html' );
            oNode.setAttribute( 'data-async-html', '' );
            
            if ( bInView )
              oNode.setAttribute( 'data-async-html-on-viewport', '' );

            
            if ( sCallStart )
            	oNode.setAttribute( 'data-async-html-callstart', sCallStart );
            if ( sCallBack )
            	oNode.setAttribute( 'data-async-html-callback',  sCallBack );
          }
          
          
          var sNotify               = oNode.getAttribute('data-revive-notify') || '';
          var sNotifyAttributeName  = oNode.getAttribute('data-revive-notify-attribute-name') || '';
          var sNotifyAttributeValue = oNode.getAttribute('data-revive-notify-attribute-value') || '';
          
          if ( sNotify && sNotifyAttributeName && bHTML )
          {
            try
            {
              var aNotifyNodes = document.querySelectorAll( sNotify );
              
              for ( var j = 0; j < aNotifyNodes.length; ++j )
                aNotifyNodes[j].setAttribute( sNotifyAttributeName, sNotifyAttributeValue );
              
            } catch( _ ){ }
          }
          
          
          oNode.setAttribute( 'data-revive-internal-processed', '' );
        }
       
        
        window.asyncHTML && asyncHTML.setup( );
  
        if ( window.performance )
          window.ga && ga( 'send', 'timing', 'AD', 'Render', Math.round(performance.now()) );
      });
    };
      
      
      
      
      
    var bPageReady = false;
    var fPageReady = function( )
    {
      if ( bPageReady )
        return false;
        
      bPageReady = true;
      fAd( );
      
      return true;
    };
    
    window.ReviveAD = fPageReady;


    if ( document.readyState == 'complete' || document.readyState == 'loaded' || (!document.attachEvent && document.readyState == 'interactive') )
      setTimeout( function( ){ fPageReady( ); }, 1 );
    
    else
    {
      var bOnDomReady = false;
      var fOnDomReady = function( )
      {
        if ( bOnDomReady )
          return;
  
        bOnDomReady = true;
  
  
        if ( document.removeEventListener )
          document.removeEventListener( 'DOMContentLoaded', fOnDomReady, false );
        
        
        if ( window.removeEventListener )
          window.removeEventListener( 'load', fOnDomReady, false );
        else if ( window.detachEvent )
          window.detachEvent( 'onload', fOnDomReady );
        
        
        
        fPageReady( );
      };
      
      
      
      if ( document.addEventListener )
        document.addEventListener( 'DOMContentLoaded', fOnDomReady, false );

      else if ( document.attachEvent )
      {
        document.attachEvent( 'onreadystatechange', function()
        {
          if ( document.readyState != 'complete' && document.readyState != 'loaded' )
            return;
          
          document.detachEvent( 'onreadystatechange', arguments.callee );
          fOnDomReady( );
        } );
        
        
        if ( document.documentElement && document.documentElement.doScroll && window == window.top )
        {
          (function()
          {
            if ( bOnDomReady )
              return;

            try
            {
              document.documentElement.doScroll( 'left' );
            }
            
            catch ( _ )
            {
              setTimeout( arguments.callee, 100 );
              return;
            }
            
            fOnDomReady( );
          })();
        }
      }
      
      
      if ( window.addEventListener )
        window.addEventListener( 'load', fOnDomReady, false );
        
      else if ( window.attachEvent )
        window.attachEvent( 'onload', fOnDomReady );
        
      else
      {
        var fOnLoad   = window.onload || null;
        window.onload = function( )
        {
          window.onload = null;
          
          fOnLoad && fOnLoad.apply( window, arguments );
          fOnDomReady( );
        };
      }
      
      
      (function()
      {
        if ( bPageReady )
          return;
        
        if ( document.readyState == 'complete' || document.readyState == 'loaded' || (!document.attachEvent && document.readyState == 'interactive') )
        {
          setTimeout( function( ){ fPageReady( ); }, 1 );
          return;
        }

        setTimeout( arguments.callee, 100 );
      })();
    }

  })();

/*]]>*/</script>

