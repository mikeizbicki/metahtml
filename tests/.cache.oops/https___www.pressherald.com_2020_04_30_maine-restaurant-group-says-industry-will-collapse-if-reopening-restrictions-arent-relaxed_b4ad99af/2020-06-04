function pos1sizes() {
    var widthOptions= [];
    var width = window.innerWidth || document.body.clientWidth;
    if(width > 1000) {
    widthOptions =  [[970, 90],[728, 90],[970, 250]];
    } else if(width > 750) {
    widthOptions =  [728, 90];   
    } else {widthOptions = [[320, 50], [300, 50], [300, 100]];}
    return widthOptions;
}
function pos2sizes() {
    var widthOptions= [];
    var width = window.innerWidth || document.body.clientWidth;
    if(width > 785) {
    widthOptions =  [[300, 600],[300, 250]];  
    } else {widthOptions = [[300, 250]];}
    return widthOptions;
}
function pos3sizes() {
    var widthOptions= [];
    var width = window.innerWidth || document.body.clientWidth;
    if(width > 785) {
    widthOptions =  [[300, 600], [300, 250]];  
    } else {widthOptions = [[300, 250]];}
    return widthOptions;
}  
