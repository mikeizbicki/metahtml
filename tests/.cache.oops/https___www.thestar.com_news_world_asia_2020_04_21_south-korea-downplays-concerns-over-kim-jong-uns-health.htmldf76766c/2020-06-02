<script type="text/javascript">
  // This checks for an email address in the URL parameters, and redirects to a URL without it, if found.
  // The referrer change is so that third-party scripts can't pick up the email address even through the referrer.
  if (window.location.search !== '') {
    var url = window.location.href
    url = url.replace(/\\?.+@.+\\..{2,}/, '')
    if (url !== window.location.href) {
      var meta = document.createElement('meta')
      meta.name = 'referrer'
      meta.content = 'no-referrer'
      document.getElementsByTagName('head')[0].appendChild(meta)
      window.location.href = url
    }
  }
</script>

