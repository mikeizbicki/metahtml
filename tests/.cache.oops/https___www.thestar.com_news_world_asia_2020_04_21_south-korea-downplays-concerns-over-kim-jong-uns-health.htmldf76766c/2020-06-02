<script type="text/javascript">
  ;(function(loc) {
    var b = window.__PRELOADED_CONFIG_DATA__.clientBaseUrl,
      m = loc.pathname === '/' && loc.hash && loc.hash.match(/^#\/(\w+)\/(.*)$/)
    if (!m) return
    switch (m[1]) {
      case 'author':
        return loc.replace(b + '/authors.' + m[2])
      case 'section':
        if (!m[2].indexOf('latestnews')) return loc.replace(b + '/news.html')
        if (!m[2].indexOf('travel')) return loc.replace(b + '/life/travel.html')
        if (!m[2].indexOf('horoscope'))
          return loc.replace(b + '/diversions/horoscope.html')
        if (!m[2].search(/^(?:gta|canada|world)/))
          return loc.replace(b + '/news/' + m[2])
      case 'article':
        return loc.replace(b + '/' + m[2])
    }
  })(window.location)
</script>

