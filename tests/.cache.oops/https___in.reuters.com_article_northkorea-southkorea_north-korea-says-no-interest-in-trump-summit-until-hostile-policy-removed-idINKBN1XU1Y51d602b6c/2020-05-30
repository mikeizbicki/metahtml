<script>
setTimeout(function(){
    var replayVideo = document.getElementById('replay-video');
    if (replayVideo) {
        replayVideo.onclick = function(){
            document.getElementById('youTubePlayer').style.display='block'; 
            player.cueVideoById('qM1BYkEA17A');
            player.setPlaybackQuality('hd720');
        };
    }
}, 3000);

