var suggestionData = [{
imageUrl: 'https://cdn.mos.cms.futurecdn.net/46UNUtB7KJ7wERiSaeT7oa-400-80.png',
articleUrl: '/15693-telescopes-beginners-telescope-reviews-buying-guide.html',
articleTitle: 'Best telescopes for the money  2020 reviews and guide'
}, {
imageUrl: 'https://cdn.mos.cms.futurecdn.net/SwXb2MtAeRy3HoHrxh5xwi-400-80.jpg',
articleUrl: '/lego-star-wars-sets-2020.html',
articleTitle: "These Lego 'Star Wars' sets for 2020 are strong with the Force"
}, {
imageUrl: 'https://cdn.mos.cms.futurecdn.net/jChHbUNTXztPZvPS4qedSA-400-80.jpg',
articleUrl: '/16149-night-sky.html',
articleTitle: 'Night sky, June 2020: What you can see this month [maps]'
}];
var addGAEventToArticleLink = function addGAEventToArticleLink(articleLink, suggestionData, index) {
articleLink.addEventListener('click', function () {
window.ga('send', 'event', 'Component', 'Click on Search Suggestion', suggestionData[index].articleTitle, Object.assign({}, window.analytics_ga_data, {
dimension16: index + 1
}));
});
};
var giveElementWithClass = function giveElementWithClass(elementType, className) {
var el = document.createElement(elementType);
el.classList.add(className);
return el;
};
var init = function init(suggestionData) {
var searchBox = document.querySelector('.burgerbar .search-box');
var parentDiv = giveElementWithClass('div', 'search-suggest');
var suggestHeading = giveElementWithClass('div', 'search-suggest__heading');
suggestHeading.textContent = 'Most popular';
var suggestionsList = giveElementWithClass('div', 'search-suggest__articles');
suggestionData.forEach(function (suggObj, i) {
var articleLink = giveElementWithClass('a', 'articles__link');
articleLink.href = suggObj.articleUrl;
addGAEventToArticleLink(articleLink, suggestionData, i);
var articleImage = giveElementWithClass('img', 'link__image');
articleImage.src = suggObj.imageUrl;
var articleHeading = giveElementWithClass('p', 'link__title');
articleHeading.textContent = suggObj.articleTitle;
articleLink.appendChild(articleImage);
articleLink.appendChild(articleHeading);
suggestionsList.appendChild(articleLink);
});
parentDiv.appendChild(suggestHeading);
parentDiv.appendChild(suggestionsList);
searchBox.appendChild(parentDiv);
};
if (document.readyState === 'complete' || document.readyState === 'interactive') {
init(suggestionData);
} else {
document.addEventListener('DOMContentLoaded', function () {
init(suggestionData);
});
}
</script>
