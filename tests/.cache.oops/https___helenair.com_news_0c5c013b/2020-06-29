    </div></div><div id="tncms-region-index-second-c" class="tncms-region "><div id="tncms-block-1767453" class="tncms-block"><section id="block-1767453" class="block emphasis-   " ><div class="clearfix"></div><div id="circularhub_module_7384" class="flipp-section-module"></div>
    <script>
        jQuery(document).ready(function($){
            function throttleFlip(func, wait){
                var timeout;
                return function(){
                    var context = this, args = arguments;
                    var later = function(){
                        timeout = null;
                        func.apply(context, args);
                    };
                    clearTimeout(timeout);
                    timeout = setTimeout(later, wait);
                }
            }
            var flipLoaded = false;
            var throttledFlip = throttleFlip(function(){
                if(!flipLoaded){
                    flipLoaded = __tnt.elementOnScreen('#circularhub_module_7384');
                    if(flipLoaded){
                        $('#circularhub_module_7384').append('<script async src="//api.circularhub.com/7384/c2dfb0f11a2563c4/circularhub_module.js"><\/script>');
                        window.removeEventListener('scroll', throttledFlip);
                    }
                }   
            }, 100);
            window.addEventListener('scroll', throttledFlip);
        });
     </script><section id="block-1767453" class="block emphasis-   " ><div class="clearfix"></div></div></div></div>
    </div>
    
    <div class="container-breakout ad-col"><div class="visible-lg visible-md"><div class="tnt-ads-container text-center hidden-xs"><div style="min-height:90px; min-width:728px;" class="tnt-ads dfp-ad dfp-ad-lite" data-dfp-adunit="/8438/helenair.com/news" data-dfp-custom-pos="breakout_bottom, btf, 70" id="breakout_bottom" data-dfp-size="[[728,90], [970,90], [970,250]]" data-lazy="true" data-subscriber="true"></div></div>
    </div><div class="visible-sm"><div class="tnt-ads-container text-center hidden-xs"><div style="min-height:90px; min-width:728px;" class="tnt-ads dfp-ad dfp-ad-lite" data-dfp-adunit="/8438/helenair.com/news" data-dfp-custom-pos="breakout_bottom, btf, 70" id="breakout_bottom-tablet" data-dfp-size="[728,90]" data-lazy="true" data-subscriber="true"></div></div>
    </div><div class="tnt-ads-container text-center hidden-sm hidden-md hidden-lg"><div style="min-height:250px; min-width:300px;" class="tnt-ads dfp-ad dfp-ad-lite" data-dfp-adunit="/8438/helenair.com/news" data-dfp-custom-pos="breakout_bottom, btf, 70" id="breakout_bottom-mobile-mobile" data-dfp-size="[[300,250],[320,50]]" data-lazy="true" data-subscriber="true"></div></div>
