onthly")) show_signup(message)\n        });\n    }\n    \n    //kill modal if blackout is clicked\n    $("#modal-box").click(function(e){ if(e.target == e.currentTarget){ kill_modal(); }});\n}\n// Disclaimer button\nfunction disclaimerButton(){\n    var popup = document.getElementById("myPopup");\n    popup.classList.toggle("show");\n}\n// Modal kill function\nfunction kill_modal(){\n    $("#modal-box").removeClass(\'active\').html(\'\');\n}\n// Load more articles\nfunction load_more(change){\n    try{\n        var load_more = document.getElementById("load-btn");\n        load_more.innerHTML = \'LOAD MORE\';\n        \n        if(change === \'change\') window.counter = 1;\n        \n        if(window.counter >= 16){\n            load_more.innerHTML = \'No More Results\';\n            load_more.disabled = "disabled";\n            return;\n        }\n        \n        var sort  = document.getElementsByName("sort")[0].value;\n        var ids   = document.getElementsByName("ids")[0].value;\n        var type  = document.getElementsByName("type")[0].value;\n        \n        var xhr = new XMLHttpRequest();\n        \n        xhr.open(\'GET\', "/ajax/add-more.php?counting="+window.counter+"&sort="+sort+"&type="+type+"&ids="+ids+"&change="+change);\n        xhr.send(null);\n        \n        xhr.onreadystatechange = function(){\n            // Request has finished\n            if(xhr.readyState === 4){\n                // Success\n                if (xhr.status === 200){\n                    var result = JSON.parse(xhr.responseText);\n                    \n                    if(result.data){\n                        window.counter++;\n                        var posts = result.data;\n                        if(result.change === "change"){\n                            document.getElementById("posts").innerHTML = "";\n                            document.getElementById("load-infinite").innerHTML = "";\n                            if(posts.length > 26){\n                                posts.pop();\n                            }\n                        }else{\n                            if(posts.length < 27){\n                                load_more.innerHTML = \'No Further Results\';\n                            }else{\n                                load_more.innerHTML = \'LOAD MORE\';\n                                posts.pop();\n                            }\n                        }\n            \n                        var infinite_section = document.createElement(\'div\');\n                        infinite_section.setAttribute("class" , \'infinite\');\n                        var writer_name, title, img, img_cont, link, card, row, title_info, writer_name_text;\n                        \n                        for(var i = 0; i < posts.length; i++){\n                            if(i == 0 || i % 2 == 0){\n                              row = document.createElement(\'div\');\n                              row.setAttribute("class" , \'card-row\');\n                            }\n                            card = document.createElement(\'div\');\n                            card.setAttribute("class" , \'card-article\');\n            \n                            link = document.createElement(\'a\');\n                            link.setAttribute("href" , posts[i].post_url);\n            \n                            img_cont = document.createElement(\'div\');\n                            img_cont.setAttribute("class" , \'card-article-img-cont\');\n            \n                            img = document.createElement(\'img\');\n                            img.setAttribute("src" , "https://mediaproxy.salon.com/width/380/" + posts[i].image);\n            \n                            title = document.createElement(\'h2\');\n                            title_info = document.createTextNode(posts[i].post_title);\n            \n                            title.appendChild(title_info);\n                            img_cont.appendChild(img);\n                            link.appendChild(img_cont);\n                            link.appendChild(title);\n                            // Otherwise use just the one author\'s name\n                            writer = posts[i].author_names;\n            \n                            if(typeof posts[i].source != "undefined" && posts[i].source != "") {\n                                writer += " - " + posts[i].source;\n                            }\n            \n                            writer_name = document.createElement(\'strong\');\n                            writer_name.setAttribute("class" , \'writer\');\n                            writer_name_text = document.createTextNode(writer);\n            \n                            writer_name.appendChild(writer_name_text);\n                            link.appendChild(writer_name);\n                            card.appendChild(link);\n                            row.appendChild(card);\n            \n                            if(i % 2 == 1) infinite_section.appendChild(row);\n                        }\n            \n                        if(result.change === "change"){\n                            document.getElementById(\'posts\').appendChild(infinite_section);\n                        }else{\n                            document.getElementById(\'load-infinite\').appendChild(infinite_section);\n                        }\n                    }\n                    // No more data\n                    else{\n                        load_more.innerHTML = \'No Further Results\';\n                    }\n                }\n                // Errored out\n                else{\n                    load_more.innerHTML = \'An Error Occurred\';\n                }\n            }\n        };\n    }\n    catch(e) {\n        console.log(e);\n    }\n}\n// Get a cookie\nfunction getCookie(cname){\n    var name = cname + "=";\n    var decodedCookie = decodeURIComponent(document.cookie);\n    var ca = decodedCookie.split(\';\');\n\n    for(var i = 0; i <ca.length; i++){\n        var c = ca[i];\n\n        while(c.charAt(0) == \' \') c = c.substring(1);\n\n        if(c.indexOf(name) == 0) return c.substring(name.length, c.length);\n    }\n    \n    return "";\n}\n// Parse json cookie\nfunction parseCookie(cvalue) {\n    try { return JSON.parse(cvalue); }\n    catch(err) { return []; }\n}\n// Set a cookie\nfunction setCookie(cname, cvalue, minutes){\n    console.log(\'setCookie called\');\n    \n    var d = new Date();\n    d.setTime(d.getTime() + (minutes*60*1000));\n    \n    var expires = "expires="+ d.toUTCString();\n    document.cookie = cname + "=" + cvalue + ";" + expires + ";domain=.salon.com;path=/";\n}\n// Search Function\nfunction focustime(){\n    var input = document.getElementById ("search_input"); \n    input.focus();\n}\n// Get Proper RTP version\nfunction getRTPVersion(){\n  try{\n      var proper_cookie = getCookie("proper_rtp_split_test");\n      \n      if(proper_cookie == "") return 0;\n      var json = JSON.parse(proper_cookie);\n      \n      if(json && json.version_id) return json.version_id;\n      else return 0;\n  }\n  catch(e){\n      return 0;\n  }\n}\n\n\n//Ad-free check (LaterPay / Scroll)\nvar adsRemovedWith = false;\n// LaterPay check\nif(getCookie(\'access\') != ""){\n    console.log(\'ads removed by Laterpay\');\n    adsRemovedWith = \'laterpay\';\n    salon_config.live_ads = 0;\n}\n// Scroll check\nelse if(getCookie(\'scroll0\') != ""){ \n    console.log(\'ads removed by scroll.com\');\n    adsRemovedWith = \'scroll\';\n    salon_config.live_ads = 0;\n}\n// Salon (stripe) check\nelse if(getCookie(\'salon_ad_free_user\') != ""){ \n    console.log(\'ads removed by Salon.com\');\n    adsRemovedWith = \'salon\';\n    salon_config.live_ads = 0;\n}\n\n// Shared Premium check \nelse if(getCookie(\'shared_premium\') != ""){ \n\n    //parse cookie\n    window.sharedPremium = parseCookie(getCookie(\'shared_premium\'));\n\n    //test if active shared subscription (exclude salon users who save $guid\'s for visitng partner sites)\n    if (window.sharedPremium.active && window.sharedPremium.active=="yes" && window.sharedPremium.site && window.sharedPremium.site!="salon") {\n        console.log(\'ads removed by shared premium - \'+window.sharedPremium.site);\n        adsRemovedWith = \'shared_premium_\'+window.sharedPremium.site;\n        salon_config.live_ads = 0;\n    }\n}\n\n// Hide ad containers for ad-free users\nif(salon_config.live_ads == 0){\n    // Fix css\n    var style = document.createElement(\'style\');\n    style.innerHTML = \'#subscription-mobile, #inline-subscription, #rc-widget-30ad7f, .outer-ad-unit-wrapper, .proper-ad-unit, #salon_auto_video { display: none !important; } .ad-topper { min-height:1em !important; height:1em !important; } .page-category .black-container { min-height:60px !important; height:60px !important; } \';\n    document.head.appendChild(style);\n    window.__SPOTIM_ADS_DISABLED__ = true;\n}\n\n// Counter for load-more button\nif(!window.counter) window.counter = 2;\n\n'}});  </script>

