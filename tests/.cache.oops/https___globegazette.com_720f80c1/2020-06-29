                jQuery(document).ready(function($){
                    function throttleIlad(func, wait){
                        var timeout;
                        return function(){
                            var context = this, args = arguments;
                            var later = function(){
                                timeout = null;
                                func.apply(context, args);
                            };
                            clearTimeout(timeout);
                            timeout = setTimeout(later, wait);
                        }
                    }
                    var iladLoaded = false;
                    var throttledIlad = throttleIlad(function(){
                        if( !iladLoaded ){
                            iladLoaded = __tnt.elementOnScreen('#ilad-iframe-placement');
                            if(iladLoaded){
                                $('#ilad-iframe-placement').append('<iframe src="https://masoncity.iloveadeal.com/widgets.php?cat_id=all" width="100%" height="250" frameborder="0" scrolling="no" allowtransparency="true"></iframe>');
                                window.removeEventListener('scroll', throttledIlad);
                            }
                        }   
                    }, 100);
                    window.addEventListener('scroll', throttledIlad);
                });
             </script>
            
