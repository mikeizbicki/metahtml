    <script>
        jQuery(document).ready(function($){
            function throttleFlip(func, wait){
                var timeout;
                return function(){
                    var context = this, args = arguments;
                    var later = function(){
                        timeout = null;
                        func.apply(context, args);
                    };
                    clearTimeout(timeout);
                    timeout = setTimeout(later, wait);
                }
            }
            var flipLoaded = false;
            var throttledFlip = throttleFlip(function(){
                if(!flipLoaded){
                    flipLoaded = __tnt.elementOnScreen('#circularhub_module_7366');
                    if(flipLoaded){
                        $('#circularhub_module_7366').append('<script async src="//api.circularhub.com/7366/e222353f8cfe9010/circularhub_module.js"><\/script>');
                        window.removeEventListener('scroll', throttledFlip);
                    }
                }   
            }, 100);
            window.addEventListener('scroll', throttledFlip);
        });
