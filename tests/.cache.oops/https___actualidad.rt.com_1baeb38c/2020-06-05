    googletag.cmd.push(function () {
        if (!isMainpage) {
            var ES_BILLBOARD_CONFIG = dynamicSlotConfig['ES_BILLBOARD'][hrefParams[3]] || dynamicSlotConfig['ES_BILLBOARD']['default'];
            var ES_SIDEBANNER_1_CONFIG = dynamicSlotConfig['ES_SIDEBANNER_1'][hrefParams[3]] || dynamicSlotConfig['ES_SIDEBANNER_1']['default'];
            var ES_SIDEBANNER_2_CONFIG = dynamicSlotConfig['ES_SIDEBANNER_2'][hrefParams[3]] || dynamicSlotConfig['ES_SIDEBANNER_2']['default'];

            if (ES_BILLBOARD_CONFIG && ES_BILLBOARD_CONFIG[deviceKey]) {
                console.log('ES_BILLBOARD_CONFIG');
                var ES_BILLBOARD = ES_BILLBOARD_CONFIG[deviceKey];
