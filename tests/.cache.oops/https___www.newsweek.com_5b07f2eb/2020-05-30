function TrafficSource(){
  if (document.referrer.search('(.*)www.googleapis.com([^/?]*)') === 0) {return 'GoogleChromeSuggestions';}
  else if (document.referrer.search('(.*)news.google.com([^/?]*)') === 0) {return 'GoogleNews';}
  else if (document.referrer.search('https?://(.*)google.([^/?]*)') === 0) {return 'Google';}
  else if (document.referrer.search('https?://(.*)bing.([^/?]*)') === 0) {return 'Bing';}
  else if (document.referrer.search('https?://(.*)yahoo.([^/?]*)') === 0) {return 'Yahoo';}
  else if (document.referrer.search('(.*).facebook.([^/?]*)') === 0) {return 'Facebook';}
  else if (document.referrer.search('https?://t.co/([^/?]*)') === 0) {return 'Twitter';}
  else if (document.referrer.search('(.*)www.newsbreakapp.com([^/?]*)') === 0) {return 'NewsBreak';}
  else if (document.referrer.search('(.*)www.smartnews.com([^/?]*)') === 0) {return 'SmartNews';}
  else if (document.referrer.search('(.*)news.ycombinator.com([^/?]*)') === 0) {return 'HackerNews';}
  else if (document.referrer.search('(.*)www.reddit.com([^/?]*)') === 0) {return 'Reddit';}
  else if (document.referrer.search('(.*)www.drudgereport.com([^/?]*)') === 0) {return 'DrudgeReport';}
  else if (document.referrer.search('(.*)www-newsweek-com.cdn.ampproject.org([^/?]*)') === 0) {return 'AmpCdn';}
  else if (document.referrer.search('(.*)www.zergnet.com([^/?]*)') === 0) {return 'Zergnet';}
  else if (document.referrer.search('(.*)apple.news([^/?]*)') === 0) {return 'AppleNews';}
  else if (document.referrer.search('(.*)com.particlenews.newsbreak([^/?]*)') === 0) {return 'NewsbreakApp';}
  else if (document.referrer.search('(.*)nativeapp.toutiao.com([^/?]*)') === 0) {return 'Toutiao';}
  else if (document.referrer.search('(.*)c.newsnow.co.uk([^/?]*)') === 0) {return 'Newsnow';}
  else if (document.referrer.search('(.*)quora([^/?]*)') === 0) {return 'Quora';}
  else if (document.referrer=='') {return 'Direct';}
  else if (referrer=='internal') {return 'Internal';}
  else {return 'Other';}
}
