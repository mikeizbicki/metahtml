                              jQuery(".shuffle").each(function(){

                                 var x = Math.floor(Math.random() * jQuery("dt", this).length);

                                 jQuery(jQuery("dt", this)[x]).show();
                              });

                              jQuery(".big-shots a").off('click').click(function(){

                                 var data_id = jQuery(this).attr('data-target');
                                 jQuery.ajax({
                                    url: "/tool/bigshot?cck=?",
                                    data: {'id_ids':data_id, 'headline':'Big Shots', 'original':jQuery(document).scrollTop()},
                                    dataType: "jsonp",
                                    success: function(json) {

                                       if(json.id_id)	location.hash = "big-shots/"+json.id_id;
                                       else	location.hash = "big-shots";

                                       //		console.log(json.html);
                                       jQuery("<div />", {"id":"big-shots-overlay"}).appendTo("body");
                                       jQuery("html,body").css({"overflow":"hidden"});

                                       jQuery("#big-shots-overlay").show().html(json.html);

                                    }
                                 });
                              });
