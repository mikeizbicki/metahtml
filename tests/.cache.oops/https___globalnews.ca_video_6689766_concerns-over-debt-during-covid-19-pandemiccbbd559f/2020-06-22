					function getVariationId( expId ) {
						var cookieName = '_gaexp';
						var cookieVal = document.cookie.match('(^|;)\\s*' + cookieName + '\\s*=\\s*([^;]+)');
    					cookieVal = cookieVal ? cookieVal.pop() : '';
