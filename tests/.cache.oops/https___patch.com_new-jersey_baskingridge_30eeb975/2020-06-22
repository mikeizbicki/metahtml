requestManager.sendAdserverRequest();}},TIMEOUT);});function defineAndFetchPrebidAds(){logHB('defineAndFetchPrebidAds');var PREBID_TIMEOUT=Drupal.settings.client_ads_dfp.prebid_timeout;var prebid_multiplier=parseFloat(Drupal.settings.client_ads_dfp.prebid_multiplier);var prebid_adder=parseFloat(Drupal.settings.client_ads_dfp.prebid_adder);var ga_enabled=Drupal.settings.client_ads_dfp.ga_enable;var ga_sampling_value=parseFloat(Drupal.settings.client_ads_dfp.ga_sampling);var adUnits=constructPrebidAdUnits(all_ad_units);logHB('pbjs_adUnits',adUnits);logHB('mobile_detect',mobile_detect);pbjs.addAdUnits(adUnits);if(ga_enabled){pbjs.que.push(function(){pbjs.enableAnalytics([{provider:'ga',options:{enableDistribution:false,sampling:ga_sampling_value}}])});}
var customConfigObject={"buckets":[{"precision":2,"min":0,"max":20,"increment":0.05},{"precision":2,"min":20,"max":50,"increment":1.00}]};pbjs.setConfig({priceGranularity:customConfigObject});pbjs.setConfig({disableAjaxTimeout:true});pbjs.setConfig({debug:false});pbjs.bidderSettings={standard:{adserverTargeting:[{key:"hb_bidder",val:function(bidResponse){return bidResponse.bidderCode;}},{key:"hb_adid",val:function(bidResponse){return bidResponse.adId;}},{key:"hb_pb",val:function(bidResponse){return bidResponse.pbCg;}},{key:"hb_size",val:function(bidResponse){return bidResponse.size;}},{key:"hb_deal",val:function(bidResponse){return bidResponse.dealId;}},{key:"hb_source",val:function(bidResponse){return bidResponse.source;}},{key:"hb_format",val:function(bidResponse){return bidResponse.mediaType;}}],bidCpmAdjustment:function(bidCpm,bid){var modified_bid=((bidCpm*prebid_multiplier)+prebid_adder);logHB("bidder: "+bid.bidderCode+", adUnit: "+bid.adUnitCode+", CPM: "+bidCpm+", modified CPM: "+modified_bid);return modified_bid;}},ix:{adserverTargeting:[{key:"hb_deal_IX",val:function(bidResponse){return bidResponse.dealId;}}],}}
