function fetchA9Bids(az_slots,timeoutA9){logHB('fetchA9Bids');if(az_slots.length){googletag.cmd.push(function(){apstag.fetchBids({slots:az_slots,timeout:timeoutA9},function(bids){logHB('about to call setDisplayBids()');apstag.setDisplayBids();requestManager.bidsBackForBidder("a9");});});}}
function defineAndFetchAmazonAds(){logHB('defineAndFetchAmazonAds');var amazon_client_id=Drupal.settings.fe_amazonads.id;var timeout=Number(Drupal.settings.fe_amazonads.timeout);if(!all_ad_units){return false;}
