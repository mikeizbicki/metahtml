(function() {
    if (!window.GPT) {
      return;
    }
    if (document.referrer) {
        var referrerMatch = document.referrer.match(/^https?:\/\/(?:[^@\/\n]+@)?(?:www\.)?([^:\/\n]+)/);
        if (referrerMatch) {
            window.GPT.referrer = referrerMatch[1];
        }
    }
    if (document.referrer || window.history.length > 2) {
        window.GPT.initialSessionPage = false;
    } else {
        window.GPT.initialSessionPage = true;
    }

    /* Google referrer check */
    if (document.referrer.match('google')) {
        if (window.GPT.tag) {
            window.GPT.tag.push('popupblock');
        } else {
            window.GPT.tag = ['popupblock'];
        }
    }
})();
</script>





