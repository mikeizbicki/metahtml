$( document ).ready(function() {
	$('a.show').click(function(){
		var id = $(this).attr('id');
		$('.showhover').hide();
		$('#date_'+id).show();
		return false;
		});
       $('span.cal_close a').click(function(){
		var id = $(this).attr('href');
		$(id).hide();
		return false;
      });
	});

