var adUnits=JSON.parse(adUnits_s);
var adUnits_nc=JSON.parse(adUnits_s);

var dfpPremium = 'std';

var posfilter = /-pr[vm]$/;
if (dfpPremium === 'prm') {
    posfilter = /-prv$/;
} else if (dfpPremium === 'prm-prw') {
    posfilter = /-prm$/;
}

var map = JSON.parse('{"div-gpt-ad-LDB1":"m","div-gpt-ad-LDB2":"c990x50_320x53","div-gpt-ad-LDB3":"mb","div-gpt-ad-MPU1":"r","div-gpt-ad-MPU2":"rb","div-gpt-ad-MPU3":"rc","div-gpt-ad-MPU4":"rd","div-gpt-ad-MPU5":"re","div-gpt-ad-MPU6":"rf","div-gpt-ad-MPU7":"rg","div-gpt-ad-MPU8":"rh","div-gpt-ad-LDB1P":"m-prm","div-gpt-ad-LDB1PRV":"m-prv","div-gpt-ad-MPU2P":"rb-prm","div-gpt-ad-MPU3P":"rc-prm","div-gpt-ad-MPU4P":"rd-prm","div-gpt-ad-MPU5P":"re-prm","div-gpt-ad-MPU1P":"r-prm","div-gpt-ad-MPU1PRV":"r-prv","div-gpt-ad-SD":"sd","div-gpt-ad-SI":"si"}');

adUnits = adUnits.filter(function(item) {
	if (item.code in map) {
        if (!map[item.code].match(posfilter)) {
            item.code = data.divId + '_' + map[item.code] + '_526a4ff00ab740d72d8b456c';
            return true;
        }
    }
    return false;
});

