    (function (window) {
      var appName = 'ueFooterBanner'
      var capaSelector = "banda_suscripcion"
      var banda

      function isEnabled () {
        var valorCookie = window.document.cookie.replace(new RegExp('(?:(?:^|.*;\\s*)'+ 'ELMUNDO_suscrp'+ '\\s*\\=\\s*([^;]*).*$)|^.*$'), '$1');
        return (valorCookie == '')
      }

      function isVisible () {
        banda || (banda = window.document.getElementById(capaSelector))
        if (banda && banda.style && banda.style.bottom === "0px") {
          return true
        } else {
          return false
        }
      }

      function hide () {
        banda || (banda = window.document.getElementById(capaSelector))
        if (banda && banda.style) {
          banda.style.bottom = "-70px";
        }
      }

      function show () {
        banda || (banda = window.document.getElementById(capaSelector))
        banda.style.bottom = "0px";
      }

      function close () {
        setCookie()
        hide()
      }

      function setCookie(){
        var days = 1;
        var myDate = new Date();
        myDate.setTime(myDate.getTime()+(days*24*60*60*1000));
        window.document.cookie = "ELMUNDO_suscrp = suscrip_yes; expires = " + myDate.toGMTString();
      }

      function checkSuscription() {
        var suscriptorSite = false;
        if ( typeof (sessionStorage.suscriptor) == "undefined" && typeof ueLoginPremium != 'undefined' && typeof ueLoginPremium.getSubscription == 'function') ueLoginPremium.getSubscription();
        if (typeof (sessionStorage.suscriptor) != "undefined" && sessionStorage.suscriptor != null) { 
            var suscriptor = JSON.parse(sessionStorage.suscriptor.toString());
             suscriptorSite = suscriptor.isPremium;
            if(suscriptor.isPremium){if(suscriptor.subscriptionPortals[0]!='5'){suscriptorSite = false;}}
        }
        
        banda || (banda = window.document.getElementById(capaSelector))
        if(!suscriptorSite){
              if(isEnabled()) {
                if (!isVisible()) {
                  show();
                  setCookie();
                } else {
                  window.document.removeEventListener("scroll", checkSuscription);
               }
        }}
      }


      var app = {
        close: close,
        hide: hide
      }

      if (window instanceof Object) {
        if (typeof window[appName] === 'undefined') {
          Object.defineProperty(window, appName, {
            value: app,
            writable: false
          })
        } else {
          console.warn('Library ' + appName + " cannot be declared because it's already exists.")
        }
      }

      window.document.addEventListener("scroll", checkSuscription);

    }(window));
    </script>
