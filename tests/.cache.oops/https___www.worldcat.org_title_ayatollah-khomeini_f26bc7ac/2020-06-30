<div class="la-gr-450 ssfixed ss-select-free" id="saveSearchPromptDiv" style="display:none">
	<!-- div header -->
	<div class="hd">
		<div class="cl"><a href="#" onclick="hideDiv('saveSearchPromptDiv');"><img src="/wcpa/images/spacer.gif" width="13" height="13" alt="Close" /></a></div>
		<div class="tx">Save this search</div>
		<div class="clr"></div>
	</div>

	<!-- div content -->
	<div class="ct">
		<div class="la-al-err" id="error_msg" style="display:none;"></div>

		<label class="la-in" for="searchTitle">Name this search <span>(75 characters)</span>:</label>
		<input class="la" name="searchTitle" id="searchTitle" type="text" />
		<div class="clr-sp"></div>
	
		<label class="la-in" for="searchDescription">Add a description <span>(optional, 500 characters)</span>:</label>
		<textarea class="la" name="searchDescription" id="searchDescription" rows="5"></textarea>
		<div class="clr-sp"></div>
	
		<label class="la-in" for="privacy_public">Select a privacy setting:</label>
		<div class="la-crc">
			<input type="radio" name="SrchPriv" id="privacy_public" /><label class="la-cr" for="privacy_public">Public</label><br />
			<input type="radio" name="SrchPriv" id="privacy_private" checked="checked"/><label class="la-cr" for="privacy_private">Private</label><br />
		</div>
