function loadingspinner() {
    //$(".spinner").show();
 $(".spinner").show();
 $(".spinner").addClass("icon-spin");

    setTimeout(function(){
    $(".spinner").hide();
     },5000);
}

jQuery(document).on('click', 'a', function () {

if (!$( this ).hasClass("exclude")) {

 $(".spinner").show();
 $(".spinner").addClass("icon-spin");

    setTimeout(function(){
    $(".spinner").hide();
     },5000);

}

});



jQuery(document).submit(function() {
 $(".spinner").show();
 $(".spinner").addClass("icon-spin");

    setTimeout(function(){
    $(".spinner").hide();
     },5000);
});



