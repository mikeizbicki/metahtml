<script async>


function toggleSlider() {

         if($('#image2').hasClass('flip2')){
             $('#image2').removeClass('flip2');
             $('#image2').addClass('flip4');    
         }else{
             $('#image2').addClass('flip2'); 
             $('#image2').removeClass('flip4');  
         }

//$("#image2").toggleClass('flip2');
  
    if ($("#panelThatSlides").is(":visible")) {
  
        $("#contentThatFades").animate(
            {
                opacity: "0"
            },
            200,
            function(){
                $("#panelThatSlides").slideUp(200);
            }
        );
    }
    else {
 
        $("#panelThatSlides").slideDown(200, function(){
            $("#contentThatFades").animate(
                {
                    opacity: "1"
                },
                200
            );
        });
    }
}
</script>







