var start = null;
var start2 = null;
 window.addEventListener("touchstart",function(event){
   if(event.touches.length === 1){
      //just one finger touched
startTime = new Date().getTime();
      start = event.touches.item(0).clientX;
      start2 = event.touches.item(0).clientY;
    }else{
      //a second finger hit the screen, abort the touch
      start = null;
      start2 = null;
    }
  });


window.addEventListener("touchend",function(event){
    var offset = 400;//at least 300px are a swipe
    if(start){
      //the only finger that hit the screen left it
 elapsedTime = new Date().getTime() - startTime;
      var end = event.changedTouches.item(0).clientX;
      var end2 = event.changedTouches.item(0).clientY;
      var vert = end2 - start2;

      if((end > start + offset) && (vert < 264 && vert > -264) && (elapsedTime <= 500)){
       //a left -> right swipe
            
        setTimeout(function(){
          window.history.go(-1);
         },100);
        
      }
      if((end < start - offset) && (vert < 264 && vert > -264 )  && (elapsedTime <= 500) ){
       window.history.go(1);
      }
    }
  });









        var feedback = function (res) {
            if (res.success === true) {
                document.querySelector('.status').classList.add('bg-success');
                document.querySelector('.status').innerHTML = '<img style="margin-top:-10px" src="https://boxden.com/images/checkmark.png" width="24" height="24" />';
 
                

 $('#vB_Editor_QR_textarea').val( $('#vB_Editor_QR_textarea').val() + " " + "\r\n[img]" + res.data.link + "[/img]" );

 
 

            }
        };

        new Imgur({
            clientid: '65489e26a46e0cf',
            callback: feedback
        });







$("#quickreplybutton").click(function() {

$( "#vB_Editor_QR_textarea" ).focus().get(0).scrollIntoView();
window.scrollBy(0, -180);

        setTimeout(function(){
         $( "#vB_Editor_QR_textarea" ).focus().get(0).scrollIntoView();
         window.scrollBy(0, -180);
         },250);

        setTimeout(function(){
         $( "#vB_Editor_QR_textarea" ).focus().get(0).scrollIntoView();
         window.scrollBy(0, -180);
         },500);

});







var a = 0;
$('.menu-ham').click(function() {
    if(a % 2 == 0){
  $('.menu').animate({right: '0px'}, 100);
$('.menu-ham').animate({right: '252px'}, 100);
$("#image1").toggleClass('flip');


   }
    else{
  $('.menu').animate({right: '-270px'}, 100);
$('.menu-ham').animate({right: '5px'}, 100);
$("#image1").toggleClass('flip');

    }
    a++;
});




</script>









