<script>

function millisecs(dateInString) {
	newsDate = new date(dateInString);		
	console.log("Date - Time in milliseconds : " + date.getTime());
}


advs = [];
i=0;
$(".ansacom-box").each(function(index) {
	pub = $(this).attr("data-pub");
	newsDate = new Date(pub);		
	time = (new Date()).getTime();
	from = (time - newsDate.getTime()) / 3600000;
	//console.log(parseInt(from)<=(24*counts[index]))
	if (counts[index]!=0 && parseInt(from)<=(24*counts[index])) { 
		advs[i] = index;
		i++;
	} 
	//console.log("from : " + from);
	//console.log(advs)
});

//	if (document.location.href.indexOf("demo_ansacom") != -1) {
		if (isMobile.any()) {
			var itemarticle = $("article")[4];
			$("div.pp-inner").find(itemarticle).append($('#ansa_native_ansacom'));

			//var length = $(".ansacom-box").length;
			var length = advs.length;
			var i = Math.floor((Math.random() * length));
			if (length > 0) $(".ansacom-box").eq(advs[i]).show();
		}
//	}
</script>


