<!-- refresh google adv -->
<script>
	window.onload = function(e) {
		if (document.location.href.indexOf('refresh_ce') != -1 && !(document.hasFocus()) && !withinCMS())
			googletag.pubads().refresh();
	}
	window.onfocus = function() {
		try {
			//console.log('focus');
			if (!withinCMS())
				googletag.pubads().refresh();
		} catch(err) {
			//console.log(document.location.href);
		}
	}; 
</script>


