                    data: [null,8,16,15,24,26,26,38,43,46,45,58,64,66,73,73,86,89,97,108,97,146,122,143,143,106,98,136,117,121,113,100,158,81,64,37,58,65,54,73,67,85,83,102,106,105,228,197,274,330,353,448,414,691,645,819,986,1100,1383,1648,1648,1937,2495,2592,2964,3476,3710,3379,4175,4736,5199,6299,6010,6152,5074,5637,7900,6798,7680,7403,6335,5678,5723,7444,8193,7039,8485,6688,4976,5561,7226,6653,6768,6409,6178,3844,4521,6755,6621,5821,5684,5312,3567,4104,5827,6842,5614,5501,4299,3600,3498,5539,5227,5284,5010,4359,3647,3477,4584,4635,4960,4663,4188,3193,3182,3785,5316,4693,4927,4140,3284,3118,4672,4929,5573,4908,4289,3444,3183,4773,5151,5003,4614,4266,3299,3438,6586,5207,5154,5104,4457,3353,3831,5446,5044,5168,4843,4556,3467,3415]                }
                /*, {
                         name: '7-day moving average',
                         type: 'spline',
                         data: null,
                         lineWidth: 2,
                         color: '#444'

                     }*/
            ],

            responsive: {
                rules: [{
                    condition: {
                        maxWidth: 800
                    },
                    chartOptions: {
                        legend: {
                            layout: 'horizontal',
                            align: 'center',
                            verticalAlign: 'bottom'
                        }
                    }
                }]
            }

        });
    </script>
