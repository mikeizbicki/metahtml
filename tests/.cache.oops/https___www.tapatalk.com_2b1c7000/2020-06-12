<script>
    function clearPiwikCookies() {
        var name_value, key;
        if (typeof document.cookie !== "undefined" && document.cookie && document.cookie != '') {
            var split = document.cookie.split(';');
            for (var i = 0; i < split.length; i++) {
                name_value = split[i].split("=");
                key = name_value[0].replace(/^ /, '');
                if (key && key.substr(0, 7) == "_pk_id.") {
                    deleteCookie(key);
                }
            }
        }
        deleteLocalStorage();
    }
    function deleteCookie(name) {
        var d = new Date();
        d.setTime(d.getTime() - 1000);
        var expires = "expires="+ d.toUTCString();
        document.cookie = name + "=" + "0" + ";" + expires + ";path=/";
    }
    function deleteLocalStorage(){
        localStorage.removeItem('Forums_info_record');
    }
    clearPiwikCookies();
</script></body>
