	<script type='text/javascript'>
		var googletag = googletag || {};
		googletag.cmd = googletag.cmd || [];
		(function() {
			var gads = document.createElement('script');
			gads.async = true;
			gads.type = 'text/javascript';
			var useSSL = 'https:' == document.location.protocol;
			gads.src = (useSSL ? 'https:' : 'http:') +
				'//www.googletagservices.com/tag/js/gpt.js';
			var node = document.getElementsByTagName('script')[0];
			node.parentNode.insertBefore(gads, node);
		})();
	</script>

        <script type='text/javascript'>

                /* get the main category to use for the MSection variable */
                var root_path = unescape(document.location.pathname);
                root_path.indexOf(1);			
                root_path.toLowerCase();				
                root_path = root_path.split("/")[1];
                var root_path = '/' + root_path;
                var root_path = root_path.replace(/[^a-z0-9\/]|\s+|\r?\n|\r/gmi, "_");
                if ( root_path == '/' ) {
                    root_path = '/homepage';
                }

                /* full_path comes from a TownNews provided variable */
                var full_path = '/news/localnews';
                if ( full_path == '/' ) {
                    full_path = '/homepage';
                }

                console.log("MSection = " + root_path);
                console.log("SubSection = " + full_path);

                googletag.cmd.push(function() {
                    googletag.pubads().setTargeting('SiteID','ORWP');
                    googletag.pubads().setTargeting('MSection',root_path); 	/* top lvl parent */
                    googletag.pubads().setTargeting('SubSection',full_path); /* current lvl parent */
		});
	</script>

	<script type='text/javascript'>
		googletag.cmd.push(function() {

			var Leaderboard = googletag.sizeMapping().
			addSize([992, 200], [728, 90]).
			addSize([0, 0], [320, 50]).
			build();


			var BottomLeaderboard = googletag.sizeMapping().
			addSize([992, 200], [[728, 90]]).
			addSize([0, 0], [320, 50]).
			build();


			var HalfPage = googletag.sizeMapping().
			addSize([992, 200], [300, 600]).
			addSize([0, 0], [320, 50]).
			build();


			var Middle = googletag.sizeMapping().
			addSize([992, 200], [300, 250]).
			addSize([0, 0], [320, 50]).
			build();


			var Middle3 = googletag.sizeMapping().
			addSize([992, 200], [300, 250]).
			addSize([0, 0], [320, 50]).
			build();

			var Middle4 = googletag.sizeMapping().
			addSize([992, 200], [300, 250]).
			addSize([0, 0], [320, 50]).
			build();


			var MiddleBanner = googletag.sizeMapping().
			addSize([992, 200], [728, 90]).
			addSize([0, 0], [320, 50]).
			build();


			var Featured = googletag.sizeMapping().
			addSize([992, 200], [300, 250]).
			addSize([0, 0], [320, 50]).
			build();


			var Pencil = googletag.sizeMapping().
			addSize([480, 0], [[1200,42]]).
			build();
			

			/* for pencil ad */
                        /* ensure it only gets called on the HOMEPAGE */
                        if ( root_path == '/homepage' || root_path == '/test' ) {
                            googletag.defineSlot('/1032081/ORWP_Pencil_1200x42', [1200, 42], 'ORWP_Pencil_1200x42').
                            defineSizeMapping(Pencil).
                            addService(googletag.pubads());
                        }
			/* for pencil ad */


                        googletag.pubads().addEventListener('slotRenderEnded', function(event) {
				/*console.log(event.slot.getAdUnitPath() + ' - position is blank?: ' + event.isEmpty + ' - lineItemID: ' + event.lineItemId + ' - creativeId: ' + event.creativeId + ' - campaignId: ' + event.campaignId);*/
                                var ad_position = event.slot.getAdUnitPath().split("/");
                                var ad_position = ad_position[ad_position.length-1];
 				if (event.isEmpty) {
                                    document.getElementById(ad_position).style.display = 'none';
				}
			});



                        /* ensure these don't get called on the HOMEPAGE */
                        if ( root_path != '/homepage' ) {
                        
                            googletag.defineSlot('/1032081/ORWP_Middle4_300x250', [300, 250], 'ORWP_Middle4_300x250').
                            defineSizeMapping(Middle3).
                            addService(googletag.pubads());

                            googletag.defineSlot('/1032081/ORWP_Right_300x600', [300, 600], 'ORWP_Right_300x600').
                            defineSizeMapping(HalfPage).
                            addService(googletag.pubads());
    
                            googletag.defineSlot('/1032081/ORWP_Top_728x90', [728, 90], 'ORWP_Top_728x90').
                            defineSizeMapping(Leaderboard).
                            addService(googletag.pubads());
                                                    
                        }


                        /*googletag.defineSlot('/1032081/ORWP_LatestNewsSponsor_300x50', [300, 50], 'ORWP_LatestNewsSponsor_300x50').addService(googletag.pubads());*/

                      	googletag.defineSlot('/1032081/ORWP_Video_300x250', [300, 250], 'ORWP_Video_300x250').
			defineSizeMapping(Featured).
			addService(googletag.pubads());

			googletag.defineSlot('/1032081/ORWP_Middle_300x250', [300, 250], 'ORWP_Middle_300x250').
			defineSizeMapping(Middle).
			addService(googletag.pubads());

			googletag.defineSlot('/1032081/ORWP_Middle3_300x250', [300, 250], 'ORWP_Middle3_300x250').
			defineSizeMapping(Middle3).
			addService(googletag.pubads());

			googletag.defineSlot('/1032081/ORWP_Bottom_728x90', [728, 90], 'ORWP_Bottom_728x90').
			defineSizeMapping(BottomLeaderboard).
			addService(googletag.pubads());


			googletag.defineSlot('/1032081/ORWP_Middle2_300x250', [300, 250], 'ORWP_Middle2_300x250').addService(googletag.pubads());
 
                        /* ensure these positions are only called on an ARTICLE page */
                        var full_url = '/news/localnews/';
                        if ( full_url.split("/").pop().substring(0,8) == 'article_' ) {
                            
                            /* don't show on articles in SPONSORED pages */
                            if (  root_path != '/sponsored' ) {
                            
