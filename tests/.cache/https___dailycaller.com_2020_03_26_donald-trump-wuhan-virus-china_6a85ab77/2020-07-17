<script>
	jQuery(document).ready(function() {
		var played = {};
		jQuery('audio').on('play', function(evt) {
			var src = jQuery(this).children('source:first-child').attr('src');
			if(!played[src]) {
				played[src] = true;
				(typeof ga == 'function') && ga('send', {
					hitType: 'event',
					eventCategory: 'Audio',
					eventAction: 'Play',
					eventLabel: src
				});
			}
		});
	});
	</script>

