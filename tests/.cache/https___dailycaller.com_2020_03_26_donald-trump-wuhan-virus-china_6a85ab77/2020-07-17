<div id="dailycaller_skin" class="ad"></div>
</div>
<script>
	googletag.cmd.push( function() {
		googletag.pubads().addEventListener('slotRenderEnded', function(event) {
			console.log("DC AD: slotRenderEnded for " + event.slot.getAdUnitPath());
			if(event.isEmpty) {
				console.log("DC AD: Empty slot: " + event.slot.getAdUnitPath());
				if(event.slot.getAdUnitPath() == '/10519169/dailycaller_sticky') {
					jQuery('.dc-sticky').css('display', 'none');
				}
				if(event.slot.getAdUnitPath() == '/10519169/dailycaller_top'){
					$("#dailycaller_top").show();
				}
				return;
			}
			if(event.size.width > 0) {
				$('div.ad div').css('width', '');
			}
			if(event.size.height > 0) {
				$('div.ad div').css('height', '');
			}
			switch(event.slot.getAdUnitPath()) {
				case '/10519169/dailycaller_top':
					$("div[class='adcontainer']").css('min-height','250px');
					$('#dailycaller_top').css('background-color', 'transparent')
					$('#dailycaller_top [id^="google_ads_iframe_"]').css('position','static');
	
					if($('#dailycaller_top [id^="google_ads_iframe_"]').height() === 90){
						$('#dailycaller_top [id^="google_ads_iframe_/10519169/dailycaller_top_0__container__"]').css('padding-top','80px');
					}
					
					break;	
				case '/10519169/dailycaller_inline2':
					$('#dailycaller_inline2, #\\/10519169\\/dailycaller_inline2').css('display', 'block');
					break;
				case '/10519169/dailycaller_inline3':
					$('#dailycaller_inline3, #\\/10519169\\/dailycaller_inline3').css('display', 'block');
					break;
				case '/10519169/dailycaller_right1':
					if(jQuery(window).width() >= dcRespMaxW) {
						$('#dailycaller_right1').css('min-height', '1010px');
						$('#dailycaller_right1').css('margin-bottom', '40px');
					}
					break;
				case '/10519169/dailycaller_left1':
					$('#dailycaller_left1 div').css('position', 'static');
					break;
				case '/10519169/dailycaller_left2':
					$('#dailycaller_left2').css('margin-bottom', '40px');
					break;
				case '/10519169/dailycaller_home3':
					$('#dailycaller_home3 div').css('position', 'static');
					break;
				case '/10519169/dailycaller_home4':
					$('#dailycaller_home4 div').css('position', 'static');
					break;
				case '/10519169/dailycaller_interstitial':
					//console.log("DC Interstitial: Opening!");
					jQuery('#dailycaller_interstitial')
						.css('width', event.size[0])
						.css('height', event.size[1]);
					jQuery('.dc-interstitial')
						.removeClass('hidden');
					break;
				case '/10519169/dailycaller_sticky':
					console.log("DC Sticky: Opening!");
					jQuery('.dc-sticky .close').click(function() {
						jQuery('.dc-sticky').css('display', 'none');
					});
					break;
				case '/10519169/dailycaller_skin':
					console.log('DC AD: SKIN disabling refresh');
					dc_disable_refresh = true;
					//console.log('DC AD: SKIN Opening!');
					var content = $('#dailycaller_skin iframe').contents();
					var imgad = content.find(".img_ad[src!=''], .img_ad img[src!='']").attr('src');
					var imghref = content.find('a')[0].href;
					$('body')
						.css('background-image', 'url(' + imgad + ')')
						.css('cursor', 'pointer');
					$(document).on('click','body',function(event){
						if (event && event.target == document.body) {
							window.open(imghref);
							console.log(`DC AD: SKIN: imghref is ${imghref}`);
							console.log(`DC AD: SKIN: imgad is ${imgad}`);
						}
						if (event.target.tagName == "MAIN"){
							window.open(imghref);
							console.log(`DC AD: SKIN: imghref is ${imghref}`);
							console.log(`DC AD: SKIN: imgad is ${imgad}`);
						}
					});			
					//console.log('DC AD: SKIN ' + imgad);
					//console.log('DC AD: SKIN ' + imghref);
					break;
	
			}		
		});
	});
