        $( document ).ready( function() {
            $('.menu_list li .icon_view').on('click', function () {
                $(".menu_list div").slideUp();
                $('.menu_list li').removeClass('on');
                if(!$(this).next().is(":visible"))
                {
                    $(this).next().slideDown();
                    $(this).closest('li').addClass('on');
                }
            });
