jqlsticky(function() {
    if (jqlsticky("#left_sticky_ad").offset()) {
        var t = jqlsticky("#left_sticky_ad").offset().top;
	//t = t+250;
        jqlsticky(window).scroll(function() {
            var i = jqlsticky(window).scrollTop(),
                c = jqlsticky("#google_translate_element").offset().top - 500;
            i > c ? jqlsticky("#left_sticky_ad").css({
                position: "absolute",
                top: c - 180
            }) : jqlsticky("#left_sticky_ad").css({
                position: "fixed",
                top: "0px"
            }), t < i || jqlsticky("#left_sticky_ad").css("position", "static")
        })
    }
    if (jqlsticky("#combi_sticky_ad").offset()) {
        jqlsticky("#combi_sticky_ad").offset().top;
        jqlsticky(window).scroll(function() {
            var t = jqlsticky("#yellow_ad_end").offset().top;
            t = t;
            var i = jqlsticky(window).scrollTop();
            t < i ? jqlsticky("#combi_sticky_ad").css({
                position: "fixed",
                top: "0px"
            }) : jqlsticky("#combi_sticky_ad").css({
                position: "absolute",
                top: t - 80
            })
        })
    }
}), jqlsticky(".updatedesktopclickcount").click(function() {
