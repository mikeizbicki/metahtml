		<script>
			// @formatter:off
			if (metro.pageData) {
				var rtaParams = {};

				if (document.referrer) {
					rtaParams.r = document.referrer;
				}

				rtaParams.rp = (metro.siteData) ? metro.siteData.deviceType : '';

				rtaParams.category = metro.pageData.topCategory;
				rtaParams.subcategory = metro.pageData.subCategory;

								rtaParams.authors = metro.pageData.author;
				rtaParams.title = metro.pageData.headline;
				rtaParams.pubDate = metro.pageData.pubDateIso8601;
				
				rtaParams.tags = [];
				if (metro.pageData.postTags) {
					rtaParams.tags = metro.pageData.postTags.split(',');
				}

				if (metro.pageData.postStyle) {
					rtaParams.tags.push(metro.pageData.postStyle);
				}

				if ( metro.pageData.isBlogPost === true ) {
					rtaParams.tags.push('blogs');
				}

				
				// Only call a view tracker if we are NOT on a video/embed page
				if (window.location.href.indexOf('video/embed') === -1) {
					if ('RTA' in window){
						window.RTA.tedEvent('view', rtaParams);
					}
				}
			}

			// This is just a pass through for legacy
			metro.ted = metro.ted || {};
			metro.ted.tedEvent = function(type, params) {
				window.RTA.tedEvent(type, params);
			}
			// @formatter:on
		</script>

		<script type='text/javascript' src='https://stats.wp.com/e-202024.js' async='async' defer='defer'></script>
<script type='text/javascript'>
	_stq = window._stq || [];
	_stq.push([ 'view', {v:'ext',j:'1:8.6.1',blog:'146859608',post:'12431557',tz:'1',srv:'metro.co.uk'} ]);
