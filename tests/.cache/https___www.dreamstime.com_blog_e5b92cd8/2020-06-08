                <script type="text/javascript">
                    function checkSearchSubmitBlog()
                    {
                        if (document.getElementById('searchblog').value) {
                            searchgg=document.getElementById('searchblog').value.replace(/[`~!@#$%^&*()_|+\-=?;:'",<>\{\}\[\]\\\/]/gi, ' ').replace(/\s{2,}/g, ' ').trim().toLowerCase();
                            searchgg=searchgg.split(" ").join("-");
                            searchgg='https://www.dreamstime.com/blog/search-tag-'+searchgg;
                            window.location=searchgg;
                        }
                        return false;
                    }
                    $(function () {
                        $('input[id="searchblog"]').keydown(function (e) {
                            if ((e.which || e.keyCode) == 13) {
                                checkSearchSubmitBlog();
                                return false;
                            }
                        });
                    });
                </script>
                                <div class="side-search">
                    <input type="text" class="input_text" name="searchblog" id="searchblog" value=""  placeholder="I'd like to read about..." />
                    <a class="submit button grey" href="#" onclick="checkSearchSubmitBlog()"><i class="icon icon-search"></i>Search blogs</a>
                </div>
                            </div>
