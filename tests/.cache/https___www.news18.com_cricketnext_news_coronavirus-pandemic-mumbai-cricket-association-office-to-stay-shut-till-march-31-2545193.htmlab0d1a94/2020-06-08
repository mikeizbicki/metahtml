	<!--Desktop Live Widget On Scroll-->
	<script>
$(document).ready(function(){
	ajaxCallTopScoreWidget();   	
});
function ajaxCallTopScoreWidget(){
    var jsonUrl = "https://cricketnext.nw18.com/json/cross_scorecard_desktop.json";
    if(jsonUrl){
		$.ajax({
			url: jsonUrl,
			// cache: true,
			dataType: "jsonp",
			jsonp: 'jsonp',
			jsonpCallback: "setLiveScores",
			crossDomain: true,
			success: function(data){
				var currentMatch ="";
				if(data == "undefined"){
					return;
				}
				arrResult = data.livematches;
				// Loop over Matches
				var count = 1;
				arrResult.forEach(function(item,index) {		
					var currentMatchId = item.matchid.toLowerCase();
					//if(item.matchid == currentMatch.toLowerCase()){ 			
						$('.statusResult_'+currentMatchId).text(item.result);
						
						if(item['teama']['scores'] != null){
						
							$('.match-score-one_'+currentMatchId).text(item['teama']['scores'][0].scores);
							var scoresA = "(" + item['teama']['scores'][0].overs + ")";
							$('.overs-one_'+currentMatchId).text(scoresA);
						}  
						if(item['status'] != null){
							$('.matchStatus_'+currentMatchId).text(item['status']);	
						}
						
						if(item['teamb']['scores'] != null){
							$('.match-score-two_'+currentMatchId).text(item['teamb']['scores'][0].scores);
							var scoresB = "(" + item['teamb']['scores'][0].overs + ")";
							$('.overs-two_'+currentMatchId).text(scoresB);
						}
					//}
				});
			},
			complete: function(data){
				setTimeout(function(){
					ajaxCallTopScoreWidget()
				}, 15000);
			}
		});
	}
}

</script>	
	<!--Desktop Live Widget On Scroll-->
