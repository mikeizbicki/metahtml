<script  src="https://images.news18.com/static_news18/pix/ibnhome/news18/cricketnext/wc2019/js/slick.min.js?v=1.3"></script>
<script src="https://images.news18.com/static_news18/pix/ibnhome/news18/cricketnext/wc2019/js/cn-common.js?v=1.12"></script>

<script>
	$(document).ready(function() {

		function detectMobileDeviceType() { 
			
			if( navigator.userAgent.match(/Android/i)){
				return 'android';
			}else if(navigator.userAgent.match(/iPhone/i) ){
				return 'iphone';
			}else {
				return '';
			}
		}	
		
		var deviceType = detectMobileDeviceType();
		
		if(deviceType=='android'){
			$('.appDownloadPrompt').attr('href','https://play.google.com/store/apps/details?id=com.divum.CricketNext&hl=en');
		}else if(deviceType=='iphone'){
			//$('.appDownloadPrompt').attr('href','https://itunes.apple.com/us/app/ibnlive-for-iphone/id395194912?mt=8');
			$('.appDownloadPrompt').attr('href','https://webapp.diawi.com/install/oRKr1o');
		}else{
			$('.appDownloadPrompt').attr('href','javascript:void(0);');
		}
		var website_type = 'desktop';
		if(website_type=='mobile'){	
			if(sessionStorage.getItem('popState') != 'close'){
				$('.getapp_wrap').css('display','block');
			}else{
				$('.getapp_wrap').hide();
			}
			$('.gtap_close').on('click', function(){
				$('.getapp_wrap').hide();
				sessionStorage.setItem("popState", 'close');
			});			
		}
		
		$(".pck").click(function(){
			$(".pcake").toggleClass('addclass');
			$(".drpdn-menu").slideToggle();
		}); 
	}); 
</script>

	<script>
		$(document).ready(function() {
			$(window).scroll(function(){
				var scroll = $(window).scrollTop();
				if (scroll >= 250) {
					$('header').addClass('fixed-h');
					$('.desktop-scard').addClass('fxdscrd');
					$('.stky-nav').addClass('fxd-stkynav');
					$('nav').addClass('fxd-nav');
					$('.drpdn-menu').addClass('fxddrpmn');
					$('.cn-homesec').addClass('scroll_padding');
					if($( "div" ).hasClass( "stickyscore_card" )){
						$('.stickyscore_card').get(0).slick.setPosition();
					}
				}else {
					$('header').removeClass('fixed-h');
					$('.desktop-scard').removeClass('fxdscrd');
					$('.stky-nav').removeClass('fxd-stkynav');
					$('nav').removeClass('fxd-nav');
					$('.drpdn-menu').removeClass('fxddrpmn');
					$('.cn-homesec').removeClass('scroll_padding');
					if($( "div" ).hasClass( "score-match-list" )){
						$('.score-match-list').get(0).slick.setPosition();
					}
				}
			});
		}); 
	</script>
