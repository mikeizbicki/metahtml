<script type='text/javascript'>//<![CDATA[ 
$(window).load(function(){
$(document).scroll(function () {
    var y = $(this).scrollTop();
    if (y > 300) {
        $('.sidebox').fadeIn();
    } else {
        $('.sidebox').fadeOut();
