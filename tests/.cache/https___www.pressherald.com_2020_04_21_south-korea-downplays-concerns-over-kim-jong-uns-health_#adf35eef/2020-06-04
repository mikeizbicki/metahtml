   /**
    * Code's purpose is to try and get the page to suppress or remove the paywall overlay
    */
(function( $, undefined ){
   'use strict';

    $(init);

    function init(){
      $('[id*="pum"]')
         .on('pumBeforeOpen', function () {
            $('body').addClass('pop-up-exposed');
            if( 'undefined' !== typeof Paywall ){
               Paywall.restore();
            }
         });

      $('[id*="pum"]')
         .on('pumBeforeClose', function () {
            if( 'undefined' !== typeof Paywall ){
               Paywall.restore();
            }
         });
   }

   
   return {};
}( jQuery ));
	

