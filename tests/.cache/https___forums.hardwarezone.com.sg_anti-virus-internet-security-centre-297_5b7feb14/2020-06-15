<script type="text/javascript">
(function(){
	if (typeof(hwz_ga) == 'undefined') return;
	var a = $$('img.forum_sponsor_logo');
	if (a.length == 0) return;
	for(var i=0;i<a.length;i++) {
		if (!a[i]) continue;
		var s = a[i].get('alt');
		if (!s) continue;
		hwz_ga_lazy(a[i], 'ForumSponsorLogo', 'Impression', s, { nonInteraction: true, position:(i+1) });
		hwz_ga_click(a[i], 'ForumSponsorLogo', 'Click', s, { nonInteraction: true, position:(i+1) });
	}
}).delay(5000);
</script>
