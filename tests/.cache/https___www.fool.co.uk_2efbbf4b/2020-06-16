        if(jQuery.cookie('utm_values') == undefined){
            var win = jQuery.url(window.location);
            var utm_values = {};
            if(win.param('utm_source')) utm_values.utm_source = Fool.Base.htmlEntities(win.param('utm_source'));
            if(win.param('utm_medium')) utm_values.utm_medium = Fool.Base.htmlEntities(win.param('utm_medium'));
            if(win.param('utm_campaign')) utm_values.utm_campaign = Fool.Base.htmlEntities(win.param('utm_campaign'));
            if(win.param('utm_content')) utm_values.utm_content = Fool.Base.htmlEntities(win.param('utm_content'));
            if(win.param('utm_term')) utm_values.utm_term = Fool.Base.htmlEntities(win.param('utm_term'));

            jQuery.cookie.raw = true;
            if(utm_values !== {} && utm_values.utm_campaign){
                jQuery.cookie('utm_values', JSON.stringify(utm_values), { path: "/", json: true });
            }
        }

