function isVisible(a) {
  var parent_cordinates= document.querySelector(".rebelbar .section-wrap").getBoundingClientRect().top
return	a.getBoundingClientRect().top - parent_cordinates < 20 ? true : false;
}

	
var space_left= true;
 function setRrebalbarMenu(){
	for (var i of document.querySelectorAll('.rebelbar .list > .list__item:not(.show)')) {
        if(space_left){
		if (!isVisible(i)) {
          if(document.querySelector(".hidden_menu_item")){}
          else{
            i.insertAdjacentHTML('beforebegin','<a class="list__item show hidden_menu_item" href="javascript:;"><span class="section-name">More</span><div class="list_dropdown"></div></a>');}
            space_left=false;
            document.querySelector(".list_dropdown").appendChild(i);
            document.querySelector(".rebelbar .section-wrap").classList.add("calculated")
        }
        }
        else{
            document.querySelector(".list_dropdown").appendChild(i);
        }
	}
 }
//setRrebalbarMenu()
