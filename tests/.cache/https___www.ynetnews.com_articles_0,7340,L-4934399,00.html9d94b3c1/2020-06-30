                            imgCredit = yq(value).children().find('.citv_credit').html() || yq(value).children().find('.ya_citv_credit').html();

     
							imgTitleFinal = ""
							if (imgTitle != null)
							{
							var lengthTitle = 90;
                            var imgTitleLength = imgTitle.length;
							var TitleTrimmed;
							TitleTrimmed = imgTitle;
							if (imgTitleLength > lengthTitle)
							{
							  TitleTrimmed = imgTitle.substring(0, 90);
							  TitleTrimmed += "...";
							}					
