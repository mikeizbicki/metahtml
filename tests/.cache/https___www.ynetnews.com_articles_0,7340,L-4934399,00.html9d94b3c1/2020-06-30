					function ATFmnpltn(PlyrInf) {
						try { 
						ref=false;
						if(window.top.document.referrer != "" && parent.performance.navigation.type != parent.PerformanceNavigation.TYPE_RELOAD){
							 ref = new URL(window.top.document.referrer).origin
						}
						
