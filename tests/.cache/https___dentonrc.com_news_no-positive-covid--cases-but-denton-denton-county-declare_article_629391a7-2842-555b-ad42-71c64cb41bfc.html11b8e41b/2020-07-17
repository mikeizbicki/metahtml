    <div class="clearfix"></div>
</form></ul>
        </li>
    </ul>
 

<script>
    $('#site-search-631345-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-631345').submit();
        return false;
    });

    $('#site-search-631345').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-631345 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-631345 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-631345-term').val() ){
            return true;
        }else{
            return false;
        }
    });</script>
