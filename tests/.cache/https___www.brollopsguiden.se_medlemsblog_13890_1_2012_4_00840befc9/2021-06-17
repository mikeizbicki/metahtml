	</div><!-- /.modal -->					<script type="text/javascript">
	function switchColumns() {
		var cl1 = '';
		var cl2 = '';
		$('#ads_right').toggle();
		if ($('#sidebar').is(':visible')) {
			cl1 = 'col-8';
			cl2 = 'col-12';
		} else {
			cl1 = 'col-12';
			cl2 = 'col-8';
		}
		$('#content').removeClass(cl1);
		$('#content').addClass(cl2);
	}
</script>


