<script>
    $('#site-search-579323-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-579323').submit();
        return false;
    });

    $('#site-search-579323').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-579323 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-579323 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-579323-term').val() ){
            return true;
        }else{
            return false;
        }
    });</script>
</div></div>
                                
