						$("html, body").animate({ scrollTop: 0 }, "slow");				
		});		
		
		$('.video-player').hide();
					
    $( document ).on( 'click', '.play-tv', function(e) {   	
          				e.preventDefault();										
									video_id = $(this).attr('id');
									$('.icon').removeClass('active');
									$(this).addClass('active');
									$('audio').hide();
									$('#audio').attr('src', '');				
									$('.audio-data').html('');
									$('.video-content').show();									
									video_title = $(this).parent().find('h4').clone().appendTo('.audio-data');		
          				$('#audio-player').animate({bottom:'0px'},1000).css('height', '240px');
          				$('.radio-player').html('<iframe src="https://www.youtube.com/embed/'+ video_id +'?enablejsapi=1&version=3&wmode=transparent&autoplay=1&controls=0" frameborder="0" allowfullscreen="allowfullscreen"></iframe>');
		});			
										
    $( document ).on( 'click', '.stop-video-player', function(e) {   	
 				e.preventDefault();	
 				$('.video-player').html('');
 		});	
							
    $('.audio-hide ').click(function (e) {
				$('#audio-player').animate({bottom:'-240px'},1000);
				$('#audio').attr('src', '');				
				$('.radio-player').html('');
				$('.icon').removeClass('active');
    });
})           

	
	function mouseOverMe(elem){
		$('.video-player').show();
		if (elem.attr('id')){
			var video_id = elem.attr('id');			
			var data = elem.find('.video-data').html();
			$('.player-video-data').html(data);
			$('.video-player').html('<iframe id="+ video_id +" src="https://www.youtube.com/embed/'+ video_id +'?enablejsapi=1&version=3&wmode=transparent&autoplay=1" frameborder="0" allowfullscreen="allowfullscreen" style="width: 100%; height: 100%;"></iframe><div class="overlay" style="display: none;"></div>');						
		}else {	
			var query = elem.find('.keywords').html();				
				
			if (!query.length) {
	            var query = elem.find('.title_keywords').html();	
	        }  						
			$.ajax({
							context: elem,
	                        url: 'https://wn.com/api/upge/cheetah-search-adv/video',                       
	                        cache: true,
	                        data: {
	                            'query': query
	         },
	                        dataType: 'jsonp',
	                        success: function(text) {
	                        	if (text.length > 0) {
	                            video_id = text[0].id;
	                            
	                            $('.video-player').html('<iframe id="+ video_id +" src="https://www.youtube.com/embed/'+ video_id +'?enablejsapi=1&version=3&wmode=transparent&autoplay=1" frameborder="0" allowfullscreen="allowfullscreen" style="width: 100%; height: 100%;"></iframe><div class="overlay" style="display: none;"></div>'); 
	                                            
	                         	}
	                       }
	                   });  										
				
			}		
	}	


</script> <script>
				
        var MediaOn = false;
        var diffMedia = false;
			
				 

			function playAudio(sourceUrl, element) {
						$('.radio-player').html('');
						$('.icon').removeClass('active');
						$('#audio-player').animate({bottom:'0px'},1000).css('height' , '40px');
						//$('audio').show();
						var title = $(element).parent().find('h4').html();
						$('.audio-data').html(title);
            var audio = document.getElementById("audio");
            audio.src = sourceUrl;
            if (MediaOn == false) {
                audio.play();												
                $(element).addClass('active');
                MediaOn = true;
								player_state = 1;
            } else {
               if ($(element).hasClass('active')) {
                    audio.pause();
										player_state = 0;                   
                    MediaOn = false;
                } else {
                     $(element).addClass('active');
                    audio.play();
										player_state = 1;
                    MediaOn = true;

                }
            }
        }

    </script>  <script type="text/javascript">	
	
$(function(){
				
  							
      var slideOn = true	
      $(".carousel-switch").click(function() {					
                          if (slideOn == true) {
                              $('#carousel-main').carousel('pause');
                              $(this).html('<i class="fa fa-play"></i>');
                              slideOn = false;
                          } else {
                              $('#carousel-main').carousel('cycle');
                              $('#carousel-main').carousel({ interval: 5000 })
                              $(this).html('<i class="fa fa-pause"></i>');
                              slideOn = true;
                          }
      });		
  		if ($(window).width() < 500) {        
          $('#carousel-main').carousel({
          	pause: true,
          	interval: false
        	});
			} else { 
			 			$('#carousel-main').carousel({ interval: 5000 });
      }


  				
