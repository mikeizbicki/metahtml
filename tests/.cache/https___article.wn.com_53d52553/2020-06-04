    window.currentTab = '';

    if(window.console && console.log && $.cookies.get('debug')){
        clog = console.log.bind(console);
    }else{
        function clog(info){}
    }

    function updateUrl(tabUrl){
        clog("Update URL:", tabUrl);
        if(tabUrl == 'home') tabUrl = '';
        document.location.hash = '#/'+tabUrl;
    }


    function loadTab(tabName){
        clog("Load tab:", tabName);
        var $activeTab = $('.nav a[data-url="' + tabName+'"]');
				
        if(tabName == 'local'){
            loadLocalNews(window.pageEdition, 10, 1, 'local');
        }else if(tabName == 'edition'){
            loadLocalNews(window.pageEdition, 10, 1, 'local');
        }else if($activeTab.attr('href')=='#content'){
            var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'content');
        }else if($activeTab.attr('href')=='#business'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'business_news');			
						load_videos('(world business news today)', '4', 'business_videos');
						$('#slider_b').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#cryptocurrency'){
					 	var query = $activeTab.data("hidden");
           	reloadNews(query, 10, 1, 'cryptocurrency_news');			
					 	load_videos('Cryptocurrency duration:long', '4', 'cryptocurrency_videos');
					 	$('#slider_crypto').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#music'){
						var query = $activeTab.data("hidden");
          	reloadNews(query, 10, 1, 'music_news');			
						$('#slider_music').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#coronavirus'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'corona_news');	
						load_wiki('Coronavirus', 'corona_wiki');	
						load_videos('intitle:coronavirus', '8', 'corona_videos');
						$('#slider_corona').trigger('updateSizes');
			 }else if($activeTab.attr('href')=='#entertainment'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 2, 'entertainment_news');			
						$('#slider_enter').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#technology'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'technology_news');			
						load_videos('(technology and news)', '4', 'technology_videos');
						$('#slider_t').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#health'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'health_news');			
						load_videos('(health science) duration:long', '4', 'health_videos');
						$('#slider_h').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#sport'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'sport_news');			
						$('#slider_s').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#tennis'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'tennis_news');			
						$('#slider_tennis').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#film'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'film_news');
       }else if(tabName == 'search'){
            setTimeout(function(){
                $('.top_query').focus();
            }, 200);
        }
    }


    function setPageEdition(edition){
        window.pageEdition = edition;
        $.cookies.set('wnedition', window.pageEdition);
        updateSelectLanguage();
    }

    function activateTab(tabName, fromClick){
        clog("Activate tab:", tabName, 'fromClick=', fromClick);

        if(tabName.match('^(local|edition)/')){ // decompose
            setPageEdition(tabName);
            tabName = 'local';
        }

        var tabUrl = (tabName == 'local') ? window.pageEdition : tabName;

        if(tabUrl == window.currentTab) return;
        window.currentTab = tabUrl;

        var $activeTab = $('.nav a[data-url="' + tabName+'"]');
        $activeTab.tab('show');
        if(!fromClick){
            $activeTab.click(); // call onclick="" handler.
        }
        updateUrl(tabUrl);
        loadTab(tabName);
				$activeTab.next('.submenu').show();
        $("html, body").animate({ scrollTop: 0 }, "slow");
				$('.player-video-data').html();
				$('.video-player').html();
    };

    function setDetectedLanguage(){
        setPageEdition('local/'+$('#select-language').val().toLowerCase());
    }

    window.useHash = (document.location.hostname != 'wn.com')

    function getUrlTab(){
        var path = document.location.pathname.replace(/^\//,'');
        var hash = document.location.hash.replace(/^#\/?/,'').replace(/\/$/,'');
        var tabName = useHash ? hash: (path || hash);
        return tabName.toLowerCase();
    }

    $(function(){
		
        window.pageEdition = $.cookies.get('wnedition');
        activateTab(getUrlTab());
        if(useHash){
	        $(window).on('hashchange', function(){
	            var tabName = getUrlTab();
	            clog("Hash updated:", tabName);
	            activateTab(tabName);
				if(!tabName){
					activateTab('home');
				}
	        });
        }
		
				
        //$("#intro").modal('show');
        $('.nav a[data-url]').click(function (e) {
                e.preventDefault();
                activateTab($(this).attr('data-url'), true);
        });
    		$('a.modalButton').on('click', function(e) {
    				    	var src = $(this).attr('data-src');			
    				    	$("#footer-content iframe").attr({'src':src});
    		});	
        if($(window).width() > 769){
		
        }else{
            $(".navbar-m li a").click(function(event) {
                if (!$(this).parent().hasClass("dropdown")) {
                    $("#news-tab-m").removeClass("active").removeClass("in");
                }
            });
        }

        var menu_expanded = true;

        if($(window).width()>769){
            $('#myTab .dropdown').hover(function() {
				
                $(this).find('.dropdown-menu').first().stop(true, true).delay(250).slideDown();
                menu_expanded = true;
            }, function() {
                if($(this).hasClass('active')) {
                    $(this).find('.dropdown-menu').first().stop(true, true).delay(250).slideDown();
                    menu_expanded = true;
                }else{
                    $(this).find('.dropdown-menu').first().stop(true, true).delay(100).slideUp();
                    menu_expanded = false;
                };
            });
            $('#myTab .dropdown-toggle').click(function() {
				$('.dropdown-menu').slideUp();
                $(this).find('.dropdown-menu').first().stop(true, true).delay(250).slideDown();
                menu_expanded = true;
            });
        }
        $('.local_news').click(function (e) {
            e.preventDefault();
            chooseEdition();
            activateTab('local');
            $('#myTab .dropdown-menu').stop(true, true).delay(100).slideUp();
            menu_expanded = false;
       });
        $('#chat-search').submit(function(event) {
                var search_string = $("#chat_query").val();
                search_string = search_string.toLowerCase().replace(/[\.\/\: _!]+/g, '_');
				window.open('https://wn.com/' + search_string + '/chat', '_blank');
                return false; // do not submit the form
         });

       	$('#weather-map').submit(function(event) {
            var search_string = $("#city_map_name").val();
            search_string = search_string.toLowerCase().replace(/[\.\/\: _!]+/g, '_');
						window.open('https://upge.wn.com/?t=cheetah-weather/map.txt&query=' + search_string, '_blank');
            return false; // do not submit the form
         });

         $('#radio-search').submit(function(event) {
                    $('.radio-search-results').show().html("$('Loading...')");
                    var query = $('.radio_query').val();
                    $.ajax({
                            data: {'query': query },
                            url: '/api/upge/cheetah-photo-search/radio_wn',
                            cache: true,
                            success: function(text) {
                                $('.radio-search-results').html($(text));
                                $('.radio-user-results').hide();
                            },
                       });
            return false; // do not submit the form
        });

        $(".sidebar-nav a").click(function () {
          $(".sidebar-nav a").removeClass("active");
          $(this).addClass("active");
        });


     $('#main-search').submit(function(event) {
