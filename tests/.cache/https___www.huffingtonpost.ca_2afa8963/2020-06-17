  (function(w) {
    var spaceId = '';
    var kvParams = function(){
      var arr = "vert=premium;pubmarket=ca;pagetype=atf:front:homepage:vert_front;publisher=huffpost;category=main".split(";");
      var len = arr.length;
      var p = {};
      for(var i=0; i < len; i++) {
        var r = arr[i].split("=");
        if (r[0] && r[1]) {
          p[r[0]] = r[1];
        }
      }
      return p;
    };

    
    spaceId = spaceId || '1197803184'
    

    

    w.JACConfig = {
      service: {
        debug: false,
        adServer: {
          '1AS': {
            region: '' || 'US',
            params: kvParams()
          }
        },
        site: {
          name: 'HuffPo Canada',
          spaceId: spaceId
        },
        host: '' || 'https://jill.fc.yahoo.com/'
      },
      client: { }
    };
  })(window);

  // HuffPost JACJILL wrapper
  var jacJill = (function(w){
    var positions = {
      service: {},
      client: {}
    };

    var state = {
      jacReady: false,
      confReady: false,
      types: { }
    };
    
    var _queryVal = function(key, s){
      var str = s || w.location.search;
      return decodeURIComponent(str.replace(new RegExp("^(?:.*[&\\?]" + encodeURIComponent(key).replace(/[\.\+\*]/g, "\\$&") + "(?:\\=([^&]*))?)?.*$", "i"), "$1")).replace(/(^\s+|\s+$)/g, "") || null;
    };
    
    var presetConf = _queryVal('presetConfJAC') === '1'; // preset config without positions to use createPosition to serve ads

    var _logger = (function(w){
      try {
        function _q(key) { var str = w.location.search; return decodeURIComponent(str.replace(new RegExp("^(?:.*[&\\?]" + encodeURIComponent(key).replace(/[\.\+\*]/g, "\\$&") + "(?:\\=([^&]*))?)?.*$", "i"), "$1")).replace(/(^\s+|\s+$)/g, "") || null; }
        var prefix = '[JAC] > ';
        var qkey = 'debugJAC';
        var debugEnabled = _q(qkey) == 1;
        var cs = { errors: [], logs: [], debugs: [] };
        function _f(m) { return cs[m] || []; }
        function _d(m, c) { if (debugEnabled) { m = prefix + m; if (c === true) { cs.debugs.push(m); } return w.console && w.console.debug(m); } }
        function _l(m, c) { m = prefix + m; if (c === true) { cs.logs.push(m); } return w.console && w.console.log(m); }
        function _e(m, c) { m = prefix + m; if (c === true) { cs.errors.push(m); } return w.console && w.console.error(m); }
        return { fetch: _f, debug: _d, log: _l, error: _e, debugEnabled: debugEnabled };
      } catch (err) {
        function _t(){ return; }
        return { fetch: _t, debug: _t, log: _t, error: _t, debugEnabled: false };
      }
    })(w);
    
    var _ts = function() { return new Date().getTime(); };
    var _random = function() { return (_ts() + "_" + Math.ceil(Math.random(999999) * 100000)); };
    var _randomId = function(id) { return id + "_" + _random(); };
    var _sizes = function(sz) { return sz.replace(/\s+/g,'').split(","); };
    var _getType = function(t) {
      if (state.types[t] !== undefined) {
        var n = ++state.types[t];
        state.types[t] = n;
        return t + '' + n;
      } else {
        state.types[t] = 0;
        return t;
      }
    };
    var _async = function(glob, cb, poll) {
      var interval;
      var test = typeof glob === 'function' ? glob : function() { return !!window[glob]; };
      if(test()) { cb(); }
      else { interval = setInterval(function(){ if(test()) { cb(); clearInterval(interval); } }, poll || 50); }
    };
    var _asyncJAC = function(cb, poll){
      return _async('JAC', cb, poll || 50);
    };
    var _onClientReady = function(cb) {
      // Does not handle case for dyncamic ad where conf has already been set.
      var f = (w.JACConfig.client && w.JACConfig.client['onReady'] && 'function' === typeof w.JACConfig.client['onReady']) ? w.JACConfig.client['onReady'] : null;
      w.JACConfig.client['onReady'] = function(){
        if ('function' === typeof f) { f(); }
        if ('function' === typeof cb) { cb(); }
        return;
      };
    };

    var startTime = _ts();
    var timeDiff = function(){ return (_ts() - startTime) + ' milliseconds'; };

    _logger.debug('Start time: ' + startTime);
    _asyncJAC(function(){ 
      state.jacReady = true; 
      _logger.debug('JAC loaded in ' + timeDiff());
    });

    if (!w.JACConfig) { throw new Error("window.JACConfig not defined!"); }
    
    var _setConfig = function(cb){
      _asyncJAC(function(){
        if (state.confReady) {
          return 'function' === typeof cb && cb();
        }
        
        state.confReady = true;
        w.JACConfig.service.positions = positions.service;
        w.JACConfig.client.positions = positions.client;
        if (_logger.debugEnabled) {
          w.JACConfig.service.debug = true;
        }

        // try { _logger.debug('setConfig: ' + JSON.stringify(w.JACConfig, null, 2)); } catch (e) {}
        JAC.setConfig(w.JACConfig);
        return 'function' === typeof cb && cb();
      });
    };

    var _createPosition = function(){
      var args = Array.prototype.slice.call(arguments);
      var type = _getType(args[0]);
      var mn = args[1];
      var elementId = args[2];
      var placement = args[3];
      var sz = args[4];
      var sizes = _sizes(sz);
      var bool = args[5] || {};

      var callback = function() {
        var el = document.querySelector("#" + elementId);
        if (el) {
          el.id = _randomId(el.id);
          var spotLogParts = [
            'MN: ' + mn,
            'Type: ' + type,
            'Sizes: ' + sz,
            'Placement: ' + placement,
            'Element: ' + el.id,
            'FlagViewable: ' + (bool.requireViewable ||  false),
            'FlagRotation: ' + (bool.rotationEnabled ||  false)
          ];
          if (bool.mobileMomentsEnabled) {
            spotLogParts.push('FlagMobileMoments: true');
          }
          var spotLog = spotLogParts.join(', ');
          
          if (state.confReady) {
            _logger.debug("Config has been set, create position dynamically (" + mn +")");
            _asyncJAC(function() {
              _logger.debug('Create Position for - ' + spotLog, true);
              JAC.createPosition(type, {
                service: {
                  alias: mn,
                  sizes: sizes
                },
                client: {
                  targetElement: el.id,
                  render: {
                    requireViewable: bool.requireViewable
                  },
                  features: {
                    rotation: {
                      enabled: bool.rotationEnabled
                    },
                    mobileMoments:{
                      enabled: bool.mobileMomentsEnabled || false
                    }
                  }
                }
              });
            });
          } else {
            _logger.debug("Config has not been set yet, update conf var (" + mn + ")");
            _logger.debug('Config Update Position for - ' + spotLog, true);
            positions.service[type] = {
              alias: mn,
              sizes: sizes
            };
            positions.client[type] = {
              targetElement: el.id,
              render: {
                requireViewable: bool.requireViewable
              },
              features: {
                rotation: {
                  enabled: bool.rotationEnabled
                },
                mobileMoments:{
                  enabled: bool.mobileMomentsEnabled || false
                }
              }
            };
          }
        } else {
          _logger.error("No target element found for:" + type + "|" + sz);
        }
      };

      if (presetConf) {
        _setConfig(callback);
      } else {
        callback();
      }
      
      return;
    };

    return {
      state: state,
      ts: _ts,
      query: _queryVal,
      logger: _logger,
      createPosition: _createPosition,
      setConfig: _setConfig,
      onClientReady: _onClientReady
    };
  })(window);
</script>
