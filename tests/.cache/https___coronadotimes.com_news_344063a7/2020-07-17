// Modal Image Hack for Imported Images
$(window).bind("load", function() {
$(".SP_Import_Photo_Img .SP_Import_Photo_Link").addClass("td-modal-image");
$(".SP_Import_Post_Content > p").removeClass("td-modal-image");
$('.SP_Import_Post_Content a:has(.td-animation-stack-type0-2)').addClass("td-modal-image");
$('.SP_Import_Post_Content a:has(.td-animation-stack-type0-2)').parents().removeClass( "td-modal-image" );
});
// END Modal Image Hack for Imported Images

// Random Below Header Adverts Plugin
$(document).ready(function() {
var randomBelowHeadAds = $(".SP_Below_Header_As_Rotate li").get().sort(function(){ 
     return Math.round(Math.random())-0.5;
});
$(randomBelowHeadAds).appendTo(randomBelowHeadAds[0].parentNode).show();
$('.SP_Below_Header_As_Rotate').fadeIn();

});
// END Random Below Header Adverts Plugin

// Random Side Adverts Plugin
$(document).ready(function() {
var randomSideAds = $(".SP_Sidebar_As_Rotate li").get().sort(function(){ 
     return Math.round(Math.random())-0.5;
});
$(randomSideAds).appendTo(randomSideAds[0].parentNode).show();
$(' .SP_Sidebar_As_Rotate').fadeIn();
});
// END Random Side Adverts Plugin

// Rotating Bottom Blog Adverts Plugin

