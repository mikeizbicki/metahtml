            </h1></div><div class="newsletter-element__form clearfix"><form method="post" action="https://link.theblaze.com/s/All+Users" id="newsletter_form_footer" onsubmit="submitNewsletter()"><input type="hidden" name="vars[source]" value="blaze_signup_footer_global"/><input type="hidden" name="template" value="All-Welcome-Email"><input type="hidden" name="redirect" value="https://www.theblaze.com/st/newsletters-thankyou"/><input placeholder="Your email" type="email" name="email" class="newsletter-element__input"/><button class="newsletter-element__submit">GET IMPORTANT NEWS</button></form></div></div></div><script>
              
  function submitNewsletter() {
        var formElem = document.getElementById("newsletter_form_footer");
        var emailVal = formElem.elements.namedItem("email").value;
        fetch("https://www.theblaze.com/core/newsletter/sailthru/optin/" + emailVal + '/').then(function( data ) {
            document.forms["newsletter_form_footer"].submit();
        });
  }
              
   
</script><!-- End User Code --></div></div></div><div class="text-element" id="sFooter_0_0_1_0_0_0_1">&#169; 2020 BLAZE MEDIA LLC. ALL RIGHTS RESERVED.</div><!-- User Code --><script>

window.onscroll = throttle(hideStickyAd,100);

function hideStickyAd(){
  var footer_div = document.getElementsByClassName("taboola-wrap")[0];
  var ad_div = document.getElementsByClassName("sticky-target")[1];
  var footer_pos = 0, ad_div_pos = 0;
  if(footer_div)
    footer_pos = footer_div.getBoundingClientRect().top;
  
  if(ad_div_pos)
    ad_div_pos = ad_div.getBoundingClientRect().top;

if(ad_div) {
  if(ad_div_pos>=footer_pos){
     ad_div.style.display = "none"; 
  } else {
     ad_div.style.display = "block"; 
  }
}
}

function throttle(callback, limit) {
    var wait = false;                  
    return function () {               
        if (!wait) {                  
            callback.call();           
            wait = true;              
            setTimeout(function () {   
                wait = false;          
            }, limit);
        }
    }
}
</script><!-- End User Code --><!-- User Code --><script type="text/javascript">
window._taboola = window._taboola || [];
_taboola.push({flush: true});
</script><!-- End User Code --></div><!-- User Code --><style>
