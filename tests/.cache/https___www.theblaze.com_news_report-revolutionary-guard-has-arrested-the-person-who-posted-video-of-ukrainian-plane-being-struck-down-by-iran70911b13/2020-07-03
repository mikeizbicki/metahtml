googletag.defineSlot("/75484061/TheBlaze.com"+secondary_ad_level, [300,250], "div-gpt-ad-In-Content_300x250_102")
.setTargeting("pos", ["In-Content_300x250_102"])
.addService(googletag.pubads());
}

if (document.getElementById('div-gpt-ad-miniscroller')) {
//if(window.console) {console.log('miniscroller');}
googletag.defineSlot("/75484061/TheBlaze.com"+secondary_ad_level, [1,1], "div-gpt-ad-miniscroller")
.setTargeting("pos", ["miniscroller"])
.addService(googletag.pubads());
}


if (document.getElementById('div-gpt-ad-top-mobile_320x50_212')) {
//if(window.console) {console.log('top-mobile_320x50_212');}
googletag.defineSlot("/75484061/TheBlaze.com"+secondary_ad_level, [320,50], "div-gpt-ad-top-mobile_320x50_212")
.setTargeting("pos", ["top-mobile_320x50_212"])
.addService(googletag.pubads());
}


if (document.getElementById('div-gpt-ad-Mobile_In-Content_300x250_102')) {
//if(window.console) {console.log('Mobile_In-Content_300x250_102');}
googletag.defineSlot("/75484061/TheBlaze.com"+secondary_ad_level, [300,250], "div-gpt-ad-Mobile_In-Content_300x250_102")
.setTargeting("pos", ["Mobile_In-Content_300x250_102"])
.addService(googletag.pubads());
}


if(document.cookie.indexOf('lightbox_ad=')==-1){
    if(window.console) {console.log('640x500_lightbox');}
  if (window.innerWidth < 768) {
 			ad_slot = googletag.defineSlot('/75484061/TheBlaze.com', [300, 250], 'div-gpt-ad-1543463723530-0').setTargeting('pos', ['300x250_lightbox_mobile']).addService(googletag.pubads());
 		} else {
 			ad_slot = googletag.defineSlot('/75484061/TheBlaze.com', [640, 500], 'div-gpt-ad-1543463723530-0').setTargeting('pos', ['640x500_lightbox']).addService(googletag.pubads());
 		}
  }
  googletag.pubads().addEventListener('slotRenderEnded', function(event) {
 if( event.creativeId ==138253255370){
    lightboxControl();
    }
  
});
  
googletag.pubads().enableSingleRequest();
googletag.pubads().enableAsyncRendering();
<!-- Criteo Disable DFP Start -->
googletag.pubads().disableInitialLoad(); //We will wait for the Criteo Bidder Call before calling the adserver
<!-- Criteo Disable DFP End -->
googletag.enableServices();
});

