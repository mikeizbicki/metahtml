var xhr = new XMLHttpRequest();
xhr.open('GET', '//mixi.media/data/js/91802.js', true);

xhr.responseType = 'json';

xhr.onload = function () {
	
    if (xhr.readyState === xhr.DONE) {
        if (xhr.status === 200) {
			var container = document.querySelector('.worth-a-look .posts-wrapper');
	 		 if(!container) {
            document.querySelector('.fark-module .mb-2.worth-a-look').innerHTML=`<div class="posts-custom posts-custom-section section-holder clearfix"><div class="posts-wrapper clearfix"></div></div>`;
              container = document.querySelector('.worth-a-look .posts-wrapper');
            }
			var widget_count=container.querySelectorAll('.widget');
			var farq_item_to_enter=12;
			if(widget_count){
              if(widget_count.length>3)
				farq_item_to_enter= 15 - widget_count.length 
		   }
          
            var data = xhr.response;
          
            var html = '';
          
            for (var i=0; i < farq_item_to_enter ; i++) {
                html += '<div class="widget farq-widget"><div class="item">';
                html += '<a href="' + data.news[i].url + '" target="_blank">'
                html += '<img class="image" src="' + data.news[i].img + '">';
                html += '</a>';
                html += '<a class="title" href="' + data.news[i].url + '" target="_blank">' + data.news[i].title + '</a>'
                html += '</div></div>';
            }
            html+= '';
            
           
            container.innerHTML += html;
        }
    }
};

xhr.send();

