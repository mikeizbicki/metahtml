function getGoogleTagsScrpt() {
  console.log('openx start at:  '+ new Date());
	jQuery.ajax({
          dataType: "script",
          cache: true,
          url: '//theblaze2-d.openx.net/w/1.0/jstag?nc=75484061-THEBLAZE',
          success: function() {
            jQuery(function () {
              jQuery.ajax({
                dataType: "script",
                cache: true,
                url: '//www.googletagservices.com/tag/js/gpt.js',
                success: function() {
                
                console.log('gpt.js loaded at: ' + new Date())
                },
				error: function(XMLHttpRequest, textStatus, errorThrown) {
				console.log("some error occured while calling gpt.js");}
              })
            })
          },
		  error: function(XMLHttpRequest, textStatus, errorThrown) {
		  console.log("some error occured while calling openx");}
        });
}

