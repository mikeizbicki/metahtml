<script>
    $('#site-search-1749534-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-1749534').submit();
        return false;
    });

    $('#site-search-1749534').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-1749534 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-1749534 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-1749534-term').val() ){
            return true;
        }else{
            return false;
        }
    });
    
    $('.dropdown-toggle-1749534').click(function(){
        setTimeout(function(){
            $('#site-search-1749534-term').focus();
        }, 50);
    });</script></div></div>
                                
