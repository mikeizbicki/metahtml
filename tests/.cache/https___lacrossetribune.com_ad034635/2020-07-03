            <script>
                jQuery(document).ready(function($){
                    var assetTracked = [];
                    var isUserAction = false;

                    function trackActive(owl){
                        if( assetTracked.length == 10 ){
                            return;
                        }else{
                            var activeThings = $(owl).find('.owl-item.active');

                            $.each(activeThings, function(index, value){
                                var assetId = $(this).find('.card').data('id');
                                var hasBeenLogged = false;
                                
                                assetTracked.forEach(function (item, index, array) {
                                    if(item == assetId){
                                        hasBeenLogged = true;
                                        return true;
                                    }
                                });
                                if( hasBeenLogged == false ){
                                    assetTracked.push(assetId);
                                    TNCMS.Tracking.addEvent({
                                        app: 'classifieds',
                                        metric: 'featured',
                                        id: assetId
                                    });
                                }
                            });
                        }
                    }

                    window.card_carousel_1734583 = $('#block-1734583 .owl-carousel');

                    card_carousel_1734583.on('initialized.owl.carousel', function(e){
                            trackActive(card_carousel_1734583);
                        });
                    
                    
                    card_carousel_1734583.owlCarousel({
                        autoplay:__tnt.client.touchOnly ? false : true,
                        autoplayTimeout:3000,
                        autoplayHoverPause:true,
                        loop:true,
                        margin:15,
                        nav:false,
                        dots:false,
                        slideBy: 'page',
                        autoHeight:false,
                        responsiveClass:true,
                        responsiveBaseElement: '#featured-classifieds-1734583',
                        responsive:{
                            0:{
                                items:1,
                                stagePadding: 40,
                                nav:false
                            },
                            600:{
                                items:1
                            }
                        }
                    });

                    // bind previous click
                    $('#block-1734583 .owl-carousel-control.prev').on(__tnt.client.clickEvent, function(e){
                        e.stopPropagation();
                        card_carousel_1734583.trigger('prev.owl.carousel');
                        
                        isUserAction = true;
                        trackActive(card_carousel_1734583);
                    });

                    // bind next click
                    $('#block-1734583 .owl-carousel-control.next').on(__tnt.client.clickEvent, function(e){
                        e.stopPropagation();
                        card_carousel_1734583.trigger('next.owl.carousel');
                        
                        isUserAction = true;
                        trackActive(card_carousel_1734583);
                    });

                    // bind dragged
                    card_carousel_1734583.on('dragged.owl.carousel', function(event){
                        isUserAction = true;
                        trackActive(card_carousel_1734583);
                    });

                    // bind translated
                    card_carousel_1734583.on('translated.owl.carousel', function(event){
                        if( isUserAction ){
                            trackActive(card_carousel_1734583);
                            isUserAction = false;
                        }
                    });
                });
            </script>
        
    </div>

