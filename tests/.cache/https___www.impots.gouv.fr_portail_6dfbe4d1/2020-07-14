<script>
var bannerWidth;
var bannerFilename = "banniere_home_classique.jpg";
var baseUrl = "https://www.impots.gouv.fr/portail";
if (window.matchMedia("(min-width: 300px) and (max-width: 767px)").matches) {
    bannerWidth = "1250";
} else if (window.matchMedia("(min-width: 768px) and (max-width: 991px)").matches) {
    bannerWidth = "1550";
} else if (window.matchMedia("(min-width: 992px)").matches) {
    bannerWidth = "2000";
}
document.getElementById("banner").style.backgroundImage = "url(" + baseUrl + "/files/banner/" + bannerWidth + "/" + bannerFilename + ")";

</script>
