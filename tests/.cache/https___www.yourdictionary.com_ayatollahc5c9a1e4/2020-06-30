        function get_sizes_for_a9(size, sizes)
        {
            if (!sizes.length)
            {
                return [size];
            }
            for(var i=sizes.length-1; i>=0; i--)
            {
                var screen_reqs = sizes[i][0];
                var sizes_to_pass_to_a9 = sizes[i][1];
                if (screen_reqs[0] <= window.innerWidth && screen_reqs[1] <= window.innerHeight)
                {
                    if ( Array.isArray(sizes_to_pass_to_a9[0]))
                    {
                        return sizes_to_pass_to_a9;
                    }
                    else
                    {
                        return [sizes_to_pass_to_a9];
                    }
                }
            }
        }

        shouldHandleScroll = true;
        $(window).scroll(function(){
            if(shouldHandleScroll){
                shouldHandleScroll = false;
                setTimeout(function(){shouldHandleScroll=true;}, 200);
                handleScroll();
            }
        });
        MOVEMENT_THRESHOLD = 1;
        currentTop = 0;
        movingUpThreshold = 0;
        movingDownThreshold = MOVEMENT_THRESHOLD;
        has_scrolled = false;
        function handleScroll()
        {
            var isMovingUp = false;
            var isMovingDown = false;
            var previousTop = currentTop;

            currentTop = $(window).scrollTop();
            if (!has_scrolled)
            {
                has_scrolled = (currentTop != 0);
            }

            if(currentTop > previousTop) {
                movingUpThreshold = currentTop - MOVEMENT_THRESHOLD;
                isMovingDown = currentTop > movingDownThreshold;
            } else {
                movingDownThreshold = currentTop + MOVEMENT_THRESHOLD;
                isMovingUp = currentTop < movingUpThreshold;
            }

            if(isMovingUp || isMovingDown) {
                checkRefreshAds(isMovingDown);
            }
        }

        window.onblur = function()
        {
            $.each(YD.ads.slots, function(key, ad_slot)
            {
                if (ad_slot['interval_id'] != "none")
                {
                    clearInterval(ad_slot['interval_id']);
                    ad_slot['interval_id'] = "none";
                }
            });
        }
        window.onfocus = function()
        {
            checkRefreshAds(false);
        }

        function checkRefreshAds(isMovingDown)
        {
            $.each(YD.ads.slots, function(key, ad_slot)
            {
                if(checkAdOnScreen(ad_slot, isMovingDown))
                {
                    if (ad_slot['interval_id'] == "none")
                    {
                        if (ad_slot['displayed'] == "no")
                        {
                            AdBridg.serve();
                            AdBridg.display(ad_slot['id']);
                            ad_slot['displayed'] = "yes";
                        }
                        ad_slot['interval_id'] = ensureRefreshEnabled(ad_slot, isMovingDown);
                    }
                }
                else
                {
                    if (ad_slot['interval_id'] != "none")
                    {
                        clearInterval(ad_slot['interval_id']);
                        ad_slot['interval_id'] = "none";
                    }
                }
            });
        }

        function checkAdOnScreen(ad_slot, isMovingDown)
        {
            if ($('#' + ad_slot['id']).length == 0) return false;
            var offscreen_threshold = 500;
            if (!has_scrolled)
            {
                offscreen_threshold = 0;
            }
            var docViewTop = $(window).scrollTop();
            var docViewBottom = docViewTop + $(window).height();
            var elemTop = $('#' + ad_slot['id']).offset().top;
            var elemBottom = elemTop + $('#' + ad_slot['id']).height();
            if(isMovingDown)
            {
                docViewBottom += offscreen_threshold;
            }
            else
            {
                docViewTop -= offscreen_threshold;
            }
            if ((elemTop >= docViewTop && elemTop <= docViewBottom) || (elemBottom >= docViewTop && elemTop <= docViewBottom))
            {
                return true;
            }
            return false;
        }

        REFRESH_INTERVAL = 30000;
        function ensureRefreshEnabled(ad_slot, isMovingDown)
        {
            if (ad_slot['interval_id'] == "none")
            {
                return setInterval(function(){
                    if (ad_slot['display_count'] < 50)
                    {
                        ad_slot['display_count']++;
                    }
                }, REFRESH_INTERVAL);
            }
            else
            {
                return ad_slot['interval_id'];
            }
        }
    </script>
