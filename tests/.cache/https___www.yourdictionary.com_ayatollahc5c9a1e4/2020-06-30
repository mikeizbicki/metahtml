    <script type="text/javascript">
        window.YD = {};
        YD.ads = {
            slots: [],
            define: function (id, unit, size, sizes, ox_sizes, delay) {
                            var ads = this;
                var sizeDef = size, cleanUnit = unit.replace(/_\d+$/, '');

                AdBridg.cmd.push(function () {
                    var mapping = AdBridg.sizeMapping(), i = 0, length = sizes.length, j = 0;
                    for (; i < length; i++) {
                        mapping.addSize(sizes[i][0], sizes[i][1]);
                    }
                    mapping = mapping.build();
                    var new_ad = AdBridg.defineSlot('/1004147/' + unit, size, id).defineSizeMapping(mapping);
                    AdBridg.useSizeMapping(new_ad, mapping);
                    YD.ads.slots.push({id: id, ad: new_ad, interval_id: "none", displayed: "no", display_count: 0});
                    checkRefreshAds(false);
                });
                        },
            refresh: function (id) {
                                AdBridg.cmd.push(function () {
                    $.each(YD.ads.slots, function(key, value) {
                        if (key == "id" && value == id)
                        {
                            if (value.display_count < 50)
                            {
                                value.display_count++;
                            }
                        }
                    });
                });
