<script>
    $('#site-search-1772961-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-1772961').submit();
        return false;
    });

    $('#site-search-1772961').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-1772961 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-1772961 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-1772961-term').val() ){
            return true;
        }else{
            return false;
        }
    });
    
    $('.dropdown-toggle-1772961').click(function(){
        setTimeout(function(){
            $('#site-search-1772961-term').focus();
        }, 50);
    });</script></div></div>
                                
