		var toSpecPage = function(currPage) {
			if ("" == currPage || isNaN(currPage))
				currPage = 1;
			else
				currPage = Number(currPage);

			var colName = 'xxgzbd';
			var parentColName = 'ztlm';
			var parentColId = 'UU145102906505319731';
			var id = 'UU157976428326282067';
			var pId = 'UU145102906505319731';

			var url = _path_ + "?id=" + id + "&pId=" + pId + "&cn=" + colName
					+ "&pcn=" + parentColName + "&pid=" + parentColId
					+ "&page=" + currPage;

			window.location = url;
		};
