				 	e.preventDefault();		
					smallerPlayer();
					fullscreenOn = false;
					playerOn = true;
			});
			
			$( document ).on( 'click', '.enlarge_player', function(e) {
		 		e.preventDefault();		
				enlargePlayer();
				fullscreenOn = true;
			});				
			$( document ).on( 'click', '.remove_player', function(e) {   	
				e.preventDefault();	
				$('.player-container').hide('slow');
				$('.player').html('');
				fullscreenOn = false;			
				playerOn = false;
			});			 
				 
