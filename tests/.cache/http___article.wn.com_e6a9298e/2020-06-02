        // look for languages alternatives using our geonaming service
        function showCountrySelection(geocode_data) {

						  
            $.ajax({
                  url: 'https://wn.com/api/upge/cheetah-photo-search/geoname',
                data: {country: geocode_data, style: 'full'},
                cache: true,
                jsonpCallback: 'cbGeoName',
                dataType: 'jsonp',
                type: 'GET',
                success: function(data, status) {
                    if (data.geonames) {
                        var geonamelangs = data.geonames[0].languages
                        langs = geonamelangs.split(',')
                        if (langs.length == 0) {
                            return false;
                        }
                        // only pick the first language
                        first_lang = langs[0]
												var lang = []
                        if (window.available_langs[first_lang] == undefined) {
                            $('#select-language option').filter(function () {
                           return ($(this).text() == 'English');
                            }).attr('selected','selected');
                        }
                        var lang = window.available_langs[first_lang].langname;
                        $('#select-language option').filter(function () {
                            return ($(this).text() == lang);
                        }).attr('selected','selected');

                        $('#detected-lang').html(lang);
                        if (lang != 'English'){
                            setPageEdition('local/'+lang);
							
                        }

                    }
                }
            });
        }
      
      	function load_wiki(query, container) {
						var html_content = $('#'+container);		
      			jQuery.ajax({
      				url: 'https://upge.wn.com/api/upge/cheetah-photo-search/wiki_page5',
      				data: {
      							'query': query,
      							'num': '15'
      				 },
        			cache: true,
      				dataType: 'jsonp',
      				success: function(text) {
          					html_content.html(jQuery(text));          					
          				}
          			});
         }  


		
		function load_videos(query, num, container) {	
			var html_content = $('#'+container);
            jQuery.ajax({
                url: 'https://upge.wn.com/api/upge/worldnews/videos05',
                data: {
                    'query': query,
                    'num': num
                },
                cache: true,
                dataType: 'jsonp',
                jsonpCallback: 'cbLoadVideos',
                success: function(text) {			
										html_content.html(text);		
                }
            });
        }		
		function load_video_search() {	
			query = $('#video-query').val();			
            jQuery.ajax({
                url: 'https://upge.wn.com/api/upge/worldnews/video_search01',
                data: {
                    'query': query
                },
                cache: true,
                dataType: 'jsonp',
                jsonpCallback: 'cbLoadVideos',
                success: function(text) {			
										$('#video-results').html(text);		
                }
            });
     }		
		
		
		function load_photos(query, container) {			
			var html_content = $('#'+container);
			html_content.html('<p class="loading">loading '+ query +' photos...</p>');	
			jQuery.ajax({
                url: 'https://wn.com/api/upge/worldnews/photos',
                data: {
                    'query': query,
                    'num': '6'
                },
                cache: true,
                dataType: 'jsonp',
                jsonpCallback: 'cbLoadPhotos',
                success: function(text) {			
					html_content.html(text);					
					prettyPhotoLoad();
                }
            });
    	}


		function hidePlayer(element){		
				$(element).closest('.tab-custom').find('.player-box').animate({	width: "0%" }, 500);						
				$(element).closest('.tab-custom').find('.player-toggle').show().html('<i class="fa fa-chevron-right"></i>');								
		}		
		function showPlayer(element){				
				$(element).closest('.tab-custom').find('.player-box').animate({	width: "100%" }, 500);		
				$(element).closest('.tab-custom').find('.player-toggle').css( 'margin-left', '-30px').html('<i class="fa fa-chevron-left"></i>');						
							
		}
		function triggerPlayer(video_id, element){	
				$('.player').html();
				$(element).closest('.tab-custom').find('.player-box').animate({ width: "100%" }, 500);		
				$(element).closest('.tab-custom').find('.player-toggle').show('slow');				
				$(element).closest('.tab-custom').find('.player').html('<iframe title="YouTube video player" class="youtube-player" type="text/html" src="https://www.youtube.com/embed/'+ video_id +'?autoplay=1" frameborder="0" allowfullscreen="allowFullScreen"></iframe>');				
							
		}
  		function updateSelectLanguage(){
            var lang  = $('#select-language').val();			
            if (lang != 'English') {
           		var newValue = "World";
				$('#select-edition option:selected').removeAttr('selected');
				$('#select-edition option[value="'+ newValue +'"]').prop('selected', true);
            } 
        }						

        function chooseEdition(){
                var country = $('#select-edition').val();				
                setPageEdition('edition/'+country.toLowerCase());     
				if (country != 'World')  {			
					var newValue = "English";
					$('#select-language option:selected').removeAttr('selected');
					$('#select-language option[value="'+ newValue +'"]').prop('selected', true);
            	}				     
        }	
		
			
        /*load_user_news*/
  		function loadLocalNews(pageEdition, num, pn, container) {		
					
						var container_html = $('#'+container).find('.content');
						var num = parseInt(num);
						var pn = parseInt(pn-1);
						var offset = num*pn;	
            container_html.html('<p style="margin: 40px 0; text-align:center;"><img src="//ecdn4.wn.com/or/assets/assets/wn/img/712.GIF" alt="" width="64" height="64"></p>');
            var language = 'English';
            var country = '';
            if(pageEdition.match('local/')){
                language = pageEdition.replace('local/', '');               
            }else{
                country = pageEdition.replace('edition/', '');
            }
            
            $.ajax({
                url: 'https://upge.wn.com/api/upge/worldnews/localnews01',
                data: {
                    'query': country,
                    'language': language,
                    'num': num,
										'offset': offset,
                    'container': container,
                    'pageEdition':pageEdition
                },
                cache: true,               
                dataType: 'jsonp',
                success: function(text) {
                    container_html.html(text);
					                    
                }
            });
        }
		
		
		
        /*load_weather*/
        function get_weather_data(geo_obj, lat, lng, country_code){
          first_geo_data = true;
          var global_geo_obj = geo_obj;
          $.ajax({
              data: {
                  lat: $.trim(lat),
                  lng: $.trim(lng),
                  cnt: 1,
              },
              dataType: 'jsonp',
              jsonpCallback: 'cbWeatherFC',
              cache: true,
              url: 'api/upge/cheetah-photo-search/weather_findcity',
              success: function(data) {
                  //weather_data = data.list[0];  
                  weather_info = ''

                  for (idx in data.list) {
                      weather_data = data.list[idx]
                      if (weather_data) {
                        var weather = weather_data.weather[0];
                        tempC = parseInt(parseFloat(weather_data.main.temp)-273.15)
                        tempF = parseInt(tempC*1.8+32)
                        weather_info += '<img src="//cdn.wn.com/or/images/weather/95/'+weather.icon+'.png" alt="" class="pull-left">'
                        if (country_code == 'us'){
                            weather_info += '<p class="pull-left">'+tempF+'</p><span class="pull-left letter">F</span>'
                        }else{
                            weather_info += '<p class="pull-left">'+tempC+'</p><span class="fa fa-circle-o pull-left"></span><span class="pull-left letter">C</span>'
                        }
                     }
                  }
                  global_geo_obj.html(weather_info);
              },
          });
        }

        function forecast_status(msg) {
            $('#weather-header').html(msg);
        }


        function get_weather(geo_obj, lat, lng, city_name, country_code){
              first_geo_data = true;
              var global_geo_obj = geo_obj;
              var city = city_name.toUpperCase();
                $.ajax({
                data: {
                    lat: $.trim(lat),
                    lng: $.trim(lng),
                    cnt: 1,
                    APPID: '0374ae89689b7ac3ba53376fa99409a0'
                },
                dataType: 'jsonp',
                cache: true,
                jsonpCallback: 'cbWeatherFindcity2',
                url: '//wn.com/api/upge/cheetah-photo-search/weather_findcity',
                success: function(data) {
                            if(!data) { $('#weather-noresults').show(); }
                            weather_info = ''
                            for (idx in data.list) {
                                weather_data = data.list[idx]

                                if (weather_data) {
                                    weather = weather_data.weather[0]
                                    clouds = weather_data.clouds
                                    m = moment(weather_data.dt)
                                    d = new Date(weather_data.dt*1000)
                                    month = d.getMonth()+1
                                    tempC = parseInt(parseFloat(weather_data.main.temp)-273.15)
                                    tempF = parseInt(tempC*1.8+32)
                                today = d.getDate() + '.'+ month + '.' + d.getFullYear()
                                weather_info += '<div class="weather-frame today col-md-3 col-sm-6 cold-xs-12">'
                                weather_info += '<img src="//cdn.wn.com/or/images/weather/95/'+weather.icon+'.png" alt="">'
                                weather_info += '<div class="info">'
                                if (country_code == 'us'){
                                    weather_info += '<div class="temp">'+tempF+' F&deg;</div>'
                                }else{
                                    weather_info += '<div class="temp">'+tempC+' &deg;C</div>'
                                }
                                weather_info += '<ul><li>Pressure: '+weather_data.main.pressure+' hPa</li>'
                                    if (weather_data.main.humidity) { weather_info += '<li>Humidity: '+weather_data.main.humidity+' %</li>'; }

                                    if (clouds) {
                                        weather_info += '<li>Clounds: '+clouds.all+'%</li>'
                                    }
                                    if (weather) {
                                        weather_info += '<li>'+weather.description+'</li>'
                                    }
                                    weather_info += '</ul></div>'

                                }
                      }
                      global_geo_obj.html(weather_info);
                      forecast_status(city);
                      var forecastBox = $('#forecast');
                      get_forecast(city_name, 4, forecastBox, country_code)
                  }
              });
    };


    function get_forecast(city_name, days_count, global_geo_obj, country_code) {
        forecast_status('<p style="margin: 40px 0; text-align:center;"><img src="//ecdn4.wn.com/or/assets/assets/wn/img/712.GIF" alt="" width="64" height="64"></p>');
        $.ajax({
            data: {
                city: city_name,
                report: 'daily'
            },
            dataType: 'jsonp',
            cache: true,
            jsonpCallback: 'cbWeatherForecast',
            url: '//wn.com/api/upge/cheetah-photo-search/weather_forecast_4days',
            success: function(data) {
                if(!data) { text = ('weater data temporarily not available'); }
                forecast_status(data.city.name);
                // loop through the list of weather info
                var weather_info = '';
                var weather_day_loop = 0;
                var weather_list = data.list;
                for(var i=0; i < weather_list.length; i++){
                    if (i == 0 || i >= days_count+1) {
                        continue;
                    }
                    var weather = weather_list[i].weather.length && weather_list[i].weather[0];
                    var value = weather_list[i];
                    var clouds = value.clouds;
                    d = new Date(value.dt*1000);
                    t = d.getMonth()+1 + '-' + d.getDate() + '-' + d.getFullYear()
                    tempC = parseInt(parseFloat(value.temp.day)-273.15);
                    tempF = parseInt(tempC*1.8+32);
                    moment.lang('en', {
                        calendar : {
                            lastDay : '[Yesterday]',
                            sameDay : '[Today]',
                            nextDay : '[Tomorrow]',
                            lastWeek : '[last] dddd',
                            nextWeek : 'dddd',
                            sameElse : 'L'
                        }
                    });
                    mobj = moment(value.dt*1000)
                    // skip today

                    weather_info += '<div class="col-md-3 col-sm-6 col-xs-12 nopadding"><div class="date">'+mobj.calendar()+'</div>';
                    weather_info += '<div class="weather-frame"><img src="//cdn.wn.com/or/images/weather/95/'+weather.icon+'.png" alt="">'
                    weather_info += '<div class="info">'
                    if (country_code == 'us'){
                    weather_info += '<div class="temp">'+tempF+' &deg;F</div>'
                    }else{
                    weather_info += '<div class="temp">'+tempC+' &deg;C</div>'
                    }
                    weather_info += '<ul><li>'+value.pressure+' hPa</li>'
                    if (value.humidity) { weather_info += '<li>Humidity: '+value.humidity+' %</li/>'; }
                    if (clouds) {
                        weather_info += '<li>Clouds: '+clouds+'%</li>'
                    }
                    if (weather) {
                        weather_info += '<li>' +weather.description+'</li>'
                    }
                    weather_info += '</ul></div></div></div>'
            }

            global_geo_obj.append(weather_info);
        }
	  });
	}



    // -->
    </script>  <script>
					function update_chat_position(el){
					    if(!el.length) return;
							$('#chat-box').attr('style','').css({display:'block'});
					        var chatIframe = document.getElementById('chat-iframe');
					        if (!chatIframe.src) {
					            chatIframe.onload = function() {
					                var targetOrigin;
					                //targetOrigin = 'http://localhost:9080';
					                //targetOrigin = 'https://test.chat.wn.com';
					                targetOrigin = 'https://chat.wn.com';
					                var targetUrl;
					                if (document.URL.indexOf('#') > 0) {
					                    targetUrl = document.URL;
					                } else {
					                    targetUrl = document.URL + '#/videos/chat';
					                }
					                chatIframe.contentWindow.postMessage({parentUrl:targetUrl},
					                                                     chatIframe.dataset.src);
					                console.log('URL msg sent to iframe');
					            }

					            chatIframe.src = chatIframe.dataset.src;

					        }
					}
					$('.chat').click(function() {
						update_chat_position($(this))
					});

					</script> <bgsound id="notification_sound"> <style> #chat-box { height: 580px; min-width: 320px; width: 100%; margin-botttom: 10px; transition: top .4s, left .4s, width .4s, height .4s; }a:active, a:focus { outline: 0px; } </style> <script>
					function receiveMessage(event) {
					    //console.log('new message received:');
					    //console.log(event.data);
					    try {
					        var chatBox = $('#chat-box');
					        if (event.data.fullscreen) {
					            document.body.scrollTop = document.documentElement.scrollTop = 0;
					            chatBox.data('position', chatBox.css('position'));
					            chatBox.data('display', chatBox.css('display'));
					            chatBox.data('top', chatBox.css('top'));
					            chatBox.data('left', chatBox.css('left'));
					            chatBox.data('z-index', chatBox.css('z-index'));
					            chatBox.css({
					                position: 'fixed',
					                top: 0,
					                bottom: 0,
					                left: 0,
					                right: 0,
					                height: ($(window).height()) + 'px',
					                width: '100%',
					                zIndex: 99999,
					            });
					            $('body').css('overflow', 'hidden');
								$('.navbar-brand').css('position', 'fixed').css('top', '0px').css('left', '10px').css('z-index', '999999');
					        } else if (event.data.fullscreen != undefined) {
					            //$('#playlist1').css('z-index', '');
					            //\$('#playlist1').css('display', 'block');
					            $('body').css('overflow', '');
								$('.navbar-brand').css('position', 'relative').css('left', '0px');
					            //chatBox.css('position', chatBox.data('position'));
