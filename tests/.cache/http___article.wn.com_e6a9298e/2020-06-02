	function prettyPhotoLoad() {
	     // apply prettyPhoto plugin for video previews
	     jQuery("a[rel^='prettyPhoto']").prettyPhoto();    
	}
		
		
	$(function(){
		var wp_url = 'https://en.wikipedia.org/w/api.php?action=query&format=json&callback=wiki_update&list=search&srwhat=text&srinfo=suggestion&srprop=&srredirects=1&srsearch=';
	    var yt_url = 'https://suggestqueries.google.com/complete/search?hl=en&client=youtube&hjson=f&jsonp=yt_update&ds=yt&cp=3&q=';
	    var bi_url = 'https://api.search.live.net/json.aspx?Appid=F550969A2EBF1211A345960C88E67163180795FE&sources=RelatedSearch&web.count=5&JsonType=callback&JSONCallback=bing_update&query=';
	    var yh_url = 'https://query.yahooapis.com/v1/public/yql?q=select%20*%20from%20search.suggest%20where%20query=""&limit=5&format=json&callback=yahoo_update';
	    var gg_url = 'http://google.com/complete/search?jsonp=gg_update&q=';

	    function wp_parse(data) {
	        var r = [];
	        if(!data.query || !data.query.search) return r;
	        $.each(data.query.search, function(key, row) {
	            r.push({
	                data: row.title.toLowerCase(),
	                value: row.title
	            });
	        });
	        return r;
	    }

	    function bing_parse(data) {
	        var r = [];
	        var t = data.SearchResponse.RelatedSearch;
	        if(!t) return r;
	        $.each(t.Results, function(key, row) {
	            r.push({
	                data: row.Title.toLowerCase(),
	                value: row.Title
	            });
	        });
	        return r;
	    }

	    function yt_parse(data) {
	        var r = [];
	        if(!data[1]) return r;
	        $.each(data[1], function(key, row) {
	            r.push({
	                data: row[0],
	                value: row[0]
	            });
	        });
	        return r;
	    }

	    function yahoo_parse(data) {
	        var r = [];
	        if(!data.results) return r;
	        $.each(data.results, function(key, row) {
	            r.push({
	                data: row[0].toLowerCase(),
	                value: row[0]
	            });
	        });
	        return r;
	    }

	    function lastWord(q){
	    	if(!q) return '';
	    	var l = q.split(/[ \t_-]/g);
	    	return l;
	    	// if( l.length >= 2){
	    	// 	return l[l.length-2] + l[l.length-1];
	    	// }
	    	// return l[l.length-1];
	    }

        var qid=100;

		$('.top_query').autocomplete({
			deferRequestBy: 150,
			lookup: function (query, done) {
		        // Do ajax call or lookup locally, when done,
		        // call the callback and pass your results:
		        var result = {
		        	suggestions: []
		            // suggestions: [
		            //     { "value": "United Arab Emirates", "data": "AE" },
		            //     { "value": "United Kingdom",       "data": "UK" },
		            //     { "value": "United States",        "data": "US" }
		            // ]
		        };

		        var c = 0;
				window.yt_update = function(data) {
					parsed = yt_parse(data);
					//result.suggestions.push('yt');
					result.suggestions.push.apply(result.suggestions, parsed);
					done(result);
			    };
				window.gg_update = function(data) {
					parsed = yt_parse(data);
					//result.suggestions.push('gg');
					result.suggestions.push.apply(result.suggestions, parsed);
					done(result);
			    };
			    window.wiki_update = function(data) {
					parsed = wp_parse(data);
					//result.suggestions.push('wi');
					result.suggestions.push.apply(result.suggestions, parsed);
					done(result);
			    };
			    window.bing_update = function(data) {
					parsed = bing_parse(data);
					//result.suggestions.push('bi'+parsed[0]);
					result.suggestions.push.apply(result.suggestions, parsed);
					done(result);
			    };
			    window.yahoo_update = function(data) {
					parsed = yahoo_parse(data);
					//result.suggestions.push('yh'+parsed[0]);
					result.suggestions.push.apply(result.suggestions, parsed);
					done(result);
			    };
			    $.ajax({
					dataType: 'jsonp',
					jsonp: 'cb',
					cache: true,
					jsonpCallback: 'wiki_update',
					url: wp_url + lastWord(query)
			    });
			    $.ajax({
					dataType: 'jsonp',
					cache: true,
					url: yt_url + lastWord(query)
			    });
			    $.ajax({
					dataType: 'jsonp',
					cache: true,
					url: gg_url + lastWord(query)
			    });
			    $.ajax({
					dataType: 'jsonp',
					cache: true,
					url: bi_url + lastWord(query)
			    });
			    $.ajax({
					dataType: 'jsonp',
					cache: true,
					url: yh_url.replace('query=""', 'query="'+query+'"')
			    });
		        //done(result);
		    },
		    onSelect: function (suggestion) {
		    	//return suggestion.value + ', ' + suggestion.data);
				$('.top_query').focus();
				//return suggestion.data+' ';
		    },
		    function (suggestion, currentValue) {
		    	return (currentValue ? currentValue + ' ': '') + suggestion;
		    }
		})
	});
	</script>  <script type="text/javascript">
    var MenuOn = true;
    $(".menu-toggle").click(function(e) {
        e.preventDefault();
        $(".main-wrapper").toggleClass("toggled");
        if (MenuOn == true) {
            $(this).html('<i class="fa fa-toggle-off"></i> Show Menu');
            MenuOn = false;
        } else {
            $(this).html('<i class="fa fa-toggle-on"></i> Hide Menu');
            MenuOn = true;
        }
    });
	
    </script> <script type="text/javascript">
    function triggerLoadSeries(file){
                var file_name = file;
                $.ajax({
                    url : '/api/upge/worldnews/tv_shows/' + file_name,
                    dataType: 'jsonp',
                    cache: true,
                    jsonpCallback: 'cbTVShows',
                    success : function (text) {
                        $(".tvshow-content").html(text);
                    }
                });
    }

    function triggerLoadFilm(file){
			$('#film_news').hide();
    	    $('#film-content').show().html('loading...');
                var file_name = file;
                $.ajax({
                    url : '/api/upge/worldnews/film_files/' + file_name,
                    dataType: 'jsonp',
                    cache: true,
                    jsonpCallback: 'cbTVShows',
                    success : function (text) {
                        $('#film-content').html(text);
						 addHistory('film/'+file);
                    }
                });
    }				

		function reloadNews(query, num, pn, container) {	
			var container_html = $('#'+container);					
      container_html.html('<p style="margin: 40px auto; text-align:center;"><img src="//ecdn4.wn.com/or/assets/assets/wn/img/712.GIF" alt="" width="64" height="64"></p>');
			var num = parseInt(num);
			var pn = parseInt(pn-1);
			var offset = num*pn;	
			var query = query
            $.ajax({
                url: '/api/upge/worldnews/news_offset13',
                data: {
                    'query': query,
                    'num': num,
                    'container': container,
                    'offset': offset
                },
                cache: true,
                dataType: 'jsonp',
                jsonpCallback: 'cbLoadNews',
                success: function(text) {
                    container_html.html(text);
                }
            });
        }
    
		function findVideo(query, container) {					
			var container_html = $('.'+container);					
           $.ajax({
                        url: '/api/upge/worldnews/videoid',
                        cache: true,
                        data: {
                            'q': query
                        },
                        success: function(text) {
                            container_html.html(text);
                            $("html, body").animate({ scrollTop: 0 }, "slow");
                        }
                   });
        }
        
		function popUp(url) {			
			window.open(url, 'ShareWindow', 'height=450, width=550, top=' + ($(window).height() / 2 - 275) + ', left=' + ($(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
			e.preventDefault();
		}
		 $('.film-content').on('click','.show_episodes',function(e){
                    var query = $(this).parent().find('a').text();
					findVideo(query, 'episodes-block');
                 
        });
        $('.tvshow-content').on('click','.show_episodes',function(e){
                    var query = $(this).parent().find('a').text();
                    findVideo(query, 'episodes-block');
      	});

    </script> <script type="text/javascript">
    window.currentTab = '';

    if(window.console && console.log && $.cookies.get('debug')){
        clog = console.log.bind(console);
    }else{
        function clog(info){}
    }

    function updateUrl(tabUrl){
        clog("Update URL:", tabUrl);
        if(tabUrl == 'home') tabUrl = '';
        document.location.hash = '#/'+tabUrl;
    }


    function loadTab(tabName){
        clog("Load tab:", tabName);
        var $activeTab = $('.nav a[data-url="' + tabName+'"]');
				
        if(tabName == 'local'){
            loadLocalNews(window.pageEdition, 10, 1, 'local');
        }else if(tabName == 'edition'){
            loadLocalNews(window.pageEdition, 10, 1, 'local');
        }else if($activeTab.attr('href')=='#content'){
            var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'content');
        }else if($activeTab.attr('href')=='#business'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'business_news');			
						load_videos('(world business news today)', '4', 'business_videos');
						$('#slider_b').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#cryptocurrency'){
					 	var query = $activeTab.data("hidden");
           	reloadNews(query, 10, 1, 'cryptocurrency_news');			
					 	load_videos('Cryptocurrency duration:long', '4', 'cryptocurrency_videos');
					 	$('#slider_crypto').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#music'){
						var query = $activeTab.data("hidden");
          	reloadNews(query, 10, 1, 'music_news');			
						$('#slider_music').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#coronavirus'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'corona_news');	
						load_wiki('Coronavirus', 'corona_wiki');	
						load_videos('intitle:coronavirus', '8', 'corona_videos');
						$('#slider_corona').trigger('updateSizes');
			 }else if($activeTab.attr('href')=='#entertainment'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 2, 'entertainment_news');			
						$('#slider_enter').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#technology'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'technology_news');			
						load_videos('(technology and news)', '4', 'technology_videos');
						$('#slider_t').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#health'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'health_news');			
						load_videos('(health science) duration:long', '4', 'health_videos');
						$('#slider_h').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#sport'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'sport_news');			
						$('#slider_s').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#tennis'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'tennis_news');			
						$('#slider_tennis').trigger('updateSizes');
       }else if($activeTab.attr('href')=='#film'){
						var query = $activeTab.data("hidden");
            reloadNews(query, 10, 1, 'film_news');
       }else if(tabName == 'search'){
            setTimeout(function(){
                $('.top_query').focus();
            }, 200);
        }
    }


    function setPageEdition(edition){
        window.pageEdition = edition;
        $.cookies.set('wnedition', window.pageEdition);
        updateSelectLanguage();
    }

    function activateTab(tabName, fromClick){
        clog("Activate tab:", tabName, 'fromClick=', fromClick);

        if(tabName.match('^(local|edition)/')){ // decompose
            setPageEdition(tabName);
            tabName = 'local';
        }

        var tabUrl = (tabName == 'local') ? window.pageEdition : tabName;

        if(tabUrl == window.currentTab) return;
        window.currentTab = tabUrl;

        var $activeTab = $('.nav a[data-url="' + tabName+'"]');
        $activeTab.tab('show');
        if(!fromClick){
            $activeTab.click(); // call onclick="" handler.
        }
        updateUrl(tabUrl);
        loadTab(tabName);
				$activeTab.next('.submenu').show();
        $("html, body").animate({ scrollTop: 0 }, "slow");
				$('.player-video-data').html();
				$('.video-player').html();
    };

    function setDetectedLanguage(){
        setPageEdition('local/'+$('#select-language').val().toLowerCase());
    }

    window.useHash = (document.location.hostname != 'wn.com')

    function getUrlTab(){
        var path = document.location.pathname.replace(/^\//,'');
        var hash = document.location.hash.replace(/^#\/?/,'').replace(/\/$/,'');
        var tabName = useHash ? hash: (path || hash);
        return tabName.toLowerCase();
    }

    $(function(){
		
        window.pageEdition = $.cookies.get('wnedition');
        activateTab(getUrlTab());
        if(useHash){
	        $(window).on('hashchange', function(){
	            var tabName = getUrlTab();
	            clog("Hash updated:", tabName);
	            activateTab(tabName);
				if(!tabName){
					activateTab('home');
				}
	        });
        }
		
				
        //$("#intro").modal('show');
        $('.nav a[data-url]').click(function (e) {
                e.preventDefault();
                activateTab($(this).attr('data-url'), true);
        });
