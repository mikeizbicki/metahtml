<script>
	window.addEventListener('load', function() {
		(function( $ ) {
			var stickyLoaded = false;

			function deciderInitSticky() {
				stickyLoaded = true;
				var isGallery = $('body.single-slideshow').length;
				// Don't float the share buttons on galleries.
				if (isGallery) {
					$('.decider-sharedaddy-wrapper').removeClass('fixedsticky');
				}
				// Opt out of `position:sticky` feature test
				FixedSticky.tests.sticky = false;
				$('.fixedsticky').fixedsticky();

				if (
					$('body.admin-bar') &&
					! window.matchMedia('(min-width: 37.5625rem)').matches
				) {
					$('.header').fixedsticky();
				}
			}

			// Wait for ads to load, if any, to prevent alignment issues.
			if ( ! $('.dfp-ad').length || 'yes' === dimension21 ) {
				deciderInitSticky();
			} else {
				// wait for ads to load
				googletag.cmd.push(function() {
					// Get a manifest of which slots are loading on this page
					var adSlots = googletag.pubads().getSlots().map((slot) => {
						return slot.getAdUnitPath();
					});

					googletag.pubads().addEventListener('slotRenderEnded', function(e) {
						// Remove this slot from manifest once it's rendered
						var slotIndex = adSlots.indexOf(e.slot.getAdUnitPath());
						if (-1 !== slotIndex) {
							adSlots.splice(slotIndex, 1);
						}

						// Once all slots have rendered (slot manifest is empty), stick the header
						if (!adSlots.length && !stickyLoaded) {
							deciderInitSticky();
						}
					});
				});

				// give the ads a 3 second chance to load before loading header and moving on
				window.setTimeout(deciderInitSticky, 3000);
			}
		})(jQuery);
	});
</script>
