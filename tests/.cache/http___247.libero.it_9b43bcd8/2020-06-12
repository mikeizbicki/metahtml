				var seconds2go = 0;  

				var setCountdown = function() {  

					seconds2go--;  

					if(seconds2go <= 0) {  
						getTweet();  
						seconds2go = 60; 
					}
				}
				
				
				var ts;
				var getTweet = function() {
					$.ajax({     
				  	type: "GET",     
				  	url: "/cgi-bin/twit?" + "q=conte+stati+generali%2Ccoppa+italia%2Cfamily+act%2Ccinema+dpcm%2Cplaystation+ps5%2Cvacanze+mef%2Cborsa+piazza+affari%2Ccoronavirus+contagi%2Cistat+trimestre%2Cf1+formula+1%2Cpetrolio+borsa%2Cgualtieri+pil%2Cbanche+abi%2Ccoronavirus+contagi%2Ctemptation+island%2Cserie+quarantena&m=81d64d5f322ef2c5bf790de5efe49380" + "&ts="+(new Date()).getTime(),    
				  	dataType: "html",     
				  	success: function(json) {  
				  		var data = eval('(' + json + ')');
				  		$.each(data, function(key, val) {
							//$('#' + key).clear();
							$('#' + key).html(val);
							if (key === 'twitterhome1') {
								$('#' + key).css("height","110px");	
							}
							
							$('#' + key + " div.iTweet").each(function(){
								var iTweetContent = $(this).html();
								/*
								$(this).html("<div class='iTweetUp'>Â </div>" +
											"<div class='iTweetCenter'>" +
											"<div class='iTweetInsideCenter'>" +
											iTweetContent +
											"</div>"+
											"</div>"+
											"<div class='iTweetBottom'>Â </div>" );
								*/
								$(this).html(	
												"<div class='iTweetCenter'>" +
												iTweetContent +
												"</div>"+
												"<div class='iTweetBottom'>Â </div>" 
											);
							});

						});
						ts = (new Date()).getTime();
						$(".iTweet a").attr("target","_blank");	
						if ($("#twitterhome1 > div").size() > 1) {
							fadeContent(ts);
						}
						else {
							$("#twitterhome1 .iTweet:hidden:first").show();
						}
						resizeDivTD();
				  	},     
				  	error: function (XMLHttpRequest, textStatus, errorThrown) {         
				  		//alert('errore');
				  	} 
				  });
				}
				
				function fadeContent(tsTmp) {
					$("#twitterhome1 .iTweet:hidden:first").fadeIn(500).delay(4000).fadeOut(500, function() {
						if ( tsTmp === ts ) {
							$(this).appendTo($(this).parent());
							fadeContent(tsTmp);
						}
					});
				}
				
				

