                    $('.list-preview').show();
                    $('.thumb-preview').hide();
                }if (cookie && cookie.indexOf("playlist-template-simple") > 0) {
                    $('.list-preview').hide();
                    $('.thumb-preview').show();
                }
            });
            for(var c in $.cookies.get()){
                if(c.match('/')){
                    $.cookies.del(c);
                }
            }
						
						(function($){
              setInterval(() => {
                  $.each($('iframe'), (arr,x) => {
                      let src = $(x).attr('src');
                      if (src && src.match(/(ads-iframe)|(disqusads)/gi)) {
                          $(x).remove();
