<script>

		function triggerFullscreen(){
			$('.add-block ').hide();
			$('.header .nav').hide();
			$('.header .chat-hide').hide();
		    $('#playercontainer1').css('position', 'fixed');
		    $('#playercontainer1').css('top', '54px');
		    $('#playercontainer1').css('left', '0px');
		    $('#playercontainer1').css('right', '0px');
		    $('#playercontainer1').css('height', $(window).height() - 54);
		    $('#playercontainer1').css('z-index', '100000');
		    $('#playercontainer1').css('background-color', '#000000');
		    $('#home').show();
		    $('body').css('overflow', 'hidden');
		    return false;
		}

		function doneFullscreen(){
			$('.add-block ').show();
			$('.header .nav').show();
			$('.header .chat-hide').show();
		    $('#playercontainer1').css('position', 'relative');
		    $('#playercontainer1').css('top', '0px');
		    $('#playercontainer1').css('height', '360px');
		    $('#playercontainer').css('z-index', '102');
		    $('#home').hide();
		    $('body').css('overflow', 'auto');
		    return false;
		}
		function triggerPlayer(element, video_id){
			$('.video-thumb').find('img').show();
			$('.video-thumb').find('.play-icon').show();
			$('.video-thumb').find('.fs_icon').show();
			$('.video-thumb').find('.player').html('');
			$('.video-thumb').animate({ width: "480px" }).animate({ height: "300px" });
			$(element).next('img').hide();
			$(element).parent().find('.play-icon').hide();
			$(element).parent().find('.fs_icon').hide();
			$(element).parent().animate({ width: "640px" }).animate({ height: "390px" });
			$(element).parent().find('.player').html('<iframe src="https://www.youtube.com/embed/'+ video_id +'?autoplay=1" frameborder="0" allowfullscreen style="width: 100%; height: 390px;"></iframe>');

		}
</script>


















        
        
