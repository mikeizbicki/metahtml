a.type="text/javascript",a.async=!0,a.src=("https:"==document.location.protocol?"https://ssl":"https://www")+".google-analytics.com/ga.js";
var b=document.getElementsByTagName("script")[0];b.parentNode.insertBefore(a,b)};gaga();
</script>

