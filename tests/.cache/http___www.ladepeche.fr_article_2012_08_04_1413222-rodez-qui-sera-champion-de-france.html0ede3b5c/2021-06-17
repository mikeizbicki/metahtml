var dataLayer = [{"env":"prod","siteCode":"ddm","siteFamily":"gdm","tags":["ddm-6426702","grand-sud","vie-locale","thema","sport","autres-sports","12202","aveyron","12A","communes","rodez,12202","rodez"],"tagsSuffix":["vie-locale-topic","grand-sud-famille","thema-topic","autres-sports-discipline","sport-famille","12202","12-dpt","12A-bdv","rodez-topic","aveyron-topic","communes-topic"],"insee":["12202"],"ville":["rodez"],"dept":["12"],"deptLabel":["aveyron"],"bassin":["12A"],"pagetype":"article","platform":"web","restriction":1,"thema":"grand-sud","topic":"vie-locale","topicN2":"","topics":["vie-locale","autres-sports","rodez"],"article":{"id":"ddm-6426702","date":"20120804","author":"Ren\u00e9 Duran","correspondant":0,"pepite":0,"source":"visio","originalId":"","hp":false},"search":{"query":"","page":""},"xiti":{"site":"111841","n2":"20","chapter1":"contenu","chapter2":"ddm","page":"1413222-rodez-qui-sera-champion-de-france::6426702"},"dfp":{"publisherid":"31755961","bloc1":"ldi_ladepeche.fr","bloc2":"desktop","bloc3":"article","bloc4":"","pagetype":"article"}}];

var tagManager = {
    cookieName: 'userdl',

    init: function () {
        var user = this.getUserData();
        dataLayer[0].user = user;

        if (dataLayer[0]['restriction'] === 3 && user.type !== 'guest' && user.type !== 'free' && user.type !== 'oldabo') {
            dataLayer[0]['restriction'] = 2;
        }
    },

    getUserData: function() {
        var data = {
            id: null,
            md5: null,
            subscriberId: '',
            status: 1,
            newsletter: 0,
            type: 'guest',
            connectionMode: ''
        };

        var jsonData = this.getCookie(this.cookieName);
        if (typeof(jsonData) !== 'undefined' && jsonData) {
            var userData = JSON.parse(jsonData);
            if (userData) {
                data.id = userData.id;
                data.md5 = userData.md5;
                data.status = userData.status;
                data.type = (userData.isSubscriber) ? 'subscriber' : 'free'; // 'subscriber' : ancien abonné pôle Est
                if (userData.category !== '') { // Toujours fourni pôle Ouest
                    data.type = userData.category;
                }
                else if (userData.contratTitre !== '') { // Abonné actif
                    data.type = (userData.contratTitre === '10') ? 'premium' : 'integral';
                }
                if (userData.isSubscriber && !userData.abonnementsActifs) {
                    data.type = 'oldabo';
                }
                if (userData.subscriberId) {
                    data.subscriberId = userData.subscriberId;
                }
                if (userData.newsletter) {
                    data.newsletter = userData.newsletter;
                }
                if (userData.connectionMode) {
                    data.connectionMode = userData.connectionMode;
                }
            }
        }

        return data;
    },

    getCookie: function (cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) === ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) === 0) {
                return decodeURIComponent(c.substring(name.length, c.length));
            }
        }
        return '';
    }
};
tagManager.init();
</script>

