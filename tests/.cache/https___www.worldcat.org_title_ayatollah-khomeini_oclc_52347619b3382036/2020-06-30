        <script type="text/javascript">
var favnam = "";
var favsym = "";
var refPage = "";
function showaddfavdiv(instname, instsymbol, referPage, privacySetting)
{
	if ((instname != null) && (instsymbol != null) && (referPage != null))
	{
		favnam = instname;
		favsym = instsymbol;
        if (favsym.startsWith('['))
        {
            var noffset = favsym.indexOf(',');
            if (noffset == -1)
            {
                noffset = favsym.indexOf(']');
            }
            if (noffset > 1)
            {
                favsym = favsym.substring(1, noffset);
            }
        }
		refPage = referPage + 'favorite=' + favsym + '&favoritename=' + encodeURIComponent(favnam);
        if ((privacySetting == '') || (privacySetting == 1))
        {
            document.getElementById("privacy_public").checked = true;
        }
        else
        {
            document.getElementById("privacy_private").checked = true;
        }

        if(document.all){
            document.getElementById('favlibname').innerText = instname;
        }else{
            document.getElementById('favlibname').textContent = instname;
        }

        document.getElementById('addfavdiv').style.display = 'block';
	}
}
function doSaveFavorite()
{
    if (document.getElementById('privacy_public').checked)
    {
        document.getElementById('favliblink').href = refPage + '&privacy=public';
    }
    else
    {
        document.getElementById('favliblink').href = refPage + '&privacy=private';
    }

    document.location = document.getElementById('favliblink').href;

    closediv();
    return true;
}

function closediv()
{ document.getElementById('addfavdiv').style.display = "none"; }

</script>

