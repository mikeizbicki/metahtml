<script type="text/javascript" language="JavaScript">

// WMS institutions opening a new window for place hold must reset main page to https as idm will set the pop up to secure,
// and the pop up must refresh the main page to reflect the logged in status
function getLocationAndHideConfirmDiv() {
	var protocol = (true == true) ? "https://" : "http://";
	var updateParentURL = protocol + location.hostname + (location.port != "" ? ":" + location.port : "") + location.pathname + location.search;
	hideConfirmDiv('req_confirmation', true, false, updateParentURL);
}



