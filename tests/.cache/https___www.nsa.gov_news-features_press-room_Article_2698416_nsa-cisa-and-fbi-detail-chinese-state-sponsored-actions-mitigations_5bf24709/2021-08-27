    <meta itemprop="datePublished" content="July 19, 2021" />

    
    <div>
        
<div id="adetail-slick-gallery" class="gallery">
    
    <div class="item">
        
        <div class="image">
            <div class="blurred-bg" style="background-image: url(https://media.defense.gov/2021/Jul/19/2002805005/1920/1080/0/210719-D-IM742-1239.JPG)"></div>
