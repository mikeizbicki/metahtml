if(Drupal.settings.client_ads_dfp.prebid_enable){requestManager.enabledBidders.push('prebid');defineAndFetchPrebidAds();}
logHB('googletag :',googletag);for(name in ad_targeting){googletag.pubads().setTargeting(name,ad_targeting[name]);}
if(membership==1){googletag.pubads().setTargeting('user','member');}
var single_request=Drupal.settings.client_ads_dfp.client_ads_dfp_enable_single_request;if(single_request){googletag.pubads().enableSingleRequest();}
var collapse=Drupal.settings.client_ads_dfp.client_ads_dfp_collapse_empty_divs;if(collapse==='true'){googletag.pubads().collapseEmptyDivs();}
var first_refresh=Drupal.settings.client_ads_dfp.first_refresh;if(first_refresh){googletag.pubads().disableInitialLoad();}
