                      <form id="profileNicknameUpdateOpenReply" method="post" class="create-username" [class]="userState.hasNickname ? 'create-username hide-username' : 'create-username show-username'" action-xhr="/internal-api/nickname-update/submit" verify-xhr="/internal-api/nickname-update/verify" custom-validation-reporting="as-you-go" on="submit:AMP.setState({
                          profileState: {
                            submitProcessing:true,
                            messageDisplay:false
                          }
                          });
                          submit-error:AMP.setState({
                            profileState: {
                              submitProcessing:false,
                              messageDisplay:false
                            }
                          });
                          submit-success:profileNicknameUpdateOpenReply.hide, AMP.setState({
                            profileState: {
                              submitProcessing:false,
                              messageDisplay:true,
                              userStored: event.response.profile
                            },
                            userState: {
                              nickname: event.response.profile.nickname,
                              hasNickname: true
                            },
                            nickname: event.response.profile.nickname
                        });">
