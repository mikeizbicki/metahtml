	$('.tab_bar_block li').on('click',function(){
		if(!$(this).hasClass('active')){
			var tabIndex = $(this).attr('tabIndex');
			$('.tab_bar_block li').removeClass('active');
			$(this).addClass('active');
			$('.list_display_block').hide();
			$('#' + tabIndex).fadeIn();
		}
	});
	
	$('#test').slimscroll({
  		height: 410
		});
