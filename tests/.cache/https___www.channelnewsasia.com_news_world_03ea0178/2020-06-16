        function initializeCm8Algolia() {
          if(window.cm8Algolia !== undefined && window.cm8Algolia !== null){
            return;
          }

          window.cm8Algolia = new Cm8Algolia({
            element: "#cm8-algolia",
            app: {
              onShow: {
                animation: "fadeIn"
              },
              onHide: {
                animation: "fadeOut"
              },
              categories: {"Cna-Current-Affairs-Podcasts":"CNA Current Affairs Podcasts","cna-current-affairs-podcasts":"CNA Current Affairs Podcasts","cna":"CNA"},
              categoryUrlMap: [],
              durationBeforeClose: 1300
            },
            news: {
              latest: {
                max: 20,
              },
              endpoint: {
                latest: "/blueprint/servlet/syndication/v1/news/json/8395986?limit=10"
              },
              defaultImage: "/blueprint/cna/lib/algolia-overlay/images/cna-img.jpg"
            },
            search:  {
              placeholder: "Search keywords or topics",
              minChars: 1,
              recent: {
                max: 5,
                hideOnEmpty: false
              },
              querySuggestion: {
                max: 10,
                medium: 5,
                small: 3
              },
