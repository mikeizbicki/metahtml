        <script src="https://hannity.com/wp-content/themes/hannity/scripts.js?uc=1590508581" ></script>
      

        <script>
    var end = new Date('11/03/2020 12:0 AM');

    var _second = 1000;
    var _minute = _second * 60;
    var _hour = _minute * 60;
    var _day = _hour * 24;
    var timer;

    function showRemaining() {
       // console.log('set time remaining');
        var now = new Date();
        var distance = end - now;
        var timeleft;

        if (distance < 0) {

            clearInterval(timer);
           // document.getElementById('countdown').innerHTML = 'EXPIRED!';

            return;
        }
        var days = Math.ceil(distance / _day);
        var hours = Math.floor((distance % _day) / _hour);
        var minutes = Math.floor((distance % _hour) / _minute);
        var seconds = Math.floor((distance % _minute) / _second);

       timeleft = days + 'days ';
       timeleft+= hours + 'hrs ';
        timeleft += minutes + 'mins ';
        timeleft+= seconds + 'secs';

      $('.e-countdown-footer .countdown, .e-countdown-wide .countdown, .e-countdown-728 .countdown').html(days + ' Days ');
       //console.log('timer = '+timeleft);
    }

   // timer = setInterval(showRemaining, 1000);
   showRemaining();

</script>



