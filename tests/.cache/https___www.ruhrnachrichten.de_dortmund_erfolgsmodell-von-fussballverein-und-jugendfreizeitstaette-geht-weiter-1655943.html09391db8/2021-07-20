(function ()
{
	var resize = function (parentSelector, frameSrc)
	{
		var innerwidth = $("." + parentSelector + " > iframe[src*='" + frameSrc + "']").width();
		var innerheight = $("." + parentSelector + " > iframe[src*='" + frameSrc + "']").height();
		var outerwidth = $("." + parentSelector + " > iframe[src*='" + frameSrc + "']").parent().width();
		var ratio = outerwidth / innerwidth;
		$("." + parentSelector + " > iframe[src*='" + frameSrc + "']").width(outerwidth);
		$("." + parentSelector + " > iframe[src*='" + frameSrc + "']").height(innerheight * ratio);
	}
	var resizeAll = function ()
	{
		var possibleParents = ['InlineHtmlCode', 'LiveTickerItemContent','StoryShowInlineHtmlBox'];
		var possibleSrcs = ['twitter', 'facebook', 'youtube', 'instagram', 'google', 'thinglink', 'vimeo'];
		for (var index = 0; index < possibleParents.length; index++)
		{
			var parentSelector = possibleParents[index];
			for (var index2 = 0; index2 < possibleSrcs.length; index2++)
			{
				var frameSrc = possibleSrcs[index2];
				resize(parentSelector, frameSrc)
			}
		}
	}
	resizeAll();
	$(window).resize(resizeAll);
})();
</script>
	
