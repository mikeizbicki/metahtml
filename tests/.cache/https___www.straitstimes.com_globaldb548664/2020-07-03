<script type="text/javascript">
document.addEventListener("DOMContentLoaded", function(event) {
var myPlayer;
var myPlayerer = videojs('myPlayerID');
myPlayerer.ready(function(){
	myPlayer = this;
	myPlayer.catalog.getPlaylist('5235909154001', function(error, playlist){
		myPlayer.catalog.load(playlist);
	});
        myPlayer.on('loadedmetadata', function() {
		var details = {
			name: myPlayer.mediainfo.name,
			description: myPlayer.mediainfo.description
		};
		populateVideoTitle( jQuery(myPlayer.el()).parents(".video-viewbox"), details);
                myPlayer.volume(0.2);
                //myPlayer.muted(true);
                //myPlayer.autoplay(true);
                //eqheight_cite();
	});
});

function populateVideoTitle(evtObj, details) {
	jQuery('#stvideos_story-headline').html("");
	jQuery('#stvideos_media-text').html("");
        var $videoViewbox,
		$videoLI, 
		$videoLI_cite_html, 
		$videoLI_p_html;
	
	if (details !== null) {
		//console.log('details', details);
		$videoViewbox = evtObj;
		$videoLI_cite_html = details.name;
		$videoLI_p_html = details.description;
	} else {
		$videoViewbox = jQuery(evtObj.target).parents(".video-viewbox");
		$videoLI = jQuery(evtObj.target).closest("li.vjs-playlist-item");
		$videoLI_cite_html = $videoLI.children("cite").html();
		$videoLI_p_html = $videoLI.children("p").html();
	}

	//var videoViewbox_mediaBody = $videoViewbox.children(".media-body");
	//videoViewbox_mediaBody.children(".media-heading").children("h3").children(".story-headline").html($videoLI_cite_html);
	//videoViewbox_mediaBody.children(".media-text").html($videoLI_p_html);
        jQuery('#stvideos_story-headline').html($videoLI_cite_html);
	jQuery('#stvideos_media-text').html($videoLI_p_html);
}

function eqheight_cite() {
  console.log('here here');
  setTimeout(function() {
        var max_cite_height = 60;
	jQuery('cite').each(function(ind,elem) {
	  if ( jQuery(elem).height() > max_cite_height ) {
	    //console.log( $(elem).height() );
	    max_cite_height = jQuery(elem).height();
	  }
	});
	jQuery('cite').each(function(ind,elem) {
	  jQuery(elem).height( max_cite_height +"px");
	});
        //console.log('here here 22', max_cite_height);
  }, 750);
}

jQuery("#stvideos_video .vjs-playlist").on("click", function(e) {
	populateVideoTitle(e,null);
});
jQuery("#stvideos_video .vjs-dock-shelf").on("click", function() {
        var regexPattern = /[^a-z0-9]+/gi;

	var videoTitle = myPlayerer.mediainfo.name;
	var videoTitleEncoded = encodeURI(videoTitle);
        //var videoTitleURLSafe = (videoTitle.toLowerCase()).replace(regexPattern, "-");

	//var urlString = "https://studio.brightcove.com/products/videocloud/media/videos/"+ myPlayerer.mediainfo.id;
        var urlString = "http://www.straitstimes.com/videos/"+ myPlayerer.mediainfo.id;
	var urlStringEncoded = encodeURIComponent(urlString);

	var share_links = [];
	share_links.push("https://www.facebook.com/sharer/sharer.php?title="+ videoTitleEncoded +"&u="+ urlStringEncoded);
	share_links.push("https://plus.google.com/share?url="+ urlStringEncoded);
	share_links.push("https://twitter.com/intent/tweet?original_referer=https%3A%2F%2Fabout.twitter.com%2Fresources%2Fbuttons&text="+ videoTitleEncoded +"&tw_p=tweetbutton&url="+ urlStringEncoded);
	
	jQuery('label.vjs-social-direct-link input').val(urlString);
	
	jQuery('.vjs-social-share-links > a').each(function(ind, elem) {
		jQuery(elem).attr('href', share_links[ind] );
	});
});

var manPlay = false;
jQuery("button.vjs-play-control").on("click touchstart", function() {
  manPlay = true;
});

jQuery(window).scroll(function() {
  var windowPos = jQuery(window).scrollTop();
  var widgetPos = jQuery("#stvideos_video").offset().top;
  var windowHgt = jQuery(window).height();
  if (!manPlay) {
    if (windowPos+windowHgt >= widgetPos && windowPos-windowHgt <= widgetPos) {
      if (typeof myPlayer !== 'undefined') { myPlayer.play(); }
    } else {
      if (typeof myPlayer !== 'undefined') { myPlayer.pause(); }
    }
  }
});
});
</script></div></div></div></div>
      </div>

      
          </div>
    

