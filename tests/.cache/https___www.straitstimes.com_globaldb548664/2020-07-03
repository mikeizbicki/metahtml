.playlist_loading_area .loading_text { margin-top: calc(117px / 2 - 0.6em); color: #eee; font-weight: bold; text-align: center; text-transform: uppercase; font-family: "Curator","Helvetica Neue",Helvetica,Arial,sans-serif; opacity: 1; animation: faderer 0.8s linear 0.5s infinite alternate; }
.video-viewbox .media-footer { bottom: 0px; }
.video-viewbox .media-footer i:after { font-family: IconFontST; margin-right: 5px; font-size: 16px; line-height: 15px; float: right; content: "\e017"; }
.video-viewbox .vjs-dock-text { display: none; }
.video-viewbox .vjs-dock-shelf { display: block; }
@keyframes faderer { 0% { opacity: 1; } 100% { opacity: 0.3; } }
