function ga_event(category, action, label) {
  window.dataLayer = window.dataLayer || [];
  window.dataLayer.push({
    'event':'custom_event',
    'eventCategory':category,
    'eventAction':action,
    'eventLabel':label
  });
}

document.addEventListener("DOMContentLoaded", function(event) {
  var curateditems = document.querySelectorAll('.cx-curated a');
  for (var i = 0; i < curateditems.length; i++) {
    curateditems[i].addEventListener('click', function(e) {
      var href_arr = e.target.getAttribute('href').split('/');
      ga_event('CxvCurateGlobal-B ', 'click', href_arr[href_arr.length - 1] );
    });
  }
