    function checkAndResetCX() {
      if(window.getCookieByName("resetstldapw") != undefined && window.getCookieByName("resetstldapw")== "yes") {
        //cxense async script to clear ids
        console.log("SharedDeviceLogin");
        var cX = window.cX = window.cX || {}; cX.callQueue = cX.callQueue || [];
        cX.callQueue.push(['clearIds']);
        var expiry = new Date();expiry.setTime(expiry.getTime() + (60*60*1000));
        document.cookie = "resetstldapw=no;path=/; expires=" + expiry.toGMTString();
        ga('send', 'event','SharedDeviceLogin','1');

      }
    }
    checkAndResetCX();
  </script>

