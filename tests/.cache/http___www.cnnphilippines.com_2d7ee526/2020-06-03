<script type="text/javascript">
jQuery(document).ready(function() {
if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
console.log("Mobile: " + window.mobilecheck);
$('#newsroom-mobile').show();
$('#newsroom-desktop').hide();
} else {
$('#newsroom-mobile').hide();
$('#newsroom-desktop').show();
}
});
function streamVideo(clip, provider, connectUrl)
{
$f("newsroom", "https://releases.flowplayer.org/swf/flowplayer-3.2.18.swf", {
ratio: 9/16,
key: '$166952631088920',
clip: {
url: clip,
live: true,
// configure clip to use influxis as our provider, it uses our rtmp plugin
provider: provider
},
// streaming plugins are configured under the plugins node
plugins: {
// here is our rtmp plugin configuration
[provider]: {
url: "flowplayer.rtmp-3.2.13.swf",
// netConnectionUrl defines where the streams are found
netConnectionUrl: connectUrl
}
},
canvas: {
backgroundGradient: 'none'
}
});
}
</script>
<script type="text/javascript">
jQuery(document).ready(streamVideo('15273.sdp', 'hddn', 'rtmp://54.251.134.121/live/'));
</script>
