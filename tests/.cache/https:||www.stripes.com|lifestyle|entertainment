				$(document).ready(function () {
					$('ul#moreList390610 li div div.row:first').css('padding-top', '20px');
					size_li = $("#moreList390610 li").size();
					x = 0;
					$('#moreList390610 li:lt(' + x + ')').show();
					$('#loadMore-bottom-box390610, #moreList-close-all390610, #after-loadMore390610').hide();
			
					$('#first-loadMore390610, #load-next-more390610').click(function () {
						$('#loadMore390610').css({'-moz-border-radius': '5px 5px 0px 0px', '-webkit-border-radius': '5px 5px 0px 0px', 'border-radius': '5px 5px 0px 0px'});
						$('#more-reveal390610 #loadMore390610').css({'border-bottom': 'solid 4px #21513b'});
						x = (x + 10 <= size_li) ? x + 10 : size_li;
						$('#moreList390610 li:lt(' + x + ')').show();
						$('#loadMore-bottom-box390610, #after-loadMore390610').show();
						$('#moreList-close-all390610, #first-loadMore390610').hide();
						if (x === size_li) {
							$('#loadMore-bottom-box390610').hide();
							$('#moreList-close-all390610').show();
						}
					});
			
					$('#moreList-close-all390610, #close-next-more390610, #after-loadMore390610').click(function () {
						$('#loadMore390610').css({'-moz-border-radius': '5px', '-webkit-border-radius': '5px', 'border-radius': '5px', 'border-bottom': 'none'});
						x = (x - size_li < 0) ? 0 : x - size_li;
						$('#moreList390610 li').not(':lt(' + x + ')').hide();
						$('#loadMore-bottom-box390610, #moreList-close-all390610, #after-loadMore390610').hide();
						$('#first-loadMore390610').show();
					});
				});
