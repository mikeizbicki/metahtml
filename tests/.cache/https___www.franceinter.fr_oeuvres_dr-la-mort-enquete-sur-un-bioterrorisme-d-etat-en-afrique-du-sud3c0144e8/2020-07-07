            'getDevice' : function () {
                var device = 'desktop';
                if (navigator.userAgent.match(/Tablet|iPad/i))
                {
                    device = 'tablet';
                } else if(navigator.userAgent.match(/Mobile|Windows Phone|Lumia|Android|webOS|iPhone|iPod|Blackberry|PlayBook|BB10|Opera Mini|\bCrMo\/|Opera Mobi/i) )
                {
                    device = 'mobile';
                }

                return device;
            },

            'encodeQueryData': function  (data) {
                var ret = [];
                for (var d in data) {
                    ret.push(encodeURIComponent(d) + '=' + encodeURIComponent(data[d]));
                }
                return ret.join('&');
            },

            'getKirbyUuid' : function  () {
                if (window.Uuid && typeof window.Uuid === 'object') {
                    if (window.Uuid.get('kirbyUuid') === null) {
                        window.Uuid.store('kirbyUuid', window.Uuid.generate());
                    }
                    return window.Uuid.get('kirbyUuid');
                } else {
                    throw 'Uuid can\'t be generataed'
                }
            }
        }
    })();
