    window.Matrix = (function () {
        return {
            'source': 'FRANCE_INTER_WEB',
            'stationId': '1',
            'chapters' : '',
            'kirbyModel': '',
            'kirbyModelUuid': '',
            'device_uuid': localStorage.getItem('kirbyUuid'),
            'hitPage' : function (params) {
                (!params)? params = {}: false;
                document.body.setAttribute('data-kirby-page', 'true');
                var obj = {
                    event_type : params.event_type ? params.event_type : 'page_view',
                    station_id : params.station_id ? params.station_id : this.stationId,
                    path : params.path ? params.path : window.location.pathname,
                    source : params.source ? params.source : this.source,
                    device_uuid: this.device_uuid
                };

                if (document.referrer !== '' && document.referrer !== window.location.origin) {
                    obj.referer = document.referrer;
                }

                if (this.kirbyModel) {
                    obj.model = this.kirbyModel;
                }

                if (params.type) {
                    obj.type = params.type;
                }

                if (this.chapters) {
                    obj.chapter1 = this.chapters.split('_|_')[0];
                    obj.chapter2 = this.chapters.split('_|_')[1];
                }
