    if (rf_species === 'inread') {
        var validateHbAdSize = "";
        if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)
                || window.navigator.userAgent.match(/Windows NT ([0-9]\.[0-9])/) ) {
             validateHbAdSize = [[2, 2]];
        } else {
             validateHbAdSize = [[533, 300]];
        }

        var customAdUnit = document.querySelector('[data-adunit]').getAttribute('data-adunit');
        customAdUnit = customAdUnit.split('/');
        customAdUnit[customAdUnit.length - 1] = rf_species;
        var goodAdUnit = customAdUnit.join('/');
        slot['adunit'] = goodAdUnit.toString();
        slot['inread'] = {'video': {'context': 'outstream'}, 'banner': {'sizes': validateHbAdSize}};
    }

    window.dejavuAds.slots.push(slot);
