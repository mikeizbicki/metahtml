                    obj.matrix_version_id = params.matrix_version_id;
                }
                if (params.dest_url) {
                    obj.dest_url = params.dest_url;
                }
                if (this.getDevice()) {
                    obj.device = this.getDevice();
                }
                this.send(obj);
            },

            'hitClick': function (params) {
                (!params)? params = {}: false;
                var obj = {
                    event_type : params.event_type ? params.event_type : 'element_click',
                    station_id : params.station_id ? params.station_id : this.stationId,
