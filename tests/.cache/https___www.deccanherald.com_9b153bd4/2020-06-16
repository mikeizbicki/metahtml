$("#page").removeClass("adjustPageTop");
$("#page").addClass("reset-page-padding");
$("header").css("top", "0px");
$("header").removeClass("adjustheaderTop");
}
function downloadBannerShow() {
setTimeout(function() {
$("#app_banner").slideDown();
$("header").addClass("adjustheaderTop");
$("#page").removeClass("reset-page-padding");
$("#page").addClass("adjustPageTop");
}, 10);
}
});
});
