$(function(){

   $("#data-carousel").contents().clone().appendTo("#data-carousel2"); 

   $("#data-carousel2 .col-sm-card").removeClass("col-md-4 col-sm-card divideby3").addClass("carousel-item no-divide");
   $("#data-carousel2 .carousel-control-prev, #data-carousel2 .carousel-control-next").attr("href","#data-carousel2")

     var divs = $("#data-carousel .carousel-inner .divideby3");
     for(var i = 0; i < divs.length; i+=3) {
             divs.slice(i, i+3).wrapAll("<div class='carousel-item'></div>");
     }

   $('#data-carousel').find('.carousel-item').first().addClass('active');

   $('#data-carousel2').find('.carousel-item').first().addClass('active');

   $('#data-carousel').find('.carousel-indicators li').first().addClass('active');

   $('#data-carousel, #data-carousel2').carousel({
         pause: 'hover'
    });
});
</script>

