<script>

$( document ).ready(function() {

    var is_mobile = false;
    if( $('#some-element').css('display')=='none') {
        is_mobile = true;       
    }

$(".sdgs").each(function( index ) {
      var id_div = $(this).attr('id');

    if ( $('#' + id_div).length ) {
        var sgds_div = "";
        var sdgs_str = $('#' + id_div + ' p').text();
        if($('#' + id_div + ' p').text().length > 0) {
        var sdgs_info = sdgs_str.split("_");
        var item_uid = sdgs_info[0];
        var item_sdgs = sdgs_info[1];
        var sdgs_arr = item_sdgs.split(",");
        $.each(sdgs_arr, function( key, value ) {
            var sdg_num = value;
            sdg_num = sdg_num.trim();
            sdg_num_no0 = sdg_num;
            if(! isNaN(sdg_num_no0)){
            var sdg_url = "http://www.fao.org/sustainable-development-goals/goals/goal-"+ sdg_num_no0 +"/en/";
            sdg_num = ('0' + sdg_num).slice(-2);
            if (is_mobile == true) {
	        sgds_div += "<div class='mob_sdg mob_sdg_"+ sdg_num +"'><span><a target='_self' href='"+ sdg_url +"'>"+ sdg_num_no0 +"</a></span></div>";
            } else {
	        sgds_div += "<span class='sdg_icon'><a target='_self' href='"+ sdg_url +"'><img width='50px' height='50px'  src='http://www.fao.org/fileadmin/templates/sdg/img/icons/SDG_EN_"+ sdg_num +".svg'></a></span>"; 
            }
            }
        });
       if (is_mobile == true) { 
             sgds_div = "<span style='color: #fff; margin-right: 5px; font-size: 20px !important;'>SDGs</span>" + sgds_div; 
       }
    
       $( ".carousel-item-" + item_uid ).append( $("<div class='sdg_container'>" + sgds_div + "</div>") );
    }
}
});
});

</script>
