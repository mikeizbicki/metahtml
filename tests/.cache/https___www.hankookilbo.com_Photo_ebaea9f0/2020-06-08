<script>

    $(document).ready(function () {
        $('#ptSort_1 a').addClass("on");
        goSubmit();
    });

    
    function goSubmit() {
        $.ajax({
            method: "POST",
            url: "/photo/photo/GalleryMainAjax",
            data: { Page: $("#RecentPage").val(), Sort: $("#RecentSort").val(), CatSeq: $("#RecentCatSeq").val(), Search: $("#search").val() },
            success: function (data) {
                $("#PhotoListAjax").html(data);
                HK.util.initGalleryBoard();
            }
        })
    }

    
    $('#submit').click(function (e) {
        $("#RecentPage").val(1);

        goSubmit();
    });

    
    function PtSort(sort) {
        $("#RecentPage").val(1);
        $("#RecentSort").val(sort);

        $("[id^='ptSort_'] a").removeClass();
        $('#ptSort_' + sort + " a").addClass("on");

        goSubmit();
    }

    
    $('.more-gallery-list').on('click', function (e) {
        e.preventDefault();
        RecentPhotoAdd();
    });

    function RecentPhotoAdd() {
        var page = $("#RecentPage").val();
        page = parseInt(page) + 1;
        $("#RecentPage").val(page);

        $.ajax({
            method: "POST",
            url: "/photo/photo/GalleryMainAjax",
            data: { Page: page, Sort: $("#RecentSort").val(), CatSeq: $("#RecentCatSeq").val(), Search: $("#search").val() },
            success: function (data) {
                if (data.indexOf('더이상 내용이 없습니다.') > -1) {
                    photoAjaxIng = true;
                    HK.util.addGalleryItem(data);
                }
                else {
                    HK.util.addGalleryItem(data);
                    setTimeout(function () {
                        photoAjaxIng = false;
                    }, 500)
                }
            }
        });
    }

    
    var photoAjaxIng = false;
    var addScroll = 1;

    $(window).scroll(function () {

        var IntTop = $(window).scrollTop();
        var DocTop = $(document).height();
        var WinTop = $(window).height();
        var GapTop = parseInt(DocTop - IntTop);

        if ((GapTop < 2000 || GapTop <= 0) && photoAjaxIng == false) {
            photoAjaxIng = true;

            RecentPhotoAdd();
        }
    });
</script>

    
