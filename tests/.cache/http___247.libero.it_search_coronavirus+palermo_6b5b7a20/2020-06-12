			function getParamValue(paramName) {	
				var retVal="";
				var re = new RegExp( "(" + paramName + "=([^&]*))" );
				if ( result = location.href.match( re ) ) {
					//console.log(result);
					retVal=result[2];
				}
				return retVal;
			}
		
			function existsParamInQuery(paramName) {
				var re = new RegExp( "(" + paramName + "=([^&]*))" );
				if ( result = location.href.match( re ) ) {
					//console.log(result);
					return true;
				}
				else {
					return false;
				}
			}
		
		
			
