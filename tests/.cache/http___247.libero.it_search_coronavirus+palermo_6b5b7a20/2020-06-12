			 	setMenuRegionAjax:function(){
			 		var self = this;
			 		$.ajax({     
						type: "GET",     
						url: "/Libero.Stash.l247.r.html?ts="+(new Date()).getTime(),    
						dataType: "html",     
						success: function(json) {  
							var data = eval('(' + json + ')');
							
							Libero.Stash.l247.r = data.r - 1;
							self.loadFromSource();
						},     
						error: function (XMLHttpRequest, textStatus, errorThrown) {         
						} 
					}); 
			 	},
			 	
			 	
			 	
			 	
