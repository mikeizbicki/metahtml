		<script>	
				function popUp(url) {					
					window.open(url, 'ShareWindow', 'height=450, width=550, top=' + ($(window).height() / 2 - 275) + ', left=' + ($(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
					 return false;
				}
		
				function share_popup(element, video_title){		
					title = video_title.replace(/[:\.\/_ ]+/g, '_');
					video_url = 'https://wn.com/' + title.replace(' ', '_');
			   		$(element).parent().find('.buttons').show().html('<div class="buttons-close"  onclick="return buttons_close(this)"><i class="fa fa-times"></i></div><a href="javascript:void(0);" id="https://twitter.com/home?status=' +video_url+ '" class="twitter" onclick="return popUp(this.id)"><i class="fa fa-twitter"></i></a><a href="javascript:void(0);" id="https://www.facebook.com/sharer.php?u=' +video_url+ '" class="facebook" onclick="return popUp(this.id)"><i class="fa fa-facebook"></i></a><a id="https://plus.google.com/share?url=' +video_url+ '" href="javascript:void(0);" class="google" onclick="return popUp(this.id)"><i class="fa fa-google-plus"></i></a><a href="javascript:void(0);" id="https://wn.com/api/upge/cheetah-photo-search/send_email?title=' +video_url+ '&url=' +video_url+ '" class="email" onclick="return popUp(this.id)"><i class="fa fa-envelope-o"></i></a></div>');
					$(element).addClass('active');
				}
				function buttons_close(element){		   		
					$(element).parent().hide();
				}
		</script>





<script>
   $(function(){
				$('#draggable').draggable();
				
				$('.play-main-video').click(function(e) {		
						var video_id = $(this).attr('id');
						$('.mainPlayer').css({display: 'block'}).html('<iframe src="https://www.youtube.com/embed/'+ video_id +'?autoplay=1" frameborder="0" allowfullscreen></iframe>').animate({opacity: 1}, 100);
				});			
				
				var fullscreenOn = false				
				var playerOn = false						
				
				if ($(window).width() < 768) {
	        
						   
	       $('.play-float-video').click(function(e) {		
		   					var video_id = $(this).attr('id');				
							e.preventDefault();							
							$('.player-container').css('width', '100%').show();
	        				$('.player').animate({ 'paddingBottom' : '56.25%' });						
							$('.player-container').addClass('fs').removeClass('small');	
							$('.details-header').html('<div class="remove_player"><i class="fa fa-times" aria-hidden="true"></i></div>');							
						 });
	         	}
	       else{
				$('.play-float-video').click(function(e) {		
	              		var video_id = $(this).attr('id');				
	          			e.preventDefault();						
						 if (playerOn == false) {
							triggerSmallPlayer(video_id);			
						 } else {		
							reloadSmallPlayer(video_id);	
						 }
					});
	        	}	        	
	        			
	       $('.small_player').live("click", function(e){  			
					 	e.preventDefault();		
						smallerPlayer();
						fullscreenOn = false;
						playerOn = true;	
				});				
				$('.enlarge_player').live("click", function(e){  
			 		e.preventDefault();		
					enlargePlayer();
					fullscreenOn = true;
				});			
				$('.remove_player').live("click", function(e){    
					e.preventDefault();	
					$('.player-container').hide('slow');
					$('.player').html('');
					fullscreenOn = false;			
					playerOn = false;
				});			 

			  
			  
	   			$('.popup').click(function(e) {
					        e.preventDefault();
					        window.open($(this).attr('href'), 'fbShareWindow', 'height=450, width=550, top=' + ($(window).height() / 2 - 275) + ', left=' + ($(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
					        return false;
				});
					
	            $(".search-type a").click(function () {
	                    var search_type = $(this).data('type');
	                    $("#search-selector").val(search_type);
	                        if(!$(this).hasClass("active"))
	                        {
	                            $(".search-type a").removeClass("active");
	                            $(this).addClass("active");
	                        }
	            });
	            $('#mainsearch').submit(function(event) {
	                    var search_type = $("#search-selector").val();
	                    if(search_type == 'videos') {
	                        var search_string = $(".top_query").val();
	                        search_string = search_string.toLowerCase().replace(/[\.\/\: _!]+/g, '_');
	                        window.open('https://wn.com/' + search_string, '_blank');
	                        return false; // do not submit the form
	                    }else{
	                        return true;
	                    }
	             });

                $(".gotop").live("click", function(){                	
                    $('html, body').animate({
        				scrollTop: $('#playercontainer1').offset().top
    				}, 1000);      
                });
                $(".logo-bm li").live("click", function(){
                    $(".logo-bm li").removeClass("active");
                    $(this).addClass("active")
                });
                $('.bn-close').click(function () {
                    $(this).parent().hide("slow");
                });
                $( ".nav li" ).click(function() {
                     if ( $( this ).hasClass( "local" ) ) {
                        $('#page-title').hide();
                    }else{
                        $('#page-title').show();
                    }
                });
                
                

            // focus on the email field when a user clicks the subscribe btn
            jQuery('#dLabel').on('click', function(e) {
                jQuery('#subscribe-email').focus();
            });
            jQuery('.emailTool').tooltip();
			jQuery('.main-article a').attr('target', '_blank');

        });
   		</script>
             
 		
 		<script>
		function triggerSmallPlayer(video_id){
					$('.player-container').addClass('small').removeClass('fs');	
					$('.player-container').show().animate({width: '400px', height: '300px'}, 500);

					$('.player-container').find('.player').html('<iframe src="https://www.youtube.com/embed/'+ video_id +'?autoplay=1" frameborder="0" allowfullscreen></iframe>');
	
					$('.details-header').html('<div class="remove_player"><i class="fa fa-times" aria-hidden="true"></i></div><div class="enlarge_player"><i class="fa fa-expand" aria-hidden="true"></i></div>');	
					
		}	

	
	function enlargePlayer(){
		$('.player-container').animate({ 'width' : '100%',  'height' : '100%' }, 100);			
		$('.details-header').html('<div class="small_player"><i class="fa fa-arrow-circle-o-left" aria-hidden="true"></i><i class="arrow"></i><span>back </span></div>');
		$('.player-container').addClass('fs').removeClass('small');	
		$("body").css("overflow", "hidden");
  }

	function smallerPlayer(){
		$('.player-container').addClass('small').removeClass('fs');	
		$('.player-container').animate({ 'width' : '400px',  'height' : '300px' }, 500).delay("slow").css('bottom', '20px').css('right', '20px').css('top', 'auto').css('left', 'auto');	
		$('.details-header').html('<div class="remove_player"><i class="fa fa-times" aria-hidden="true"></i></div><div class="enlarge_player"><i class="fa fa-expand" aria-hidden="true"></i></div>');
		$("body").css("overflow", "scroll");
	
    }	

	function reloadSmallPlayer(video_id){
			 $('.player').html('<iframe src="https://www.youtube.com/embed/'+ video_id +'?autoplay=1" frameborder="0" allowfullscreen></iframe>');
		
   }
		
	function report_rights(video_id) {
			$('.img-content').find('.img-body').html("The video on this page is embedded from YouTube. <br />To report a rights infringement in this video please <a href='https://www.youtube.com/copyright_complaint_form?v=https%3A%2F%2Fwww.youtube.com%2Fwatch%3Fv%3D"+ video_id +"' target='_blank' style='color: #2a72ba'>click here</a> <br />and notify YouTube of the issue. <br />The URL of this video is <br /><a target='_blank' style='color: #2a72ba' href='https://www.youtube.com/watch?v="+video_id+"'>https://www.youtube.com/watch?v="+video_id+"</a>");
	}

	// -->
	</script>
        






		<script type="text/javascript" src="//ecdn3.wn.com/or/assets/ling/article-events.js" async="true" defer="true" nopack="1"></script>

		
    <!-- AUTOCOMPLETE JS -->
    <script nopack="1" type="text/javascript" src="//ecdn3.wn.com/or/js/jquery.autocomplete.min.js"></script>
    <!-- /AUTOCOMPLETE JS -->

		
		<script>
    $(function(){
        var wp_url = 'https://en.wikipedia.org/w/api.php?action=query&format=json&callback=wiki_update&list=search&srwhat=text&srinfo=suggestion&srprop=&srredirects=1&srsearch=';
        var yt_url = 'https://suggestqueries.google.com/complete/search?hl=en&client=youtube&hjson=f&jsonp=yt_update&ds=yt&cp=3&q=';
        var bi_url = 'https://api.search.live.net/json.aspx?Appid=F550969A2EBF1211A345960C88E67163180795FE&sources=RelatedSearch&web.count=5&JsonType=callback&JSONCallback=bing_update&query=';
        var yh_url = 'https://query.yahooapis.com/v1/public/yql?q=select%20*%20from%20search.suggest%20where%20query=""&limit=5&format=json&callback=yahoo_update';
        var gg_url = 'https://google.com/complete/search?jsonp=gg_update&client=firefox&q=';

        function wp_parse(data) {
            var r = [];
            if(!data.query || !data.query.search) return r;
            $.each(data.query.search, function(key, row) {
                r.push({
                    data: row.title.toLowerCase(),
                    value: row.title
                });
            });
            return r;
        }

        function bing_parse(data) {
            var r = [];
            var t = data.SearchResponse.RelatedSearch;
            if(!t) return r;
            $.each(t.Results, function(key, row) {
                r.push({
                    data: row.Title.toLowerCase(),
                    value: row.Title
                });
            });
            return r;
        }

        function yt_parse(data) {
            var r = [];
            if(!data[1]) return r;
            $.each(data[1], function(key, row) {
                r.push({
                    data: row[0],
                    value: row[0]
                });
            });
            return r;
        }

        function yahoo_parse(data) {
            var r = [];
            if(!data.results) return r;
            $.each(data.results, function(key, row) {
                r.push({
                    data: row[0].toLowerCase(),
                    value: row[0]
                });
            });
            return r;
        }

        function lastWord(q){
            if(!q) return '';
            var l = q.split(/[ \t_-]/g);
            return l;
            // if( l.length >= 2){
            //  return l[l.length-2] + l[l.length-1];
            // }
            // return l[l.length-1];
        }

        var qid=100;
        $('.top_query').autocomplete({
            deferRequestBy: 150,
            lookup: function (query, done) {
                // Do ajax call or lookup locally, when done,
                // call the callback and pass your results:
                var result = {
                    suggestions: []
                    // suggestions: [
                    //     { "value": "United Arab Emirates", "data": "AE" },
                    //     { "value": "United Kingdom",       "data": "UK" },
                    //     { "value": "United States",        "data": "US" }
                    // ]
                };

                var c = 0;
                window.yt_update = function(data) {
                    parsed = yt_parse(data);
                    //result.suggestions.push('yt');
                    result.suggestions.push.apply(result.suggestions, parsed);
                    done(result);
                };
                window.gg_update = function(data) {
                    parsed = yt_parse(data);
                    //result.suggestions.push('gg');
                    result.suggestions.push.apply(result.suggestions, parsed);
                    done(result);
                };
                window.wiki_update = function(data) {
                    parsed = wp_parse(data);
                    //result.suggestions.push('wi');
                    result.suggestions.push.apply(result.suggestions, parsed);
                    done(result);
                };
                window.bing_update = function(data) {
                    parsed = bing_parse(data);
                    //result.suggestions.push('bi'+parsed[0]);
                    result.suggestions.push.apply(result.suggestions, parsed);
                    done(result);
                };
                window.yahoo_update = function(data) {
                    parsed = yahoo_parse(data);
                    //result.suggestions.push('yh'+parsed[0]);
                    result.suggestions.push.apply(result.suggestions, parsed);
                    done(result);
                };
                $.ajax({
                    dataType: 'jsonp',
                    jsonp: 'cb',
                    cache: true,
                    jsonpCallback: 'wiki_update',
                    url: wp_url + lastWord(query)
                });
                $.ajax({
                    dataType: 'jsonp',
                    cache: true,
                    url: yt_url + lastWord(query)
                });
                $.ajax({
                    dataType: 'jsonp',
                    cache: true,
                    url: gg_url + lastWord(query)
                });
                $.ajax({
                    dataType: 'jsonp',
                    cache: true,
                    url: bi_url + lastWord(query)
                });
                $.ajax({
                    dataType: 'jsonp',
                    cache: true,
                    url: yh_url.replace('query=""', 'query="'+query+'"')
                });
                //done(result);
            },
            onSelect: function (suggestion) {
                //return suggestion.value + ', ' + suggestion.data);
                $('.top_query').focus();
                //return suggestion.data+' ';
            },
            function (suggestion, currentValue) {
                return (currentValue ? currentValue + ' ': '') + suggestion;
            }
        })
    });

</script>






<script type="text/javascript">
  var _gaq=_gaq||[];
  _gaq.push(['_setAccount', 'UA-1177289-7']);
  _gaq.push(['_trackPageview']);
  _gaq.push(["_setAccount","UA-1177289-31"]);
  _gaq.push(["_setPageGroup", 1, "article"]);
  _gaq.push(["_setPageGroup", 2, "system"]);
  _gaq.push(['_trackPageview']);
  _gaq.push(["_setDomainName",".wn.com"],["_trackPageview"]);
  var scookie=$.cookies.get('wnsearch');
  if(scookie){
      _gaq.push(['_trackPageview', 'https://article.wn.com/view/?qs='+scookie]);
      $.cookies.set('wnsearch', '');
  }
function gaga(){var a=document.createElement("script");
a.type="text/javascript",a.async=!0,a.src=("https:"==document.location.protocol?"https://ssl":"https://www")+".google-analytics.com/ga.js";
var b=document.getElementsByTagName("script")[0];b.parentNode.insertBefore(a,b)};gaga();
</script>

