<script>
$(document).ready(function(){
  $('#cookie_dialog').css('visibility','visible');
  $('#cookie_dialog a.yes, #cookie_dialog a.no').on('click',function(){
    var dat = new Date(new Date().setFullYear(new Date().getFullYear() + 1))
    var dateString = dat.toUTCString();
    var dat = new Date();
    var dateNow = dat.toISOString();
    var cname = "finance_cookies";
    var cval = "accept="+($(this).hasClass('yes') ? "all" : "no")+"|datetime="+dateNow+"|js";
    var cookieString = cname+"="+cval;
    document.cookie = cookieString + ";expires="  + dateString + ";domain=.deutsche-wirtschafts-nachrichten.de;path=/";
    $('#cookie_overlay').remove();
    return false;
  });
});
</script></div>
