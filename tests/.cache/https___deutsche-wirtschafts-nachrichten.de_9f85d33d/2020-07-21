	if ( $( ".altnaslovka.ure1" ).length ) {
		if(screen.width <= 479){
			var ureBox2 = $( ".altnaslovka.ure1 > .newsbox.newsbox-m.boxcnt_2" )[0].outerHTML;
			var ureBox3 = $( ".altnaslovka.ure1 > .newsbox.newsbox-m.boxcnt_3" )[0].outerHTML;
