function car_check(){
 $('.carousely').each(function(i,e){
   var $v = $(e);
   if (!($v.hasClass('on'))){
    console.log('owl: carousel switch on, max ht is',$v.css('max-height'));
     $v.owlCarousel({
      afterInit: function(){
       console.log('carousel after init for ',e,'height',$(e).outerHeight());
       $v.addClass('on');   
      },
      autoPlay: false, 
          navigation : true,
          navigationText: ["&lt;","&gt;"],
                    items : 4,
          itemsCustom: [[0,2],[600,2],[900,4]],
                    stopOnHover : true,     
          pagination: false,      
          paginationNumbers: false,
          lazyFollow : true,
          lazyEffect : "fade",
          afterMove: function(){ $('body').trigger('fin2-ajax-done');}
     });
     console.log('owl: after enable');
   }
  });
/*
