"UNTERNEHMEN","TECHNOLOGIE","RATGEBER"]},{"dfp_slot_name":"Interscroller","fin_slot_name":"fin_Interscroller","sizes":"[1,1]","collapseEmptyDiv":"false, false","section_ads":["HOME","GEOPOLITIK","DEUTSCHLAND","WELTWIRTSCHAFT","FINANZEN","UNTERNEHMEN","TECHNOLOGIE","RATGEBER"]},{"dfp_slot_name":"300x250_bottom","fin_slot_name":"adpos-m2","sizes":"[300,250]","collapseEmptyDiv":"true, true","section_ads":["HOME","GEOPOLITIK","DEUTSCHLAND","WELTWIRTSCHAFT","FINANZEN","UNTERNEHMEN","TECHNOLOGIE","RATGEBER"]},{"dfp_slot_name":"300x250_express","fin_slot_name":"fin_Express","sizes":"[300,250]","collapseEmptyDiv":"true, true","section_ads":["HOME","GEOPOLITIK","DEUTSCHLAND","WELTWIRTSCHAFT","FINANZEN","UNTERNEHMEN","TECHNOLOGIE","RATGEBER"]},{"dfp_slot_name":"analytics_px","fin_slot_name":"fin_analytics_px","sizes":"[1,1]","collapseEmptyDiv":"false, false"},{"dfp_slot_name":"2x2_DWN_Meetrics","fin_slot_name":"fin_2x2_meetrics","sizes":"[2,2]","collapseEmptyDiv":"false, false"},{"dfp_slot_name":"meetrics_px","fin_slot_name":"fin_meetrics_px","sizes":"[2,2]","collapseEmptyDiv":"false, false"}]';
  var site_ads_obj = JSON.parse(site_ads);
  var dfp_customer_id = '18356479'; // added quotes so empties don't break -- c0w 2020-01
  var dfp_slots = {};
  var dfp_slots_settings = {};
  var dfp_section_ads = {};
	var dfp_yieldlab_settings = {};

	if( NodeList.prototype.isPrototypeOf(document.querySelectorAll('div[id^="div-gpt-ad-' + testEd + '-"]')) && document.querySelectorAll('div[id^="div-gpt-ad-' + testEd + '-"]').length >= 1 ){
		var activeEd = testEd;
	}

  // loop through all NEO->admin->site_settings->ads and prepare for DFP
  // console.log('There are all ads: ',site_ads_obj);
  for (var prop in site_ads_obj) {
    dfp_slots[site_ads_obj[prop]['dfp_slot_name']] = (site_ads_obj[prop]['sizes'] == 'false') ? false : JSON.parse("[" + site_ads_obj[prop]['sizes'] + "]");
    dfp_slots_settings[site_ads_obj[prop]['dfp_slot_name']] = site_ads_obj[prop]['collapseEmptyDiv'];
    if(site_ads_obj[prop].hasOwnProperty('section_ads')){
      if( site_ads_obj[prop]['section_ads'] == 'banana' ){
        dfp_section_ads[site_ads_obj[prop]['dfp_slot_name']] = 'allowed';
      }
    }
		if (typeof yl !== 'undefined'){
			if(site_ads_obj[prop].hasOwnProperty('yield_id')){
				dfp_yieldlab_settings[site_ads_obj[prop]['dfp_slot_name']] = site_ads_obj[prop]['yield_id'];
			}
		}
  }
	
	//console.log("yieldlab settings:", dfp_yieldlab_settings);
	//console.log("dfp_slots:", dfp_slots);

	googletag.cmd.push(function() {
		console.log('[c0wsky dfp] Active edition is ' + activeEd);
		if( !activeEd ) return;
		// settings
		switch(activeEd) {
			default:
				// slots got dynamiczzz
				var slots = dfp_slots;
				// since we use the same targetting everywhere we can do it on the global level -- c0w 2019-06-14
				for (var i in dfp_tgt){
					console.log('[c0wsky dfp] setTargeting default',i,dfp_tgt[i]);
					googletag.pubads().setTargeting(i,dfp_tgt[i]);
				}
				if (typeof extra_tgt === 'object'){ // did we receive any extra targetting? such as from bart?
					for (var i in extra_tgt){
						console.log('[c0wsky dfp] setTargeting extra',i,extra_tgt[i]);
						googletag.pubads().setTargeting(i,extra_tgt[i]);
					}
				}
				// OK, here we go
				for (var slot_name in slots){
					var sizes = slots[slot_name];

					console.log('[c0wsky dfp] doing slot',slot_name,'sizes',sizes);
					var me = false;
					if (sizes){ // regular
            // var section_slot_name = ( dfp_section_ads[slot_name] == 'allowed' ) ? section+'_'+slot_name : slot_name;
            // console.log('slot sizes',dfp_customer_id,'activeEd',activeEd,'slot_name',slot_name);
						//.setForceSafeFrame(true) out
						me = googletag.defineSlot('/'+dfp_customer_id+'/' + activeEd + '_'+slot_name, sizes, 'div-gpt-ad-' + activeEd + '-'+slot_name).addService(googletag.pubads());
					}
					else { // out of page
            // var section_slot_name = ( dfp_section_ads[slot_name] == 'allowed' ) ? section+'_'+slot_name : slot_name;
            // console.log('section rules?',section_slot_name);
						// .setForceSafeFrame(true) out
						me = googletag.defineOutOfPageSlot('/'+dfp_customer_id+'/' + activeEd + '_'+slot_name, 'div-gpt-ad-' + activeEd + '-'+slot_name).addService(googletag.pubads());
					}
					// any extra setup?
					// yes we do! in object!

					// testing yield object and set target
					if (typeof yl !== 'undefined'){
						if(dfp_yieldlab_settings.hasOwnProperty(slot_name) ){
							console.log("testing yieldlab setting",dfp_yieldlab_settings[slot_name],"for slot",slot_name);
							if(yl.YpResult.get(dfp_yieldlab_settings[slot_name])){
								me.setTargeting('yl_id',dfp_yieldlab_settings[slot_name])
								console.log('[c0wsky dfp] setTargeting yl_id',me,dfp_yieldlab_settings[slot_name]);
							}
						}
					}
          var div_settings = Boolean(dfp_slots_settings[slot_name]);
          me.setCollapseEmptyDiv(div_settings);
          //me.setCollapseEmptyDiv(true, true);
          console.log('Settings for slot:',slot_name,'value:',dfp_slots_settings[slot_name]);

				} // slots	
				// one single listener
				googletag.pubads().addEventListener('slotRenderEnded', function(event) {
					var did = event.slot.getAdUnitPath();
					console.log('[c0wsky dfp] somebody got rendered',did);
					switch (did){
						case '/'+dfp_customer_id+'/'+activeEd+'_predoglas':
							setTimeout(function() {
								if( document.getElementById("predoglas-redirect") != null && document.getElementById("predoglas-redirect") != "undefined" ){
									var predoglasRedirectUrl = document.getElementById("predoglas-redirect").href;
									var myurl = encodeURIComponent(window.location.href);
									window.location.href = predoglasRedirectUrl + '&url='+myurl;
								}
							}, 3000);
							var containsAd = !event.isEmpty;
						break;
						default:
							var containsAd = !event.isEmpty;
					}
				});
			break;
		}
		// googletag.pubads().collapseEmptyDivs(true);
		// googletag.companionAds().setRefreshUnfilledSlots(true);
		// googletag.pubads().enableVideoAds();   
		googletag.pubads().enableSingleRequest();
		googletag.enableServices();
	}); // push
  
	var adunits = document.querySelectorAll('div[id^="div-gpt-ad-' + activeEd + '-"]');
	for (var i = 0; i < adunits.length; i++) {
		adid = adunits[i].getAttribute('id');
		console.log('We got item:' + i + '/' + adunits.length + ' and AdSlot:' + adid + ' ... loaded');
		googletag.cmd.push(function() { 
			googletag.display(adid); 
		});
	}
} // run_dfp

var dfp_happened = false;

run_dfp();
dfp_happened = 'dfp active';
console.log('dfp_happened from App.dfp.tpl.php: ', dfp_happened);
</script>
