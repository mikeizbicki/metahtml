									<script type="text/javascript">
			var dfpBuiltMappings = {}, dfpAdUnits = {};
			var dfpSizeMapping = [];
			var dfpAdLayer = {"post_id":"535983","title":"The Corner - Article"};
			var dfpAdDetails = [];
			var adLayersScreen = screen.width < 768 ? 'mobile' : 'desktop';
			var adLayersPage = ('Homepage' === dfpAdLayer.title) ? 'homepage' : 'other';
			var loggedInUnits = {
									desktop_homepage: [
													'rec_atf',
											],
									desktop_other: [
													'rec_atf',
											],
									mobile_homepage: [
													'inline_1_mobile',
											],
									mobile_other: [
											],
							};

			// Determine if an ad unit should render.
			var shouldRenderDfpUnit = function(path) {
				if ('undefined' === typeof adLayersScreen) {
					return true;
				}

				if (
					('mobile' === adLayersScreen && path.includes('nr.desktop'))
					|| ('desktop' === adLayersScreen && path.includes('nr.mobile'))
				) {
					return false;
				}

				if ('undefined' === typeof window.pbjs || 'undefined' === typeof window.pbjs.adUnits) {
					var adEnv = adLayersScreen + '_' + adLayersPage;
					if ('undefined' === typeof loggedInUnits[adEnv]) {
						return true;
					}
					var unit = path.split('/')[2];
					if ('undefined' === typeof unit) {
						return true;
					}

					if (-1 === loggedInUnits[adEnv].indexOf(unit)) {
						return false;
					}
				}
				return true;
			};

