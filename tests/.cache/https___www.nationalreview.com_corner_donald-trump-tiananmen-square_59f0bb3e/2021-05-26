	googletag.pubads().addEventListener('slotRenderEnded', function(event) {
		if ('function' === typeof window.nrHandleSlotRender) {
			window.nrHandleSlotRender(event);
		}
	});

	// Add event listener for when the ad has completely rendered to screen.
	// @see https://developers.google.com/doubleclick-gpt/reference#googletageventsimpressionviewableevent
	googletag.pubads().addEventListener('impressionViewable', function(event) {
		var slot = document.getElementById(event.slot.getSlotElementId());;
		if (! slot) {
			return;
		}

		if ('function' === typeof slot.closest) {
			var adUnit = slot.closest('.ad-unit');
			if (adUnit) {
				adUnit.classList.add('ad-unit--ad-slot-rendered');
			}
		}
	});

	googletag.cmd.push(function() {
		/**
		 * Set targeting based on logged in state
		 */
		var loggedIn = 'true' === localStorage.getItem('nrLoggedIn');
		googletag.pubads().setTargeting(
			'isLoggedIn',
			loggedIn.toString()
		);

		/**
		 * Provide Ad-Testing Parameter for URLS
		 */
		var testad = window.location.href.toString().match(/testad=([\w\d]+)/);
		if ( testad !== null ) {
			testad = testad[1].toString().replace(/testad=/, "");
			googletag.pubads().setTargeting('testad',[testad]);
		}

		/**
		 * Load Rubicon
		 */
		function callAdserver(gptSlots = false) {
			if (pbjs.adserverCalled) return;
			pbjs.adserverCalled = true;

			if (false === gptSlots) {
				new AdLayersAPI().refreshAll();
			} else {
				googletag.pubads().refresh(gptSlots);
			}
		}

		// request pbjs bids when it loads
		pbjs.que.push(function() {
			pbjs.rp.requestBids({
				callback: callAdserver,
				gptSlotObjects: Object.values(dfpAdUnits),
			});
		});

		// failsafe in case PBJS doesn't load
		setTimeout(function() {
			console.log('callAdserver timeout');
			callAdserver();
		}, 3500);
	});
	
				if ( typeof AdLayersAPI === 'undefined' || ! AdLayersAPI.isDebug() ) {
					googletag.enableServices();
				}
			});
