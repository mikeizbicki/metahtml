<script>

// Get the modal
const $modal = document.getElementById("myModal");
$modal.style.background = "rgba(0,0,0,0.5)";

const $modalContent = document.querySelector('.modal-content');

// Get the button that opens the modal
const $liveButton = document.querySelector(".button-red.large");

// Get the window options
const $closeStreaming = document.getElementsByClassName("closeStreaming")[0];
const $minimizeStreaming = document.querySelector('.minimizeStreaming');
const $maximizeStreaming = document.querySelector('.maximizeStreaming');

//Get the <input type="range"> element that control the modal's opacity
const $opacityStreaming = document.getElementById('inputOpacityStreaming');
let opacityStreamingValue = 0.5;

function rangeValue(){
  opacityStreamingValue = $opacityStreaming.value;
  $modal.style.background = "rgba(0,0,0,"+opacityStreamingValue+")";
}

$opacityStreaming.addEventListener("input", rangeValue);

function showHiddenWindowOptions() {
  if(isStreamingMin){
    $maximizeStreaming.classList.remove('d-none');
    $minimizeStreaming.classList.add('d-none');
    
  }else{
    $maximizeStreaming.classList.add('d-none');
    $minimizeStreaming.classList.remove('d-none');
  }
}

showHiddenWindowOptions();

function overflowController(){
  if(window.screen.width <= 900 && isActiveStreaming == true){
    document.getElementsByTagName("html")[0].classList.add('overflow-hidden');
    //document.getElementsByTagName("html")[0].style.position = "relative";
    document.getElementsByTagName("body")[0].classList.add('overflow-hidden');
    document.getElementsByTagName("body")[0].classList.add('p-relative');
    
  }else{
    document.getElementsByTagName("html")[0].classList.remove('overflow-hidden');
    //document.getElementsByTagName("html")[0].style.position = "unset";
    document.getElementsByTagName("body")[0].classList.remove('overflow-hidden');
    document.getElementsByTagName("body")[0].classList.remove('p-relative');
    
  }
}

function showHiddenStreaming(state) {
  if(state === 'show'){
    isActiveStreaming = true;
    $modal.style.display = "block";
    mostrarStreaming();
    overflowController();
  }

  if(state === 'hidden'){
    isActiveStreaming = false;
    $modal.style.display = "none";
    document.getElementsByTagName("html")[0].style.overflow = "auto";
    ocultarStreaming();
    overflowController();
  }
}

function isMinimize(modalIsMinimize) {

  const $iframeEnVivo = document.querySelector('.box-streaming #envivoid');
  const $liveRedes = document.querySelector('.liveRedes');
  const $opacityOptions = document.querySelector('.opacityOptions');

  if(modalIsMinimize){
  
    isStreamingMin = true;

    document.getElementsByTagName("html")[0].style.overflow = "auto";
  
    $modal.classList.remove('modal');
    $modal.classList.add('min-modal');
    $modal.style.background = "transparent";
  
    $modalContent.classList.remove('modal-content');
    $modalContent.classList.add('min-modal-content');
  
    $iframeEnVivo.classList.add('min-envivoid');
  
    $liveRedes.classList.add('d-none');
  
    $opacityOptions.classList.add('d-none');
    
    showHiddenWindowOptions();

  }else{
  
    isStreamingMin = false;

    //document.getElementsByTagName("html")[0].style.overflow = "hidden";
  
    $modal.classList.add('modal');
    $modal.classList.remove('min-modal');
    $modal.style.background = "rgba(0,0,0,"+opacityStreamingValue+")";
  
    $modalContent.classList.add('modal-content');
    $modalContent.classList.remove('min-modal-content');
  
    $iframeEnVivo.classList.remove('min-envivoid');
  
    $liveRedes.classList.remove('d-none');
  
    $opacityOptions.classList.remove('d-none');
    
    showHiddenWindowOptions();
  }
}

//MOSTRAR EL MODAL
$liveButton.onclick = function() {
  showHiddenStreaming('show');
}

//CERRAR EL MODAL
$closeStreaming.onclick = function() {
  showHiddenStreaming('hidden');
}

// When the user clicks anywhere outside of the modal, close it
window.onclick = function(event) {
  if (event.target == $modal) {
    showHiddenStreaming('hidden');
  }
}

//MINIMIZAR MODAL
document.querySelector('.minimizeStreaming').addEventListener('click', () => isMinimize(true) );
window.addEventListener('scroll', () => isActiveStreaming ? isMinimize(true) : '' );

//MAXIMIZAR MODAL
document.querySelector('.maximizeStreaming').addEventListener('click', () => isMinimize(false) );
</script>
	
	
	

</header>



    <!-- Container -->
    <div id="container">
        











<script>
    var win = null;
    function NewWindow(mypage,myname,w,h,scroll){
    LeftPosition = (screen.width) ? (screen.width-w)/2 : 0;
    TopPosition = (screen.height) ? (screen.height-h)/2 : 0;
    settings =
    'height='+h+',width='+w+',top='+TopPosition+',left='+LeftPosition+',scrollbars='+scroll+',resizable'
    win = window.open(mypage,myname,settings)
    }
    function abrir2(url) {
    open(url,'','top=70,left=120,width=445,height=500, scrollbars =NO, resizable=NO') ;
    }
    function MM_openBrWindow(theURL,winName,features) {
    window.open(theURL,winName,features);
    }
</script>

<script> 
jQuery(function( $ ) {
        $( function(){$( "#cajaPlayerUltra" ).draggable({ snap: true });});
    $("#svivo").append('<iframe width="100%" height="455" align="middle" frameborder="0" mozallowfullscreen="" webkitallowfullscreen="" scrolling="no" allowfullscreen="" id="envivoid" name="envivo" src="https://mblive.telesur.ultrabase.net"></iframe>');
        $("#tit-svivo").addClass("menuA");
        if ((screen.width >= 320) && (screen.width <= 980)) {
            $("#btn-svivo").css({ background: "#9a1212 !important" });
    }
        $("#btn-svivo").click(function () {
            $("#cajaPlayerUltra").html('<iframe width="100%" height="455" align="middle" frameborder="0" mozallowfullscreen="" webkitallowfullscreen="" scrolling="no" allowfullscreen="" id="envivoid" name="envivo" src="https://mblive.telesur.ultrabase.net"></iframe>');
            $("#cajaPlayerUltraAudio").html("");
            $("#cajaPlayerUltraAudio").css("display", "none");
            $("#cajaPlayerUltra").css("display", "block");
            $("#tit-svivo").addClass("menuA");
            $("#tit-saudio").removeClass("menuA");
        });
        $("#btn-saudio").click(function () {
            $("#cajaPlayerUltraAudio").html("");
            $("#cajaPlayerUltraAudio").append('<iframe width="100%" height="100%" align="middle" frameborder="0" mozallowfullscreen="" webkitallowfullscreen="" allowfullscreen="" id="envivoid" class="envivioadio" name="envivo" src="https://mblive.telesur.ultrabase.net/audioonly.html"></iframe>');
            $("#cajaPlayerUltra").html("");
            $("#cajaPlayerUltra").css("display", "none");
            $("#cajaPlayerUltraAudio").css("display", "block");
            $("#tit-saudio").addClass("menuA");
            $("#tit-svivo").removeClass("menuA");
        });

});
function myFunction() {
 $('#cajaPlayerUltra').animate({'width':'520px','height':'395px','max-height':'395px','margin-left':'5px','left':'0px','top':'0px' },100).removeClass('fixedlive');
}
        var offscroll = false;
      jQuery(function( $ ) {$( "#cajaPlayerUltra" ).draggable({ snap: true });});
        $(window).scroll(function() {    
            var scroll = $(window).scrollTop();
            var offsetplayer =  $('#cajaPlayerUltra').offset().top;
            if (scroll >= offsetplayer) {
                $('#cajaPlayerUltra').animate({'width':'300px','height':'248px !important','position': 'fixed','max-height':'248px','margin-left':'5px','top':'100px','left':'25px'},10).addClass('fixedlive');
               $('#cajaPlayerUltra').append('<a href="#" class="closePlay"></a>'); 
                  $('#envivoid').css({'max-height':'248px !important'});
                offscroll = true;
            }
            if(scroll < 200 && offscroll){
                offscroll = false;
                $('.closePlay').remove();
                myFunction();
            }
        });
</script>
