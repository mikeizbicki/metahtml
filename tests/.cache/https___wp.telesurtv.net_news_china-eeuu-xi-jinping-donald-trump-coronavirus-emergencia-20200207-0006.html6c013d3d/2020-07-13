<script>
let isStreamingMin = false;
let isActiveStreaming = false;

function mostrarStreaming() {
  document.getElementById("cajaPlayerStreaming").style.display="block";
  document.getElementById("cajaPlayerStreaming").innerHTML='<iframe width="100%" height="455" align="middle" frameborder="0" mozallowfullscreen="" webkitallowfullscreen="" scrolling="no" allowfullscreen="" id="envivoid" name="envivo" src="https://mblive.telesur.ultrabase.net"></iframe>';
  if(isStreamingMin){
  	const iframeEnVivo = document.querySelector('.box-streaming #envivoid');
  	iframeEnVivo.classList.add('min-envivoid');
  }
}

function ocultarStreaming(){
  document.getElementById("cajaPlayerStreaming").style.display="none";
  document.getElementById("cajaPlayerStreaming").innerHTML=" ";
}
</script>

