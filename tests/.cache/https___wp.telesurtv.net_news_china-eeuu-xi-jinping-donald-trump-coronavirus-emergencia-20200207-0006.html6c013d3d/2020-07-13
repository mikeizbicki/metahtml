					 				setTimeout(function() {
									$(".contentBolFlot").fadeOut(1500);
								   	},200); 		 				
					 			});
					 		}
					 		else{
					 			$("#msjReg").html("Esta dirección de correo ya se encuentra registrada");
					 			$("#btnCerrar2").click(function () {
					 				$("#RegistroBloque2").css("display", "none");
					 				$("#msjReg").html(" ");
					 				$("#btnCerrar1").css("display", "block"); 
					 			});
					 		}
					 	}
					 	else if (n>-1){
					 		$("#msjReg").html("El correo que ha ingesado está vacío o es incorrecto");
