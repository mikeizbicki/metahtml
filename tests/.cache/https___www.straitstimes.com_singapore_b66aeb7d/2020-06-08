          '<li class="nav-uppassword"><a class="mysph_user_name" href="javascript:;">Update your password</a></li>'+
          '<li class="nav-logout"><a class="mysph_logout" href="javascript:;">Log out</a></li>';
          $('.menu-user').html(st_login_info);
          if(data.ldapw){
            _data.at = data.ldapw;
            var meta = document.createElement('meta');
            meta.name = 'ldapw';
            meta.content = data.ldapw;
            document.getElementsByTagName('head')[0].appendChild(meta);
            cX.callQueue.push(['addExternalId', {'id': data.ldapw, 'type': 'sph'}]);
          }
          cX.callQueue.push(['setCustomParameters', {'sph-visitorcat': _data.visitorcat, 'isSubscriber': 'y', 'sph-propenmo': propenmo}]);
        } else if(data.usertype == "anoy") {
                    var login_header_track = "if(typeof(tag) != 'undefined') return tag.click.send({elem:this, name:'click loginheader', level2:'"+_data.level2+"', type:'action'});";

          var st_login_info = '<li class="nav-login"><a href="javascript:;" onclick="'+login_header_track+'" id="sph_login" class="mysph_login">Log in</a></li>'+
          '<li class="nav-subscribe"><a href="https://www.sphsubscription.com.sg/eshop/?r=products/newsubscriptionpackages&pcode=st&utm_campaign=st_subscription&utm_medium=web&utm_source=st&utm_content=subscribetext-header&utm_term=singapore" target="_blank">Subscribe</a></li>';
          $('.menu-user').html(st_login_info);
          //if($("body.page-home").length==1 || (isArticlePage && _data.contentcat==2)) {
            trySingleSignOnLogin();
          //}
          cX.callQueue.push(['setCustomParameters', {'sph-visitorcat': _data.visitorcat, 'isSubscriber': 'n', 'sph-propenmo': propenmo}]);
        }else{
          _data.visitorcat = 4; // sph annoy
          cX.callQueue.push(['setCustomParameters', {'sph-visitorcat': _data.visitorcat, 'isSubscriber': 'n', 'sph-propenmo': propenmo}]);
        }
        //cxense
        cX.CCE = cX.CCE || {}; cX.CCE.callQueue = cX.CCE.callQueue || [];
        cX.CCE.callQueue.push(['sendPageViewEvent','sph', '8ab0d3648952076495023cb6cb4678395a8640c8']);
        
        // Dispatch the event
        var event = document.createEvent('Event');
        event.initEvent('ldapLoginDetailsReceived', true, true);
        document.dispatchEvent(event);
      });
      
      
      
      // Logout
      /* jQuery('.menu-user').on('click', '.nav-logout a', function (e) {
        e.preventDefault();
        jQuery.ajax({url: "/amlogout.php", success: function(result){
            var separator = (window.location.href.indexOf("?")===-1)?"?":"&";
            window.location.href = "https://acc-auth.sphdigital.com/amserver/UI/Logout?logoutdest="+encodeURIComponent(window.location.href+separator + "loggedout=true");
        }});
      }); */
      
    })(jQuery);

    function htmlEncode(value){
      //create a in-memory div, set it's inner text(which jQuery automatically encodes)
      //then grab the encoded contents back out.  The div never exists on the page.
      return jQuery('<div/>').text(value).html();
    }

    function trySingleSignOnLogin() {
      var f = document.createElement('iframe');
      f.src = "/p/login.php"; 
      f.style.width = "0px"; 
      f.style.height = "0px";
      f.style.border = "none";
      jQuery('body').append(f);
    }

    function loadSubscriberContent() {
      jQuery.post( "/p/login_details?article_id="+_data.articleid, function( data ) {
        data = JSON.parse(data);
        // Login & Logout :)
         _data.visitorcat = data.service_type_value;
         _data.ac =  data.service_type_value;
        if ( (data.usertype == 'sub' || data.usertype == 'reg') && data.login_id != "") {
          var st_login_info = '<li class="nav-user"><a href="javascript:;" class="dropdown-toggle" name="login-user-name" data-toggle="dropdown"><span class="user-name-hi">Hi </span><span id="sph_user_name">'+htmlEncode(data.login_id)+'</span><span class="caret"></span></a><ul class="dropdown-menu"><li><a class="mysph_user_name" href="javascript:;">Update your password</a></li><li><a class="mysph_logout" href="javascript:;">Log out</a></li></ul></li>'+
          '<li class="nav-uppassword"><a class="mysph_user_name" href="javascript:;">Update your password</a></li>'+
          '<li class="nav-logout"><a class="mysph_logout" href="javascript:;">Log out</a></li>';
          jQuery('.menu-user').html(st_login_info);
          if(data.ldapw){
            _data.at = data.ldapw;
            var meta = document.createElement('meta');
            meta.name = 'ldapw';
            meta.content = data.ldapw;
            document.getElementsByTagName('head')[0].appendChild(meta);
          }
        } else if(data.usertype == "anoy") {
          var login_header_track = "if(typeof(tag) != 'undefined') return tag.click.send({elem:this, name:'click loginheader', level2:'"+_data.level2+"', type:'action'});";
          var st_login_info = '<li class="nav-login"><a class="mysph_login" href="javascript:;" onclick="'+login_header_track+'">Log in</a></li>'+
          '<li class="nav-subscribe"><a href="https://www.sphsubscription.com.sg/eshop/?r=products/newsubscriptionpackages&pcode=ST&utm_campaign=st_subcription&utm_medium=referral&utm_source=web-st&utm_content=subscribetext-homepageheader" target="_blank">Subscribe</a></li>';
          jQuery('.menu-user').html(st_login_info);
        }else{
          _data.visitorcat = 4; // sph annoy
        }
        //
        
        if(isArticlePage && jQuery(".field-name-body .field-items .field-item .premium-read-more").length>=1 && data.full_content!= "") {
          //Remove all but imu ads container
          jQuery(".field-name-body .field-items .field-item").children().not('.ads.clearleft').remove();
          jQuery(".field-name-body .field-items .field-item").prepend(data.full_content);
        } else if(isArticlePage) {
          reloadPaywall();
        }
      });
    }

    function reloadPaywall() {
        var cxpageContext = { "categories": {} };
        cxpageContext["categories"]["sph-visitorcat"] = _data.visitorcat.toString(10);
      cX.CCE.callQueue.push(['run',{
          widgetId: '0290372c69e05c377ddf359350fc0bccc54daa08',
          targetElementId: 'cx_0290372c69e05c377ddf359350fc0bccc54daa08'
      },{ context: cxpageContext }]);
    }

    //to get full content of verified registered user
    function getRegisteredUserContent() {
      jQuery.post( "/p/login_details?reg_user=reg_st&article_id="+_data.articleid, function( data ) {
        data = JSON.parse(data);
        if(isArticlePage && jQuery(".field-name-body .field-items .field-item .premium-read-more").length>=1 && data.full_content!= "") {
          //Remove all but imu ads container
          jQuery(".field-name-body .field-items .field-item").children().not('.ads.clearleft').remove();
          jQuery(".field-name-body .field-items .field-item").prepend(data.full_content);
        }
      });
    }

  </script>


    
