                top: 3px;
                right: 3px;
                position: absolute;
                width: 20px;
                height: 20px;
                display: block;
                cursor: pointer;
                background: #000 url(https://38golu1f1ik01c9tej2um08s-wpengine.netdna-ssl.com/wp-content/themes/njglobe/assets/prestitial/close.png);
                background-size: contain;
                border-radius: 20px;
            }
        </style>
        <div class="img_prestitial" id="mobile_ad_widget-2">
            <!-- /21683694079/NJG-ATF/NJG-BottomOfPage -->
<div id='div-gpt-ad-1548967101543-0'>
<script>
googletag.cmd.push(function() { googletag.display('div-gpt-ad-1548967101543-0'); });
</script>
</div>            <div class="closePrestitial"></div>
        </div>
        <script>
            jQuery(function ($) {
                var maxViews = 0;
                var storageKey = "mobile_ad_widget-2";

                function getDay() {
                    var now = new Date();
                    return Math.floor(now/8.64e7);
                }

                function getViews() {
                    localStorage.removeItem(storageKey+(getDay()-1));
                    var views = parseInt(localStorage.getItem(storageKey+getDay()) || 0);
                    return views ? views : 0;
                }

                function setViews(views) {
                    localStorage.setItem(storageKey+getDay(), views);
                }

                var ad = $('#mobile_ad_widget-2');
                var image = ad.find('img');
                var close = ad.find('.closePrestitial');
                var userAgent = navigator.userAgent || navigator.vendor || window.opera;

                if((maxViews == 0 || localStorage && getViews() < maxViews) && window.innerWidth < 1024) {
                    setViews(getViews()+1);
                    ad.show();
                } else {
//                    ad.hide();
                }
                close.click(function () {
                    ad.hide();
                });
            });
        </script>
        			</div><!-- .header-ads-wrapper -->
		</div>
	</div><!-- .logo-ads-wrapper -->

