		custom_targeting = false;
		if (key.length > 0 && values.length > 0) {
			custom_targeting = true;
		}
		
		var targetingArray             = new Array();
		targetingArray['tn_author']    = ['ahmad-j'];
		targetingArray['tn_articleid'] = [337068];
		targetingArray['tn_ptype']     = ['article'];
		targetingArray['tn_keyword']   = ['donald-tru', 'human-righ', 'iran', 'nuclear-ir', 'sanctions'];
		targetingArray['tn_subject']   = ['armed-conf', 'civil-unre', 'foreign-po', 'health-and', 'middle-eas', 'regions-an', 'war-and-pe', 'world'];
		targetingArray['tn_slp']       = []; 
		
					call_ad_new('leaderboard','tn_article','ad-leaderboard','leaderboard_1',targetingArray);		
				
		
