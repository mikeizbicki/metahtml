				if( isMobile.any() ){
					call_ad_new('halfpage','tn_article','ad-halfpage-337068-0','rectangle_1',{"tn_author":"'ahmad-j'","tn_articleid":337068,"tn_ptype":"article","tn_keyword":"'donald-tru', 'human-righ', 'iran', 'nuclear-ir', 'sanctions'","tn_subject":"'armed-conf', 'civil-unre', 'foreign-po', 'health-and', 'middle-eas', 'regions-an', 'war-and-pe', 'world'","tn_slp":""});	
