 we found.","permalink":"https://www.technologyreview.com/2021/07/07/1027916/we-tested-ai-interview-tools/","authors":[{"name":"Sheridan Wall","url":"https://www.technologyreview.com/author/sheridan-wall/","avatar":""},{"name":"Hilke Schellmann","url":"https://www.technologyreview.com/author/hilke-schellmann/","avatar":"https://wp.technologyreview.com/wp-content/uploads/2021/02/hilke-schellmann.jpg?w=32"}]},"children":[],"componentGroups":{"default":[]}}],"componentGroups":{"default":[]}},{"name":"events-promo-module","config":{"heading":"Events","moreText":"More events","moreLink":"https://events.technologyreview.com/"},"children":[{"name":"ad-unit","config":{"adUnit":"__site/__events-right-rail","shouldLoad":true,"sizes":[[300,250]],"sizeMapping":[]},"children":[],"componentGroups":{"default":[]}}],"componentGroups":{"default":[]}}],"componentGroups":{"default":[]}},{"name":"sidebar","config":{"gtmTargetingClass":"","isSticky":true,"pinBottomItem":false},"children":[{"name":"ad-unit","config":{"adUnit":"__site/__square-only","shouldLoad":true,"sizes":[[1,1],[300,250]],"sizeMapping":[],"index":123},"children":[],"componentGroups":{"default":[]}},{"name":"topics-module","config":{"topics":[{"id":9,"name":"Artificial intelligence","url":"https://www.technologyreview.com/topic/artificial-intelligence/"},{"id":32,"name":"Biotechnology","url":"https://www.technologyreview.com/topic/biotechnology/"},{"id":27,"name":"Blockchain","url":"https://www.technologyreview.com/topic/blockchain/"},{"id":21,"name":"Climate change","url":"https://www.technologyreview.com/topic/climate-change/"},{"id":12,"name":"Computing","url":"https://www.technologyreview.com/topic/computing/"},{"id":16935,"name":"Election 2020","url":"https://www.technologyreview.com/topic/election-2020/"},{"id":16,"name":"Humans and technology","url":"https://www.technologyreview.com/topic/humans-and-technology/"},{"id":18053,"name":"Pandemic technology project","url":"https://www.technologyreview.com/topic/covid