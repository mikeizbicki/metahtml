		$(document).ready(function() {
			$("#menuButton").click(function(){
				$('.ubermenu-item-has-children.ubermenu-has-submenu-mega').before('<i class="menu-arrow fas fa-angle-down"></i>');
				$('#myMain-menu').addClass('myMain-menu-overlay');
				document.getElementById("myMain-menu").style.width = "100%";
				$('ul#menu-main-menu').css({
					display: 'block',
					padding: '20px 20px 0'
				});
				$('a.closebtn').css({
					display: 'block !important'
				});
				$('body').addClass('mob-full-menu');
				$('.main-menu-content').removeClass('main-menu-content');
		                $('.nav-mb-menu-nodisplay').removeClass('nav-mb-menu-nodisplay');
			});

			$('#main-menu-mb-content li>ul').on('click', '.menu-arrow', function(){
				$(this).children('sub-menu').slideToggle(1000);
				$(this).toggleClass('active');
				$('.sub-menu').toggleClass('active');
			});
		});

		function closeNav() {
			jQuery(document).ready(function($){
				$('.menu-arrow').remove();
				$('.mob-full-menu').removeClass('mob-full-menu');
				$('.myMain-menu-overlay').removeClass('myMain-menu-overlay');
				document.getElementById("myMain-menu").style.width = "0%";
				$('ul#menu-main-menu').css({
					display: 'none',  
					padding: '0'
				});
				$('#myMain-menu').addClass('nav-mb-menu-nodisplay');
			});
		}
		</script>
