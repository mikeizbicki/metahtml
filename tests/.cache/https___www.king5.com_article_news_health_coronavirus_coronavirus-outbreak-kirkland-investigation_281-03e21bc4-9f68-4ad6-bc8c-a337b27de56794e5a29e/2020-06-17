         data-subtopic=""
         data-related-playlist-id="AvSw2Xp3"
         data-related-media-id="TUM9WVHp"
         data-use-trending="true">
        <div class="video__ratio-enforcer">
            <div class="video__ratio-enforced">
                <div class="video__inner">
                    <div class="video__teaser video__teaser_visible_true">
                        <img class="video__thumbnail" data-asset-fallback="default" src="https://media.king5.com/assets/KING/images/bb073bea-1158-4318-8fc3-200709693dd5/bb073bea-1158-4318-8fc3-200709693dd5_1920x1080.jpg" />
                        <div class="video__error"></div>
                        <div class="video__button video__button_play"></div>
                    </div>
                    <div class="video__player-container">
                        <div class="video__player"></div>
                    </div>
                </div>
            </div>
        </div>
