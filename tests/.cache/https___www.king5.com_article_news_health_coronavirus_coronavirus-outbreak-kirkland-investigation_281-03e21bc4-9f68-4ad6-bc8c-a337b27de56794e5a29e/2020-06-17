         data-use-trending="true">
        <div class="video__ratio-enforcer">
            <div class="video__ratio-enforced">
                <div class="video__inner">
                    <div class="video__teaser video__teaser_visible_true">
                        <img class="video__thumbnail" data-asset-fallback="default" src="https://media.king5.com/assets/KING/images/b1675ea7-beb8-4220-8f58-378a57848087/b1675ea7-beb8-4220-8f58-378a57848087_1920x1080.jpg" />
