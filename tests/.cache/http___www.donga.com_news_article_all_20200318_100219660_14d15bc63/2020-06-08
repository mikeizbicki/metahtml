            var preBestClickIdx = 0 ;
            $('.trend_cont .tab_menu li').mouseover ( function() {
                $('.trend_cont .tab_menu li').eq(preBestClickIdx).removeClass('on');
                $('.trend_box').eq(preBestClickIdx).hide() ;
                var idx = $('.trend_cont .tab_menu li').index(this);
                $('.trend_cont .tab_menu li').eq(idx).addClass ('on') ;
                $('.trend_box').eq(idx).show() ;
                preBestClickIdx =idx  ;
                return false;
            }) ;
            $('.trend_cont .tab_menu li').eq(0).mouseover() ;
        </script>
