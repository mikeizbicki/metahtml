<script>
$(function(){
	xinhuaVide();
})
function xinhuaVide(){
	var XH_video= $.trim($("#spUrl").html());
	
	if(XH_video.indexOf("ttp:")>0){
		if($(".domPC").is(":hidden")){
			$("#mobileVideo iframe").attr("src",XH_video).show();
		}else{
			$("#pcVideo iframe").attr("src",XH_video).show();
		}
	}
}

</script>
