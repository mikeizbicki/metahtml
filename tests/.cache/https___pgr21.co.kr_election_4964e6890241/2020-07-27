<script>
	if (window.localStorage && localStorage.menu_visibility) $(localStorage.menu_visibility).show();
	function toggle_menu(id) {
		var obj = $(id);
		$(id).toggle();
		if (window.localStorage) localStorage.menu_visibility = obj.is(':visible') ? id : '';
	}
</script>

