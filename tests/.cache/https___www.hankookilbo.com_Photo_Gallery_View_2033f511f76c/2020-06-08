<script>
    var maxPage = "1";
    var minPage = "1";

    $(document).ready(function () {
        var strHash = location.hash;
        strHash = strHash.replace("#","").split("_");
        if (strHash == undefined) {
            strHash = "";
        }
        goSubmit(strHash[0]);
    });

    
    function goSubmit(Nid) {
        

        $.ajax({
            method: "POST",
            url: "/photo/photo/GalleryListAjax",
            data: { PgKey: $("#PgKey").val(), PType: $("#PType").val(), Page: $("#Page").val(), Nid: Nid },
            success: function (data) {
                $("#PhotoDetailListAjax").html(data);
                itemPhoto(Nid);

                maxPage = $("#Page").val();
                minPage = $("#Page").val();
            }
        });
        
        jAjax("PhotoFrmAjax", "/photo/photo/GalleryListBottomAjax?CatSeq=21");
    }

    function itemPhoto(Nid) {
        $.ajax({
            method: "POST",
            url: "/photo/photo/GalleryListAjaxImg",
            data: { PgKey: $("#PgKey").val(), PType: $("#PType").val(), Page: $("#Page").val(), Nid: Nid },
            success: function (data) {
                $("#PhotoDetailListAjax2").html(data);
            }
        });
    }

    
    function appendPhotos() {
        var page = $("#Page").val();
        page = parseInt(page) + 1;

        if (page > parseInt(maxPage)) {
            $.ajax({
                method: "POST",
                url: "/photo/photo/GalleryListAjax",
                data: { PgKey: $("#PgKey").val(), PType: $("#PType").val(), Page: page },
                success: function (data) {
                    if ($.trim(data) != "") {
                        HK.util.addGalleryPhoto('append', data);
                        $("#Page").val(page);
                        maxPage = page;
                    }
                }
            });
        }
    }

    
    function prependPhotos() {
        var page = parseInt(minPage) - 1;

        if (page >= 1) {
            $.ajax({
                method: "POST",
                url: "/photo/photo/GalleryListAjax",
                data: { PgKey: $("#PgKey").val(), PType: $("#PType").val(), Page: page },
                success: function (data) {
                    if ($.trim(data) != "") {
                        HK.util.addGalleryPhoto('prepend', data);
                        minPage = page;
                    }
                }
            });
        }
    }

     
</script>

    
