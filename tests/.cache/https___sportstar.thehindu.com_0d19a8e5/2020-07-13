$(document).ready(function(){
if(DevIdf.isDesktop){
var crict = $('.scoreCardlive').find('.cricket').length;
if( crict == 1){
loadSyncURL("https://ss.thgim.com/static/js/scCricket.min.js");
}
var ftb = $('.scoreCardlive').find('.football').length;
if( ftb == 1){
loadSyncURL("https://ss.thgim.com/static/js/scFootball.min.js");
}
var tenis = $('.scoreCardlive').find('.tennis').length;
if( tenis == 1){
loadSyncURL("https://ss.thgim.com/static/js/scTennis.min.js");
}
}
else{
var scact = $('.scoreCardlive.active').length;
var target = $('.scoreCardlive.active').attr('id');
if(scact==1){
loadSyncURL("https://ss.thgim.com/static/js/"+target+".min.js");
}
$('button.btn.btn-primary.sc').click(function() {
lazyScore($(this));
$(this).siblings().removeClass('active')
$(this).addClass('active');
});
}
});
function lazyScore(selector) {
var url= $(selector).attr('data-url');
var target = $(selector).attr('rel');
$("#"+target).show().siblings("div").hide();
loadSyncURL(url);
}
</script>
