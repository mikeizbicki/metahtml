// Swiper: Slider
var mySwiper = new Swiper('.latestNewsSlider.swiper-container', {
loop: true,
autoplay: {
delay: 3000,
},
paginationClickable: true,
slidesPerView: 4,
breakpoints: {
480: {
slidesPerView: 1,
spaceBetween: 2
},
767: {
slidesPerView: 2,
spaceBetween: 2
},
1199: {
slidesPerView:3,
spaceBetween: 2
}, 1920: {
slidesPerView: 4,
spaceBetween: 2
}
}
});
$('.latestNewsSlider.swiper-container').on('mouseenter', function(e){
mySwiper.autoplay.stop();
})
$('.latestNewsSlider.swiper-container').on('mouseleave', function(e){
mySwiper.autoplay.start();
})
});
</script>
