<script type="872dcffd2fb7b44bc9168401-text/javascript">
$(document).ready(function() {
/*function resizelnSwiper() {
$('.latestNewsSlider.swiper-container').height($('.latestNewsSlider .swiper-slide img').height())
}
onImagesReady: function (swiper) {
resizelnSwiper();
swiper.onResize();
}
