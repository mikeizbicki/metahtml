function daysleft(enddate){
var day_start = new Date();
var day_end = new Date(enddate);
day_end.setHours(23,59,59,999);
var enddatefinal = new Date(day_end);
var total_days = (enddatefinal - day_start) / (1000 * 60 * 60 * 24);
total_days = total_days - 1;
if(Math.ceil(total_days) < 1){
total_days = "0";
}
return Math.ceil(total_days);
}
function logout(){
if(typeof sendTaiData !== "undefined") {
taiDataObject['etime'] = "" + new Date().getTime();
taiDataObject['userinfo'] = isUserDetails;
taiData.push(taiDataObject);
sendTaiData(taiData, 'signout');
}
if(typeof clevertap != "undefined" ){
clevertap.event.push("Sign out",{
"Platform":DevIdf.isMobile == true ? 'MWEB' : 'WEB',
"Date": new Date()
});
}
fetchApi("https://tai.thehindu.com/v1/auth/logout/HINDU",{
method: 'POST',
body: JSON.stringify({"userId":userId,"deviceId":deviceId(), "siteId":"SSL-PH6EG8"}),
mode: 'cors', // no-cors, cors, *same-origin
credentials: 'include' // include, *same-origin, omit
},function(res){
if(res.status == "success"){
deleteCookie("thuser");
window.location.reload();
}
else if (typeof res.reason != "undefined" && res.reason == 1){
deleteCookie("thuser");
window.location.reload();
}
}); }
function isloggedinUser() {
var userFullName = JSON.parse(isUserDetails.userInfo);
var userName = userFullName.FullName;
if (userName != "") {
userName = userName;
} else if (isUserDetails.userName != ""
&& isUserDetails.userName != undefined) {
userName = isUserDetails.userName;
} else {
userName = "User";
}
document.getElementById("sso-signin").innerHTML = "<i class='fa fa-user'></i>"+userName;
/* $("#sso-signin").html("<i class='fa fa-user'></i>"+userName); */
/* $("#sso-signin").append(userName); */
/* var codeBlock = "&#xf007" ;
document.getElementById("sso-signin").innerText = codeBlock; */
/* document.getElementById("sso-signin").innerText += userName; */
var dropdown = '<ul class="dropdown-menu sso-signinuser" >'
+ '<li class="mobileusername"><a class="" ><img src="https://ss.thgim.com/static/theme/default/base/img/user_navigation.png" class="mobusericon"><span class="mobileusernamespan"> Hello <br><b>'
+ userFullName.FullName
+ '</b></span><span class="closeicon"> &#10005;</span></a>'
+ '<li class="Subscribebtnremove" data-category="Hi user" data-label="SUBSCRIBE NOW"><a class="Subscribebtn" href="https://sportstarsubscription.thehindu.com/subscription?utm_source=thusermenu&utm_medium=subscribebutton&utm_campaign=subscription">Subscribe Now</a>'
+ '<li data-category="Hi user" data-label="My profile"><a href="https://sportstarsubscription.thehindu.com/profile" target="_blank" class="sso-profile">My Profile</a>'
+ '<li data-category="Hi user" data-label="My Plan"><a href="https://sportstarsubscription.thehindu.com/profile?info=subscriptioninfo" target="_blank" class="sso-profile">My Plan</a>'
+'<li data-category="Hi user" data-label="Transaction History"><a href="https://sportstarsubscription.thehindu.com/profile?info=transactioninfo" target="_blank" class="sso-profile">Transaction History</a>'
+'<li data-category="Hi user" data-label="Feedback"><a href="https://sportstarsubscription.thehindu.com/profile?info=feedback" target="_blank" class="sso-profile">Feedback</a>'
+ '<li data-category="Hi user" data-label="FAQ"><a href="https://sportstarsubscription.thehindu.com/faq" target="_blank" class="sso-profile">FAQS</a>'
+ '<li data-category="Hi user" data-label="Contact us"><a href="https://sportstar.thehindu.com/contacts/" target="_blank" class="sso-profile">Contact Us</a>'
+ '<li data-category="Hi user" data-label="Sign Out"><a class="sso-logout" onclick="logout()" >Sign Out</a>'
+ '</ul>';
document.getElementById("sso-signin").removeAttribute("href");
document.getElementById("sso-signin").classList.remove("sso-signin");
document.getElementById("mobilesigninlink").removeAttribute("href");
document.getElementById("mobilesignin").classList.remove("sso-signin");
document.getElementById("mobilesignin").classList
.add("sso-signinmobile");
var els = document.querySelectorAll("#main-menu>ul>li:nth-last-child(-n+2)");
[].forEach.call(els, function(els) {	els.style.display = "none";
});
document.getElementsByClassName("subsEntrymenu")[0].style.display = "block";
document.getElementsByClassName("submobileentry")[0].style.display = "block";
document.getElementsByClassName("mobileadsubentry")[0].style.display = "none";
var list = document.querySelectorAll(".signinbtn");
for (var i = 0; i < list.length; ++i) {
list[i].classList.add('ssologin');
list[i].insertAdjacentHTML('beforeend', dropdown);
}
var listmobile = document.querySelectorAll(".sigininmobile");
for (var i = 0; i < listmobile.length; ++i) {
listmobile[i].insertAdjacentHTML('beforeend', dropdown);
}
var NextRenewalDate = isUserDetails.nextRenewalDate;
if(typeof isUserDetails.nextRenewalDate == "undefined"){
NextRenewalDate = 0;
}
if (isUserDetails.userPlanList.length > 0) {
for (var j = 0; j < isUserDetails.userPlanList.length; ++j) {
if (isUserDetails.userPlanList[j].status == "success" && isUserDetails.userPlanList[j].amount != "0" && daysleft(NextRenewalDate) > 10 ) {
var btnremove = document.querySelectorAll(".Subscribebtnremove");
SubscribedUserPlan.purchased = true;
for (var m = 0; m < btnremove.length; ++m) {
btnremove[m].style.display = "none";
}
document.getElementById("sso-signin").parentElement.classList
.add("signedinuserbtnwidth");
/* $(".ssologin").addClass("signedinuserbtnwidth"); $('.dropdown-menu.sso-signinuser').addClass('dropdwnHide');*/
/* document.getElementsByClassName("ssologin").classList.add("signedinuserbtnwidth"); */
/* var element = document.getElementsByClassName("ssologin"); */
/* element.classList.add("signedinuserbtnwidth"); */
/* addClass(element, 'signedinuserbtnwidth'); */
}
else if(isUserDetails.userPlanList[j].status == "success" && isUserDetails.userPlanList[j].amount == "0" && daysleft(NextRenewalDate) > 0){
var btnremove = document.querySelectorAll(".Subscribebtnremove");
SubscribedUserPlan.purchased = true;
for (var m = 0; m < btnremove.length; ++m) {
btnremove[m].style.display = "none";
}
}
else if (isUserDetails.userPlanList[j].status == "success" && isUserDetails.userPlanList[j].amount != "0") {
SubscribedUserPlan.purchased = true;
}
else if (isUserDetails.userPlanList[j].status == "success" && isUserDetails.userPlanList[j].amount == "0") {
SubscribedUserPlan.freetrail = true;
}
}
}
}
