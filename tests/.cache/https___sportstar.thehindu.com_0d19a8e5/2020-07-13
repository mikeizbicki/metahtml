<script type="872dcffd2fb7b44bc9168401-text/javascript">
$(document).ready(function() {
function resizeShowSwiper() {
$('.showcaseContainer .swiper-container').height($('.showcaseContainer .swiper-slide img').height())
}
// Swiper: Slider
var mySwiper = new Swiper('.showcaseContainer .swiper-container', {
loop: false,
navigation: {
nextEl: '.showCase-next.swiper-button-next',
prevEl: '.showCase-prev.swiper-button-prev',
},
slidesPerView: 5,
// spaceBetween: 4%,
paginationClickable: true,
breakpoints: {
1920: {
slidesPerView: 5,
// spaceBetween: 40,
noSwiping: false,
allowSlidePrev: false,
allowSlideNext: false
},
1028: {
slidesPerView:4,
//spaceBetween: 20,
noSwiping: false,
allowSlidePrev: false,
allowSlideNext: false
}, 960: {
slidesPerView:3,
//spaceBetween: 20,
noSwiping: true,
allowSlidePrev: true,
allowSlideNext: true
}, 767: {
slidesPerView: 3,
// spaceBetween: 20,
noSwiping: true,
allowSlidePrev: true,
allowSlideNext: true
},
480: {
slidesPerView: 2,
// spaceBetween: 0,
noSwiping: true,
allowSlidePrev: true,
allowSlideNext: true
}
}
});
$(window).resize(function(){
resizeShowSwiper()
});
if($(".slider .slide").length == 1) {
$('.showcaseContainer .swiper-wrapper').addClass( "disabled" );
$('.showcaseContainer .swiper-pagination').addClass( "disabled" );
}
});
</script>
