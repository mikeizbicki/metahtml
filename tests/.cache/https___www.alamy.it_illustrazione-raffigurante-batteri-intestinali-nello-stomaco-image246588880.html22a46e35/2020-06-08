      <script type="text/javascript">
          $(window).load(function () {
              // Measure a view of product details. This shows the detail view occurs on pageload,
              // and also tracks a standard pageview of the details page.
              var licenceName;
              var imageType = (imageHeader.imageType == 1) ? 'flipped' : 'original';
              if (typeof selectedLicenseName === 'undefined' || selectedLicenseName == null || selectedLicenseName == undefined) {
                  licenceName = "";
              }
              else {
                  licenceName = selectedLicenseName;
              }
              if (imageHeader.hasFlippedImage == true) {
                  imageType = 'foriginal';
              }
              var imgref = (imageHeader.imageType == 1) ? imageHeader.flipImgRef : imageHeader.imageRef;
              if (licenceName != "") {
                  if (imageHeader.getpicturetype() == "Images") {
                      dataLayer.push({
                          'event': 'detailview',
                          'ecommerce': {
