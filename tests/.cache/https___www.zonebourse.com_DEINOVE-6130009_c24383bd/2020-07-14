		MAJ_alertes = function()
		{
		  this.OnSuccess = function()
		  {
			 var texte = this.GetResponseText();
			 if(texte.split("|"))
				var aElems = texte.split("|");
			 if(aElems[1] == "0")
			 {
				   alert("Erreur : "+texte);
				   return;
			 }
			 var id_client = aElems[2];
			 var sOnOff = aElems[3];
			 var sTextOnOff = aElems[4];
			 var type_alerte = aElems[5];
			 
			 var sImgAlertes = window.document.getElementById("alertes_"+type_alerte+"_"+id_client);  
			 sImgAlertes.title = sOnOff;
			 sImgAlertes.innerHTML = sTextOnOff;
		  }

		  this.Exec = function(id_client, type_alerte)
		  {
			  var sOnOff = window.document.getElementById("alertes_"+type_alerte+"_"+id_client).title;
			  if(sOnOff == "on")
				 sOnOff = "off";
			  else
				 sOnOff = "on";
			  var uri = "/mods_a/membre/AJAX_liste.php?iEntree="+id_client+"&sTypeAlerte="+type_alerte+"&sAction=alertes_"+sOnOff;
			  this.InitializeRequest('POST', uri);
			  this.Commit(null);
		  }
		} 
		MAJ_alertes.prototype = new ajax(); 
		myMAJ_alertes = new MAJ_alertes();

		myMAJ_add_inpage = new MAJ_add();
		 
		myMAJ_add_inpage.dt = true;
		 
		</script>
