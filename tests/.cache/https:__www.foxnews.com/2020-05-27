                        adPrefetchMarginMobSectionBucket = parseInt( strikeConfigDynamicProps.adPrefetchMarginMobSectionBucket, 10) || 250,
                        adRefreshRateTabSectionBucket = parseInt( strikeConfigDynamicProps.adRefreshRateTabSectionBucket, 10) || 30,
                        adPrefetchMarginTabSectionBucket = parseInt( strikeConfigDynamicProps.adPrefetchMarginTabSectionBucket, 10) || 250;

