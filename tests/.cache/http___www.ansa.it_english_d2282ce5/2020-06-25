<script type="text/javascript">
	var userAccept = $.cookie('cb-enabled');
	//var cookieAccepted = (userAccept != null && userAccept.indexOf('accepted')!=-1);
	var cookieAccepted = (userAccept != null);
	//var enable_delivery = (!cookieAccepted);
	var enable_delivery = (!isConsentGiven());
	//console.log(cookieAccepted);
	var forceNoADV = false;

	function checkADV() {
		// prima foto
		var noADV = false;
		var loc = document.location.href;
		var caption = "";
		//if (!cookieAccepted || !isConsentGiven())
		if (!isConsentGiven())
			return true;
		if (forceNoADV) return true;
		if ($('.news-caption em').length) {
			caption = $('.news-caption em').html();
			if ((caption.indexOf('ANSA/AP') != -1))
				noADV = true;
		} else if ($('#owl-news-photostory').length) {
			caption = $('#owl-news-photostory').html();
			if (caption.indexOf('ANSA/AP') != -1)
				noADV = true;
		} else if (loc.indexOf('videogallery\/mondo\/201') != -1 || loc.indexOf('videogallery\/spettacolo\/201') != -1  || loc.indexOf('notizie\/postit') != -1) {
			noADV = true;
		}
		//console.log("return="+noADV);
		return noADV;
		//return false;
	}
</script>


