    var _sf_async_config = {};

    _sf_async_config.uid = "50924";
    _sf_async_config.domain = "www.bbc.co.uk";
    _sf_async_config.sections = "News, News - birmingham-and-black-country, News - MAP, News - birmingham-and-black-country - MAP, News - news-category";
    <!-- if page is an index, add the edition to the path -->
