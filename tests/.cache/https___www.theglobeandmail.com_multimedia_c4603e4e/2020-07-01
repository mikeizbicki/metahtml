    window._dw = window._dw || [];
    
      function dwSegment_promotionscem1504() {
        window._dw.push(['global', 'before', function onBefore(api) {
          api.segment('subs', function () {
  const isSubscriber = getSegment('identity.role.id', 2);
  const darwinState = getDarwinState() || {};

  const isNotPb = window.location.href.indexOf('/ellipsis/') === -1 && window.location.href.indexOf('/pb/') === -1;
  const isArticle = window.location.href.indexOf('/article') > -1;
  const isNotInteractive = isArticle && !getSegment('article.pagesubtype', 'interactive') && !getSegment('article.pagesubtype', 'longread');

  const newsletters = window.tgam.datalayer && window.tgam.datalayer.identity && window.tgam.datalayer.identity.newsletter || '';
  const subSection = window.tgam.datalayer && window.tgam.datalayer.page && window.tgam.datalayer.page.hierarchy;
  const showCanadaNewsletters = getSegment("page.channel", 'canada') && !(newsletters.indexOf('356') > -1 && newsletters.indexOf('113') > -1 && newsletters.indexOf('354') > -1);
  const showOpinionNewsletters = getSegment("page.channel", 'opinion') && !(newsletters.indexOf('352') > -1 && newsletters.indexOf('335') > -1 && newsletters.indexOf('87') > -1);
  const showFinanceNewsletters = subSection.indexOf('investing:personalfinance') > -1 && !(newsletters.indexOf('91') > -1 && newsletters.indexOf('184') > -1 && newslette.indexOf('87') > -1);
  const showLifeNewsletters = getSegment("page.channel", 'life') && !(newsletters.indexOf('335') > -1 && newsletters.indexOf('358') > -1 && newslette.indexOf('108') > -1);
  const isNewsletterpg = getSegment('page.hierarchy','newsletters');
  var count = count || 1;
  var isQA = window.location.href.indexOf("CEMQA1504") > -1;

  if (darwinState.hasOwnProperty("cem1504") && count == 1 && darwinState.cem1504.variant) {
    count += 1;
    setDarwinVariant('promotions-cem1504', darwinState.cem1504.variant);
  }

  var section = showCanadaNewsletters ? 'canada' : showOpinionNewsletters ? 'opinion' : showFinanceNewsletters ? 'finance' : showLifeNewsletters ? 'life' : '';
  setDarwinState('cem1504', 'section', section);

  return notIE && (isSubscriber || isQA) && isNotPb && ((isNotInteractive && (showCanadaNewsletters || showOpinionNewsletters || showFinanceNewsletters || showLifeNewsletters)) || isNewsletterpg);

});
        }]);
      }
      window.tgam.darwin.tests.segments.push(dwSegment_promotionscem1504);
    
    
      function dwExecute_promotionscem1504(data) {
        window._dw.push(['global', 'ready', function onReady(api) {
          var variant = api.get("promotions-cem1504");
          var variantData = data;
          const isNewsletterpg = getSegment('page.hierarchy', 'newsletters');
if (variant.is('control')) {
  console.info('promotions:cem-1504:control');
  setUnitState("cem1504", "control");
}

if (variant.is('experience')) {
  console.info('promotions:cem-1504:experience');
  setUnitState("cem1504", "experience");
  if (!isNewsletterpg) {
    observeCaptcha();
  }
}
        }]);
      }
      
      window.tgam.darwin.tests.after.push(dwExecute_promotionscem1504);
    
  })();
