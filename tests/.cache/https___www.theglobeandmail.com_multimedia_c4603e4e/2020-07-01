  /**
   * convert propensity from number to range, set local storage
   * @param {Number} input
   */
  function convertPropensityAndSetLocalStorage(input) {
    var propensityType = 'no';
    switch (true) {
      case (input >= 1 && input <= 30):
        propensityType = 'low';
        break;
      case (input > 30 && input <= 70):
        propensityType = 'med';
        break;
      case (input > 70 && input <= 100):
        propensityType = 'high';
        break;
      default:
    }
    setDarwinState('cem1493', 'propensity', propensityType);
    return propensityType;
  }

  /**
   * fetch endpoint to get propensity (experience group, pencil or paywall firstvisit only)
   * @param {string} type 'pencil' 'paywall'
   */
  function getPropensity(type, variant) {
    var sophiId = getCookie('sp_tgamid.a05e');
    if (sophiId && sophiId.split('.')[0]) {
      // sophi id for testing: 68af448a-685b-4253-88a0-ea31f7d43c5e
      var endpoint = "https://tgamuserprofile.theglobeandmail.ca/main/userprofilescore?sophiId=" + sophiId.split('.')[0];
      getapi(endpoint, [type, variant]);
    }
  }



  /**
   * @param {string} type 'paywall', 'pencil'
   * @param {string} variant 'control', 'experience'
   * @param {string} propensity 'no', 'low'...
   * @param {bool} firstVisit paywall firstvisit, true or false
   */
  function updatePencilPaywallPrice(type, variant, propensity) {
    var priceData = {
      no: { price: '49', duration: '24', code: 'DPT04924', propensityTag: '-no' },
      low: { price: '99', duration: '24', code: 'DPT09924', propensityTag: '-low' },
      med: { price: '$1.49', duration: '24', code: 'DPT14924', propensityTag: '-med' },
      high: { price: '$3.99', duration: '52', code: 'DYNPT39952', propensityTag: '-high' },
      control: { price: '$1.99', duration: '24', code: 'digital', propensityTag: '-' + propensity }
    }
    var priceObj = variant === 'experience' ? priceData[propensity] : priceData.control;
    var redirectURL = [
      'https://subscribe.theglobeandmail.com/#/',
      priceObj.code,
      '?cid=GUPrice-cem1493_',
      variant,
      priceObj.propensityTag || ''
    ].join('');
    // console.log('cem1493 url:', redirectURL)

    /*
      if control - update paywall/pencil URL
      if experience - update price and paywall/pencil URL
    */
    if (type === 'paywall') {
      var dynamicPaywallInnerHTML = [
        '<!-- Paywall content copied from CEM-1566 --> <div class="c-keytar-header"> This content is available to <span>globeandmail.com</span> subscribers. </div> <div class="c-keytar-content"> <div class="c-keytar-content-subscriber__container"> <div class="c-keytar-content-subscriber__header">Support Quality Journalism</div> <div class="c-keytar-action"> <div class="c-keytar-action__price"> Just ',
        priceObj.price,
        '</div> <div class="c-keytar-action__info"> <p>per week for the first ',
        priceObj.duration,
        ' weeks</p> </div> <a href="',
        redirectURL,
        '" class="c-button c-button--utility-yellow-secondary c-button--wall-width c-button--no-border"> <span class="c-button__text"> Start Today </span> </a> </div> <div class="c-keytar-content-subscriber__text"> <p class="c-keytar-content-subscriber__text-h1"> Powerful. Important. Impactful. </p> <p class="c-keytar-content-subscriber__text-h2"> The value of quality journalism </p> <hr> <p class="c-keytar-content-subscriber__text-h2"> When you subscribe to globeandmail.com, you get access to: </p> <ul class="c-keytar-content-subscriber__text-list"> <li id="odd">Every story ever published on globeandmail.com</li> <li> Investigative stories of national and international importance </li> <li id="odd">First-hand accounts of US and world politics</li> <li>The most experienced business journalists in the country</li> <li id="odd"> The Globe\' s unwavering commitment to provide the truth </li> </ul> </div> </div> </div> <div class="c-keytar-footer"> <a class="c-keytar-footer__logo" href="https://www.theglobeandmail.com"> <img src="https://www.theglobeandmail.com/files/cem/logos/GMLogo.svg" alt="The Globe and Mail"> </a> <div class="c-keytar-footer__content"> <p> Already a print newspaper subscriber? <a href="https://subscribe.theglobeandmail.com/#/multiplatform" class="js-keytar-algt"> Get full access to globeandmail.com </a> </p> <p> Already subscribed to globeandmail.com? <a href="https://sec.theglobeandmail.com/user/#/login" class="js-keytar-algt"> Log in to keep reading </a> </p> </div> </div> <style> .c-button--wall-width { width: 280px; } @media (max-width: 768px) { .c-button--wall-width { margin-top: 10px; } } </style>',
      ].join('')

      var paywall = { 'red': { 'name': 'red', 'type': 'hardPaywall' }, 'thresh': { 'name': 'thresh', 'type': 'meteredPaywall' } };
      var isHermes = getSegment("hermesInScope", true);
      var paywallType = getSegment('article.paywallStatus', 'red') || isHermes ? paywall.red.type : paywall.thresh.type;
      var dynamicPaywallTemplate = function () { return dynamicPaywallInnerHTML };

      setKeytarWall(paywallType, 'teaser', { template: dynamicPaywallTemplate });
    } else {
      var pencilDesktop = document.getElementById('bottom_subspencil-n');
      var pencilMobile = document.getElementById('mb-fixedpencil');
      // update pencil URLs
      pencilDesktop.getElementsByTagName('button')[0].setAttribute('onclick', 'window.open("' + redirectURL + '")');
      pencilMobile.getElementsByTagName('a')[0].href = redirectURL;
      // update pencil price, durations
      pencilDesktop.getElementsByClassName('o-sub-pencil__cost__just-price')[0].innerText = priceObj.price;
      pencilDesktop.getElementsByClassName('o-sub-pencil__cost__per')[0].innerHTML = '<span>per week<br/>for the first ' + priceObj.duration + ' weeks</span>';
      pencilMobile.getElementsByClassName('o-sub-pencil__offer__cost-xs')[0].innerHTML = [
        'just ',
        priceObj.price,
        ' week<span class="o-sub-pencil__offer__time-xs"> for the first ',
        priceObj.duration,
        ' weeks </span>'
      ].join('');
    }

    var analyticsLabel = variant + '-' + type + priceObj.propensityTag;
    // logs just for testing purpose:
    // console.log('cem-1493 analytics push laebl: ', analyticsLabel)

    /* push analytics */
    analyticsLayer.push({
      "event": "clickEvent",
      "clickEvent": {
        "testId": "cem1493",
        "feature": "promotions",
        "action": 'display',
        "type": "test",
        "label": analyticsLabel,
        "variantName": variant,
        "segmentName": "nonsub-paywall"
      }
    })
  }

</script> </div> </div> <div class="darwin_pages pb-layout-item pb-chain pb-c-default-chain" id="cvsNJi20bvVv3s"> <div class="u-wrapper pb-feature pb-layout-item pb-f-darwin-test" id="flvg4S10bvVv3s"> <script id="dwExecute_regwallcem1497v2">(function(){window._dw=window._dw||[];function dwSegment_regwallcem1497v2(){window._dw.push(["global","before",function onBefore(api){api.segment("anon-crosswords",function(){var darwinState=getDarwinState()||{};var isAnonymous=getSegment("identity.role.id",0);var isQA=window.location.href.indexOf("CEMQA1497")>-1;var isNotPB=/(pb|ellipsis)?/im.test(window.location.href);var regwallExist=getSegment("hermesData.wall","regwall");var isCrosswordsPage=/(puzzles-and-crosswords\/(cryptic-crossword|universal-crossword|sudoku-classic)\/)/im.test(window.location.href);
