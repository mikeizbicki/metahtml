<article role="article" about="/diversity-discrimination/fox-news-next-target-attack-anti-racism-measures-us-military" typeof="schema:Article" class="article article--compact editorial-compact">
  
  <h3 class="editorial-compact__title">
    <a href="/diversity-discrimination/fox-news-next-target-attack-anti-racism-measures-us-military">

<!-- THEME DEBUG -->
<!-- THEME HOOK: 'field' -->
<!-- FILE NAME SUGGESTIONS:
