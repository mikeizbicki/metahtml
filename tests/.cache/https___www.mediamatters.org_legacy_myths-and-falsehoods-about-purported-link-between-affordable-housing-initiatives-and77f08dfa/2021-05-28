<article  role="article" about="/legacy/myths-and-falsehoods-about-purported-link-between-affordable-housing-initiatives-and" typeof="schema:Article" class="editorial-full">
  <header class="editorial-full__header">
    
    
<h1     
  
                  
  
  
 class="editorial-full__title page-title"
>

<!-- THEME DEBUG -->
<!-- THEME HOOK: 'field' -->
<!-- FILE NAME SUGGESTIONS:
   * field--node--title--article--full.html.twig
   * field--title--full.html.twig
   * field--node--title--article.html.twig
