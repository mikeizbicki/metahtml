            g += "=";
            for (var i = document.cookie.split(";"), f = 0; f < i.length; f++) {
                for (var h = i[f]; h.charAt(0) == " ";) {
                    h = h.substring(1, h.length)
                }
                if (h.indexOf(g) == 0) {
                    return h.substring(g.length, h.length)
                }
            }
            return null
        };
        comm = window.__dsns_.GetCookie("dsnscomm");
    } catch (e) {
        comm = ""
    }
    if (comm && comm.length > 0) {
