        var feedback = function (res) {
            if (res.success === true) {
                document.querySelector('.status').classList.add('bg-success');
                document.querySelector('.status').innerHTML = '<img style="margin-top:-10px" src="https://boxden.com/images/checkmark.png" width="24" height="24" />';
 
                

 $('#vB_Editor_QR_textarea').val( $('#vB_Editor_QR_textarea').val() + " " + "\r\n[img]" + res.data.link + "[/img]" );

 
 

            }
        };

        new Imgur({
            clientid: '65489e26a46e0cf',
            callback: feedback
        });











var a = 0;
$('.menu-ham').click(function() {
    if(a % 2 == 0){
  $('.menu').animate({right: '0px'}, 100);
$('.menu-ham').animate({right: '252px'}, 100);
$("#image1").toggleClass('flip');


   }
    else{
  $('.menu').animate({right: '-270px'}, 100);
$('.menu-ham').animate({right: '5px'}, 100);
$("#image1").toggleClass('flip');

    }
    a++;
});





var b = 0;
$('.menu-ham2').click(function() {
    if(b % 2 == 0){
  $('.menu2').animate({right: '0px'}, 100);
$('.menu-ham2').animate({right: '280px'}, 100);
$("#image12").toggleClass('flip3');


   }
    else{
  $('.menu2').animate({right: '-300px'}, 100);
$('.menu-ham2').animate({right: '-5px'}, 100);
$("#image12").toggleClass('flip3');

    }
    b++;
});

$(function() {
    if ( document.location.href.indexOf('#sections') > -1 ) {
         $('.menu2').animate({right: '0px'}, 100);
         $('.menu-ham2').animate({right: '280px'}, 100);
         $("#image12").toggleClass('flip3');
         b++;
    }
});




</script>









