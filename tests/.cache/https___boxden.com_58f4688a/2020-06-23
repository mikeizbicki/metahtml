$('[id^=props]').click(function(){
var clickedId= $(this).attr("id2");

    num = parseInt($(".count" + clickedId).text());
       $(".pop" + clickedId).addClass("ring_p");
    
    if ($(".count" + clickedId).hasClass("props")) {

        setTimeout(function(){
        $(".count" + clickedId).text(num+1);
        $(".count" + clickedId).prepend('+'); $(".count" + clickedId).addClass("counter_p");
         },900);
        
        } else if ($(".count" + clickedId).hasClass("slap")) {
        
       setTimeout(function(){
         $(".count" + clickedId).text(num+1); $(".count" + clickedId).addClass("counter_s");
         },900);
         
    
    } else {
    $("#replybutton" + clickedId).fadeOut().delay(300).fadeIn();
    setTimeout(function(){
    $("#counts" + clickedId).append('<span class="notifications2"><img src="https://boxden.com/smilies/oW3j9eY2.png" style="padding-right:20px" height="24" width="27"><span class="notification-counter3 a' + clickedId + '">+1</a></span></span>') ;
     $(".a" + clickedId).addClass("counter_p");
     },700);

    }
  
})




$('[id^=slap]').click(function(){
var clickedId= $(this).attr("id2");

    num = parseInt($(".count" + clickedId).text());
$(".pop" + clickedId).addClass("ring_s");
    
    if ($(".count" + clickedId).hasClass("props")) {
    
        
        setTimeout(function(){
        $(".count" + clickedId).text(num-1);
        $(".count" + clickedId).prepend('+'); $(".count" + clickedId).addClass("counter_p");
        },900);
      
        
        } else if ($(".count" + clickedId).hasClass("slap")) {
        
         setTimeout(function(){
         $(".count" + clickedId).text(num-1); $(".count" + clickedId).addClass("counter_s");
         },900);
    
    } else {
    $("#replybutton" + clickedId).fadeOut().delay(300).fadeIn();
    setTimeout(function(){
    $("#counts" + clickedId).append('<span class="notifications2"><img src="https://boxden.com/smilies/floydthinking2.png"  style="padding-right:20px" height="24" width="24"><span class="notification-counter4 a' + clickedId + '">-1</a></span></span>') ;
     $(".a" + clickedId).addClass("counter_s");
     },700);

    }
  
})



