<script>
document.addEventListener("DOMContentLoaded", function(e)
{
jQuery(document).ready(function($)
{
$(document).on('click', '.E-news-letter-btn-dh', function()
{
var email = $(this).parent('.article-optin-cont-r').find('.dk_nl_email_dh' ).val();
var agreed = false;
var name = '';
if( email == "" || email === null )
{
$('.E-news-letter-Error-dh').empty();
//$('.E-news-letter-Error-dh').append("Email field is empty!");
$(this).parent('.article-optin-cont-r').find('.E-news-letter-Error-dh').append("Email field is empty!");
$('.E-news-letter-Error-dh').show().delay(10000).fadeOut();
}
else
{
var $t = $(this);
$.ajax
({
type: 'post',
data: 'name=' + name + '&email=' + email + '&list=1&boolean=true&gdpr=' + agreed + '&Sourcewidget=article-widget',
url: '/newsletter/subscribe',
error: function(error)
{
$('.E-news-letter-Error-dh').empty();
$('.E-news-letter-Error-dh').append( "Subscription failed!" );
$('.E-news-letter-Error-dh').show().delay(20000).fadeOut();
},
success: function( data )
{
var messages  = '';
var error_type = 0;
if( data == true )
{
$('.dh-newsletter-msg').empty();
$('.dh-newsletter-msg').append('<div class="E-news-letter-veri-dh"><img src="/sites/dh/files/article_images/success-block-icon.png"></div><div class="E-news-letter-flt-text-dh"><span class="E-news-letter-close-btn">X</span><p class ="vari_text">Verification email has been sent to your inbox. Please verify to complete subscription. </p></div>');
error_type = 1;
}
else
{
if( data == "Invalid email address." )
{
messages = "Please enter a valid Email id";
}
else
{
messages = data;
}
error_type = 0;
}
if( error_type == 1 )
{
//$('.E-news-letter-Error-dh').empty();
$t.parent('.article-optin-cont-r').find('.E-news-letter-Error-dh').empty();
//$('.E-news-letter-Error-dh').append( messages );
$t.parent('.article-optin-cont-r').find('.E-news-letter-Error-dh').append(messages);
$('.E-news-letter-Error-dh').show().delay(10000).fadeOut();
}
else
{
//$('.E-news-letter-Error-dh').empty();
$t.parent('.article-optin-cont-r').find('.E-news-letter-Error-dh').empty();
//$('.E-news-letter-Error-dh').append( messages );
$t.parent('.article-optin-cont-r').find('.E-news-letter-Error-dh').append(messages);
$('.E-news-letter-Error-dh').show().delay(10000).fadeOut();
}
}
});
}
});
});
});
</script>
