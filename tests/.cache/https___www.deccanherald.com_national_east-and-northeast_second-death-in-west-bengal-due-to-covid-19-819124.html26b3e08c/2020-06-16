var scrollTop     = $(window).scrollTop(),
elementOffset = $(vuukleId).offset().top,
distance      = (elementOffset - scrollTop);
//if($(vuukleId).parent().prev().isInViewport() ) {
if(distance < 1500 ) {
$(vuukleId).addClass('vkAdprocessed');
