        $(function() {

            $(document).ready(function(){
                $('.header-main #ui-header-land, .header-main #ui-header-up').find('li a[href="https://www.20minutos.es/videos/"]').parent().remove();
                $('.header-main #ui-header-land, .header-main #ui-header-up').find('li a[href="'+window.location.origin +'/videos/"]').parent().remove();
                $('.default-menu').prepend("<li itemprop=\"name\" class=\"selected\"><h1><a itemprop=\"url\" href=\"/videos/\"><span>Videos</span></a></h1></li>");
                var maxMenu = 6;
                $('.default-menu li').not($( ".default-menu li.primary-more" )).each(function( index, element ) {
                    maxMenu--;
                    if(maxMenu < 0){
                        $(this).remove();
                    }
                });

            });
        });
