$(window).scroll(function () {
var scroll = $(window).scrollTop();
var headerHeight = $('.header').outerHeight() || 0;
var bodyHeight = $('.header').offset().top || 0;
var skyscraperSticky = $('body > div[id^="adtechAdContent_main"]') || 0;
var annonsLabel = $('body > div[id^="adtechAdContent_main"] > div') || 0;
annonsLabel.css({"display": "none"});
skyscraperSticky.css({"position": "absolute", "top": topOffset + "px"});
var topOffset = headerHeight + bodyHeight + 25;
if ( scroll >= topOffset ) {
skyscraperSticky.css({"position": "fixed", "top": "25px"});
} else {
skyscraperSticky.css({"position": "absolute", "top": topOffset + "px"});
}
});
});
