$(function(){
var allLinks=$("#wrapperContainer article").find("a");
$.each(allLinks,function(index,item){
var link = $(item).attr("href");
if(window.location.href.includes(link)){
var replacementIndex=7;
if(index===0 || index ===3 || index===6){
replacementIndex=8;
}
var mainId="index".concat(index);
var replacementId="index".concat(replacementIndex);
var mainelement = document.getElementById(mainId).innerHTML;
var replacement = document.getElementById(replacementId).innerHTML;
document.getElementById(mainId).innerHTML=replacement;
document.getElementById(replacementId).innerHTML=mainelement;
}
});
});
