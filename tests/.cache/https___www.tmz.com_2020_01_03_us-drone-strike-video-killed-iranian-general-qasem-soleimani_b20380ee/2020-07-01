<script>
    window.disqus_config = function () {
    this.page.url = 'https://www.tmz.com/2020/01/03/us-drone-strike-video-killed-iranian-general-qasem-soleimani/';
    this.page.identifier = 'f5afdf7e-ea73-4b14-bc33-cee9a38d7646';
  };

  wbq.push(function renderComments() {
    var response = 'unknown';

    function accept() {
      if (response === 'accepted') {
        return;
      }

      $('#comments-target-f5afdf7e-ea73-4b14-bc33-cee9a38d7646').html($('#comments-accepted-f5afdf7e-ea73-4b14-bc33-cee9a38d7646').html());

      function showComments() {
                  $('#comments-view-f5afdf7e-ea73-4b14-bc33-cee9a38d7646').prev().remove();
          $('#comments-view-f5afdf7e-ea73-4b14-bc33-cee9a38d7646').remove();
                $('#comments-wrapper-f5afdf7e-ea73-4b14-bc33-cee9a38d7646').addClass('active').slideDown();
        window.TMZ.utils.loadDisqusJs().catch(reject);
        window.TMZ.dispatch(window.TMZ.actions.clickLink(
          'https://www.tmz.com/2020/01/03/us-drone-strike-video-killed-iranian-general-qasem-soleimani/',
          null,
          'tmz:article:f5afdf7e-ea73-4b14-bc33-cee9a38d7646',
          null,
          {
            section: 'comments',
            name: 'click-to-view',
          }
        ));
      }

      window.addEventListener('hashchange', function() {
        if (window.location.hash === '#comments') {
          showComments();
        }
      }, { once: true });

      // if user clicked comment icon in blogroll or permalink
      if (window.location.hash === '#comments') {
        showComments();
      } else { // on regular scroll down
        $('#comments-view-f5afdf7e-ea73-4b14-bc33-cee9a38d7646').on('click', function() {
          showComments();
        });
      }

      response = 'accepted';
    }

    function reject() {
      if (response === 'rejected') {
        return;
      }

      $('#comments-target-f5afdf7e-ea73-4b14-bc33-cee9a38d7646').html($('#comments-rejected-f5afdf7e-ea73-4b14-bc33-cee9a38d7646').html());
      response = 'rejected';
    }

    watchConsent(function (canUse) {
      canUse('disqus') ? accept() : reject();
    });
  });
</script>
