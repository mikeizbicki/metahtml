<script type='text/javascript'>
    

    
    var hbBiddersParams = {"header_desk_sticky": {"criteo": {"zoneId": "1088147"}, "appnexus": {"placementId": "12389966"}, "openx": {"unit": "539313278", "delDomain": "motleyfool-d.openx.net"}, "ix": {"siteId": "357443", "size": "[[970,90], [728,90]]"}, "rubicon": {"zoneId": "1232038-2", "accountId": "12312", "siteId": "248904"}}};
    //if there are bids -> push them
    if(hbBiddersParams) {
        var params = [];
        hbBiddersParams = hbBiddersParams['header_desk_sticky'];
        //push each bidder settings
        for (var bidder in hbBiddersParams) {
            if (!hbBiddersParams.hasOwnProperty(bidder)) continue;

            // IX bidder requires special treatment
            if (bidder === 'ix') {

              let sizeAsString = hbBiddersParams[bidder]['size'];
              let siteId = hbBiddersParams[bidder]['siteId'];

              // cannot run JSON.parse() on undefined, so check that size is configured first
              if (sizeAsString) {
                let sizeAsArray = JSON.parse(sizeAsString);
                  // according to docs we have to send one bid per size for IX
                  sizeAsArray.forEach(function(unitSize){
                    params.push({
                        bidder: bidder,
                        params: {
                          'size': unitSize,
                          'siteId': siteId
                        }
                    });
                  });
              }
              else {
                params.push({
                        bidder: bidder,
                        params: {
                          'siteId': siteId
                        }
                    })
              }
            }
            // if bidder not IX
            else {
              params.push({
                bidder: bidder,
                params: hbBiddersParams[bidder]
              })
            }
        }
        adUnits = window.adUnits || [];
        adUnits.push({
            code: 'div-header_desk_sticky-8318',
            mediaTypes: {
                banner: {
                    sizes: [[728, 90], [970, 90]]
                }
            },
        bids: params
        });
    }
    

    googletag.cmd.push(function() {
        slots['div-header_desk_sticky-8318'] = googletag.defineSlot('/3910/investing/header_desk_sticky', [[728, 90], [970, 90]], 'div-header_desk_sticky-8318')
    .addService(googletag.pubads())
        
    .setTargeting('pos', adCount)
        
    .setTargeting('placement', 'header_desk_sticky')
        
    .setTargeting('ArticleNum', 'article-' + window.article_count_id)
        
    .setCollapseEmptyDiv(true);
    });

    
</script>




