                        if (lytics_segments_string != "") {
                            lytics_segments_string = lytics_segments_string + ", " + data.segments[i];
                        } else {
                            lytics_segments_string = data.segments[i];
                        }

                    }
                }
                window.googletag.pubads().setTargeting("LyticsSegments", lytics_segments_string);
            });
        }
    });
