<script>
ga("create", "UA-1616916-22","auto", "stn");
window.addEventListener("message",function(event) {
    if(event.data.type == "s2nEvent" && event.data.s2nEvent == "readyToPlay") {
        console.log("***** readyToPlay @ "  + performance.now()/1000 + " ******");
        ga("stn.send",  {
            hitType: "event",
            eventCategory: "Experimental",
            eventAction: "readyToPlay",
            eventLabel: event.data.playerKey,
            eventValue: Math.round((performance.now()/1000),2),
            nonInteraction: true,
            dimension5: HDN.dataLayer.content.objectType
        });
    }
});
</script>
