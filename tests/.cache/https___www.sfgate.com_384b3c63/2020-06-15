<script type="text/javascript">
(function () {
    if (window.HDN && HDN.dataLayer && HDN.dataLayer.content) {
        var _hdn = HDN.dataLayer.content;
        if (_hdn.hasOwnProperty('objectType') && _hdn.objectType === 'slideshow') {
            if (_hdn.hasOwnProperty('galleryListType') && _hdn.galleryListType === 'list') {
                var hfs = document.createElement('style');
                hfs.innerHTML = '#subscription { display: none;'
                document.getElementsByTagName("head")[0].appendChild(hfs);
            }
        }
    }
})();
</script>
<!-- END ENG 3884 suppress the 'Subscribe' button from SFGate listicle headers ONLY -->

