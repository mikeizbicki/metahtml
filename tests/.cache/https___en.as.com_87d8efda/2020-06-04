"mpu5" : [300, 250],
"mpu6" : [300, 250],
"nstdX" : [1,2,3,4,5,6,7,9],
                };
            }
            var publi_opcs = {
                'asy' : true,
                'ced' : true,
                'adunit' : gtpadunit,
                'divid' : gtpdivid,
                                'key' : {
                    'askey' : listadoKW
                },
                'ads': huecosPBS,
                   
            }
            PBS.setAds(publi_opcs);
        }
        else
        {
            var PBS={display:function(){},asym:{refresh:function(){}}};
        }
    }
</script>
