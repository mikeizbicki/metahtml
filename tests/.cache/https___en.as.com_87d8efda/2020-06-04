for(var i = 0; i <ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == ' ') {
        c = c.substring(1);
    }
    if (c.indexOf(c_name) == 0) {
        e_name = c.substring(c_name.length, c.length);
    }
}

