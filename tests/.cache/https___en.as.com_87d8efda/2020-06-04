<script type="text/javascript">
    function bloque_lomasvisto_wpo(){
     $(document).ready(function() {
         $('.loultimo').css("display", "none");
         $('.lomasvisto').css("display", "block");

         $("#tabs-lomasvisto li a").click(function(e) {
          e.preventDefault();
             $("#tabs-lomasvisto li a").removeClass("active");
             $(this).toggleClass("active");
             var selected = $(this).attr("id").split('-')[1];

             $('.loultimo, .lomasvisto').css("display", "none");
             $('.' + selected).css("display", "block");
         });
     });
   }
    </script>
