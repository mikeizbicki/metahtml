	<h1 class="screen-reader-text">The Irish Sun</h1>

	<section id="main-articles" class="sun-container__home-section">

			<div class="new-block sun-container__home-content sun-row-v2 teaser teaser--main first-row" ><div class="col sun-col-4"><!-- LARGE TEASER - 620PX -->

<div class="teaser-item teaser__large theme-main" >

	<!-- TEASER IMAGE -->
<div class="teaser__image-container" >
	<div class="teaser-grad"></div>
