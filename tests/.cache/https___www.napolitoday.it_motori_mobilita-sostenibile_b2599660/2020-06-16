<script type="text/javascript">/*<![CDATA[*/


  (function()
  {
    if ( !window.al )
      return;
      
      
    var bAdBlockOn = false;
    var fAdBlockOn = function( )
    {
      if ( bAdBlockOn )
        return;
        
      bAdBlockOn = true;
      
      
      window.ga && ga( 'send', 'event', 'AdBlock', 'AdBlock', location.href, {nonInteraction:true} );
      window.AdBlock = true;
      
      if ( (function(_){ return (_ = document.cookie.match(/\b__adb=([^;]*)/i)) && parseInt(_[1]) ? true : false; })() )
        return;
      
      if ( /^\/+$/.test(location.pathname) )
        return;

      
