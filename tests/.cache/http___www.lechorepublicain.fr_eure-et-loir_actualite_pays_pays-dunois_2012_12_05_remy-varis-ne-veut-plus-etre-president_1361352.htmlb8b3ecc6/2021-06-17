<script>
	document.getElementById("header").classList.remove("b-header");
	new Vue({
		methods: {
			handleChange: debounce(function (value) {
				fetch(`/api/centreref/geo/communes/autocomplete?q=${value}&size=5&boostDepartements=${this.boostDepartements.join()}`)
					.then(response => response.json())
					.then(result => {
						this.cities = result;
					});
			}, 500)
		},
		store: cf.default.store,
		created: function () {
			fetch("/static/bloc/ripolinage/header/boostDepartementsBySite.json")
				.then(response => response.json())
				.then(result => {
					if (result[headerJsonSite.code]) {
						this.boostDepartements = result[headerJsonSite.code]
					}
				});
		},
		data: function () {
			return {
				user: headerJsonUser,
				links: headerJsonEssentiel,
				site: headerJsonSite,
				menu: headerJsonMenu[0],
				boostDepartements: [],
				cities: [],
			};
		},
	}).$mount("#header-app");
</script>
