<script type="text/javascript">
	var jsonrpc = null;
	function simpleJsonRPCRequest(method, params) {
		$.ajax({
            url: '/json-rpc',
            data: JSON.stringify({id:0, method: method, params:params}),
            type:"POST",
            dataType:"json",
            contentType:"applicaton/json; charset=UTF-8",
            async:false
        });
	}
	function getJsonRPC() {
		if (jsonrpc == null) {
			$.ajax({
				url: '/js/cfc/jsonrpc-min.js',
				dataType: "script",
				async: false,
				cache: true
			});
			try {
				jsonrpc = new JSONRpcClient('/json-rpc');
			} catch(err) {
				jsonrpc = {
					rs : {
						signalerAbus : function() { },
						incrementNbLu : function() { },
						incrementNbRecomFacebook : function() { },
						incrementNbPlusOneGoogle : function() { },
						incrementNbRecomTwitter : function() { },
						getArticlesInZoneGeographique : function() { }
					}
				};
			}
		}
		return jsonrpc;
	}
</script>


