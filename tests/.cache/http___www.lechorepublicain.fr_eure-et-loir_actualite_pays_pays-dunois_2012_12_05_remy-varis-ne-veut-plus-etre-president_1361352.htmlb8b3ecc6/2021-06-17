<script type="text/javascript">
    (function() {

        var modeCommsHidden = true;

        function signalerAbus(comment) {
            console.log(comment);
            simpleJsonRPCRequest('rs.signalerAbus', [comment, 'ER']);
            alert('L\'abus a été signalé, merci pour votre collaboration.');
        }


        function repondre(comment) {
            $('#comm-alert').hide();
            var commentaireConnectForm = $("#connectCommentForm-361352");
            if (commentaireConnectForm.length > 0) {
                commentaireConnectForm.submit();
            } else {
                var commentaireForm = $("#formCommentaire-361352");
                if (commentaireForm.length > 0) {
                    var titre = 'Envoyer mon commentaire';
                    var uidCom = '-1';
                    if(comment > uidCom) {
                        titre = 'Envoyer ma réponse';
                        uidCom = comment;
                    }
                    commentaireForm.find('#uidParentCommentaire').val(uidCom);
                    commentaireForm.find('#poster-commentaire').val(titre);
                    $("#formCommentaire-361352 #texte").focus();
                }
            }
        }

        
        function plusDeReactions() {
            $('.b-commentaires-element-toggable-361352').toggle(modeCommsHidden);
            $('#plus-reactions-361352').toggle(!modeCommsHidden);
            $('#moins-reactions-361352').toggle(modeCommsHidden);

            modeCommsHidden = !modeCommsHidden;
        }

        jQuery('#plus-reactions-361352, #moins-reactions-361352')
                .click(function() {
                    plusDeReactions();
                })
    })();

</script>




