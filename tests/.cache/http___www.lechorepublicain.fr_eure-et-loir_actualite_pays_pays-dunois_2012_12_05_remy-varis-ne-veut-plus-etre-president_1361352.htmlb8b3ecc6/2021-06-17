<script type="text/javascript">

function displayErrorMessage(message, time, title) {
	messageCleanClasses();
	if (title != null) {
		$("#message_popup_title").html(title);
	} else {
		$("#message_popup_title").html("Erreur survenue");
	}
	$("#message_popup_wrapper").addClass("error-popup");
	$("#message_popup_desc").html(message);
	if (time != null) {
		if(time > 0) {
			setTimeout(hideMessage, time);
		} else {
			setTimeout(hideMessage, 5000);
		}
	}
	showPopin($("#message_popup"));
}
function displaySuccessMessage(message, time, title) {
	messageCleanClasses();
	if (title != null) {
		$("#message_popup_title").html(title);
	} else {
		$("#message_popup_title").html("Traitement effectué");
	}
	$("#message_popup_wrapper").addClass("success-popup");
	$("#message_popup_desc").html(message);
	if (time != null && time > 0) {
		setTimeout(hideMessage, time);
	} else {
		setTimeout(hideMessage, 5000);
	}
	showPopin($("#message_popup"));
}
function displayLoadingMessage(message, time, title) {
	messageCleanClasses();
	if (title != null) {
		$("#message_popup_title").html(title);
	} else {
		$("#message_popup_title").html("Chargement en cours");
	}
	$("#message_popup_wrapper").addClass("loading-popup");
	if(message == null) {
		message = "Merci de patienter...";
	}
	$("#message_popup_loading").html('<div class="text-center"><img src="/css/theme/images/picto/loader.gif" class="loader"/><p>'+message+'</p></div>')
	$("#message_popup_desc").html(message);
	if (time != null && time > 0) {
		setTimeout(hideMessage, time);
	}
	showPopin($("#message_popup"));
}
function messageCleanClasses() {
	$("#message_popup_wrapper").removeClass();
}
function hideMessage() {
	messageCleanClasses();
	hidePopin($("#message_popup"));
}
/* AUTOREFRESH */
function refreshPage() {
	if (window.autoRefresh) {
		if (window.location.href.indexOf("#refresh") < 0) {
			window.location.href = window.location.href + "#refresh";
		}
		window.location.reload();
	}
}
function stopAutoRefresh() {
	$(document).idleTimer("destroy");
}
function initAutoRefresh() {
	if (window.autoRefresh){
		$(document).idleTimer(window.autoRefresh);
		$(document).bind("idle.idleTimer", refreshPage);
	}
}
function goToVieLocale(value) {
	if (typeof value != 'Object' && value.id != null && value.id != '') {
		var urlVieLocale = getJsonRPC().rs.getUrlVieLocale(value.id);
		window.location.href = urlVieLocale;
	}
}
$(function() {
		//initAutoRefresh();
		autocompleteCommune('#menuCommuneSelector', '#menuSelectorChosenComm', '/ajax/tree/commune', null, 'Commune', null, goToVieLocale);
	
	// workaround problem flex safari
	if($(".b-liste-simple").length > 0) {
		if(!!navigator.userAgent.match(/Version\/[\d\.]+.*Safari/) && $(".b-liste-simple .b-article-simple").height() == 0) {
			$(".b-liste-simple .b-article-simple, .b-liste-simple .b-titre-bloc").height(85).css("display","inline-block");
		}
	}
	
});
</script>

