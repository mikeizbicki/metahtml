<script type="text/javascript">
	var captchaSendByMailContainer = null;
	
	function afterPost(data) {
		var jsonRes = data;
		if(typeof data == 'string') {
			console.log(data);
			jsonRes = JSON.parse(data);
		}
		if (jsonRes.success) {
			$("#successSendFriend .message").text(jsonRes.message);
			$("#successSendFriend").show();
			setTimeout(resetFields, 2500);
		} else {
			$("#errorSendFriend .message").text(jsonRes.message);
			$("#errorSendFriend").show();
			for(var k in jsonRes.errors) {
				var err = jsonRes.errors[k];
				$("."+err.fieldName+"-wrapper").addClass("has-error");
				if(err.message) {
					$("."+err.fieldName+"-wrapper .formErrorMessage").text(err.message);
				}
			}
			$("#envoyerAmiBtn").removeAttr("disabled");
		}
	}
	function beforePost() {
		$("#envoyerAmiBtn").attr('disabled', 'disabled');
		hideErrors();
		return true;
	}
	function onErrorPost() {
		$("#errorSendFriend .message").text("Une erreur est survenue, le contenu n'a pas pu être envoyé au destinataire.");
		$("#errorSendFriend").show();
		$("#envoyerAmiBtn").removeAttr("disabled");
	}
	function hideErrors() {
		$("#envoyerAmi_popup .alert").hide();
		$("#envoyerAmi_popup .has-error").removeClass("has-error");
		$("#envoyerAmi_popup .formErrorMessage").empty();
	}
	function resetFields() {
		hideErrors();
		$("#formAmi input").val('');
		$("#envoyerAmiBtn").removeAttr("disabled");
	}
	
	$(function(){
		initAjaxForm('#formAmi');
	});
</script>
<script type="text/javascript">
	function printArticle() {
		window.print();
	}
	function changeScaleFont(toAdd) {
		var selector = $(".bloc-article-bottom .contenu");
		curSize= parseInt(selector.css('font-size'));
		var newValue = curSize+toAdd;
		if (newValue >= 12 && newValue <= 21) {
			selector.css('font-size', newValue);
		}
	}
	$(function() {
		simpleJsonRPCRequest('rs.incrementNbLu', [361352,'ARTICLE']);
	});
</script>


	

	<script type="text/javascript">
	
	var blocArticle = $('.bloc-article-bottom .b-article');
	var blocContenu = blocArticle.find('.contenu');
	
	$(document).on("mouseup", function (e) {
    	$('#dummyPopoverPos').popover("destroy").remove();
	});
	
	function replaceTextParamInLink(link, text){
		var url = link.attr('href');
		url = url.substring(0, url.indexOf('&text=')+6)+text;
		link.attr('href', url);
	}
	
	blocContenu.on("mouseup", function (e) {
		if (!window.getSelection || !window.getSelection().getRangeAt || !window.getSelection().getRangeAt(0).getBoundingClientRect) return;
		
		e.stopPropagation();
    	$('#dummyPopoverPos').popover("destroy").remove();
		
		var selection = window.getSelection();
		var bRect = selection.getRangeAt(0).getBoundingClientRect();
		
		var xpos = bRect.left + bRect.width/2;
		var ypos = bRect.top;
		
	    var text = "" + selection;
	    if (text.length > 4) {
			var dummyPopoverPos = $('<div id="dummyPopoverPos" ></div>');
	        blocContenu.after(dummyPopoverPos);
	        
	        dummyPopoverPos
		       .css("position", "absolute")
		       .css("top", ypos - blocArticle.get(0).getBoundingClientRect().top)
		       .css("left", xpos - blocArticle.get(0).getBoundingClientRect().left);
		    
	        dummyPopoverPos.popover({
	            html: true,
			    content: function () {
			    	var buttons = $('.bloc-article-bottom .c-social.list-inline:first').clone();
			    	buttons.find(':nth-child(n+4)').remove();
			    	buttons.css("marginTop", 0);
			    	
			    	var textShared = encodeURIComponent(text);
			    	replaceTextParamInLink(buttons.find('.p-twitter'), textShared);
			        return buttons;
			    },
	            animation: false,
	            trigger: "manual",
	            placement: "top"
	        }).popover('show');
	        
	        var popover = dummyPopoverPos.next("div.popover");
	        popover.on("mouseup", function (e) {
				e.stopPropagation();
	        });
  
	    }
	});
</script></div>

