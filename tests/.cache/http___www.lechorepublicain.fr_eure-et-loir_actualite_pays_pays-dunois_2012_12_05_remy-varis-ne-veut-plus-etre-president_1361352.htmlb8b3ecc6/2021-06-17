<div id="header-app" class="cf-header">
		<cf-header :site="site" :menu="menu" :is-logged="user.logged" :is-premium="user.isPremium" :is-fixed="true"
		:autocomplete-props="{items: cities, textSerializer: item => `${item.nom} (${item.match.slice(-5)})`, urlSerializer: item => `/${item.slug}`, placeholder: 'Saisissez une commune'}"
		@autocomplete-change="handleChange" ></cf-header>
	<div class="header-padding">
		<cf-sub-menu v-if="links.length > 0" :links="links" :is-logged="user.logged" :is-premium="user.isPremium"
			class="mb-3" ></cf-sub-menu>
		<b-toaster name="b-toaster-cf" class="m-0"></b-toaster>
	</div>
