    $('#site-search-631346-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-631346').submit();
        return false;
    });

    $('#site-search-631346').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-631346 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-631346 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-631346-term').val() ){
            return true;
        }else{
            return false;
