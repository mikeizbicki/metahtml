    //pageview_candidate
    function pageviewcandidate(){
   // alert("here");
        $.ajax({ 
            url:'/pv_candidate.php', 
            type:'GET', 
            cache:false, 
            success:function(pv_candidate){ 
                console.log("candidate_comescore fired");                
            }, 
            error:function(){ 
                console.log('Something went wrong..'); 
            } 
        }); 
    }
    //update URL according to the current article id
    function updateUrl(title, url) {
        console.log(title+" == title");
        console.log(url+" == url");
        // colombia.clearData();
        setTimeout(function() {
            var src = "";
            $('.fp-lazy').each(function() {
                src = $(this).data("src");
                $(this).attr("src", src);
                src = "";
            });
        }, 1500);
        var cleanTitle = title;
        cleanTitle = cleanTitle.replace("'", "");
        cleanTitle = cleanTitle.replace('"', '');
        cururl      = window.location.href.split('?')[0];  
        
        if(cururl != url)
        {
            History.pushState(null, cleanTitle, url);    
            //Analytics update here
            //ga push
            url = url.replace("https://www.firstpost.com", "");
            ga('set', 'page', url);
            ga('send', 'pageview');
            //event
            ga('send', 'event', 'Pageview', 'Longscroll', url);
            //ComScore
             if (window.topScroll < window.previousScroll) {
                console.log("back");
                window.isbackScroll=true
              
            }else{
                 window.isbackScroll=false;
            }     
            window.previousScroll = window.topScroll;

            if(!window.isbackScroll){
             console.log("Comscore called"); self.COMSCORE && COMSCORE.beacon({c1: "2", c2: "6683813"}); 
           //page view
            var id = $("#myScrollspy li.active").data("id");
            id = id.replace("aid_", "");
            if(id != post_id){ 
            pageviewcandidate();
            }
            }
             
             if(id != post_id){ 
                callComScore();
                }
                        //alsoWatch();
        }
    }
    //loads ads, teads & colombia
    var teadsHTML = '';
    //categoryName = $("#main").data("category-name");
    categoryName = 'India';
    
    
    function loadAds(id,adCount) {
        localStorage.setItem('fpal_' + id, 1);
        // Teads
        //activateOutbrain(id);
