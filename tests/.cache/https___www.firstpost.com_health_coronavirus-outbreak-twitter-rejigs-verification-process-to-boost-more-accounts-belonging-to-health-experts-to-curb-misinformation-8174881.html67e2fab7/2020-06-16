    function createAd1(slot_id,adCount) {
        if(adCount != 0){
                googletag.cmd.push(function() {
                var slot = googletag.defineSlot('/1039154/FirstPost/FirstPost_' + categoryName + '/FirstPost_' + categoryName + '_Internal_Set' + (adCount+1) + '/FirstPost_' + categoryName + '_Internal_Set' + (adCount+1) + '_300x600', [300, 600], slot_id).addService(googletag.pubads());
                googletag.display(slot_id);
                //googletag.pubads().refresh([slot]);
            });
        }
    }
    

