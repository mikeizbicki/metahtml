<script type="application/javascript">
    var mgid_ads_enable = "0";
    var fpcategory_mgid_ads_enable = '1';
    var outbrain_ads_enable = "1";
        
    $(".zealAds").hide();    
    // Continuous Scroll
    //    $(function() {
    window.alsoWatchset = 1;
    window.alsoWatchsetOhter = 1;        
    window.alsowatchdata ="";
    window.otheralsohtml="";
    window.playerUrl = "https://images.firstpost.com/wp-content/uploads/assets/javascript/spark_player.js?customer=network18";
    $(document).ready(function(){
	myUpcharWidget();
    });
    var initCont = function() {
        //alert("FPCS loaded");
        var thisAid = $("#main").data("article-id");
        var thisCid = $("#main").data("category-id");
        var thisCname = $("#main").data("category-name");
        window.isbackScroll=false;
        window.previousScroll = 0;
        window.alsoWatchset = 1;
        //zealAd(thisAid);
        //$('html, body').bind('DOMMouseScroll MouseScrollEvent MozMousePixelScroll wheel scroll', function() {
        $(window).scroll(function(){
            if($('#ADID_2_'+thisAid).contents().length > 0){                    
                $(".zealAds").show();
            }
            $(".live-blog").remove();
         
            var scrollTop = $(window).scrollTop();
            window.topScroll = $(window).scrollTop();
            if (!$('body').hasClass('cson')) {
                $('body').addClass('cson');
                $("#myScrollspy li").each(function(index) {
                    if ($(this).hasClass("active")) {
                        //don't crawl that
                    } else {
                        //console.log( "VS: " + index + ": " + $( this ).data('url') );
                        var id = $(this).data('id');
                        id = id.replace("aid_", "");
                        var url = $(this).data('url');
                        if (window.location.hostname == "silex.firstpost.com" || window.location.hostname == "firstpost.staging" || window.location.hostname == "staging.firstpost.com" || window.location.hostname == "stgrevamp.firstpost.com" || window.location.hostname == "devlive.firstpost.com" || window.location.hostname == "dev.firstpost.com" || window.location.hostname == "firstpost.prod.ssl") {
                            url = url.replace("https://www.firstpost.com", "http://"+window.location.hostname);
                        }
                        //resetting ads localstorage
                        localStorage.removeItem('fpal_' + id);
                        setTimeout(function() {
                            loadArticle(id, url);
                        }, 500);
                        setTimeout(function() {
                            $("#myScrollspy").fadeIn();
                        }, 1200);
                    }
                });
                //$(window).off('scroll');
            }
        });
        //for mobile
        //on click, show more news article strip
        $("#stripToggle").on('click', function() {
            $(".articlesStripWrap").toggleClass("open");
            var text = $(this).text();
            if (text == "More") {
                $(this).text("Close");
            } else {
                $(this).text("More");
            }
        });
        $(".articlesStrip a").on('click', function(e) {
            e.preventDefault();
            e.stopPropagation();
            var url = $(this).attr("href");
            var id = url.replace("#aid_", "");
            var title = $(this).attr("title");
            var cleanTitle = title.replace("'", "");
            cleanTitle = cleanTitle.replace('"', '');
            $("#myScrollspy li").removeClass("active");
            $(this).parent("li").addClass("active");
            var adCount =$('.articlesStrip').find('li.active').data('adcount');
            //var url = $("#myScrollspy li.active").data("url");
            updateUrl(cleanTitle, url);
            loadAds(id,adCount);

            var elm = $(this).attr("href");
            $('html, body').animate({
                scrollTop: $(elm).offset().top
            }, 1000);
            //mobile
            if($(window).width() < 700){


                $(".articlesStripWrap").removeClass("open");
                var trigger = $("#stripToggle");
                var text = trigger.text();
                if (text == "More") {
                    trigger.text("Close");
                } else {
                    trigger.text("More");
                }
            }
        });
        
         $(window).on('activate.bs.scrollspy', function() { 
            var url = $("#myScrollspy li.active").data("url");
            if (window.location.hostname == "dev.firstpost.com" || window.location.hostname == "stagrevamp.firstpost.com" || window.location.hostname == "live.firstpost.com") {
                url = url.replace("https://www.firstpost.com", "http://"+window.location.hostname);
            }
            var id = $("#myScrollspy li.active").data("id");
            id = id.replace("aid_", "");
            var title = $("#myScrollspy li.active").data("title");
            
            var adCount =$('.articlesStrip').find('li.active').data('adcount');
            
            //check if ads are loaded
            //if not, load them, else do nothing
            var checkFpal = localStorage.getItem('fpal_' + id);
            if (checkFpal) {
                //console.log('ads for '+id+' are already loaded...');
            } else {
                //console.log('ads for '+id+' are NOT loaded...');
                loadAds(id,adCount);
            }            
            updateUrl(title, url);
        }); //on URL change
        

        $(document).on('click', 'a.comment-btn', function(e){
            e.preventDefault();
            e.stopPropagation();
            var target = $(this).attr("href");
            $('html, body').animate({
                scrollTop: $(target).offset().top
            }, 1000);
        });
    };
      
        setTimeout(function () {
          initCont();
      },2000);

       
    function loadArticle(id, url) {
        //making sure article doesn't get loaded more than once
        var ifExists = $("#aid_" + id + ".article").length;
        if (ifExists > 0) {
            //scrollTo("#article_"+id);
            //article has already been loaded
            return false;
        } else {
            //console.log('vs: loading article '+id);
            //load article
            if ($(window).width() < 800) {
                $("#aid_" + id).append("<div class='clearfix'><div class='post-break' style='height: 105px'><div class='show-next'>Next Story<svg version='1.1' viewBox='0 0 129 129' enable-background='new 0 0 129 129' width='512px' height='512px'><g><path d='m121.3,34.6c-1.6-1.6-4.2-1.6-5.8,0l-51,51.1-51.1-51.1c-1.6-1.6-4.2-1.6-5.8,0-1.6,1.6-1.6,4.2 0,5.8l53.9,53.9c0.8,0.8 1.8,1.2 2.9,1.2 1,0 2.1-0.4 2.9-1.2l53.9-53.9c1.7-1.6 1.7-4.2 0.1-5.8z' fill='#FFFFFF'/></g></svg></div></div><div class='ad text-center'></div></div><div class='dynamicArticle clearfix'></div>");
            } else {
                $("#aid_" + id).append("<div class='clearfix'><div class='post-break'><div class='show-next'>Next Story<svg version='1.1' viewBox='0 0 129 129' enable-background='new 0 0 129 129' width='512px' height='512px'><g><path d='m121.3,34.6c-1.6-1.6-4.2-1.6-5.8,0l-51,51.1-51.1-51.1c-1.6-1.6-4.2-1.6-5.8,0-1.6,1.6-1.6,4.2 0,5.8l53.9,53.9c0.8,0.8 1.8,1.2 2.9,1.2 1,0 2.1-0.4 2.9-1.2l53.9-53.9c1.7-1.6 1.7-4.2 0.1-5.8z' fill='#FFFFFF'/></g></svg></div></div></div><div class='dynamicArticle clearfix'></div>");
            }
            //get the HTML
            $("#aid_" + id + " .dynamicArticle").load(url + " #main .article", function() {
                $("#aid_" + id + " #myupchar_widget").remove();
                $("#aid_" + id + " .featured-vdo-section").remove();
                /*$("#aid_" + id + " .comment-btn").hide();*/
                $("#aid_" + id + " .sidebar-video").remove();
                $("#aid_" + id + " .nw18-widget").hide();
                $("#aid_"+id+"  .contentMainArticle").removeClass("sticky-page-div");
                
                if ($(window).width() < 800) {
                    $("#aid_" + id + " .panel-score-widget").hide();
                };
                
                if(mgid_ads_enable == '1' && fpcategory_mgid_ads_enable == '1'){
                    //compositeAds();
                    //compositeAdsInArticle(id);
                }
                
                if(outbrain_ads_enable == '1' && fpcategory_mgid_ads_enable == '1'){
                    outbrainAds();
                }
                myUpcharWidget();
            });
            //$("#aid_" + id + " .featured-vdo-section").remove();
        }
    } //loadArticle
    //comscore
     
    function callComScore() {
        console.log("insideeee const");
       // alert("h");
        var curntUrl = window.location.href;
       // curntUrl = curntUrl.replace(window.location.hostname,"www.firstpost.com");
            var _comscore = _comscore || [];
            _comscore.push({
                c1: "2",
                c2: "6683813",
                c7: curntUrl
            });
            (function() {
                var s = document.createElement("script"),
                        el = document.getElementsByTagName("script")[0];
                s.async = true;
                s.src = (document.location.protocol == "https:" ? "https://sb" : "http://b") + ".scorecardresearch.com/beacon.js";
                el.parentNode.insertBefore(s, el);
            })();

    }
        
