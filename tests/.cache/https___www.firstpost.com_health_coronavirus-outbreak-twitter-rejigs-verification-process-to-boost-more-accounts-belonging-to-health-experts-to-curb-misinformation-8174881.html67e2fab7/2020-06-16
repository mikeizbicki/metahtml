    //state change action
    $("#coronoVirusStateChange").change(function(){
    	var selState = $('option:selected', this).val();
    	var selectedState = selState;
      var radioSelected = $('.covid-radio-label').text();
    	if(selState == 'Select State/UT'){
    		selectedState = "India"; 
    	}

         if(selectedState != "India")
         {
            $("#stateDepDiv").show();
            document.getElementById('radio1').checked = true;  
         }
         else
         {
             $("#stateDepDiv").hide();
              document.getElementById('radio1').checked = true;      
         }
      if (radioSelected.indexOf('MOHFW') > -1) {
         populateStateData(selectedState,false);
      }else{
         populateStateData(selectedState,true);   
      }
    	//populateStateData(selectedState);
    });


   $(".stateRadio").click(function(){
      var selState =$(this).next('label:first').html();
      var selectedState1 = $("#coronoVirusStateChange").val();
      if (selState.indexOf('MOHFW') > -1) {
         populateStateData(selectedState1,false);
      }else{
         populateStateData(selectedState1,true);   
      }
   });

