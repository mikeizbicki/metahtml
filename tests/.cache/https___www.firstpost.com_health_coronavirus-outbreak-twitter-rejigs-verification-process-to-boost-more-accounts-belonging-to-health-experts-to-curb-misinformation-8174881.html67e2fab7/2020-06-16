    var colombiaCount = '';
    colombiaCount = 2;

    function activateColombia(pp) {
        if ($("#div-clmb-ctn-217884-" + colombiaCount).length == 0) {
            if (!navigator.userAgent.match(/Android/i) && !navigator.userAgent.match(/webOS/i) && !navigator.userAgent.match(/iPhone/i) && !navigator.userAgent.match(/iPod/i) && !navigator.userAgent.match(/iPad/i) && !navigator.userAgent.match(/Blackberry/i)) {
                $("#aid_" + pp + " .dynamicArticle").find('.colombiaWidget').html('<div id="div-clmb-ctn-217884-' + colombiaCount + '" style="float:left;min-height:2px;width:100%;" data-slot="217884" data-position="' + colombiaCount + '" data-section="0" class="colombia"></div><div id="div-clmb-ctn-217883-' + colombiaCount + '" style="float:left;min-height:2px;width:100%;" data-slot="217883" data-position="' + colombiaCount + '" data-section="0" class="colombia"></div>');
            } else {
                $("#aid_" + pp + " .dynamicArticle").find('.colombiaWidget').html('<div id="div-clmb-ctn-217885-' + colombiaCount + '" style="float:left;min-height:2px;width:100%;" data-slot="217885" data-position="' + colombiaCount + '" data-section="0" class="colombia"></div>');
            }
            colombiaCount = parseInt(parseInt(colombiaCount) + 1);
        }
        //colombia.clearData();
    }
