<script>
(function() {
  var spotLogParts = {};
  function getSpotLog(o){ try { if (hpUACLogger.debugEnabled) { var r = []; for (var k in o) { if (o.hasOwnProperty(k)) { r.push(k + ': ' + o[k]); } } return r.join(', '); } else { return ''; } } catch(err) { return ''; } }

  var disableAdRotation = "" === "1";
  var adsDiv = document.querySelector('#adtech_right_rail_300x250_lower');
  adsDiv.removeAttribute('id');

  var defaultDelivery = "f";
  

  
  
  // enableQueue: ""
 
    
      
        waitForGlobal(function() {
          return window.htmlAdWH && window.adSetSyncCalled;
        }, function() {
          // override adsPl in template
          if ('function' === typeof window.adSetPlId) {
            if ('undefined' === typeof window.origAdsPlId) { window.origAdsPlId = window.adsPl; }
            if ('undefined' !== typeof window.origAdsPlId) { window.adSetPlId(window.origAdsPlId); }
            
          }

          // override ads NetId in template
          if ('function' === typeof window.adSetNetId) {
            if ('undefined' === typeof window.origNetId) { window.origNetId = window.adsNt; }
            if ('undefined' !== typeof window.origNetId) { window.adSetNetId(window.origNetId); }
            
          }

          adsDiv.reloadCount = 0;
          var recoveryId = '';
          var needsRecovery = recoveryId && recoveryId.length && kraken__adBlock && kraken__adBlock.active;
          var adId = needsRecovery ? recoveryId : '93436393';
          // try { adId = io_c3sd.ads["right_rail_300x250_lower"] || adId; } catch(e) {}

          spotLogParts['MN'] = adId;
          spotLogParts['Placement'] = 'right_rail_300x250_lower';

          
          

          // Override adtech_call_type
          var delivery_type = "" || "";
          var adtech_call_type = "";
          switch (delivery_type) {
            case "by_request": adtech_call_type = "ajax"; break;
            case "text": adtech_call_type = "text"; break;
            case "iframe": adtech_call_type = "iframe"; break;
            case "iframe_proxy": adtech_call_type = "f"; break;
            case "collapsed": adtech_call_type = "fc"; break;
            default: adtech_call_type = defaultDelivery; break;
          }

          var args = ['RR', 'RR'
          ];

          
          

          
          spotLogParts['Sizes'] = args[0] + 'x' + args[1];

          spotLogParts['FlagViewable'] =  'false';

          
          adSetInView('1');
          spotLogParts['FlagViewable'] =  'true';
          hpUACLogger.debug("adSetInView('1'): " + adId);
          

          

          
          
          

          

          if(needsRecovery) {
            waitForGlobal('readyToRecover', function() {
              adsDiv = document.querySelector('.right_rail_300x250_lower');
              //htmlAdWH(adId, args[0], args[1], "fileless", adsDiv);
            });
          } else {
            console.log('calling MN ' + adId.toString());
            htmlAdWH(adId, args[0], args[1], adtech_call_type, adsDiv);
            if (!disableAdRotation) {
              spotLogParts['FlagRotation'] = 'true';
              adsRotateMult(adsDiv);
            } else {
              spotLogParts['FlagRotation'] = 'false';
            }
            hpUACLogger.debug(getSpotLog(spotLogParts), true);
          }

          if (typeof debugadcode == 'function') { debugadcode('AdTech', {placement: 'right_rail_300x250_lower', mnid: adId}, adsDiv); }

          waitForGlobal('jQuery', function() {
            jQuery(document).on('reloadAd', function(ev, elem) {
              if (elem && !elem.contains(adsDiv)) { return; }
              var refreshRate = 1;
              adsDiv.reloadCount++;
              if (adsDiv.reloadCount % refreshRate === 0 && typeof adsReloadAd === 'function') { adsReloadAd(adsDiv); }
            });
          });

          if ( !window.__adtechForceInView ) {
            adSetInView('0');
            hpUACLogger.debug("adSetInView('0') probably for next UAC ad call");
          }

          
        }); // wFG:htmlAdWH
      
    
   //modulous queue closing
}());
</script></div></div></div></div></div><div class="coupons" id="coupon-module" data-rapid="marko-sec"><p class="coupons__label">HuffPost Offers</p><p class="coupons__description">Save online with coupons and promo codes from these featured retailers.</p><ul class="coupons__list"><li class="coupons__list__item"><a class="coupons__list__item__link" href="https://coupons.huffpost.com/jcpenney" data-ylk="subsec:Homepage;itc:0;cpos:1;pos:1;elm:btn">JC Penney</a></li><li class="coupons__list__item"><a class="coupons__list__item__link" href="https://coupons.huffpost.com/dicks-sporting-goods" data-ylk="subsec:Homepage;itc:0;cpos:1;pos:2;elm:btn">Dick&#x27;s Sporting Goods</a></li><li class="coupons__list__item"><a class="coupons__list__item__link" href="https://coupons.huffpost.com/groupon" data-ylk="subsec:Homepage;itc:0;cpos:1;pos:3;elm:btn">Groupon</a></li><li class="coupons__list__item"><a class="coupons__list__item__link" href="https://coupons.huffpost.com/famous-footwear" data-ylk="subsec:Homepage;itc:0;cpos:1;pos:4;elm:btn">Famous Footwear</a></li><li class="coupons__list__item"><a class="coupons__list__item__link" href="https://coupons.huffpost.com/petsmart" data-ylk="subsec:Homepage;itc:0;cpos:1;pos:5;elm:btn">Petsmart</a></li><li class="coupons__list__item"><a class="coupons__list__item__link" href="https://coupons.huffpost.com/sephora" data-ylk="subsec:Homepage;itc:0;cpos:1;pos:6;elm:btn">Sephora</a></li><li class="coupons__list__item"><a class="coupons__list__item__link" href="https://coupons.huffpost.com/urban-outfitters" data-ylk="subsec:Homepage;itc:0;cpos:1;pos:7;elm:btn">Urban Outfitters</a></li><li class="coupons__list__item"><a class="coupons__list__item__link" href="https://coupons.huffpost.com/nordstrom" data-ylk="subsec:Homepage;itc:0;cpos:1;pos:8;elm:btn">Nordstrom</a></li><li class="coupons__list__item"><a class="coupons__list__item__link" href="https://coupons.huffpost.com/the-home-depot" data-ylk="subsec:Homepage;itc:0;cpos:1;pos:9;elm:btn">Home De