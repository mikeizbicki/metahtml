<script>
(function() {
  var spotLogParts = {};
  function getSpotLog(o){ try { if (hpUACLogger.debugEnabled) { var r = []; for (var k in o) { if (o.hasOwnProperty(k)) { r.push(k + ': ' + o[k]); } } return r.join(', '); } else { return ''; } } catch(err) { return ''; } }

  var disableAdRotation = "" === "1";
  var adsDiv = document.querySelector('#adtech_leaderboard_flex_middle');
  adsDiv.removeAttribute('id');

  var defaultDelivery = "f";
  

  
  
  // enableQueue: ""
 
    
      
        waitForGlobal(function() {
          return window.htmlAdWH && window.adSetSyncCalled;
        }, function() {
          // override adsPl in template
          if ('function' === typeof window.adSetPlId) {
            if ('undefined' === typeof window.origAdsPlId) { window.origAdsPlId = window.adsPl; }
            if ('undefined' !== typeof window.origAdsPlId) { window.adSetPlId(window.origAdsPlId); }
            
          }

          // override ads NetId in template
          if ('function' === typeof window.adSetNetId) {
            if ('undefined' === typeof window.origNetId) { window.origNetId = window.adsNt; }
            if ('undefined' !== typeof window.origNetId) { window.adSetNetId(window.origNetId); }
            
          }

          adsDiv.reloadCount = 0;
          var recoveryId = '';
          var needsRecovery = recoveryId && recoveryId.length && kraken__adBlock && kraken__adBlock.active;
          var adId = needsRecovery ? recoveryId : '963863270';
          // try { adId = io_c3sd.ads["leaderboard_flex_middle"] || adId; } catch(e) {}

          spotLogParts['MN'] = adId;
          spotLogParts['Placement'] = 'leaderboard_flex_middle';

          
          

          // Override adtech_call_type
          var delivery_type = "default" || "";
          var adtech_call_type = "";
          switch (delivery_type) {
            case "by_request": adtech_call_type = "ajax"; break;
            case "text": adtech_call_type = "text"; break;
            case "iframe": adtech_call_type = "iframe"; break;
            case "iframe_proxy": adtech_call_type = "f"; break;
            case "collapsed": adtech_call_type = "fc"; break;
            default: adtech_call_type = defaultDelivery; break;
          }

          var args = ['lb', 'lb'
          ];

          
          

          
          spotLogParts['Sizes'] = args[0] + 'x' + args[1];

          spotLogParts['FlagViewable'] =  'false';

          
          adSetInView('1');
          spotLogParts['FlagViewable'] =  'true';
          hpUACLogger.debug("adSetInView('1'): " + adId);
          

          

          
          
          

          

          if(needsRecovery) {
            waitForGlobal('readyToRecover', function() {
              adsDiv = document.querySelector('.leaderboard_flex_middle');
              //htmlAdWH(adId, args[0], args[1], "fileless", adsDiv);
            });
          } else {
            console.log('calling MN ' + adId.toString());
            htmlAdWH(adId, args[0], args[1], adtech_call_type, adsDiv);
            if (!disableAdRotation) {
              spotLogParts['FlagRotation'] = 'true';
              adsRotateMult(adsDiv);
            } else {
              spotLogParts['FlagRotation'] = 'false';
            }
            hpUACLogger.debug(getSpotLog(spotLogParts), true);
          }

          if (typeof debugadcode == 'function') { debugadcode('AdTech', {placement: 'leaderboard_flex_middle', mnid: adId}, adsDiv); }

          waitForGlobal('jQuery', function() {
            jQuery(document).on('reloadAd', function(ev, elem) {
              if (elem && !elem.contains(adsDiv)) { return; }
              var refreshRate = 1;
              adsDiv.reloadCount++;
              if (adsDiv.reloadCount % refreshRate === 0 && typeof adsReloadAd === 'function') { adsReloadAd(adsDiv); }
            });
          });

          if ( !window.__adtechForceInView ) {
            adSetInView('0');
            hpUACLogger.debug("adSetInView('0') probably for next UAC ad call");
          }

          
        }); // wFG:htmlAdWH
      
    
   //modulous queue closing
}());
</script></div></div><div class="row row--7"><div data-rapid="marko-sec" class="content-buckets" id="department-feed"><div class="zone-politics"><section class="zone zone--bucket-politics zone--layout-bucket"><div class="zone__title"><h2 class="zone__title__text">Politics</h2></div><div class="zone__content"><div class="card"><a class="card__image__link" aria-label="Carol Barr, Wife Of GOP Rep. Andy Barr, Dies Suddenly At Age 39" aria-hidden="true" tabindex="-1" href="https://www.huffpost.com/entry/kentucky-congressman-barr-wife-dead-dies_n_5eea2321c5b639761201bccc" data-ylk="subsec:zone-politics;g:7d44cc1a-0e3d-397e-a77d-597f3ce4e3a7;itc:0;cpos:1;pos:1;elm:img"><div class="card__image" role="presentation"><div class="card__image__src img-sized img-sized--responsive"><div class="img-sized__placeholder"><div style="padding-top:56.25%"></div></div><picture><source type="image/webp" srcSet="https://img.huffingtonpost.com/asset/5eea23e2250000352deb3823.jpeg?cache=3GfQGpWqIw&amp;ops=160_90&amp;format=webp"/><img class="img-sized__img landscape" loading="lazy" alt="" width="160" height="90" src="https://img.huffingtonpost.com/asset/5eea23e2250000352deb3823.jpeg?cache=3GfQGpWqIw&amp;ops=160_90"/></picture></div></div></a><div class="card__text"><div class="card__headlines"><a class="card__headline card__headline--long" href="https://www.huffpost.com/entry/kentucky-congressman-barr-wife-dead-dies_n_5eea2321c5b639761201bccc" data-ylk="subsec:zone-politics;g:7d44cc1a-0e3d-397e-a77d-597f3ce4e3a7;itc:0;cpos:1;pos:2;elm:hdln"><h3 class="card__headline__text">Carol Barr, Wife Of GOP Rep. Andy Barr, Dies Suddenly At Age 39</h3></a></div></div></div><div class="card"><a class="card__image__link" aria-label="GOP Cowardice On Trump Takes Center Stage In Scathing New Supercut" aria-hidden="true" tabindex="-1" href="https://www.huffpost.com/entry/gop-cowards-meidas-touch-ad_n_5ee9f530c5b670a2f40edfb0" data-ylk="subsec:zone-politics;g:5b2b457f-1b37-3dd6-bd22-82401d90bca8;itc:0;cpos:2;po