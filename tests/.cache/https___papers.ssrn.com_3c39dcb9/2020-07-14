<script language="JavaScript">
    $(document).ready(function() {
        if($('.maint-msg').length > 0 && $('#advanced_search').val().length > 0)  {
            $('#advanced_search').focus();
        } else if ($('.maint-msg').length > 0 && $('#txtAuthorsName').val().length > 0) {
            $('#txtAuthorsName').focus();
        }
    });

    function CheckSubmitAS() {
        var strKeyWords = $.trim(document.frmSearch.advanced_search.value);
        var strName = $.trim(document.frmSearch.txtAuthorsName.value);
        var strNetworksSelected = $.trim(document.frmSearch.networkSelected.value);
        var messageToShow  =""
        var checkAll = false;
        //Set the varialbe checkAll
        if ($('#checkAllButton').prop('checked') ) {
            checkAll = true;
        } else {
            checkAll = false;
        }

        //If not select a search criteria then display a warning message.
        if ((strKeyWords.length < 1) && (strName.length < 1)) {
            alert("Please enter search criteria.");
            document.frmSearch.advanced_search.focus();
            return false;
        }

        //If not select any network then display a warning message.
        if ((strNetworksSelected.length) < 1 && !checkAll){
            







	
	    





            messageToShow = 'You must select at least one SSRN subject network to search.';
            alert(messageToShow);
            return false;
        }

        document.frmSearch.txtKey_Words.value = strKeyWords;
        document.frmSearch.isQuickSearch.value = "false";
        document.frmSearch.inSearch.value = "true";
        document.frmSearch.submit();
    }
</script>

