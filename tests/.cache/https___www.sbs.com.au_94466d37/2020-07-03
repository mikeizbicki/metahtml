          </ul>
        </div>
      </div>
    </div>
  </div>
  <div class="live-widget-content live-widget-panel">
    <div class="live-widget-inset-shadow top"></div>
    <div class="live-widget-inset-shadow bottom"></div>
    <div class="live-widget-loading-indicator live-widget-panel"></div>

    <div class="live-widget-panel menu">
      {{> liveWidgetMenuTemplate }}
    </div>

    <div class="live-widget-channel-content live-widget-panel">
      {{> liveWidgetContentTemplate}}
    </div>
  </div>
</script>

