                    elem.setAttribute('aria-hidden', 'true');
                    elem.setAttribute('tabindex', '0');
                    document.getElementById('ad-mobile-8').parentNode.appendChild(elem);
                    AdController.render('iqadtile8');
                }
            }
        }
    </script>
