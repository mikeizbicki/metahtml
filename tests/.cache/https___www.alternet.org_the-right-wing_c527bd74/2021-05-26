       }
       else {
        sidebar_sticky_element_2.removeAttribute('style');
       }
    } 
    else {
        sidebar_sticky_container_2.classList.remove('fixed-now');
    }
   }
}
window.Ithrottle = function (callback, limit) {
    var wait = false;
    return function () {
      if (!wait) {

        callback.apply(null, arguments);
        wait = true;
        setTimeout(function () {
          wait = false;
        }, limit);
      }
    }
  }

