window.REBELMOUSE_LOWEST_TASKS_QUEUE.push(function(){

    
        var subscription = Cookies.get("rs_admiral_subscription");
var user_type = null;
window.subscription = subscription;
window.user_type = user_type;

if (subscription != "active") {
  console.log("### ADMIRAL ### connecting to API");
  window.admiral = window.admiral ||
    function () {
      (admiral.q = admiral.q || []).push(arguments);
    };
  
  
  function onLoggedIn(input){
      Cookies.set("rs_admiral_subscription", "active", { expires: 7 });
      window.user_type = "subscriber";
      window.hideAds();
  }
  
  window.admiral("after", "transact.subscribed", onLoggedIn);
  
  window.admiral("after", "measure.detected", function (user) {
    if (user.subscribed) {
      console.log("### ADMIRAL ### user is subscribed");
      onLoggedIn();
    } else if (user.whitelisted) {
      console.log("### ADMIRAL ### user is whitelisted");
      window.user_type = "whitelisted";
    } else if (user.adblocking) {
      console.log("### ADMIRAL ### user is adblocking");
      window.user_type = "adblocking";
    } else {
      console.log("### ADMIRAL ### user is unknown");
      window.user_type = "general";
    }
  });
}

window.hideAds=function() {
    document.getElementById('cp-info-bar')?document.getElementById('cp-info-bar').remove():"" //infobar
    document.querySelector('.feed-network-wrap')? document.querySelector('.feed-network-wrap').remove():""; // ad on post sidebar
    document.querySelector('[id*="zergnet-widget"')? document.querySelector('[id*="zergnet-widget"]').remove():"" // zergnet on post body
  document.querySelector('.paypal')? document.querySelector('.paypal').remove():"";
    document.querySelector('[id*="rcjsload_940fc9"]')?  document.querySelector('[id*="rcjsload_940fc9"]').remove():"";
    //document.getElementById('rc-widget-d9572f').remove()
    document.getElementsByClassName('sidebar_sticky_container')[0].remove()
  document.getElementsByClassName('sidebar_sticky_container_2')[0]?document.getElementsByClassName('sidebar_sticky_container_2')[0].remove():"";
    var properAdUnit = document.getElementsByClassName('proper-ad-unit');
    while (properAdUnit.length > 0) {
        properAdUnit[0].parentNode.removeChild(properAdUnit[0]);
    }
  var gptAds = document.querySelectorAll('div[id*="div-gpt"]');
    while (gptAds.length > 0) {
        gptAds[0].parentNode.removeChild(gptAds[0]);
    }
   var adFreeButton= document.getElementsByClassName('ad-free-button_link');
    while (adFreeButton.length > 0) {
        adFreeButton[0].parentNode.removeChild(adFreeButton[0]);
    }

    var rsAdBlock = document.getElementsByClassName('rs_ad_block');
    while (rsAdBlock.length > 0) {
        rsAdBlock[0].insertAdjacentHTML('afterend', "<div class='rs_ad_msg' style='margin: 10px 0px; background: #eaeaea; padding: 10px;'><div class='header' style='margin: 5px 0px; font-size: 12px; color: #666; text-align:center;'>THANKS FOR SUBSCRIBING! ALL ADS REMOVED!</div></div>");
        rsAdBlock[0].parentNode.removeChild(rsAdBlock[0]);
    }
}
    

});

