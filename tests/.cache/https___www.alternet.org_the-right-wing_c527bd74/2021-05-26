                                    data-runner-img-md="https://www.alternet.org/media-library/eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpbWFnZSI6Imh0dHBzOi8vYXNzZXRzLnJibC5tcy8yNjQ3ODE5Ni9vcmlnaW4ucG5nIiwiZXhwaXJlc19hdCI6MTYzNjIyMTIwOH0.Vtpw61ABaORIY9eVjfzMvUj8K9cHVAPZOpffMc6PjiQ/image.png?width=600&quality=85&coordinates=114%2C0%2C60%2C0&height=400"
                                    data-runner-img-sd="https://www.alternet.org/media-library/eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpbWFnZSI6Imh0dHBzOi8vYXNzZXRzLnJibC5tcy8yNjQ3ODE5Ni9vcmlnaW4ucG5nIiwiZXhwaXJlc19hdCI6MTYzNjIyMTIwOH0.Vtpw61ABaORIY9eVjfzMvUj8K9cHVAPZOpffMc6PjiQ/image.png?width=600&quality=85&coordinates=114%2C0%2C60%2C0&height=400"
                                    data-runner-style='padding-bottom: 66.6666666667%;'
                                    style='padding-bottom: 66.6666666667%;'
                                        data-type="image"
