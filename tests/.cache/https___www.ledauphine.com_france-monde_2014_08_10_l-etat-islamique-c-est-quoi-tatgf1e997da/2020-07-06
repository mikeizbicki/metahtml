    // gestion du temps de lecture
    var readingTime = Math.ceil(parseInt(2130) / 1500);
    var str = readingTime.toString();
    document.getElementById('readingTime').innerHTML = str + ' min';

    initZoom();
    
    $(".GFWK_tagsList .lastTag a").on("click", function(e) {
        e.preventDefault();
        $(".tag.hidden").toggleClass("hidden");
		$(".lastTag").toggleClass("hidden");
    });
});
