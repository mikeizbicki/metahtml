<script>
    $(document).ready(function () {

        var RpUdate = "";
        var RpUdateObj;
        var diff = 0;
        var NowDate = new Date();
        var CommentsTime = "";
        var currMin = 60 * 1000;// 시 * 분 * 초 * 밀리세컨
        var currHour = 60 * 60 * 1000;// 시 * 분 * 초 * 밀리세컨
        var currDay = 24 * 60 * 60 * 1000;// 시 * 분 * 초 * 밀리세컨

        $("section.comments").find(".time").each(function () {

            RpUdate = $(this).val();
            RpUdateObj = new Date(RpUdate.substring(0, 4), RpUdate.substring(4, 6) - 1, RpUdate.substring(6, 8), RpUdate.substring(8, 10), RpUdate.substring(10, 12), RpUdate.substring(12, 14));
            diff = NowDate - RpUdateObj;
            diffMin = parseInt(diff / currMin);

            
            if (diffMin < 60) {
                CommentsTime = diffMin + "분 전";
            }
                
            else if (diffMin < 1440) {
                CommentsTime = parseInt(diff / currHour) + "시간 전";
            }
                
            else {
                CommentsTime = parseInt(diff / currDay) + "일 전";
            }
            $(this).before("<span>" + CommentsTime + "</span>");
        });

    });
</script>

    
