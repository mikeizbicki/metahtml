    <script>
      wbq.push(function renderVideoBlock() {
                var page = window.TMZ.select(window.TMZ.selectors.getPage);
        // there is a weird bug with iPhone 6+ on iOS 9.3.4 where the spinner gets revealed even after its
        // event listener has been removed, so for now this stupid flag can correct for that.
        var isResolved = false;
        if (!page.isTurbolinksPreview) {
          function displayPlayer(player) {
            var playerContainer = player.closest('.embed-responsive-item--kaltura.invisible');
            playerContainer.classList.remove('invisible');
          }
          function removeImageHolder() {
            var imageHolder = document.getElementById('video-player-eaea305e-e12f-559a-9e1d-7997a9906007-1200969318-image-holder');
            imageHolder.parentNode.removeChild(imageHolder);
          }
          function removeFooter() {
            var footer = document.getElementById('video-player-eaea305e-e12f-559a-9e1d-7997a9906007-1200969318-footer');
            if (footer) {
              footer.classList.add('hidden');
            }
          }
          function hideSpinner() {
            var spinner = document.getElementById('video-player-eaea305e-e12f-559a-9e1d-7997a9906007-1200969318-progress-spinner');
            spinner.classList.add('below');
            spinner.classList.remove('above');
          }
          function revealSpinner() {
            if (isResolved) {
              return;
            }
            var spinner = document.getElementById('video-player-eaea305e-e12f-559a-9e1d-7997a9906007-1200969318-progress-spinner');
            spinner.classList.add('above');
            spinner.classList.remove('below');
          }
          window.TMZ.videos.observe(document.getElementById('video-player-eaea305e-e12f-559a-9e1d-7997a9906007-1200969318'), {
                          onObserve: function() {
                document.getElementById('video-player-eaea305e-e12f-559a-9e1d-7997a9906007-1200969318-play-btn').addEventListener('mousemove', revealSpinner, { once: true });
              },
              onResolve: function(player) {
                isResolved = true;
                var playButton = document.getElementById('video-player-eaea305e-e12f-559a-9e1d-7997a9906007-1200969318-play-btn');
                playButton.removeEventListener('mousemove', revealSpinner);
                playButton.addEventListener('click', function() {
                  player.sendNotification('doPlay');
                  player.sendNotification('changeVolume', 0.75);
                  displayPlayer(player);
                  removeImageHolder();
                  removeFooter();
                  window.TMZ.dispatch(window.TMZ.actions.clickLink(
                    'https://www.tmz.com/videos/062620-jay-cutler-4809147-0-gzomom40/',
                    null,
                    'tmz:video:eaea305e-e12f-559a-9e1d-7997a9906007',
                    null,
                    {
                      section: 'blogroll',
                      name: 'video',
                      action: 'click'
                    }
                  ));
                }, { once: true });
                hideSpinner();
              }
                      });
        }
      });
    </script>
    </section><!--/#cb-dea457a41f66a6edad5fd50a37167c7f-->

