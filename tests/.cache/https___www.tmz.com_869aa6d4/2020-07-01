    <div class="video-block">
      <div class="w-100">
        <div class="embed-responsive embed-responsive-16by9">
          <div id="video-player-77650c9f-fb76-585d-bafb-055ed833d6d1-2042559761-progress-spinner" class="backdrop below">
            <div class="spinner"></div>
          </div>
          <div class="embed-responsive-item">
            <div id="video-player-77650c9f-fb76-585d-bafb-055ed833d6d1-2042559761-image-holder" class="video-block-image-holder">
