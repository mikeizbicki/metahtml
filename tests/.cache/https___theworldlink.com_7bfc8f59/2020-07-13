<script>
    $('#site-search-1817811-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-1817811').submit();
        return false;
    });

    $('#site-search-1817811').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-1817811 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-1817811 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-1817811-term').val() ){
            return true;
        }else{
            return false;
        }
    });</script>
