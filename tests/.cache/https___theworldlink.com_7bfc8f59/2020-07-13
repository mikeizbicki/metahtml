        $('#site-search-1817563').submit();
        return false;
    });

    $('#site-search-1817563').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-1817563 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-1817563 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-1817563-term').val() ){
            return true;
        }else{
            return false;
