  <script type="text/javascript">
    document.addEventListener("DOMContentLoaded", function() {
        tp = window.tp || [];
        var logInButtons = document.getElementsByClassName("menu-item-login");
        var myAccountButton = document.getElementById("menu-item-my-account");
        tp.push(["addHandler", "loginSuccess", function(data){
          for (var i = logInButtons.length - 1; i >= 0; i--) {
            logInButtons[i].children[0].innerHTML = "Log Out";
          }
          myAccountButton.hidden = false;
          document.cookie = "abrPianoUid="+ data['params']['uid'] +"; expires=Session; path=/";
          window.location.reload();
        }]);
        tp.push(["init", function() {
            if(tp.pianoId.isUserValid()){
              for (var i = logInButtons.length - 1; i >= 0; i--) {
                logInButtons[i].children[0].innerHTML = "Log Out";
              }
              myAccountButton.hidden = false;
            }else{
              myAccountButton.hidden = true;
              for (var i = logInButtons.length - 1; i >= 0; i--) {
                logInButtons[i].children[0].innerHTML = "Login";
              }
            }
            var loginFunction = function(e){
              e.preventDefault();
              if(tp.pianoId.isUserValid()){
                tp.pianoId.logout();
                document.cookie = "abrPianoUid=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
                for (var i = logInButtons.length - 1; i >= 0; i--) {
                  logInButtons[i].children[0].innerHTML = "Login";
                }
                myAccountButton.hidden = true;
                window.location.reload();
              }else{
                tp.pianoId.show({screen: "login"});
              }
            };
            for (var i = logInButtons.length - 1; i >= 0; i--) {
              logInButtons[i].onclick = loginFunction;
            }
        }]);
        tp.push(["addHandler", "customEvent", function(event, b, c, d) {
          switch (event.eventName) {
            case "meter-close-modal":
            if ((typeof event.params.source != 'undefined') && (event.params.source == "alreadyReceivedButton")) {
              var d = new Date();
              var exdays = 360; // expires after a year
              d.setTime(d.getTime() + (exdays*24*60*60*1000));
              var expires = "expires="+ d.toUTCString();
              document.cookie = "_pc_AlreadyReceivedNewsletter=true;" + expires + ";path=/";
            }
            break;
          }
        }]);

    });
  </script>
