<path style="fill:#231F20;" d="M336.9,189.6c-21,12.1-28.2,39-16,60c12.1,21,39,28.2,60,16c21-12.1,28.2-39,16.1-60  C384.8,184.7,357.9,177.5,336.9,189.6L336.9,189.6z"/>
<path style="fill:#231F20;" d="M251,319.4c-21-12.1-47.9-4.9-60,16.1s-4.9,47.9,16.1,60s47.8,4.9,60-16.1  C279.2,358.4,272,331.5,251,319.4z"/>
