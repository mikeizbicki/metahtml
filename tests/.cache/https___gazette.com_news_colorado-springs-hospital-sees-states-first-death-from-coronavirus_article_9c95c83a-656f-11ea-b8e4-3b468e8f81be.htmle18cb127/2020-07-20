                });
                
                var owl = $('#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal');
                owl.on('changed.owl.carousel', function(event) {if( !$('#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal .photo-container').eq(event.item.index).hasClass('engagement-stage') ){
                        setTimeout(function(){
                            $("#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal .owl-stage-outer").height( $('#carousel-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal .owl-item.active').height() );
                        }, 300);
                    }
                 
                 if( gallery_ad_9c95c83a_656f_11ea_b8e4_3b468e8f81be_photo_modal ){
                                $('#tnt-gallery-ad').hide();
                                try {
            
            var bOnlyDFP = false;
            if( typeof gptAdSlots != 'undefined' ){
                    bOnlyDFP = true;
                googletag.cmd.push(function(){
                        googletag.pubads().clear([gptAdSlots['fixed-big-ad-gallery']]);
                    });}} catch(e) {
            __tnt.log(e);
        }
                                gallery_ad_9c95c83a_656f_11ea_b8e4_3b468e8f81be_photo_modal = false;
                                $sGalleryAdInView = false;
                            }
                            
                            if( $('#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal .photo-container').eq(event.item.index).hasClass('photo-ad') ){
                                /** if any gallery in page is on ad slide and another gallery serves ad, advance last instance */
                                if( typeof $sGalleryAdInView !== 'undefined' && $sGalleryAdInView && $sGalleryAdInView !== '#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal'){
                                    $($sGalleryAdInView).data('owlCarousel').next();
                                }
                                var adHTML = $('#tnt-gallery-ad').detach().show();
                                $('#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal .photo-container').eq(event.item.index).find('.gallery-ad').append(adHTML);

                                try {
            
            var bOnlyDFP = false;
            if( typeof gptAdSlots != 'undefined' ){
                    bOnlyDFP = true;
                
                        googletag.cmd.push(function(){
                            googletag.pubads().clear([gptAdSlots['fixed-big-ad-gallery']]);
                            googletag.pubads().refresh([gptAdSlots['fixed-big-ad-gallery']]);
                        });}
                if( !bOnlyDFP && typeof TNCMS.AdManager != 'undefined' ){
                        TNCMS.AdManager.refresh({region: 'fixed-big-ad-gallery'});}
            } catch(e) {
            __tnt.log(e);
        }

                                gallery_ad_9c95c83a_656f_11ea_b8e4_3b468e8f81be_photo_modal = true;
                                /** set instance of carousel containing active ad slide. */
                                $sGalleryAdInView = '#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal';
                            }
                            if( gallery_changed_9c95c83a_656f_11ea_b8e4_3b468e8f81be_photo_modal ){
                                
                            }
                            gallery_changed_9c95c83a_656f_11ea_b8e4_3b468e8f81be_photo_modal = true;
                        

                        
                            var currItem = $('#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal .photo-container').eq(event.item.index).data('count');
                            if(currItem){
                                $('#carousel-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal .current-count').html(currItem);
                            }
                            $('.modal.in, #carousel-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal .item').removeClass('hover');
                        
                        
                });
                
                var photo_carousel_9c95c83a_656f_11ea_b8e4_3b468e8f81be_photo_modal = $('#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal').data('owlCarousel');
                $('#carousel-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal .photo-carousel-control.left').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_9c95c83a_656f_11ea_b8e4_3b468e8f81be_photo_modal.prev();
                });
                $('#carousel-9c95c83a-656f-11ea-b8e4-3b468e8f81be-photo-modal .photo-carousel-control.right').on(__tnt.client.clickEvent, function(e){
                    photo_carousel_9c95c83a_656f_11ea_b8e4_3b468e8f81be_photo_modal.next();
                });
            }); /*]]>*/</script>
<script>/*<![CDATA[*/ 
                // open photo modal
                jQuery(document).ready(function($){
                    $('[data-photo-target]').on('click', function(e){
                        e.preventDefault();
                        var item_target = $(this).data('target');
                        // get item index
                        var item_index = $(item_target +' '+$(this).data('photo-target')).parent().index();
                        // get current instance of carousel
                        var carousel_instance = $(this).data('instance');
                        var carousel_instance_data = $(carousel_instance).data('owlCarousel');
                        // fix non explict widths at viewports less than 900.
                        if(carousel_instance_data.width() < 320 & carousel_instance_data.viewport() < 900){
                            carousel_instance_data.onThrottledResize();
                        }
                        // load item from index
                        carousel_instance_data.to(item_index, 100, true);
                        // open modal
                        $(item_target).modal({'show':true});
                        __tnt.trackEvent({'category':'tnt-lightbox','action':'carousel-open', 'label':'article', 'value':'1'});
                        return false;
                    });
                    $('.photo-carousel-caption-toggle').on(__tnt.client.clickEvent, function(e){
                        var item_instance = $(this).data('instance');
                        $(item_instance +' .photo-carousel-caption-toggle').find('[data-fa-i2svg]').attr('data-icon', $(item_instance +' .photo-carousel-caption-toggle').find('[data-fa-i2svg]').attr('data-icon') == 'minus-circle' ? 'plus-circle' : 'minus-circle');
                    });
                }); /*]]>*/</script>
<script>/*<![CDATA[*/ 
                jQuery(document).ready(function($) {
            
                var gallery_ad_9c95c83a_656f_11ea_b8e4_3b468e8f81be = false;
                var gallery_changed_9c95c83a_656f_11ea_b8e4_3b468e8f81be = false;
                var sUrl =  window.location.href,
                        sUuid = sUrl.split("_").pop(),
                        sId = sUuid.substring(0, sUuid.indexOf('.')),
                        aInlineImages = document.querySelectorAll('.inline-image .image, .inline-image .expand');
                     for (i = 0; i < aInlineImages.length; ++i) {
                        var oImage = aInlineImages[i],
                            sInstance = oImage.getAttribute('data-instance'),
                            sTarget = oImage.getAttribute('data-target');
                        if (!oImage.classList.contains("tnt-asset-presentation-infographic")) {
	                        if (sInstance != null && sTarget != null){
	                            if (!sInstance.includes(sId)){
	                                oImage.setAttribute('data-instance', '#gallery-items-' + sId + '-photo-modal');
	                            }
	                            if (!sTarget.includes(sId)){
	                                oImage.setAttribute('data-target', '#photo-carousel-' + sId);
	                            }
	                        }
                        }
                     }
                $('#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be').owlCarousel({
                    items: 1,
                    autoHeight: true,
                    lazyLoad: true,
                    loop: false,
                    navRewind: true,
                    responsiveRefreshRate: 500,
                    
                    
                    dots : false,
                    
                    onTranslated:function(){
                        
                        
                        
                            __tnt.trackEvent($('#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be .photo-container').eq(this._current).data('track-object'));
                        

                        
                        

                        

                    }
                });
                
