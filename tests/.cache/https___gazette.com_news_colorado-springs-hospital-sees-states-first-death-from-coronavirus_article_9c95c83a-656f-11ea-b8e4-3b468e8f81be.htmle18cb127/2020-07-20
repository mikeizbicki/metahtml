                        }, 300);
                    }
                 
                 if( gallery_ad_9c95c83a_656f_11ea_b8e4_3b468e8f81be ){
                                $('#tnt-gallery-ad').hide();
                                try {
            
            var bOnlyDFP = false;
            if( typeof gptAdSlots != 'undefined' ){
                    bOnlyDFP = true;
                googletag.cmd.push(function(){
                        googletag.pubads().clear([gptAdSlots['fixed-big-ad-gallery']]);
                    });}} catch(e) {
            __tnt.log(e);
        }
                                gallery_ad_9c95c83a_656f_11ea_b8e4_3b468e8f81be = false;
                                $sGalleryAdInView = false;
                            }
                            
                            if( $('#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be .photo-container').eq(event.item.index).hasClass('photo-ad') ){
                                /** if any gallery in page is on ad slide and another gallery serves ad, advance last instance */
                                if( typeof $sGalleryAdInView !== 'undefined' && $sGalleryAdInView && $sGalleryAdInView !== '#gallery-items-9c95c83a-656f-11ea-b8e4-3b468e8f81be'){
                                    $($sGalleryAdInView).data('owlCarousel').next();
                                }
