        // fb auth 
        var fbAccessToken = ''; 
        var fbUserID = ''; 
        var fbUserName = '';  
        FBcomments.initFb(); 
        var nnFBloggedIn = check_nnCookie();  
   			if (nnFBloggedIn == true) {
   				fbLoginDisplay(true); 

   			}
   			else {
   				fbLoginDisplay(false); 
   			}

function inputFocus(thisFor, thisClass) {
    $('#'+ thisFor).focus(function() {
        $('.'+ thisClass).animate({ opacity: "0" }, "fast");
    }).blur(function() {
        if($(this).val() == "") {
            $('.'+ thisClass).animate({ opacity: "1" }, "fast");
        }
    });
}

