<div class="bootstrap-scope">
<!-- BOOTSTRAP SCOPE START -->

  <div class="wrapper">
    <div class="container fixed">
      
  <div class="clearfix"></div>

  <div class="row doc doc-document doc-published">
    <div class="col-md-3 doc-aside doc-details">
  <div class="row">
