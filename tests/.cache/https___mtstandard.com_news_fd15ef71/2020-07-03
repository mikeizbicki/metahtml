    $('#site-search-1766185-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-1766185').submit();
        return false;
    });

    $('#site-search-1766185').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-1766185 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-1766185 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-1766185-term').val() ){
            return true;
        }else{
            return false;
        }
    });
    
