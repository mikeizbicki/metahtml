<script>
window.getztg = function(cxId) {
  var newBucket = -1;
  if (cxId && cxId.length) {
    var newHash = 0;
    for (var i = 0; i < cxId.length; i++) {
      newHash = (newHash << 5) - newHash + cxId.charCodeAt(i);
      newHash|=0;
    }
    newBucket = Math.abs(newHash)%100;
  }
  else {
    newBucket = Math.floor(Math.random()*100);
  }
  return newBucket;
}
window.invokecxsphwidget = function() {
  var propenmo = window.propenmo1;
  if (typeof propenmo === "undefined") {
    propenmo = -1;
    console.error("propenmo -1");
  }

  var cX = window.cX = window.cX || {}; cX.callQueue = cX.callQueue || [];
  cX.CCE = cX.CCE || {}; cX.CCE.callQueue = cX.CCE.callQueue || [];

  var ztg = getztg( cX.getUserId() );

  var cxpageContext = { "categories": {} };
  cxpageContext["categories"]["sph-visitorcat"] = _data.visitorcat.toString(10);
  cxpageContext["categories"]["sph-propenmo"] = propenmo.toString(10);
  cxpageContext["categories"]["sph-bucket"] = ztg.toString(10);
  if(!geSpecialArticle || (geSpecialArticle && !isDesktopSafari() ) ) {
  cX.callQueue.push(['invoke', function() {
    cX.CCE.init(null, 'sph', '8ab0d3648952076495023cb6cb4678395a8640c8');
    cX.CCE.run({
      widgetId: '31caa3532b26ba81c608b5fb64a7f9b0dfccdddb',
      targetElementId: 'cx_31caa3532b26ba81c608b5fb64a7f9b0dfccdddb'
    },
    { context: cxpageContext });
  }]);
  }
}
window.initcxsphwidget = function() {
  if (window.hasinitcxsphwidget) {
    return;
  }
  window.hasinitcxsphwidget = true;

  if (_data.visitorcat >= 1) {
    if (!document.querySelector('#cx_31caa3532b26ba81c608b5fb64a7f9b0dfccdddb')) {
      console.log("_data bb", _data.visitorcat );
      var keywordsArea = document.querySelector(".field-name-body > div > div"); //.story-keywords
      var cxDiv = document.createElement("div");
      cxDiv.setAttribute('id', 'cx_31caa3532b26ba81c608b5fb64a7f9b0dfccdddb');
      var textnode = document.createTextNode(" ");
      cxDiv.appendChild(textnode);
      
      keywordsArea.appendChild(cxDiv);

      window.invokecxsphwidget();
    }
  } else {
    console.log("_data bb none");
  }
}
</script>
