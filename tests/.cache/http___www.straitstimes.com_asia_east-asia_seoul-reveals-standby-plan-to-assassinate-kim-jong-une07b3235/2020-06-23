<script>
var geSpecialArticle = false;
function isDesktopSafari() {
var desktopSafari = false;
if (navigator.userAgent) {
  var clientplatform = (navigator.userAgent).toLowerCase();
  var isSafari = false;
  if ((clientplatform.indexOf("safari") > -1) && (clientplatform.indexOf("chrome") == -1)) {
    isSafari = true;
  }
  if (isSafari && window.innerWidth >= 740) {
    console.log('this is safari desktop');
desktopSafari = true;
  }
}
return desktopSafari;
}
