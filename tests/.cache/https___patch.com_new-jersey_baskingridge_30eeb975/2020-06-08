<script type="text/javascript">var HBDEBUG=getParameterByName('adtech_debug');var HBSTART=(new Date()).getTime();var TIMEOUT=2000;var slot_container={};var googletag=googletag||{};googletag.cmd=googletag.cmd||[];(function($){logHB('START');var jwt=Cookies.get('patch_secret');var user=jwt?parseJwt(jwt):null;var isMember=user?user.member:null;var membership_enable=Drupal.settings.client_ads_dfp.membership_enable;var membership=(membership_enable)?isMember:0;var deviceType=getDeviceType();var all_ad_units=Drupal.settings.client_ads_dfp.ad_units;var ad_targeting=Drupal.settings.client_ads_dfp.ad_targeting;var requestManager={adserverRequestSent:false,enabledBidders:[],returnedBidders:[],allBiddersBack:function(){return this.returnedBidders.length===this.enabledBidders.length;},bidsBackForBidder:function(bidder){logHB('bidsBackForBidder: '+bidder);if(this.adserverRequestSent){logHB('bidsBackForBidder: already sent');return;}
this.returnedBidders.push(bidder);if(this.allBiddersBack()){this.sendAdserverRequest();}},sendAdserverRequest:function(){logHB('sendAdserverRequest');logHB('sendAdserverRequest: bidders returned = '+this.returnedBidders.toString());if(this.adserverRequestSent){logHB('sendAdserverRequest: [WARNING] already sent');return;}
this.adserverRequestSent=true;googletag.cmd.push(function(){if(Drupal.settings.client_ads_dfp.prebid_enable&&pbjs){pbjs.setTargetingForGPTAsync();}
