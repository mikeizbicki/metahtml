            <script type="text/javascript">var uploadaudio = {
    addMethod:{
        palabras_clave:     'Please, write at least 1 key word. i.e.: sports, football, league, refereeing.',
        nombre_programa:    'Please, show us the program, 3 characters minimum.',
        nombre_canal:       'Warning, the name is too short',
        is_available:       'The name is in use',
        eu_date:            'The date is not valid',
        nom_prog_req:       'Please, write the programs name',
        programa_required:  'Please, write the programs name',
		desc_prog_req:		'Please, write a detailed description.',
		subcat_prog_req:	'Por favor, selecciona 1 subcategora a la que pertenezca el programa.'
    },
    messages: {
        servername: {
            required:   'Por favor, espera a que finalice la subia del audio.'
