<script>
    $(document).ready(function(){

        // $("#gdpr-modal").modal({
        //     show: true
        // });
        // $('.gdpr-bottom').on("scroll", function(){
        //     $('.gdpr-bottom').hide();
        // });
        $('#gdpr-modal').modal().on('loaded.bs.modal', function(){
            $(this).parents('.modal-scrollable').scroll(function(){
                $('#gdpr-modal').modal('hide');
            });
        });
        // $(window).on('scroll',function(){
        //     $(".gdpr-bottom").hide();
        //     $.get("https://gb.ivoox.com/en/ajx-manage-policy_v9_acceptCookiePolicy_implicit_1.html", function(resp) {
        //         //window.location.reload();
        //         $('body').append(resp);
        //     });
        // });
        $(window).on("click",function(e) {

            var container = $(".gdpr-bottom");

            if (!container.is(e.target) && container.has(e.target).length === 0) { 
            //Se ha pulsado en cualquier lado fuera de los elementos contenidos en la variable container
                 $(".gdpr-bottom").hide();
                 $.get("https://gb.ivoox.com/en/ajx-manage-policy_v9_acceptCookiePolicy_implicit_1.html", function(resp) {
                //window.location.reload();
                    $('body').append(resp);
                });
            }
        });
    });

</script>
                    
