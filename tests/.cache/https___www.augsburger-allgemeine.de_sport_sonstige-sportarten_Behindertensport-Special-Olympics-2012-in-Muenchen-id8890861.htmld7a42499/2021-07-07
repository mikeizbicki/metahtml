	tp.push(["setContentAuthor", ""]);
	tp.push(["setContentSection", "sport_sonstige_Sportarten"]);
	tp.push(["setContentIsNative", "false"]);

	tp.push(["setCustomVariable", "contentDocumentId", "8890861"]);
	tp.push(["setCustomVariable", "contentEncodedUrl", "https%3A%2F%2Fwww.augsburger-allgemeine.de%2Fsport%2Fsonstige-sportarten%2FBehindertensport-Special-Olympics-2012-in-Muenchen-id8890861.html%3Fts%3D1625690310436"]);
	tp.push(["setCustomVariable", "contentEncodedUrlSuccess", "https%3A%2F%2Fwww.augsburger-allgemeine.de%2Fsport%2Fsonstige-sportarten%2FBehindertensport-Special-Olympics-2012-in-Muenchen-id8890861.html%3Fts%3D1625690310436%26loginSuccess%3Dtrue"]);
