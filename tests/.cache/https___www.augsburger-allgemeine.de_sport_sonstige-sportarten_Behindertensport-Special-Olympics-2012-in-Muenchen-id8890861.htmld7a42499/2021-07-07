	tp.push(["setCustomVariable", "contentExclusiveState", "false"]);
	tp.push(["setCustomVariable", "contentEvergreenState", "false"]);
	tp.push(["setCustomVariable", "contentIsComment", "false"]);
	tp.push(["setCustomVariable", "contentIsMultimedia", "false"]);
