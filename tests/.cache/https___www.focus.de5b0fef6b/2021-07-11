<script>
	var staerkerZusammenSlotId = 'staerker-zusammen-banner-630x162';
</script>
<script>
(function(){
    var element = document.querySelector(`#${staerkerZusammenSlotId}`);

    function visible() {
        element.classList.add('startAnimation');
        window.removeEventListener('scroll', onScroll);
    }

    function onScroll() {
        var position = element.getBoundingClientRect();
        if(position.top >= 0 && position.bottom <= window.innerHeight) {
            visible();
        }
    }

    onScroll();
    window.addEventListener('scroll', onScroll);
})();
</script><div class="block-line clearfix"></div>

