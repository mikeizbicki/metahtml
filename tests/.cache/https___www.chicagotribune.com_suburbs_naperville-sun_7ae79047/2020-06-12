            if (!window.oPageUnload) {
              oAv.push("trb_ad_300x250_1_/suburbs/naperville-sun/");
              oVa["trb_ad_300x250_1_/suburbs/naperville-sun/"] = ["NULL"];
            } else {
              oPageUnload('trb_ad_300x250_1_/suburbs/naperville-sun/', true);
            }
          }
        
        function loadAd() {
            dfpService.appendAd({
                id: 'triblocal_naperville_415',
                optimeraId: 'trb_ad_300x250_1_/suburbs/naperville-sun/',
                adType: 'cube',
                slotName: '/4011/trb.chicagotribune/triblocal/naperville',
                displayMobile: true,
                displayDesktop: true,
                isOOP: false,
                is5x1: false,
                dimensionsString: '[[300, 250]]',
                sizemapString: '',
                sizemapRefresh: 'false',
                refreshOnPhotoChange: false,
                adRefreshInterval: 0,
                includeInA9: true,
                includeInIndexExchange: true,
                loadImmediately: true
            }, false, true)
        }

        loadAd();
      
    })()
  </script> </div> </div> </section> </div> </div> <div class="pb-layout-item pb-chain pb-c-double-chain full" data-pb-name="Double Chain" data-pb-id="naperville-chain-2" id="crPHrG1fHu6C1s"> <div class="flex-grid"> <section class="col col-desktop-4 col-tablet-6 col-mobile-6"> <div class="flex-grid"> <div class="wrapper clearfix col col-desktop-6 col-tablet-6 col-mobile-6 pb-feature pb-layout-item pb-f-homepage-story-feed" data-pb-name="Feed-Driven Flex Feature" data-pb-curated="automated" data-pb-id="naperville-river-2" id="fGN2mv1fHu6C1s"> <ul class="flex-grid story--clln"> <li class="col col-desktop-6 col-tablet-6 col-mobile-6"> <div class="crd crd--cnt "> <div class="crd--cnt align-items-start flex-container-row justify-space-between"> <div class=" art width-100 spaced spaced-right spaced-lg flex-2-col river hddn"> </div> <div class="width-100 flex-container-column flex river"> <div class=""> <ul class="tag-list-wrapper list-unstyled flex-container-row spaced spaced-sm spaced-bottom hddn-tbl hddn-dktp hddn-mble hddn-mble " role="presentation"> <li> <a class="tag tag-outline tag-first" href="https://www.chicagotribune.com/suburbs/lake-county-news-sun/#nt=taxonomy-article"> Lake County News-Sun </a> </li> </ul> <h2 class="r-mb h5"><a class="no-u" href="/suburbs/lake-county-news-sun/ct-lns-ent-suburban-summer-fests-0410-20200417-u2xpt7lvofhfbmueb52757egxa-story.html">Suburban summer festivals canceled because of coronavirus: a running list</a></h2> </div> <p class="preview-text spaced spaced-top story-preview spaced-md hddn-mble hddn-dktp hddn-tbl hddn-mble">Here are suburban summer festival cancellations due to the coronavirus pandemic.</p> <div class="spaced spaced-md spaced-top byline-container flex-container-row align-items-center"> <div class="byline-image-container flex-container-row"> </div> <div class="byline-timestamp-container flex-container-column align-items-start"> <div class="byline-wrapper flex-container-row"> <span class="byline ">By <a class="upper