var status_result = $("#status_result");
var scan_submit = $("#scan_submit");
formValidateScan = $("#website_scan_tool");
formValidateScan.validate({
rules: {
site: {
required: true,
domain: true
}
},
messages: {
site: {
required: "Please enter url"
}
},
submitHandler: function(form) {
status_result.removeClass().addClass('status-result');
status_result.empty();
$('#scan_submit').addClass('green').val('Scanning');
status_result.hide();
status_result.removeClass('blink_text');
$( ".scan-widget .scan-widget-body .status-box .status-bar-wrap .status-bar" ).removeAttr('style');
$( ".scan-widget .scan-widget-body .status-box .status-bar-wrap .status-bar" ).removeClass('no-risk suspicious high-risk');
$( ".scan-widget .scan-widget-body .status-box .status-bar-wrap .status-bar" ).addClass('no-risk');
$( ".scan-widget .scan-widget-body .status-box .status-bar-wrap .status-bar" ).animate({
width: "100%"
}, 2000, function() {
// Animation complete.
$('#scan_submit').removeClass('green').val('Start the Scan');
status_result.show();
});
url = "https://hackercombat.com/wp-content/themes/hotmagazine/website-scan-tool-api.php";
param = formValidateScan.serializeArray();
$.ajax({
url: url,
type: 'POST',
data: param,
cache: false,
dataType: "json",
success: function (response) {
if (response.success && response.mssg == "successful" && response.add_css_class) {
$( ".scan-widget .scan-widget-body .status-box .status-bar-wrap .status-bar" ).removeClass('no-risk');
$( ".scan-widget .scan-widget-body .status-box .status-bar-wrap .status-bar" ).addClass(response.add_css_class);
if (response.add_css_class == 'suspicious' || response.add_css_class == 'high-risk'){
status_result.addClass('blink_text');
}
status_result.addClass(response.add_css_class);
status_result.html(response.display_text);
} else {
alert(response.mssg);
}
},
beforeSend: function() {
scan_submit.attr("disabled", "disabled");
scan_submit.css('opacity', 1);
},
complete: function() {
scan_submit.removeAttr("disabled");
},
error: function (response) {
alert('ajax error' + JSON.stringify(response));
}
});
return false;
}
});
btn_submit = $("#security_tip_btn");
formValidate = $("#security_tip_form");
formValidate.validate({
rules: {
name: {
required: true
},
email: {
required: true,
email: true
},
phone: {
required: true,
phone: true
}
},
messages: {
name: {
required: "Please enter name"
},
email: {
required: "Please enter email",
email: "Please enter valid email"
},
phone: {
required: "Please enter phone"
}
},
submitHandler: function(form) {
url = "https://hackercombat.com/wp-content/themes/hotmagazine/website-scan-tool-api.php";
param = formValidate.serializeArray();
$.ajax({
url: url,
type: 'POST',
data: param,
cache: false,
dataType: "json",
success: function (response) {
if(response.success && response.status=="successful") {
$(".valid").val('');
alert(response.mssg);
formValidate[0].reset();
} else if(response.hasOwnProperty('fieldname') && response.fieldname.length) {
$.each(response.fieldname, function(i,item) {
$("#"+item.key+"-error").html(item.mssg).show();
});
} else {
alert(response.mssg);
}
},
beforeSend: function() {
btn_submit.attr("disabled", "disabled");
btn_submit.css('opacity', 1);
},
complete: function() {
btn_submit.removeAttr("disabled");
},
error: function (response) {
alert('ajax error' + JSON.stringify(response));
}
});
return false;
}
});
