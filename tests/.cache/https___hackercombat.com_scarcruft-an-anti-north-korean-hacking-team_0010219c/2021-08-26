$(".subscribe-form-sidebar").validate({
rules: {
subscribe_full_name: {
required: true
},
subscribe_email: {
required: true,
email: true
},
subscribe_region: {
required: true
}
},
submitHandler: function (form) {
var url = "/wp-content/themes/hotmagazine/subscribe-now.php";
var param = $(form).serializeArray();
var validator = this;
var btn_submit = $(form).find('button[type=submit]');
$.ajax({
url: url,
type: 'POST',
data: param,
cache: false,
dataType: "json",
success: function (response) {
if (response.success) {
$(form)[0].reset();
alert("Thank you for subscribe with us!");
} else if (response.errors && typeof(response['errors']) == 'object') {
validator.showErrors(response['errors']);
}
},
beforeSend: function () {
btn_submit.attr("disabled", "disabled");
btn_submit.css('opacity', 1);
},
complete: function () {
btn_submit.removeAttr("disabled");
},
error: function (response) {
//console.log('ajax error' + JSON.stringify(response));
}
});
return false;
}
});
