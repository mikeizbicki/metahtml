	<script type="text/javascript">
	function setCookie(c_name,value,exdays){
		document.getElementById('message').style.display = 'none';		
		var exdate=new Date();
		exdate.setDate(exdate.getDate() + exdays);
		var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
		document.cookie=c_name + "=" + c_value + ";domain=.swedroid.se;path=/";;
	}
	function switchTheme(sheet) {
		document.getElementById("theme").setAttribute("href", "https://swedroid.se/wp-content/themes/swedroid/"+sheet);  
		setCookie ('theme', sheet, 90);
		if (sheet == 'global.css') {
			document.getElementById('whi').style.color = '#dc6424';
			document.getElementById('aut').style.color = '#fafbfb';
		} else if (sheet == 'global-dark.css') {
			document.getElementById('whi').style.color = '#fafbfb';
			document.getElementById('aut').style.color = '#dc6424';
		}
	}
	</script>
	
