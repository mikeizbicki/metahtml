    $('#site-search-1760601-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-1760601').submit();
        return false;
    });

    $('#site-search-1760601').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-1760601 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-1760601 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-1760601-term').val() ){
            return true;
        }else{
            return false;
        }
    });
    
