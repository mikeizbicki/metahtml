                };
            }

            var comment_count = 1;

            jQuery(function(){
                if(comment_count == 0){
                    toggle_comments();
                }
            });
        </script>

        <div class="comment_toggle" id="comment_toggle" onclick="toggle_comments();">Show Comments</div>
        
