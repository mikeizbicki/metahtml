<script>

  // function for grabbing a URL parameter
  function getUrlParameter(name) {
      name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
      var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
      var results = regex.exec(location.search);
      return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
  };

  // grab the EA-QA url parameter if it exists
  var everyActionQA = getUrlParameter('EA-QA');

  // if the param is set to true, lift the veil and show the hidden QA items
  if (everyActionQA  == 'true') {
     console.log('EA QA is live');
