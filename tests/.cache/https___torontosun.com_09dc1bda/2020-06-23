						(function($){
							$('.multisports .tabs li').on('click', function(){
								$('.multisports .tabs li').removeClass("active");
								$(this).addClass('active');
								var sport = $(this).attr("data-sport");
								$('.multisports .tab-content > div').removeClass("active");
								$('.multisports').find("#"+sport).addClass("active");
							});
						})(jQuery);
