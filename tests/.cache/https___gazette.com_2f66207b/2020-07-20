<div id="notificationsDrawer" class="hideDrawer">
    <span onclick="toggleDrawer()" class="close-btn"></span>
    <div class="push-notifications-nav">
        <a href="#pushNotifications" class="pushNavButton">Notifications</a>
        <a href="#pushSettings" class="pushNavButton active">Settings</a>
    </div>
    <div class="push-notifications-content">
        <div id="pushNotifications" style="display: none;">
            PUSH NOTIFICATIONS GO HERE
        </div>
        <div id="pushSettings">
            <div class="promo">
                <div class="promo-b">
                    <div class="text">
                        <img src="https://s3.amazonaws.com/assets.gazette/masthead_logo.png" alt="The Gazette Notifications" style="vertical-align: middle;"><p>Get up-to-the-minute news sent straight to your desktop.</p>
                        <a class="turnOnButton" href="/notifications/" target="_blank"><i class="fa fa-bell" aria-hidden="true"></i> Manage Notifications</a>
                        <br><a href="javascript:void()" onclick="parent.toggleDrawer();" class="noThanksButton">No thanks</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
