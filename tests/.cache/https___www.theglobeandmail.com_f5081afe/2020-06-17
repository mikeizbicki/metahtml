  let coronavirusNumbersWidget = (function () {
    function numFormat(num) {
      return new Intl.NumberFormat("en-US").format(Number(num));
    }
    function initWidget() {
      fetch(
        "https://covid-19-2020-prod-dc5q4g5x5w7l.s3.ca-central-1.amazonaws.com/summary.json"
      )
        .then(function (response) {
          return response.json();
        })
        .then(updateCoronavirusNumbersWidget);
    }
    function updateCoronavirusNumbersWidget(_data) {
      const totalCountCdaEl = document.querySelector(
        ".gi-coronavirus-numbers-widget"
      );
      totalCountCdaEl.innerHTML =
        '<a data-analyticsclick="{&quot;type&quot;:&quot;link&quot;,&quot;feature&quot;:&quot;coronavirusinfo&quot;,&quot;label&quot;:&quot;latest maps and charts&quot;,&quot;position&quot;:1,&quot;hierarchy&quot;:1}"  href="https://www.theglobeandmail.com/canada/article-coronavirus-cases-canada-world-map-explainer/"><span class="gi-coronavirus-numbers-widget_headline">Coronavirus has led to at least <strong>' +
        numFormat(_data.cases) +
        "</strong> cases, <strong>" +
        numFormat(_data.recoveries) +
        "</strong> recoveries  and <strong>" +
        numFormat(_data.deaths) +
        '</strong> deaths reported in Canada so far. <div class="gi-coronavirus-numbers-widget_cta">See the latest maps and charts.</div></span><span class="gi-coronavirus-numbers-widget_timestamp">As of ' +
        _data.updated +
        "</span></a>";
    }
    initWidget();
  })();
</script> <div class="c-card c-card--lead-story c-card--lead-story--section coronavirusinfo u-space-mt u-space-mb"> <div class="coronavirusinfo__image"> <picture class="coronavirusinfo__image"> <source srcset=" https://www.theglobeandmail.com/files/dev/uxtest/corona/canada3.webp " type="image/webp"/> <img src="https://www.theglobeandmail.com/files/dev/uxtest/corona/canada3.png" alt="Canada map: coronavirus" class="c-image" loading="lazy"/> </picture> </div> <div class="gi-coronavirus-numbers-widget"></div> <div class="coronavirusinfo--links"> <ul class="coronavirusinfo__links u-clearfix"> <li> <a href="https://www.theglobeandmail.com/world/article-coronavirus-covid19-latest-news-canada-explainer/" rel="external" data-analyticsclick='{"feature":"coronavirusinfo","type":"link","label":"live updates coronavirus guide","position":1,"hierarchy":2}'>live updates: what you need to know</a> </li> <li> <a href="https://www.theglobeandmail.com/newsletters?utm_source=onsite&utm_medium=coronavirusinfo&utm_campaign=Corona_mkt&utm_term=newsletter&utm_content=corona#coronavirus-update" data-analyticsclick='{"feature":"coronavirusinfo","type":"link","label":"sign up daily newsletter","position":1,"hierarchy":2}'>sign up: coronavirus newsletter</a> </li> <li> <a href="https://www.theglobeandmail.com/canada/article-coronavirus-rules-by-province-physical-distancing-open-closed/ " rel="external" data-analyticsclick='{"feature":"coronavirusinfo","type":"link","label":"rules and guidelines by province","position":1,"hierarchy":2}'>Rules and guidelines by province</a> </li> </ul> </div> </div></div> </div> </div> <div class="u-wrapper u-space-mtn u-space-mbn pb-feature pb-layout-item pb-f-generic-text" id="fwFFxu1I5aPc2s"> <div class="gtext"> <div class="gtext-body"><style>

