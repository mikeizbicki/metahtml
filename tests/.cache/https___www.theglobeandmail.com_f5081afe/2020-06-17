  ;(function() {
    window._dw = window._dw || [];
    
      function dwSegment_productscem1614() {
        window._dw.push(['global', 'before', function onBefore(api) {
          api.segment('unsub', function () {
  const darwinState = getDarwinState() || {};
  const isNotSubscriber = !getSegment('identity.role.id', 2);
  const isNotPb = window.location.href.indexOf('/ellipsis/') === -1 && window.location.href.indexOf('/pb/') === -1;
  const isQA = window.location.href.indexOf('CEMQA1614') > -1;
  const isNotInteractive = window.location.href.indexOf('/article') > -1 ? (!getSegment('article.pagesubtype', 'interactive') && !getSegment('article.pagesubtype', 'longread')) : true;
  const isNotProductPage = !getSegment("page.hierarchy", "globeproducts");

  var count = count || 1;
  if (darwinState.hasOwnProperty('cem1614') && count === 1 && darwinState.cem1614.variant) {
    count += 1;
    setDarwinVariant('products-cem1614', darwinState.cem1614.variant);
  }

  return notIE && (isQA || isNotSubscriber) && isNotPb && isNotInteractive && isNotProductPage;
});
        }]);
      }
      window.tgam.darwin.tests.segments.push(dwSegment_productscem1614);
    
    
      function dwExecute_productscem1614(data) {
        window._dw.push(['global', 'ready', function onReady(api) {
          var variant = api.get("products-cem1614");
          var variantData = data;
          const isHermes = getSegment('hermesInScope', true);
const meterCount = localStorage.getItem('tgam.keytar.wall.subscriber.free');
const isPaywallRightArticle = isHermes ? (getSegment('hermesData.wall', 'redwall')) : (getSegment(
  'article.paywallStatus', 'red') || (getSegment('article.paywallStatus', 'yellow') && meterCount !== null &&
  JSON.parse(meterCount).split(/[(,*?)]/).length === 6));

var type = isPaywallRightArticle ? 'paywall' : 'pencil';

if (variant.is('control')) {
  console.info('products:cem-1614:control');
  setUnitState("cem1614", "control");
  getpropensity('control');
  updateSubscriberLinks(type,'control');
}

if (variant.is('experience')) {
  console.info('products:cem-1614:experience');
  setUnitState("cem1614", "experience");
  getpropensity('experience');
  updateSubscriberLinks(type,'experience');
}

function getpropensity(variant) {
  var testVal = [type,variant];
  var sophiId = getCookie('sp_tgamid.a05e');
  if (sophiId && sophiId.split('.')[0]) {
    getapi("https://tgamuserprofile.theglobeandmail.ca/main/userprofilescore?sophiId=" + sophiId.split('.')[0], testVal);
  } else {
    pushMultiOfferAnalytics(-1, testVal);
  }
}
        }]);
      }
      
      window.tgam.darwin.tests.before.push(dwExecute_productscem1614);
    
  })();
  </script> <script>
  const isMobileView = document.documentElement.clientWidth < 768;
  var darwinState = {};

  // Update the subscriber links
  function updateSubscriberLinks(type, variant) {
    darwinState = getDarwinState() || {};
    var userpropensity = darwinState.cem1614.propensity || 'low';

    // update subscriber btn
    var subsbtn = 'subscribermenu';
    var redirectURL = variant === 'control' ? "https://subscribe.theglobeandmail.com/#/digital?intcmp=" + subsbtn +
      "-cem1614_control_" + userpropensity : '/globe-products/dualoffer/?intcmp=' + subsbtn +
      '-cem1614_experience_' + userpropensity;

    updateSubscriberBtn(redirectURL);

    redirectURL = variant === 'control' ? "https://subscribe.theglobeandmail.com/#/digital?intcmp=" + type +
      "-cem1614_control_" + userpropensity : '/globe-products/dualoffer/?intcmp=' + type + '-cem1614_experience_' +
      userpropensity;

    // paywall
    if (type === 'paywall') {
      updatePaywall(redirectURL);


      // pencil
    } else {
      updatePencil(redirectURL);
    }
  }

  // update subscriber btn
  function updateSubscriberBtn(redirectURL) {
    if (isMobileView) {
      document.getElementById('c-site-header-subscribe-button').id = 'c-site-header-subscribe';
      document.getElementById("c-site-header-subscribe").setAttribute("onclick", "window.location.href='" +
        redirectURL + "'");
    } else {
      document.getElementById('c-site-header-subscribe-button').setAttribute('onclick', 'updateSubscriberMenu(\'' +
        redirectURL + '\')');
    }
  }

  // update paywall
  function updatePaywall(redirectURL) {

    // update paywall URL
    const paywall = {
      'red': {
        'name': 'red',
        'type': 'hardPaywall'
      },
      'thresh': {
        'name': 'thresh',
        'type': 'meteredPaywall'
      }
    };
    const paywallType = getSegment('article.paywallStatus', 'red') || isHermes ? paywall.red.type : paywall.thresh
      .type;

    var paywallInnerHTML = '<div class="c-keytar-header"> This content is available to <span>globeandmail.com</span> subscribers. </div> <div class="c-keytar-content '+darwinState.cem1614.variant+'"> <div class="c-keytar-content-subscriber__container"> <div class="c-keytar-content-subscriber__header">Support Quality Journalism</div> <div class="c-keytar-action"> <div class="c-keytar-copy">Get full digital access to the Globe and Mail</div> <div class="c-keytar-action__info"> <p>Starting at</p> </div> <div class="c-keytar-action__price"></div> <div class="c-keytar-action__info"> <p>per week</p> <p class="u-uppercase">for the first 24 weeks</p> </div> <a href="' + redirectURL + '" class="c-button c-button--utility-yellow-secondary c-button--wall-width c-button--no-border"> <span class="c-button__text"> </span> </a> </div> <div class="c-keytar-content-subscriber__img"> <img class=" js-lazyimage--loaded" src="https://www.theglobeandmail.com/files/cem/gufunnel/gu-header.png" alt="From left to right, globeandmail.com on mobile, desktop and tablet"> </div> <div class="c-keytar-content-subscriber__text"> <p class="c-keytar-content-subscriber__text-h1"> Powerful. Important. Impactful. </p> <p class="c-keytar-content-subscriber__text-h2"> The value of quality journalism </p> <hr> <p class="c-keytar-content-subscriber__text-h2"> When you subscribe to globeandmail.com, you get access to: </p> <ul class="c-keytar-content-subscriber__text-list"> <li id="odd">Every story ever published on globeandmail.com</li> <li> Investigative stories of national and international importance </li> <li id="odd">First-hand accounts of US and world politics</li> <li>The most experienced business journalists in the country</li> <li id="odd"> The Globe\' s unwavering commitment to provide the truth </li> </ul> </div> </div> </div> <div class="c-keytar-footer"> <a class="c-keytar-footer__logo" href="https://www.theglobeandmail.com"> <img src="https://www.theglobeandmail.com/files/cem/logos/GMLogo.svg" alt="The Globe and Mail"> </a> <div class="c-keytar-footer__content"> <p> Already a print newspaper subscriber? <a href="https://subscribe.theglobeandmail.com/#/multiplatform" class="js-keytar-algt"> Get full access to globeandmail.com </a> </p> <p> Already subscribed to globeandmail.com? <a href="https://sec.theglobeandmail.com/user/#/login" class="js-keytar-algt"> Log in to keep reading </a> </p> </div> </div> <style> .control .c-button__text::after{ content: "Start Today"; } .experience .c-button__text::after{ content: "See my options"; } .experience .c-keytar-copy { font-size: 25px; padding-bottom: 10px; } .experience .c-keytar-action__price::after { content: "$1.49"; } .control .c-keytar-action__price::after { content: "Just $1.99"; } .experience .c-keytar-action__info:nth-of-type(2) { text-transform: uppercase; font-size: 23px; } .control .c-keytar-copy, .control .c-keytar-action__info:nth-of-type(2), .experience .u-uppercase { display: none; } .c-button--wall-width { width: 280px; } @media (max-width: 768px) { .c-button--wall-width { margin-top: 10px; } } </style>';
      var paywallTemplate = function () {
      return paywallInnerHTML
    };

    setKeytarWall(paywallType, 'teaser', {
      template: paywallTemplate
    });
  }

  // update pencil
  function updatePencil(redirectURL) {
    if (darwinState.cem1614.variant === 'experience') {
      // update pencil styles
      document.getElementById('subspencil').className = "subsoffer";
      document.getElementsByClassName('o-sub-pencil__cost')[0].parentElement.className =
        'l-col-sm-8 l-col-md-11 l-col-lg-12';
      var pencilprice_elem = document.getElementsByClassName('o-sub-pencil__cost__just-text')[0];
      pencilprice_elem.innerHTML = 'Get full digital access to<br>The Globe and Mail <span>starting at</span>'
      document.getElementsByClassName('o-sub-pencil__cost')[0].prepend(pencilprice_elem);
      document.getElementsByClassName('o-sub-pencil__cost__just-price')[0].innerHTML = '$1.49';
      document.getElementsByClassName("o-sub-pencil__cost__per")[0].innerHTML = "<span>per week</span>";
      document.getElementsByClassName('o-sub-pencil__offer__cost-xs')[0].innerHTML = 'Starting at $1.49 per week';
    }
    // update pencil URL
    document.getElementById('bottom_subspencil-n').getElementsByTagName('button')[0].setAttribute('onclick',
      'window.open("' + redirectURL + '")');
    document.getElementById('mb-fixedpencil').getElementsByTagName('a')[0].href = redirectURL;

  }

  // display analytics push
  function pushMultiOfferAnalytics(propensity, testVal) {

    // check propensity
    var propensityRange = "low";

    switch (true) {
      case (propensity >= 31 && propensity <= 70):
        propensityRange = "med";
        break;
      case (propensity >= 71 && propensity <= 100):
        propensityRange = "high";
        break;
      default:
        propensityRange = "low";
        break;
    }

    setDarwinState('cem1614', 'propensity', propensityRange);

    analyticsLayer.push({
      "event": "clickEvent",
      "clickEvent": {
        "testId": "cem1614",
        "feature": testVal[0],
        "action": "display",
        "type": "test",
        "label": "display",
        "variantName": testVal[1],
        "segmentName": "nonsub"
      }
    });
  }

  // update SubscriberMenu
  function updateSubscriberMenu(redirectURL) {
    if (document.getElementsByClassName('c-package--subscribe')[0]) {
      const subscriberMenu = document.getElementsByClassName("c-card__grid");
      // subscriber menu
      if (subscriberMenu) {
        subscriberMenu[1].href = redirectURL;
        if (darwinState.cem1614.variant === 'experience') {
          subscriberMenu[3].href = redirectURL;
          subscriberMenu[4].href = redirectURL;
        }
      }
    } else {
      setTimeout(function () {
        updateSubscriberMenu(redirectURL);
      }, 200);
    }
  }

