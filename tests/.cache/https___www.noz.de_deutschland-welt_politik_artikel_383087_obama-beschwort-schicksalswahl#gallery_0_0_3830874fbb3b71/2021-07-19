                                    </script><!-- / AdDefend --><!-- stickyHeader --><script>
            if(nozUtils.isMobile()) {
                // check window width (scrollbar included)
                var checkWindowWidth = function(MqL) {
                    var e = window,
                        a = 'inner';
                    if (!('innerWidth' in window)) {
                        a = 'client';
                        e = document.documentElement || document.body;
                    }
                    if (e[a + 'Width'] >= MqL) {
                        return true;
                    } else {
                        return false;
                    }
                };

                window.stickyHeader = (function() {
                    var body = document.querySelector('body');
                    var header = document.querySelector('.row.header');
                    var useAnimationFrame = (window.requestAnimationFrame !== undefined && window.cancelAnimationFrame !== undefined);
                    var headerHeight = header.getBoundingClientRect().height;
                    var scrollAnimationFrame = 0;

                    // expandable = document.querySelector('#active-expandable iframe');
                    // expandable = document.querySelector('#nozmhn_inread iframe');

                    // var logo = header.querySelector('img.logo');
                    // var nozMediaLogo = document.createElement('img');
                    // nozMediaLogo.id = 'noz-media-logo';
                    // nozMediaLogo.className = 'no-hover';
                    // nozMediaLogo.src = logo.dataset.nozMediaSrc;
                    // logo.insertAdjacentHTML('afterend', nozMediaLogo.outerHTML);

                    var stickyHeader = function() {
                        if (header) {
                            var expandable = document.querySelector('#nozmhn_active_expandable_ad_1 iframe');
                            if (expandable) {
                                var expandableTop = expandable.getBoundingClientRect().top + 'px';
                            }
                            var lastScrolledPosition = 0;

                            var stickyHeaderFunc = function() {
                                var scrollTop = -(document.querySelector('html').getBoundingClientRect().top);

                                if (checkWindowWidth(767)) {
                                    nozUtils.removeClass(header, 'sticky');
                                    body.style.paddingTop = 0;
                                } else {
                                    if (scrollTop > lastScrolledPosition) {
                                        //console.log('DOWN');
                                        lastScrolledPosition = scrollTop;
                                        if (scrollTop > headerHeight) {
                                            nozUtils.setClass(header, 'slide-top');
                                            if (typeof expandable !== "undefined" && expandable !== null) {
                                                expandable.style.top = "0px";
                                                if (scrollTop >= window.screen.height * 1.5) {
                                                    expandable.style.position = "sticky";
                                                }
                                            }
                                        }
                                    } else if (scrollTop < lastScrolledPosition) {
                                        //console.log('UP');
                                        lastScrolledPosition = scrollTop;
                                        nozUtils.removeClass(header, 'slide-top');
                                        if (typeof expandable !== "undefined" && expandable !== null) {
                                            expandable.style.top = expandableTop;
                                        }
                                    }
                                }
                            };

                            window.addEventListener('resize', function() {
                                if (useAnimationFrame) {
                                    window.cancelAnimationFrame(scrollAnimationFrame);
                                    scrollAnimationFrame = window.requestAnimationFrame(stickyHeaderFunc);
                                } else {
                                    window.clearTimeout(scrollAnimationFrame);
                                    scrollAnimationFrame = window.setTimeout(stickyHeaderFunc, 50);
                                }
                            });

                            window.addEventListener('scroll', function() {
                                if (useAnimationFrame) {
                                    window.cancelAnimationFrame(scrollAnimationFrame);
                                    scrollAnimationFrame = window.requestAnimationFrame(stickyHeaderFunc);
                                } else {
                                    window.clearTimeout(scrollAnimationFrame);
                                    scrollAnimationFrame = window.setTimeout(stickyHeaderFunc, 50);
                                }
                            });
                        }
                    };

                    document.addEventListener('DOMContentLoaded', function() {
                        body.style.paddingTop = headerHeight + 'px';
                        nozUtils.setClass(header, 'sticky');
                        stickyHeader();
                    });

                    return stickyHeader;
                })();
            }
        </script><!-- /stickyHeader End --><script>
                                            $(function() {
    if ($('.comment-count').length > 0) {
        $('.comment-count').each(function() {
            var elem = $(this);
            $.ajax({
                url: elem.data('url')
            }).done(function (response) {
                if(response.success) {
                    elem.html(response.short);
                    if(response.long != $('.comment-long-count').html()) {
                        $('.comment-long-count').html(response.long);
                    }
                    if(response.bubble != $('.comment-bubble-big .icon-comment-text').html()) {
                        $('.comment-bubble-big .icon-comment-text').html(response.bubble);
                    }
                }
            });
        });
    }
});

                                /*
 * jQuery autoResize (textarea auto-resizer)
 * @copyright James Padolsey http://james.padolsey.com
 * @version 1.04
 *
 * EDITED BY Amir El Sayed to load only when needed
 * elsayed@basecom.de
 */

(function($){
    $.fn.autoResize = function () {
        return this.each(function () {
            var element = $(this);

            if (element.data('autoResize')) {
                return;
            }

            var autoResize = new AutoResize(this);

            element.data('autoResize', autoResize);
        });
    };

    var AutoResize = function (element) {
        var elem = $(element);
        var obj = this;

        // Just some abstracted details,
        // to make plugin users happy:
        var settings = {
            onResize : function(){},
            animate : true,
            animateDuration : 150,
            animateCallback : function(){},
            extraSpace : 0,
            limit: 500
        };

        // Only textarea's auto-resize:
        // Get rid of scrollbars and disable WebKit resizing:
        var textarea = elem.css({resize:'none','overflow-y':'hidden'}),

        // Cache original height, for use later:
            origHeight = textarea.height(),

        // Need clone of textarea, hidden off screen:
        clone = (function(){

            // Properties which may effect space taken up by chracters:
            var props = ['height','width','lineHeight','textDecoration','letterSpacing'],
                propOb = {};

            // Create object of styles to apply:
            $.each(props, function(i, prop){
                propOb[prop] = textarea.css(prop);
            });

            // Clone the actual textarea removing unique properties
            // and insert after original textarea to retain HTML error message on the original text field:
            return textarea.clone().removeAttr('id').removeAttr('name').css({
                position: 'absolute',
                top: 0,
                left: -9999
            }).css(propOb).attr('tabIndex','-1').removeAttr('required').insertAfter(textarea);

        })(),

        lastScrollTop = null,
        updateSize = function() {

            // Prepare the clone:
            clone.height(0).val(elem.val()).scrollTop(10000);

            // Find the height of text:
            var scrollTop = Math.max(clone.scrollTop(), origHeight) + settings.extraSpace,
                toChange = elem.add(clone);

            // Don't do anything if scrollTip hasen't changed:
            if (lastScrollTop === scrollTop) { return; }
            lastScrollTop = scrollTop;

            // Check for limit:
            if ( scrollTop >= settings.limit ) {
                elem.css('overflow-y','');
                return;
            }
            // Fire off callback:
            settings.onResize.call(this);

            // Either animate or directly apply height:
            settings.animate && textarea.css('display') === 'block' ?
                toChange.stop().animate({height:scrollTop}, settings.animateDuration, settings.animateCallback)
                : toChange.height(scrollTop);
        };

            // Bind namespaced handlers to appropriate events:
            textarea
                .off('.dynSiz')
                .on('keyup.dynSiz', updateSize)
                .on('keydown.dynSiz', updateSize)
                .on('change.dynSiz', updateSize);

    };

})(jQuery);

$(window).on('load', function() {
    $('textarea[data-setting=autoresize]').autoResize();
});

$(document).ajaxComplete(function() {
    $('textarea[data-setting=autoresize]').autoResize();
});
                                (function ($) {
    var CharCounter = function (element, minimum, maximum, warning) {
        var elem = $(element),
            obj = this,
            formButton = elem.closest('form').find('button[type="submit"], submit');

        if (typeof minimum === 'undefined') {
            minimum = 1;
        }

        if (minimum > 0) {
            formButton.prop('disabled', true);
        }

        elem.on('paste keyup', function (e) {
            var counter = $('#counter'),
                charCount = $(this).val().length;

            if (charCount < minimum) {
                formButton.prop('disabled', true);
            } else {
                formButton.prop('disabled', false);
            }

            if (charCount >= maximum) {
                e.preventDefault();
                if (charCount > maximum) {
                    $(this).val($(this).val().slice(0, maximum));
                }
            }

            if (charCount >= maximum - warning) {
                if (counter.length === 0) {
                    $(this).after('<div id="counter" class="errors"></div>');
                    counter = $('#counter');
                }
                var remaining = maximum - charCount;
                counter.text('noch ' + remaining + ' Zeichen');
                elem.trigger('countLow');
            } else {
                if (counter.length !== 0) {
                    counter.remove();
                }
            }
        });
    };

    $.fn.charCounter = function (minimum, maximum, warning) {
        return this.each(function () {
            var element = $(this);

            if (element.data('charCounter')) {
                return;
            }

            var charCounter = new CharCounter(this, minimum, maximum, warning);

            element.data('charCounter', charCounter);
        });
    };
})(jQuery);
                                $('body').on('click', '.answer', function (e) {
    e.preventDefault();
    var tmp = $(this).attr('id').split('_');
    var parent = $(this).parent().parent();
    if(parent.find('.user-comment form.comment').length > 0) {
        parent.find('.user-comment form.comment textarea').focus();
        return false;
    }
    $(this).nozAjax({
            url: '/comments/' + tmp[0] + '/comment/' + tmp[1]
        },
        function (response) {
            parent.append(response);

            var level2 = parent.find('.level-2');

            if (level2.length > 0) {
                var textarea = level2.find('textarea').last();
                $('html, body').animate({
                    scrollTop: (textarea.offset().top - 200)
                }, 500);

                textarea.focus();
            }
        },
        false
    );
    return false;
});

$('body').on('click', '.pagination a', function () {
    $('html, body').animate({
        scrollTop: $('#comments_container').offset().top
    }, 500);
});

$('body').on('click', '.report_comment', function (e) {
    var $this = $(this);
    e.preventDefault();
    $(this).closest('form').nozAjax(
        {url: $(this).closest('form').attr('action')},
        function () {
            $this.parent('span').html('gemeldet');
        },
        true
    );
});

$(document).ajaxComplete(function () {
    $('.comment-wrapper textarea').charCounter(0, 2000, 100);
});

(function () {
    var commentContainer = $('.comments');
    commentContainer.paginator({
        position: 'bottom',
        url: commentContainer.data('ajaxUrl'),
        pagerTemplate: '<div class="pagination pagination-right"><ul></ul></div>',
        show: 'needed',
        page: commentContainer.data('page')
    });
    if (typeof(Storage) !== "undefined") {
        var comment = localStorage.getItem('commentToPost');
        if (comment !== null) {
            comment = JSON.parse(comment);
            nozUtils.sendAjaxRequest(comment['url'], comment['data'], 'POST', function (response) {
                if (response['success'] === true) {
                    location.reload(true);
                }
            }, true);
            localStorage.removeItem('commentToPost');
        }
    }
})();

//# sourceURL=http://NOZBasketScripts/nonCoreJS/comments.js
                                (function () {
    $(document).ajaxComplete(function() {
        $('#weiterleiten_box').find('textarea').charCounter(0, 500, 50);
    });

    $('.btn-mail-share').on('click', function (e) {
        e.preventDefault();
        var elem = $(this);
        $(this).nozAjax({
            url: elem.attr('href')
        }, function (data) {
            $('html, body').animate({
                scrollTop: 0
            }, "slow");
            $('body').append('<div class="overlay"></div>').append(data);
            $('body > .overlay').addClass('overlayForm').on('click', function() {
                closeBox();
            });

            function closeBox() {
                $('body > .overlay').remove();
                $('#weiterleiten_box').remove();
            }

            function initializeBox() {
                $('#weiterleiten_box form').on('submit', function (e) {
                    e.preventDefault();
                    var recommendBox = $('#weiterleiten_box');
                    recommendBox.find('span.label-success').remove();
                    $(this).nozAjax({
                        url: elem.attr('href')
                    }, function (response) {
                        if (response['success']) {
                            $('#weiterleiten_box form').html($(response['form']).find('form').html());
                            recommendBox.find('.recommend-body').hide();
                            document.body.scrollTop = 0;
                            var successText = $('<div class="alert alert-success">Die Nachricht wurde erfolgreich gesendet.</div><a href="#" class="refresh-modal"><i class="icon-double-angle-left"></i> den Artikel nochmals empfehlen</a> ');
                            recommendBox.prepend(successText);
                            recommendBox.find('.refresh-modal').on('click', function () {
                                recommendBox.find('.recommend-body').show();
                                successText.remove();
                            });
                            recommendBox.find('.close-box').on('click', function () {
                                closeBox();
                            });
                        } else {
                            recommendBox.replaceWith(response);
                            initializeBox();
                        }
                    }, true);
                });

                $('#weiterleiten_box > .close-button').on('click', function () {
                    closeBox();
                });
            }

            initializeBox();

        });
    });
})();
                                (function () {
    $('.social-media-bar').find('.social-popover').popover({
        trigger: 'manual',
        animation: false
    }).on('click', function () {
        $('.social-media-bar').find('.social-popover').popover('destroy');
        $(this).popover('show');
    });
    $('body, html').on('click touchstart', function (e) {
        if (
            e.originalEvent
            && !$(e.originalEvent.target).hasClass('social-popover')
            && !$(e.originalEvent.target).hasClass('popover')
            && $(e.originalEvent.target).parents('.social-popover, .popover').length === 0
        ) {
            $('.social-media-bar').find('.social-popover').popover('destroy');
        }
    });

    $('.social-media-bar').on('openBar', function () {
        //Mailshare
        $(this).find('.btn-mail-share').off('click').on('click', function (e) {
            e.preventDefault();
            var elem = $(this);
            $(this).nozAjax({
                url: elem.attr('href')
            }, function (data) {
                $('html, body').animate({
                    scrollTop: 0
                }, "slow");
                $('body').append('<div class="overlay"></div>').append(data);
                $('body > .overlay').addClass('overlayForm').on('click', function () {
                    closeBox();
                });

                function closeBox() {
                    $('body > .overlay').remove();
                    $('#weiterleiten_box').remove();
                }

                function initializeBox() {
                    $('#weiterleiten_box form').on('submit', function (e) {
                        e.preventDefault();
                        var recommendBox = $('#weiterleiten_box');
                        recommendBox.find('span.label-success').remove();
                        $(this).nozAjax({
                            url: elem.attr('href')
                        }, function (response) {
                            if (response['success']) {
                                $('#weiterleiten_box form').html($(response['form']).find('form').html());
                                recommendBox.find('.recommend-body').hide();
                                document.body.scrollTop = 0;
                                var successText = $('<div class="alert alert-success">Die Nachricht wurde erfolgreich gesendet.</div><a href="#" class="refresh-modal"><i class="icon-double-angle-left"></i> den Artikel nochmals empfehlen</a> ');
                                recommendBox.prepend(successText);
                                recommendBox.find('.refresh-modal').on('click', function () {
                                    recommendBox.find('.recommend-body').show();
                                    successText.remove();
                                });
                                recommendBox.find('.close-box').on('click', function () {
                                    closeBox();
                                });
                            } else {
                                recommendBox.replaceWith(response);
                                initializeBox();
                            }
                        }, true);
                    });

                    $('#weiterleiten_box > .close-button').on('click', function () {
                        closeBox();
                    });
                }

                initializeBox();
            });
        });

        $(this).find('.social-feedback').on('click', function (e) {
            e.preventDefault();
            $('#give-feedback').trigger('click');
        });
    });

    $('.favorite').on('click', function (e) {
        e.preventDefault();
        if (typeof (window.sidebar && window.sidebar.addPanel) !== 'undefined') { // Mozilla Firefox Bookmark
            window.sidebar.addPanel(document.title, window.location.href, '');
        } else if (typeof (window.external && window.external.AddFavorite) !== 'undefined') { // IE Favorite
            window.external.AddFavorite(location.href, document.title);
        } else if (typeof (window.opera && window.print) !== 'undefined') { // Opera Hotlist
            this.title = document.title;
            return true;
        } else if (screen.width >= 768) {
            alert('Drücken Sie ' + (navigator.userAgent.toLowerCase().indexOf('mac') !== -1 ? 'Command/Cmd' : 'STRG') + ' + D um diese Seite zu favorisieren.');
        }
    });
})();

                                (function ($) {
    var BscArticleGallery = function (element, options) {

        var settings = $.extend({}, options),
            parent = $(element),
            bigImage = parent.find('.gallery-big-image'),
            preloadContainer = parent.find('.gallery-img-container'),
            previewSlides = parent.find('.gallery-preview-slider .picture-container').children('a'),
            galleryHashes = [],
            actionsRight = parent.find('.gallery-actions-right'),
            actionsLeft = parent.find('.gallery-actions-left'),
            arrowTimeout = null,
            imgViews = {},
            userLoggedIn = (actionsLeft.parent().data('uli') == 'true' || actionsLeft.parent().data('uli') == true),
            userCanVote = (actionsLeft.parent().data('ucv') == 'true' || actionsLeft.parent().data('ucv') == true),
            imageVotePositions = actionsLeft.parent().data('imagePositions'),
            ivwCanTrigger = false,
            galleryId = (preloadContainer.data('gallery') != undefined && preloadContainer.data('gallery') != '' ? parseInt(preloadContainer.data('gallery'), 10) : false),
            iconsBlock = bigImage.find('.article-icons-block'),
            plusIconsBlock = bigImage.find('.plus-article-icons-block');

        /**
         * aufrufbare Initialisierungsmethode, die das Hasharray füllt und die Eventhandler anhängt
         */
        this.init = function () {
            if (imageVotePositions == undefined) imageVotePositions = {};
            // Fülle das Hasharray neu mit den Hashes der sichtbaren Galeriebilder
            refillHashArray();
            // Wenn der reine Artikel aufgerufen wurde, hole den ersten Hash
            if (window.location.hash == '') {
                window.location.hash = getNextHash(window.location.hash);
            }
            // Gibt es bereits ein bestimmtes Bild, das angezeigt werden soll?
            hashHasChanged(window.location.hash);
            // Eventhandler auf hashchange und den Bilderwechsel
            initChangeListener();
            // Eventhandler für Voting und Views
            initVotingListener();
            // Initialisierungsmethode für Sortierung
            initSort();
            // Initialisierungsmethode für Suche
            initSearch();
        };

        /**
         * Generelle Initialisierungsmethode für die Eventhandler.
         * hashchange, swipe und click triggern jeweils den Bildwechsel.
         * Zusätzlich kann mit Hilfe der Pfeiltasten navigiert werden.
         */
        var initChangeListener = function () {
            // Eventlistener für den Hashchange, also den Klick auf ein anderes Bild
            $(window).on('hashchange', function () {
                hashHasChanged(window.location.hash);
            });

            // Click Eventhandler für Bildwechsel auf dem großen Bild
            bigImage.on('click', '> img', function () {
                // Setze Hash auf Hash des nächsten Bildes und triggere so den Bildwechsel
                ivwCanTrigger = true;
                window.location.hash = getNextHash(window.location.hash);
            });

            // Mit Hilfe der Pfeiltasten kann auch navigiert werden
            $(document).on('keydown', function (e) {
                switch (e.which) {
                    case 39:
                        // Setze Hash auf Hash des nächsten Bildes und triggere so den Bildwechsel
                        ivwCanTrigger = true;
                        window.location.hash = getNextHash(window.location.hash);
                        break;
                    case 37:
                        // Setze Hash auf Hash des vorherigen Bildes und triggere so den Bildwechsel
                        ivwCanTrigger = true;
                        window.location.hash = getPreviousHash(window.location.hash);
                        break;
                }
            });

            // auch mit den Pfeilen auf dem Bild
            bigImage.find('.icon-circle-arrow-left').on('click', function () {
                ivwCanTrigger = true;
                window.location.hash = getPreviousHash(window.location.hash);
            });
            bigImage.find('.icon-circle-arrow-right').on('click', function () {
                ivwCanTrigger = true;
                window.location.hash = getNextHash(window.location.hash);
            });

            // die Pfeile und die Social Media Bar müssen bei resize geschoben werden
            $(window).on('resize load', function () {
                var arrows = bigImage.find('.icon-circle-arrow-left, .icon-circle-arrow-right'),
                    newArrowTop = (bigImage.find('> img').height() / 2) - (arrows.first().height() / 2);
                arrows.css('top', newArrowTop);
                try {
                    var ssb = bigImage.find('.social-share-bar'),
                        newSsbTop = (bigImage.find('> img').height() - 63);
                    ssb.css('top', newSsbTop);
                } catch (e) {
                }

            });
            // Pfeilaktionen und Swipe Handler nur für Touchgeräte
            if (Modernizr.touch) {
                // die Pfeile sollen beim Scroll und Click wieder sichtbar werden
                $(window).on('scroll', function () {
                    clearTimeout(arrowTimeout);
                    bigImage.find('.icon-circle-arrow-left, .icon-circle-arrow-right').removeClass('hide-arrows');
                    arrowTimeout = setTimeout(hideArrows, 2000);
                });

                bigImage.off('click');
                bigImage.hammer().on('tap', '> img', function () {
                    ivwCanTrigger = true;
                    window.location.hash = getNextHash(window.location.hash);
                    try {
                        $('.social-share-bar').hide();
                    } catch (e) {
                    }
                });
                try {
                    bigImage.hammer().on('hold', '> img', function () {
                        $('.social-share-bar').show();
                    });
                    bigImage.get(0).addEventListener('contextmenu', function (e) {
                        e.preventDefault();
                    }, false);
                    bigImage.get(0).addEventListener("MSHoldVisual", function (e) {
                        e.preventDefault();
                    }, false);
                } catch (e) {
                }

                bigImage.find('.icon-circle-arrow-left, .icon-circle-arrow-right').on('click', function () {
                    clearTimeout(arrowTimeout);
                    bigImage.find('.icon-circle-arrow-left, .icon-circle-arrow-right').removeClass('hide-arrows');
                    arrowTimeout = setTimeout(hideArrows, 2000);
                });
                bigImage.hammer().on('swipeleft', '> img', function () {
                    // Setze Hash auf Hash des nächsten Bildes und triggere so den Bildwechsel
                    ivwCanTrigger = true;
                    window.location.hash = getNextHash(window.location.hash);
                }).on('swiperight', '> img', function () {
                    // Setze Hash auf Hash des vorherigen Bildes und triggere so den Bildwechsel
                    ivwCanTrigger = true;
                    window.location.hash = getPreviousHash(window.location.hash);
                });
            }
        };

        /**
         * Hilfsmethode um Arrows zu verstecken
         */
        var hideArrows = function () {
            bigImage.find('.icon-circle-arrow-left, .icon-circle-arrow-right').addClass('hide-arrows');
        };

        /**
         * Initialisierungsmethode für Votinghandler und Viewcounthandler
         */
        var initVotingListener = function () {
            // Handler für den Click auf das Sponsorlogo
            actionsRight.find('.gallery-sponsor-container a').on('click', function () {
                // Gibt es noch kein clicks Objekt, dann erstelle eines
                if (galleryId) {
                    if (imgViews.clicks == null || imgViews.clicks == "undefined" || imgViews.clicks.gallery != galleryId) {
                        imgViews.clicks = {gallery: galleryId, clicks: 0};
                    }
                    imgViews.clicks.clicks++;
                }
            });

            // Ajaxhandler für den Voteclick
            actionsLeft.find('#gallery-vote').on('click', function () {
                if (userCanVote) {
                    userCanVote = false;
                    $(this).nozAjax({
                        url: $(this).data('vote'),
                        data: {
                            fingerprint: typeof cookies.get != "undefined" && cookies.get('keyValFP') ? cookies.get('keyValFP') : localStorage.getItem('keyValFP'),
                            noCache: Math.random().toString(36).substring(7),
                            cache: false
                        }
                    }, function (result) {
                        if (result) {
                            result = JSON.parse(result);
                            var messageContainer = parent.find('.gallery-voting-hint');
                            switch (parseInt(result[1].value)) {
                                case 0:
                                    messageContainer.html("(Sie haben bereits abgestimmt)");
                                    break;
                                case 1:
                                    messageContainer.html("(Sie haben bereits abgestimmt - Sie können in 59 Minuten erneut abstimmen)");
                                    break;
                                default:
                                    messageContainer.html("(Sie haben bereits abgestimmt - Sie können in 23:59 Stunden erneut abstimmen)");
                                    break;
                            }
                            actionsLeft.find('#gallery-vote').attr('disabled', 'disabled').parent().next().css('float', 'none');
                        } else {
                            userCanVote = true;
                        }
                    });
                }
            });

            // Handler für das Auswahlfeld für "Weitere Informationen"
            parent.find('#textSelect').on("change", function () {
                // Zuerst alle Hinweistexte verstecken
                parent.find('#votingMessages').children().addClass('hidden');
                if ($(this).val() !== null && $(this).val() !== undefined && $(this).val() !== '') {
                    // den richtigen wieder anzeigen
                    parent.find('#votingMessages').children($(this).val()).removeClass('hidden');
                }
                parent.find('#textSelect').val('default');
            });

            // Handler für das Schließen eines Hinweistextes
            parent.find('#votingMessages .voting i').on("click", function () {
                $(this).parent().addClass('hidden');
            });

            // Handler auf unload Event, der die views ans BE schickt
            $(window).on('unload', function () {
                if (galleryId) {
                    $.ajax({
                        url: "/gallery/" + galleryId + "/imageviews",
                        type: "POST",
                        async: false,
                        data: {views: JSON.stringify(imgViews)},
                        complete: function () {
                            return;
                        }   //Notwendig um Browserkompatibilität zu gewährleisten
                    });
                }
            });
        };

        /**
         * Initialisiert die Eventhandler für die Suche / den Filter
         */
        var initSearch = function () {
            actionsLeft.find('#gallery-search-input').on('keyup', function () {
                //Zeige erstmal alle Vorschaubilder wieder
                previewSlides.css('display', 'block');
                //Prüfe, ob überhaupt zu suchen ist
                if ($(this).val() != undefined && $(this).val().trim() != '') {
                    //Sucheingabe nach Leerzeichen aufsplitten
                    var searchkeys = $(this).val().trim().split(' ');
                    previewSlides.each(function () {
                        var display = false;
                        for (var index = 0; index < searchkeys.length; index++) {
                            if ($(this).data('searchValue').toLowerCase().indexOf(searchkeys[index].toLowerCase()) > -1) {
                                display = true;
                            }
                        }
                        if (!display) $(this).css('display', 'none');
                    });
                }
                // Die Abstände im Slider wieder aktualisieren
                $('.content-slider').trigger('reinitialize');

                // Fülle das Hasharray neu mit den Hashes der sichtbaren Galeriebilder
                refillHashArray();
            });
        };

        /**
         * Initialisiert die Eventhandler für den Sortierdropdown
         */
        var initSort = function () {
            actionsLeft.find('#gallery-sort-select').on('change', function () {
                // Welche sortiermethode wird benötigt
                switch ($(this).val()) {
                    case 'date':
                        previewSlides.sort(orderByDate);
                        break;
                    case 'vote':
                        previewSlides.sort(orderByVote);
                        break;
                    default :
                        previewSlides.sort(orderByHash);
                        break;
                }

                // Fülle das Hasharray neu mit den Hashes der sichtbaren Galeriebilder
                refillHashArray();
                // Vorschaubilder umsortieren
                previewSlides.each(function () {
                    var tmpParent = $(this).parent();
                    tmpParent.append($(this).detach());
                });
                previewSlides = parent.find('.gallery-preview-slider .picture-container').children('a');
                // Die Abstände im Slider wieder aktualisieren
                $('.content-slider').trigger('reinitialize');
            });
        };

        /**
         * Sortiermethode für Sortierung nach Hash
         * @param a erstes Element
         * @param b zweites Element
         * @returns {number} Ergebnis (0 wenn gleich, -1 wenn b größer, 1 wenn a größer)
         */
        var orderByHash = function (a, b) {
            if ($(a).attr('href') > $(b).attr('href')) return 1;
            if ($(a).attr('href') < $(b).attr('href')) return -1;
            return 0;
        };

        /**
         * Sortiermethode für Sortierung nach Datum (Absteigend)
         * @param a erstes Element
         * @param b zweites Element
         * @returns {number} Ergebnis (0 wenn gleich, 1 wenn b größer, -1 wenn a größer)
         */
        var orderByDate = function (a, b) {
            if ($(a).data('imgDate') > $(b).data('imgDate')) return -1;
            if ($(a).data('imgDate') < $(b).data('imgDate')) return 1;
            return 0;
        };

        /**
         * Sortiermethode für Sortierung nach Vote
         * @param a erstes Element
         * @param b zweites Element
         * @returns {number} Ergebnis (0 wenn gleich, -1 wenn b größer, 1 wenn a größer)
         */
        var orderByVote = function (a, b) {
            var valA = imageVotePositions[parseInt($(a).data('imgId'))],
                valB = imageVotePositions[parseInt($(b).data('imgId'))];
            if (typeof valA != 'undefined' && typeof valA['position'] != 'undefined') {
                valA = parseInt(valA['position']);
            } else {
                valA = 999999;
            }
            if (typeof valB != 'undefined' && typeof valB['position'] != 'undefined') {
                valB = parseInt(valB['position']);
            } else {
                valB = 999999;
            }
            if (isNaN(valA) && isNaN(valB)) return 0;
            if (isNaN(valA)) return 1;
            if (isNaN(valB)) return -1;
            if (valA > valB) return 1;
            if (valA < valB) return -1;
            return 0;
        };

        /**
         * Liefert den Hash des nachfolgenden Bildes zu einem gegebenen Hash
         * @param oldHash Hash des aktuellen Bildes
         * @returns String Hash des nachfolgenden Bildes
         */
        var getNextHash = function (oldHash) {
            // Suche nach altem Hash im Hasharray
            var hashFoundPos = $.inArray(oldHash, galleryHashes);
            if (hashFoundPos > -1) {
                // Wenn es einen nachfolgenden Hash gibt, gebe diesen zurück. Ansonsten gibt den ersten Hash zurück.
                return (galleryHashes.length > hashFoundPos + 1 ? galleryHashes[hashFoundPos + 1] : galleryHashes[0]);
            }
            // Falls kein Hash gefunden
            return galleryHashes[0];
        };

        /**
         * Liefert den Hash des vorhergehenden Bildes zu einem gegebenen Hash
         * @param oldHash Hash des aktuellen Bildes
         * @returns String Hash des vorhergehenden Bildes
         */
        var getPreviousHash = function (oldHash) {
            // Suche nach altem Hash im Hasharray
            var hashFoundPos = $.inArray(oldHash, galleryHashes);
            if (hashFoundPos > -1) {
                // Wenn es einen vorhergehenden Hash gibt, gebe diesen zurück. Ansonsten gibt den letzten Hash zurück.
                return (hashFoundPos - 1 < 0 ? galleryHashes[galleryHashes.length - 1] : galleryHashes[hashFoundPos - 1]);
            }
            // Falls kein Hash gefunden
            return galleryHashes[galleryHashes.length - 1];
        };

        /**
         * Handlermethode für das Hashchange Event
         * Triggert den Bild- und Textwechsel und erstellt gegebenenfalls img-tags, falls Bilder noch nicht vorgeladen waren.
         * @param newHash Der neue Hash, zu dem das passende Bild gesucht werden soll
         */
        var hashHasChanged = function (newHash) {
            // Dataattribute neu holen, da sie sich durch ESI eventuell verändert haben
            //userLoggedIn = (actionsLeft.parent().data('uli') == 'true' || actionsLeft.parent().data('uli') == true);
            //userCanVote = (actionsLeft.parent().data('ucv') == 'true' || actionsLeft.parent().data('ucv') == true);

            // Suche nach neuem Hash im Hasharray
            var img = previewSlides.filter(function () {
                return ($(this).attr('href') == newHash);
            }).eq(0);
            if (img.length == 1) {
                // TODO Icons verstecken oder zeigen
                var imgPos = previewSlides.index(img);
                if(imgPos <= 0) {
                    iconsBlock.show();
                    plusIconsBlock.show();
                } else {
                    iconsBlock.hide();
                    plusIconsBlock.hide();
                }
                // Bild vorladen
                var preImg = preloadImg(img);//Reload all share buttons

                try {

                    if (Modernizr.touch) {
                        $('.social-share-bar').hide();
                    }

                    img = previewSlides.filter(function () {
                        return ($(this).attr('href') == newHash);
                    }).eq(0);
                    var shareCaption = img.data('imgCaption') != '' ? img.data('imgCaption') : $('.social-share-bar').data('description'),
                        shareURL = 'https://' + window.location.hostname + window.location.pathname,
                        shareInfos = img.data('shareInfos');
                    if(shareInfos != undefined && shareInfos != '') {
                        shareURL = 'https://' + window.location.hostname + '/voting/' + btoa(shareInfos);
                    }
                    shareURL = shareURL + '?utm_medium=Link&utm_campaign=Artikel_teilen';
                    $('.social-share-bar .facebook-container a').attr('href', 'https://www.facebook.com/sharer/sharer.php?u=' + encodeURIComponent(shareURL + '&utm_source=Facebook'));
                    $('.social-share-bar .twitter-container a').attr('href', 'https://twitter.com/intent/tweet?text=' + encodeURIComponent(shareCaption) + '&url=' + encodeURIComponent(shareURL + '&utm_source=Twitter') + '&original_referer=' + encodeURIComponent(shareURL + '&utm_source=Twitter'));
                    $('.social-share-bar .whatsapp-container a').attr('href', 'whatsapp://send?text=' + encodeURIComponent(shareCaption + ': ' + shareURL + '&utm_source=WhatsApp'));
                } catch (e) {
                }
                // Benachbarte Bilder schon einmal vorladen, um bessere UX zu bieten
                preloadAdjacentPictures(newHash);

                // Bild austauschen
                preloadContainer.append(bigImage.find('> img').detach());
                bigImage.prepend(preImg);
                // Text austauschen
                bigImage.find('.img-titel').html(img.data('imgCaption'));

                // Platzierung der Bilder anpassen
                var posMessage = "Derzeit noch keine Platzierung";

                var curPos = imageVotePositions[parseInt(img.data('imgId'))];
                if (typeof curPos != 'undefined' && typeof curPos['position'] != 'undefined') {
                    curPos = parseInt(curPos['position']);
                } else {
                    curPos = 0;
                }
                if (curPos > 0) {
                    posMessage = "Derzeit " + curPos + ". Platz";
                }
                actionsLeft.find('.gallery-ranking').html(posMessage);

                // Views im Objekt speichern um sie später abschicken zu können
                if (img.data('imgId') != null && imgViews['' + img.data('imgId')] !== undefined) {
                    imgViews['' + img.data('imgId')]++;
                } else if (imgViews['' + img.data('imgId')] == undefined) {
                    imgViews['' + img.data('imgId')] = 1;
                }

                // nur Vorschau auf active setzen
                previewSlides.removeClass('active-preview');
                img.addClass('active-preview');

                // Trackingcode
                triggerIvw();
                //_gaq.push(['_setCustomVar', 5, 'contentype', 'Galerie_ '+$('.ivw-tag').data('ivwcode')+'_ '+preloadContainer.data('createdat')+'_ ', 3]);
                //_gaq.push(['_trackPageview']);

                checkVoteButton(img);
            }
        };

        /**
         * Läd ein Bild vor, sofern es noch nicht geladen wurde
         * @param img Die Vorschau des zu ladenenen Bildes als jQuery-Objekt
         * @returns {*} jQuery Object des vorgeladenen Bildes
         */
        var preloadImg = function (img) {
            if (img.length == 1) {
                // Bild bereits vorgeladen?
                var imgId = img.data('imgId');
                var preloadImg = $.merge($.merge([], preloadContainer.children('.gallery-img-num-' + imgId)), bigImage.children('.gallery-img-num-' + imgId));
                if (preloadImg.length == 0) {
                    var imgCaption = (typeof img.data('imgCaption') != 'undefined' && img.data('imgCaption') != '' ? img.data('imgCaption') : img.find('img').attr('alt'));
                    preloadImg = $('<img class="gallery-img-num-' + imgId + '" src="' + img.data('imgSrc') + '" alt="' + imgCaption + '">');
                    preloadContainer.append(preloadImg);
                }
                return preloadImg;
            }
        };

        /**
         * Prüft, ob der Votebutton aktiviert sein darf
         * @param img
         */
        var checkVoteButton = function (img) {
            // erstmal generell deaktivieren
            actionsLeft.find('#gallery-vote').prop('disabled', 'disabled');

            if (img.data('imgVote') && img.data('imgVote') != "" && userCanVote && userLoggedIn) {
                actionsLeft.find('#gallery-vote').prop('disabled', false).data('vote', img.data('imgVote')).parent().next().css('float', 'right');
            } else {
                actionsLeft.find('#gallery-vote').parent().next().css('float', 'none');
            }
        };

        /**
         * Triggert den IVW trackingcode
         */
        var triggerIvw = function () {
            if(ivwCanTrigger) {
                //Bei mobil müssten wir andere Werte mitschicken
                if (Modernizr.touch && Modernizr.mq('screen and (min-device-width: 768px)')) {
                    if (typeof iam_data !== 'undefined') {
                        var local_iam_data = iam_data;
                        local_iam_data['cp'] = "mobil_galerie_" + $('.ivw-tag').data('ivwcode');
                    }

                    if (typeof iam_data !== 'undefined') {
                        if (typeof iom !== 'undefined') {
                            iom.c(local_iam_data, 2);
                        }
                    }
                } else {
                    if (typeof iam_data !== 'undefined') {
                        if (typeof iom !== 'undefined') {
                            iom.c(iam_data, 2);
                        }
                    }
                }
            }
        };

        /**
         * Erstellt für die umliegenden Bilder schon einmal IMG-Tags, damit die Bilder vorgeladen werden
         * und wir kein flackern beim Bildwechsel mehr haben
         * @param hash hash des aktuellen Bildes
         */
        var preloadAdjacentPictures = function (hash) {
            // Umliegende Hashes ermitteln
            var prevHash = getPreviousHash(hash),
                nextHash = getNextHash(hash),
                prevImg = previewSlides.filter(function () {
                    return ($(this).attr('href') == prevHash);
                }).eq(0),
                nextImg = previewSlides.filter(function () {
                    return ($(this).attr('href') == nextHash);
                }).eq(0);
            preloadImg(prevImg);
            preloadImg(nextImg);
        };

        /**
         * Befüllt das Hasharray neu mit den sichtbaren Vorschaubildern
         */
        var refillHashArray = function () {
            galleryHashes = [];
            previewSlides.filter(function () {
                return $(this).css('display') != 'none';
            }).each(function (index, element) {
                galleryHashes[index] = $(element).attr('href');
            });
        }
    };

    $.fn.bscarticlegallery = function (options) {
        return this.each(function () {
            var element = $(this);

            // Wenn es bereits eine Instanz gibt, wird keine weitere erstellt
            if (element.data('bscarticlegallery')) {
                return;
            }

            // Instanz erstellen und Optionen übergeben
            var plugin = new BscArticleGallery(this, options);

            // Plugin im data-Attribute speichern
            element.data('bscarticlegallery', plugin);
        });
    };
})($);

$(function () {
    var gallery = $('.gallery-parent-container');
    if (gallery.length > 0) {
        for (var i = 0; i < gallery.length; i++) {
            $(gallery[i]).bscarticlegallery();
            var galleryEntity = $(gallery[i]).data('bscarticlegallery');
            galleryEntity.init();
        }
    }
});

                                $(function () {
    var currentAnimationFrameHandler = null;
    var articleDialogId = $('#article[data-article-dialog-id]').data('articleDialogId');

    $(window).on('load nozLoad scroll resize', function () {
        if (!currentAnimationFrameHandler) {
            currentAnimationFrameHandler = window.requestAnimationFrame(checkInlineGalleries);
        }
    });

    var checkInlineGalleries = function () {
        var inlineGalleries = $('.inline-gallery:not(.loaded)');
        inlineGalleries.each(function () {
            var gallery = $(this);
            if (nozUtils.isElementInViewport(gallery[0])) {
                $.ajax({
                    url: '/gallery/inline/load/' + gallery.data('galleryId') + '/' + articleDialogId,
                    method: 'GET',
                    success: function (data) {
                        gallery.html(data);
                        gallery.find('.content-slider').contentSlider({mousewheel: false});
                    },
                    error: function () {
                        console.error('Galerie ' + gallery.data('galleryId') + ' konnte nicht als inline-Galerie geladen werden und wird aus dem DOM entfernt.');
                        gallery.remove()
                    }
                });
                gallery.addClass('loaded');
            }
        });
        currentAnimationFrameHandler = null;
    };
    var resizeTimeout = false;
    $(window).on('resize', function () {
        if (resizeTimeout) {
            window.clearTimeout(resizeTimeout);
        }
        resizeTimeout = window.setTimeout(function () {
            $('.inline-gallery.loaded').find('.content-slider').trigger('reinitialize');
        }, 300);
    });
});
                                (function () {
    var initArticleViews = function () {
        var articleViews = document.getElementById('article-views');
        if (articleViews) {
            var id = articleViews.getAttribute('data-article-views'),
                articleViewsStorage = localStorage.getItem('articleViews');
            if (!articleViewsStorage) {
                articleViewsStorage = [];
            } else {
                articleViewsStorage = JSON.parse(articleViewsStorage);
            }
            if (!nozUtils.inArray(articleViewsStorage, id)) {
                if (!localStorage.getItem('articleViewsDate')) {
                    var date = new Date();
                    localStorage.setItem('articleViewsDate', date.toJSON());
                }
                articleViewsStorage.push(id);
                localStorage.setItem('articleViews', JSON.stringify(articleViewsStorage));
            }
        }
    };

    var initMecButton = function () {
        var editorBtns = $('#mec-button, #ce-edit-button');
        if (!nozUtils.isMobile() && editorBtns.length > 0) {
            var userInfo = JSON.parse(userInformation);
            if (userInfo.type === 'editor') {
                editorBtns.show();
            }
        }
    };

    initArticleViews();
    initMecButton();
})();
                                (function () {
    var updateDateFunction = function(elem) {
        var dateHolder = elem.querySelector('span.elapsed-date'),
            date = new Date(elem.getAttribute('data-date-elapsed')),
            now = new Date(),
            elapsed = (now.getTime() - date.getTime()) / 1000,
            dateString = '';

        if(dateHolder === null) {
            elem.innerHTML = elem.innerHTML + '<span class="elapsed-date"></span>';
            dateHolder = elem.querySelector('span.elapsed-date');
        }

        if (elapsed <= 60) {
            dateString =  Math.floor(elapsed) + ' Sekunde' + (Math.floor(elapsed) > 1 ? 'n' : '');
        } else if (elapsed <= 60 * 60) {
            dateString =  Math.floor(elapsed / 60) + ' Minute' + (Math.floor(elapsed / 60) > 1 ? 'n' : '');
        } else if (elapsed <= 60 * 60 * 24) {
            dateString =  Math.floor(elapsed / (60 * 60)) + ' Stunde' + (Math.floor(elapsed / (60 * 60)) > 1 ? 'n' : '');
        } else {
            dateHolder.parentNode.childNodes[0].nodeValue = dateHolder.parentNode.childNodes[0].nodeValue.replace("vor", "am");
            dateString = date.getDate() + '.' + (date.getMonth()+1) + '.' + date.getFullYear();
        }
        //else if (elapsed <= 60 * 60 * 24 * 30) {
        //    dateString =  Math.floor(elapsed / (60 * 60 * 24)) + ' Tag' + (Math.floor(elapsed / (60 * 60 * 24)) > 1 ? 'en' : '');
        //} else if (elapsed <= 60 * 60 * 24 * 365) {
        //    dateString =  Math.floor(elapsed / (60 * 60 * 24 * 30)) + ' Monat' + (Math.floor(elapsed / (60 * 60 * 24 * 30)) > 1 ? 'en' : '');
        //} else if (elapsed > 60 * 60 * 24 * 365) {
        //    dateString =  Math.floor(elapsed / (60 * 60 * 24 * 365)) + ' Jahr' + (Math.floor(elapsed / (60 * 60 * 24 * 365)) > 1 ? 'en' : '');
        //}
        dateHolder.innerHTML = dateString;
    };

    var dateElems = document.querySelectorAll('[data-date-elapsed]');
    for(var i = 0; i < dateElems.length; i++) {
        updateDateFunction(dateElems[i]);
    }
})();
                                //# sourceURL=http://NOZBasketScripts/nonCoreJS/outbrain.js
$(function () {
    var outbrainContainer = $('.outbrain-container');
    var body = $('body');
    var initializedWidget = false;
    var resizeBlocker = false;
    var supportsAnimationFrame = (window.requestAnimationFrame && window.cancelAnimationFrame);

    var checkMobile = function () {
        var mq = window.matchMedia('(max-width: 767px)');
        if (mq && typeof mq.matches !== 'undefined') {
            mq = mq.matches;
        }
        return mq;
    };

    var addOutbrainCode = function (widgetCode) {
        if (widgetCode.indexOf('AR_') === 0) {
            $('<div>').attr({
                "data-src": $('link[rel="canonical"]').attr("href"),
                "data-widget-id": 'AR_5',
                "data-ob-template": 'DE_noz.de'
            }).addClass('OUTBRAIN').appendTo(outbrainContainer);
        }
        $('<div>').attr({
            "data-src": $('link[rel="canonical"]').attr("href"),
            "data-widget-id": widgetCode,
            "data-ob-template": 'DE_noz.de'
        }).addClass('OUTBRAIN').appendTo(outbrainContainer);

        $('<script>').attr({
            type: 'text/javascript',
            async: true,
            src: 'https://widgets.outbrain.com/outbrain.js'
        }).appendTo(outbrainContainer);
    };

    var initOutbrain = function () {
        resizeBlocker = false;
        var isMobile = checkMobile();

        if (initializedWidget) {
            if ((initializedWidget === 'mobile' && isMobile) || (initializedWidget === 'desktop' && !isMobile)) {
                outbrainContainer.show();
            } else {
                outbrainContainer.hide();
            }
        } else {
            var articleContainer = $('#article'),
                isPaywallHit = articleContainer.data('articleIsHitPaywall') && articleContainer.data('articleIsHitPaywall') === true,
                isPlusArticle = articleContainer.data('articleIsPlus') && articleContainer.data('articleIsPlus') === true,
                isNonCompliantTrigger = $('[data-is-outbrain-non-compliant="true"]').length > 0,
                isForceCompliant = outbrainContainer.data('forceCompliant'),
                widgetCode = 'AR_11',
                consoleString = 'Init Outbrain-Widget mit',
                widgetCodeKey = "11",
                plusWidgetCodeKey = "12";

            if (isPaywallHit && isPlusArticle) {
                if (isMobile) {
                    consoleString += ' Mobile';
                    widgetCode = 'MB_' + plusWidgetCodeKey;
                } else {
                    consoleString += ' Desktop';
                    widgetCode = 'AR_' + plusWidgetCodeKey;
                }
                consoleString += ' Non-Compliant-Code';
            } else if (isPaywallHit || isNonCompliantTrigger && !isForceCompliant) {
                widgetCodeKey = $(outbrainContainer).data('widget-code-non-compliant');
                if (isMobile) {
                    consoleString += ' Mobile';
                    widgetCode = 'MB_' + widgetCodeKey;
                } else {
                    consoleString += ' Desktop';
                    widgetCode = 'AR_' + widgetCodeKey;
                }
                consoleString += ' Non-Compliant-Code';
            } else {
                widgetCodeKey = $(outbrainContainer).data('widget-code-compliant');
                if (isMobile) {
                    consoleString += ' Mobile';
                    widgetCode = 'MB_' + widgetCodeKey;
                } else {
                    consoleString += ' Desktop';
                    widgetCode = 'AR_' + widgetCodeKey;
                }
                consoleString += ' Compliant-Code';
            }
            addOutbrainCode(widgetCode);
            initializedWidget = (isMobile ? 'mobile' : 'desktop');

            $(window).on('resize', function () {
                if (resizeBlocker) {
                    if (supportsAnimationFrame) {
                        window.cancelAnimationFrame(resizeBlocker);
                    } else {
                        window.clearTimeout(resizeBlocker);
                    }
                }

                if (supportsAnimationFrame) {
                    window.requestAnimationFrame(initOutbrain);
                } else {
                    window.setTimeout(initOutbrain, 200);
                }
            });
            console.log(consoleString);
        }
    };

    if (outbrainContainer.length > 0) {
        if (body.data('paywallStatus') && body.data('paywallStatus') === 'ready') {
            initOutbrain();
        } else {
            body.on('paywallStatusReady', initOutbrain);
        }
    }
});

                                $(function () {
    var personalNewsButton = $('.right-header .personal-news-button');
    var outputChannelSectionId = personalNewsButton.data('output-channel-section-id');
    var addSectionsToUserPath = personalNewsButton.data('path-section-add');
    var outputChannelSectionCheckout = personalNewsButton.data('path-output-channel-section-checkout');
    var userId = personalNewsButton.data('user-id');
    var content = personalNewsButton.data('html-content');

    if (outputChannelSectionId) {
        var showPopover = false;
        var ajaxCallFinished = false;

        personalNewsButton.popover({
            content: content,
            html: true,
            placement: 'bottom',
            trigger: 'manual'
        });

        var addListener = function() {
            $('.right-header .personal-news-notification-add-button').on('click', function(event) {
                event.preventDefault();

                if (userId) {
                    $('.personal-news-notification-content').nozAjax({
                        type: 'GET',
                        url:  addSectionsToUserPath,
                        data: {
                            'originOutputChannelSectionId': outputChannelSectionId,
                            'sectionIds': outputChannelSectionId,
                        }
                    }, function(data) {
                        if (data.status === "success") {
                            $('.personal-news-notification-content').hide();
                            $('.personal-news-notification-content-added').show();
                            $('.personal-news-popover').addClass('content200').removeClass('content150');
                        }
                    });
                } else {
                    $('.personal-news-notification-add-button').hide();
                    $('.personal-news-notification-content-login').show();
                    $('.personal-news-popover').addClass('content490').removeClass('content150');
                }
            });

            $('body, html').on('click touchstart', function(e) {
                if (e.originalEvent && !$(e.originalEvent.target).hasClass('personal-news-popover') && !$(e.originalEvent.target).hasClass('popover') && $(e.originalEvent.target).parents('.personal-news-popover, .popover').length == 0) {
                    personalNewsButton.popover('destroy');
                }
            });
        };

        personalNewsButton.nozAjax({
            type: 'GET',
            url:  outputChannelSectionCheckout,
            data: {
                'sectionId': outputChannelSectionId,
            }
        }, function(data) {
            ajaxCallFinished = true;

            if (data.status === "success" && data.checked !== true) {
                showPopover = true;
            }
        });

        var showPersonalNewsPopover = function() {
            if (!ajaxCallFinished) {
                setTimeout(showPersonalNewsPopover, 2000);
                return;
            }

            if (showPopover) {
                personalNewsButton.popover('show');
                addListener();
            }
        };

        // add an event to trigger the popover,
        // trigger with $('.right-header .personal-news-button').trigger('popover:show');
        personalNewsButton.on('popover:show', function () {
            showPersonalNewsPopover();
        });
    }
});
